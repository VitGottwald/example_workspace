*%GOTO ACEEPLS;                     /* BILINGUAL HEADER                 01410000
         MACRO                                                          01420000
         IHAACEE                                                        01430000
ACEE     DSECT  ,ACEEPTR            ACCESSOR ENVIRONMENT ELEMENT        01440000
ACEEACEE DS    CL4                  ACRONYM IN EBCDIC -ACEE-            01450000
ACEECORE DS    0F                   ACEE SUBPOOL AND LENGTH             01460000
ACEESP   DS    AL1                  ACEE SUBPOOL NUMBER                 01470000
ACEELEN  DS    AL3                  LENGTH OF ACEE                      01480000
ACEEVRSN DS    AL1                  ACEE VERSION NUMBER          @L4A   01490000
ACEEVR01 EQU   1                    ACEE VERSION NUMBER = 1.     @M1A   01500000
ACEEVR02 EQU   2                    ACEE VERSION NUMBER = 2.     @M1A   01510000
ACEEVR03 EQU   3                    ACEE VERSION NUMBER = 3.     @LAA   01520000
ACEECURV EQU   3                    ACEE CURRENT VERSION NUMBER  @LAC   01530000
ACEESBVR DS    CL3                  ACEE SUB-Version             @LJC   01530015
ACEEIEP  DS    A                    RESERVED FOR INSTALLATION.          01550000
*                                   IF USED, IT MUST POINT TO A         01560000
*                                   ONE BYTE SUBPOOL FOLLOWED BY        01570000
*                                   A THREE BYTE LENGTH.                01580000
ACEEINST DS    A                    USER DATA ADDRESS:  POINTS TO  @02C 01590000
*                                   A 1-BYTE LENGTH FIELD FOLLOWED @02C 01600000
*                                   BY THE INSTALLATION DATA       @02A 01610000
*                                   SPECIFIED IN THE USER PROFILE. @02A 01620000
*                                   THE LENGTH INCLUDES THE 1-BYTE @02A 01630000
*                                   LENGTH FIELD.  THE ADDRESS IS  @02A 01640000
*                                   ZERO IF (1) NO VALID USERID    @02A 01650000
*                                   WAS PROVIDED OR (2) NO DATA    @02A 01660000
*                                   WAS PRESENT IN THE PROFILE.    @02A 01670000
ACEEUSER DS    0CL9                 USERID INFORMATION                  01680000
ACEEUSRL DS    AL1                  USERID LENGTH                       01690000
ACEEUSRI DS    CL8                  CONTAINS THE VALID RACF USERID @02C 01700000
*                                   UNLESS (1) THE USERID ON THE   @02C 01710000
*                                   VERIFY CALL WAS '*BYPASS*' FOR @02A 01720000
*                                   AUDITABLE WORK THAT BYPASSES   @02A 01730000
*                                   AUTHORIZATION CHECKING, OR     @02A 01740000
*                                   (2) NO USERID WAS GIVEN SO THE @02A 01750000
*                                   FIELD CONTAINS AN '*'.         @02A 01760000
ACEEGRP  DS    0CL9                 GROUP NAME INFORMATION              01770000
ACEEGRPL DS    AL1                  GROUP NAME LENGTH                   01780000
ACEEGRPN DS    CL8                  VALID CONNECT GROUP, UNLESS    @02C 01790000
*                                   ACEEUSRI IS '*BYPASS*' OR '*'. @02A 01800000
*                                   FOR THESE TWO CASES ACEEGRPN   @02A 01810000
*                                   WILL BE '*'.                   @02A 01820000
ACEEFLG1 DS    B                    USER FLAGS                          01830000
ACEESPEC EQU   X'80'                1 - SPECIAL ATTRIBUTE               01840000
ACEEADSP EQU   X'40'                1 - AUTOMATIC DATA SECURITY         01850000
*                                   PROTECTION                 @PC002PY 01860000
ACEEOPER EQU   X'20'                1 - OPERATIONS ATTRIBUTE            01870000
ACEEAUDT EQU   X'10'                1 - AUDITOR ATTRIBUTE      @PC002PY 01880000
ACEELOGU EQU   X'08'                1 - USER IS TO HAVE MOST RACF       01890000
*                                       FUNCTIONS LOGGED       @PC002PY 01900000
ACEEROA  EQU   X'04'                1 - READ-ONLY AUDITOR               01910000
*                                       ATTRIBUTE                  @LHA 01910100
ACEEPRIV EQU   X'02'                1 - USER IS A STARTED PROCEDURE     01920000
