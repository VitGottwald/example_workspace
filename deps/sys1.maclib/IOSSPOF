.***AL.L12 IOSSPOF FMT=LIST MF=S!L PARAMETERS                           00491000
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00491100
.*                                                                      00491200
&ZOPN                 SETA  &ZOPN+1                                     00491300
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00491400
&ZOPO(&ZOPN)          SETC  'DC'                                        00491500
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00491600
&ZOPD(&ZOPN)          SETC  '++ IOSSPOF ARG PARMS'                      00491700
.*                                                                      00491800
.ZLMFSLX              ANOP  ,                                           00491900
.*-------------------------------------------------------------------   00492000
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00492100
&ZPLLEN               SETA 170                                          00492200
.*                                                                      00492300
.******************************************************************     00492400
.***AI.L01 IOSSPOF FMT=LIST FILL IN PARAMETER AREA                      00492500
.******************************************************************     00492600
                      ACTR  10000                                       00492700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00492800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00492900
&ZCVAL                SETC  'LAE'          XAX MODE                     00493000
.ZIMFXA               ANOP ,                                            00493100
.*                                                                      00493200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00493300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00493400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00493500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00493600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00493700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00493800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00493900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00494000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00494100
&ZOPN                 SETA  &ZOPN+1                                     00494200
&ZOPL(&ZOPN)          SETC  ' '                                         00494300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00494400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00494500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00494600
                      AGO   .ZIMFNX                                     00494700
.*                                                                      00494800
.ZIMFN2               ANOP ,                                            00494900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00495000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00495100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00495200
&ZOPN                 SETA  &ZOPN+1                                     00495300
&ZOPL(&ZOPN)          SETC  ' '                                         00495400
&ZOPO(&ZOPN)          SETC  'LAE'                                       00495500
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00495600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00495700
                      AGO   .ZIMFNX                                     00495800
.ZIMFNX1              ANOP ,                                            00495900
&ZOPN                 SETA  &ZOPN+1                                     00496000
&ZOPL(&ZOPN)          SETC  ' '                                         00496100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00496200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00496300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00496400
.ZIMFNX               ANOP ,                                            00496500
.********************************************************************   00496600
.***AI.L02 IOSSPOF FMT=LIST INITIALIZE PARAMETER AREA                   00496700
.********************************************************************   00496800
