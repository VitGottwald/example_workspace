.***AC.PLV01  PROCESS PLISTVER                                      *   00111300
.********************************************************************   00111400
&XPLISTVER      SETA  1                                                 00111500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00111600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00111700
&XPLISTVER      SETA  1                                                 00111800
.ZPLV0          ANOP  ,                                                 00111900
.*                                                                      00112000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00112100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00112200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00112300
.*-------------------------------------------------------------------   00112400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00112500
&ZMS            SETC  '"PLISTVER='                                      00112600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00112700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00112800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00112900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00113000
                AGO   .ZPVCK2                                           00113100
.ZPVCK3         ANOP  ,                                                 00113200
.*-------------------------------------------------------------------   00113300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00113400
&ZMS            SETC  '"PLISTVER='                                      00113500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00113600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00113700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00113800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00113900
                AGO   .ZPVCKE                                           00114000
.ZPVCK2         ANOP  ,                                                 00114100
.*-------------------------------------------------------------------   00114200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00114300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00114400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00114500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00114600
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00114700
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00114800
.ZPVCK6         ANOP  ,                                                 00114900
&ZMS            SETC  '"PLISTVER='                                      00115000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00115100
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00115200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00115300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00115400
&XPLISTVER      SETA  1                                                 00115500
                AGO   .ZPVCKE                                           00115600
.*-------------------------------------------------------------------   00115700
.ZPVCK4A        ANOP  ,                                                 00115800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00115900
&ZMS            SETC  'PLISTVER='                                       00116000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00116100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00116200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00116300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00116400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00116500
                AGO   .ZPVCK5                                           00116600
.ZPVCK4         ANOP  ,                                                 00116700
&XPLISTVER      SETA  &PLISTVER(1)                                      00116800
.ZPVCK5         ANOP  ,                                                 00116900
.ZPVCKE         ANOP  ,                                                 00117000
.********************************************************************   00117100
