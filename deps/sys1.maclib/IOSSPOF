*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00605200
*%IOSSPOF: MACRO KEYS(PERFORM_CHECK,DEVN1,SCHSET1,DSN1,DEVN2,SCHSET2,   00605300
*                 DSN2,VOLSER1,VOLSER2,DEVLIST,DEVCOUNT,DSNLIST,        00605400
*                 SCHSETSPEC,VOLLIST,VOLCOUNT,SPOFAREA,HCMSG,HANDLE,    00605500
*                 REXX,WTO,IND_CHECKS,SWITCH_CHECKS,CU_CHECKS,RETCODE,  00605600
*                 RSNCODE,PLISTVER,MF);                                 00605700
*  ANS('?'!!MACLABEL!!' '!!'IOSSPOFP '!!MACLIST!!                       00605800
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00605900
*%END IOSSPOF;                                                          00606000
