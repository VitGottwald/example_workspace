&ZOPV(&ZOPN)          SETC  '94(44,1),&ZCGPR'                           00579000
&ZOPD(&ZOPN)          SETC  '++ &ZXDSN2 XDSN2'                          00579100
.*                                                                      00579200
.ZI43E                ANOP  ,                                           00579300
.*-------------------------------------------------------------------   00579400
.***AI.L18  XHANDLE CL16                                                00579500
         AIF   ('&XHANDLE' NE '*').ZI44                                 00579600
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZI44  CK *        00579700
&ZXHANDLE             SETC  ''             RESET INPUT/OUTPUT           00579800
.ZI44                 ANOP  ,                                           00579900
.*-------------------------------------------------------------------   00580000
.***AI.L25  XHANDLE CL16                                                00580100
                      AIF   ('&ZXHANDLE' EQ '').ZI44E                   00580200
                      AIF   ('&ZXHANDLE' EQ 'OUTPUT').ZI44E             00580300
&ZCGPR                SETC  '&XHANDLE'                                  00580400
         AIF   ('&XHANDLE' NE '*').ZI44A                                00580500
         AIF   ('&XMFSCK' EQ 'COMPLETE').ZI44E                          00580600
.*                                                                      00580700
&ZCGPR                SETC  '=XL16''00'''                               00580800
.ZI44A                ANOP  ,                                           00580900
.*                                                                      00581000
                      AIF   ('&XHANDLE'(1,1) NE '(').ZI44B              00581100
&ZCGPR   SETC  '0('.'&XHANDLE'(2,K'&XHANDLE-2).')'                      00581200
.ZI44B                ANOP  ,                                           00581300
.*                                                                      00581400
&ZOPN                 SETA  &ZOPN+1                                     00581500
&ZOPL(&ZOPN)          SETC  ' '                                         00581600
&ZOPO(&ZOPN)          SETC  'MVC'                                       00581700
&ZOPV(&ZOPN)          SETC  '138(16,1),&ZCGPR'                          00581800
&ZOPD(&ZOPN)          SETC  '++ &ZXHANDLE XHANDLE'                      00581900
.*                                                                      00582000
.ZI44E                ANOP  ,                                           00582100
.*-------------------------------------------------------------------   00582200
&ZOPN1                SETA &ZOPN                                        00582300
.*-------------------------------------------------------------------   00582400
.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00582500
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00582600
.*******************************************************************    00582700
.***  PROCESS IOSSPOF OUTPUT PARAMETERS                                 00582800
.*******************************************************************    00582900
                      ACTR  10000                                       00583000
.*------------------------------------------------------------------    00583100
.***AO.L02 XSPOFAREA A                                                  00583200
         AIF   ('&ZXSPOFAREA' EQ 'OUTPUT').ZO32A  OUTPUT                00583300
         AIF   ('&ZXSPOFAREA' NE 'INOUT').ZO32C  INOUT                  00583400
.ZO32A    AIF   ('&XSPOFAREA'(1,1) NE '(').ZO32B  ADDR IN GPR           00583500
&ZCGPR   SETC  '&XSPOFAREA'(2,K'&XSPOFAREA-2)                           00583600
&ZOPN                 SETA  &ZOPN+1                                     00583700
&ZOPL(&ZOPN)          SETC  ' '                                         00583800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00583900
&ZOPV(&ZOPN)          SETC  '0(4,&ZCGPR),16(1)'                         00584000
&ZOPD(&ZOPN)          SETC  '++ XSPOFAREA A'                            00584100
                      AGO   .ZO32C                                      00584200
.*                                                                      00584300
.ZO32B                ANOP  ,                                           00584400
&ZAWRK   SETA  K'&XSPOFAREA                                             00584500
         AIF   ('&XSPOFAREA'(&ZAWRK,1) NE ')').ZO32G                    00584600
&ZN      SETA  2                                                        00584700
.ZO32H    ANOP                                                          00584800
         AIF   (&ZN GE &ZAWRK).ZO32G                                    00584900
         AIF   ('&XSPOFAREA'(&ZN,1) NE '(').ZO32I                       00585000
&ZC      SETC  '&XSPOFAREA'(1,&ZN).'4'.','                              00585100
&ZC      SETC  '&ZC'.'&XSPOFAREA'(&ZN+1,&ZAWRK-&ZN).',16(1)'            00585200
         AGO   .ZO32J                                                   00585300
.ZO32I    ANOP                                                          00585400
&ZN      SETA  &ZN+1                                                    00585500
         AGO   .ZO32H                                                   00585600
.ZO32G    ANOP                                                          00585700
&ZC      SETC  '&XSPOFAREA.(4),16(1)'                                   00585800
.ZO32J    ANOP                                                          00585900
&ZOPN                 SETA  &ZOPN+1                                     00586000
&ZOPL(&ZOPN)          SETC  ' '                                         00586100
&ZOPO(&ZOPN)          SETC  'MVC'                                       00586200
&ZOPV(&ZOPN)          SETC  '&ZC'                                       00586300
&ZOPD(&ZOPN)          SETC  '++ XSPOFAREA A'                            00586400
.ZO32C                ANOP  ,                                           00586500
.*******************************************************************    00586600
.***AO.X02  RETURN CODE PROCESSING                                      00586700
.*******************************************************************    00586800
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00586900
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00587000
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00587100
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00587200
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00587300
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00587400
&ZOPN                 SETA  &ZOPN+1                                     00587500
&ZOPL(&ZOPN)          SETC  ' '                                         00587600
&ZOPO(&ZOPN)          SETC  'ST'                                        00587700
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00587800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00587900
                      AGO   .ZMRETX                                     00588000
.*                                                                      00588100
.ZMRET2               ANOP  ,                                           00588200
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00588300
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00588400
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00588500
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00588600
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00588700
&ZOPN                 SETA  &ZOPN+1                                     00588800
&ZOPL(&ZOPN)          SETC  ' '                                         00588900
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00589000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00589100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00589200
.ZMRETX               ANOP  ,                                           00589300
.*******************************************************************    00589400
.***AO.X03  REASON CODE PROCESSING                                      00589500
.*******************************************************************    00589600
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00589700
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00589800
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00589900
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00590000
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00590100
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00590200
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00590300
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00590400
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00590500
&ZOPN                 SETA  &ZOPN+1                                     00590600
&ZOPL(&ZOPN)          SETC  ' '                                         00590700
&ZOPO(&ZOPN)          SETC  'ST'                                        00590800
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00590900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00591000
                      AGO   .ZMRSNX                                     00591100
.*                                                                      00591200
.ZMRSN2               ANOP  ,                                           00591300
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00591400
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00591500
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00591600
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00591700
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00591800
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00591900
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00592000
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00592100
&ZOPN                 SETA  &ZOPN+1                                     00592200
&ZOPL(&ZOPN)          SETC  ' '                                         00592300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00592400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00592500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00592600
.ZMRSNX               ANOP  ,                                           00592700
.*******************************************************************    00592800
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00592900
.*******************************************************************    00593000
.ZMFLX                ANOP  ,              MF=L PROCESSING              00593100
.*******************************************************************    00593200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00593300
.*******************************************************************    00593400
                      ACTR  10000                                       00593500
.*                                                                      00593600
                      LCLA  &ZLC           COLUMN POSITION              00593700
                      LCLA  &ZOPX          LIST TABLE INDEX             00593800
                      LCLC  &ZLL           LABEL                        00593900
                      LCLC  &ZLOP          OPERATION                    00594000
                      LCLC  &ZLV           VALUE                        00594100
                      LCLC  &ZLD           DESCRIPTION                  00594200
.*                                                                      00594300
&ZOPX                 SETA  1                                           00594400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00594500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00594600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00594700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00594800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00594900
                      AIF   (K'&ZLD LE 0).ZL5                           00595000
&ZLC                  SETA  35             COMMENT COLUMN               00595100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00595200
&ZLC                  SETA  33             COMMMENT COLUMN              00595300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00595400
&ZLV                  SETC  '&ZLV'.' '                                  00595500
                      AGO   .ZL3                                        00595600
.*                                                                      00595700
.ZL4                  ANOP  ,                                           00595800
&ZLV                  SETC  '&ZLV'.' '                                  00595900
.ZL5                  ANOP  ,                                           00596000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00596100
.*                                                                      00596200
&ZLL     &ZLOP &ZLV                                                     00596300
.*                                                                      00596400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00596500
                      AGO   .ZL2           CONTINUE LOOPING             00596600
.*                                                                      00596700
.ZLX                  ANOP  ,                                           00596800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00596900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00597000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00597100
.*********************************************************************  00597200
.* Gets addressability to the IOVT and check to see if space-        *  00597300
.* switching PCs are enabled.  If not, return a return code 16 and   *  00597400
.* reason code 1 to the caller.                                      *  00597500
.*                                                                   *  00597600
.* Note: This handles the cases where a user may attempt to call     *  00597700
.*       PC at a point where the PC tables are not yet               *  00597800
.*       initialized                                                 *  00597900
.*********************************************************************  00598000
         AIF   ('&SYSAM64' EQ 'YES').LCVTA64 ++ GOTO IF AMODE64   @P2A  00598100
         L     15,16(0,0)         ++ GET CVT ADDRESS                    00598200
         L     14,124(15,0)       ++ GET IOCOM ADDRESS                  00598300
         AGO   .TESTSSP           ++ GOTO TEST IOSAS ACTIVE       @P2A  00598400
.LCVTA64 LLGT  15,16(0,0)         ++ GET CVT ADDRESS              @P2A  00598500
         LLGT  14,124(15,0)       ++ GET IOCOM ADDRESS            @P2A  00598600
.TESTSSP L     14,208(14,0)       ++ GET IOVT ADDRESS                   00598700
         TM    5(14),B'00001000'  ++ CHECK IOVTSSPC FLAG                00598800
         JNZ   SPF1&SYSNDX        ++ IF FLAG IS ON, ISSUE PC      @P2C  00598900
         LA    15,16              ++ SET ERROR RETURN CODE              00599000
         LA    0,1                ++ SET ERROR REASON CODE              00599100
         J     SPF2&SYSNDX        ++ BRANCH AROUND PC     @P2C          00599200
.*********************************************************************  00599300
.*                                                                   *  00599400
.*  PC linkage                                                       *  00599500
.*                                                                   *  00599600
.*********************************************************************  00599700
SPF1&SYSNDX DS    0H                                                    00599800
         L     14,772(15,0)       ++ GET SFT ADDRESS                    00599900
.*********************************************************************  00600000
.*                                                                   *  00600100
.*  Get LX/EX for IOSSPOF Service                                    *  00600200
.*                                                                   *  00600300
.*  Note: LX is calculated by multiplying the PC number (129)        *  00600400
.*        times the length of a PC/AUTH table entry (4) and          *  00600500
.*        then subtracting 4.                                        *  00600600
.*                                                                   *  00600700
.*        EX for IOSSPOF service is 1                                *  00600800
.*                                                                   *  00600900
.*********************************************************************  00601000
         L     14,512(14,0)       ++ GET LX/EX FOR IOSWITCH Service     00601100
         AHI   14,1               ++ GET EX FOR IOSSPOF           @P2C  00601200
         PC    0(14)              ++ PC TO IOSVSPFR                     00601300
SPF2&SYSNDX DS    0H                                                    00601400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00601500
.ZLINKX               ANOP  ,                                           00601600
.*******************************************************************    00601700
.***AP.X01  FORMAT GENERATED CODE                                       00601800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00601900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00602000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00602100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00602200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00602300
                      AIF   (K'&ZLD LE 0).ZL51                          00602400
&ZLC                  SETA  35             COMMENT COLUMN               00602500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00602600
&ZLC                  SETA  33             COMMENT COLUMN               00602700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00602800
&ZLV                  SETC  '&ZLV'.' '                                  00602900
                      AGO   .ZL31                                       00603000
.*                                                                      00603100
.ZL41                 ANOP  ,                                           00603200
&ZLV                  SETC  '&ZLV'.' '                                  00603300
.ZL51                 ANOP  ,                                           00603400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00603500
.*                                                                      00603600
&ZLL     &ZLOP &ZLV                                                     00603700
.*                                                                      00603800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00603900
                      AGO   .ZL21          CONTINUE LOOPING             00604000
.*                                                                      00604100
.ZLX1                 ANOP  ,                                           00604200
.*******************************************************************    00604300
.***AP.X02  END OF IOSSPOF MACRO                                        00604400
&ZIOSSPOF             SETC  'YES'          SET IOSSPOF USED             00604500
         POP   PRINT                                                    00604600
*                                            IOSSPOF-1                  00604700
         SPACE 1                                                        00604800
.ZLX2                 ANOP  ,                                           00604900
         MEND                                                           00605000
