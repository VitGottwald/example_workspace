*/********************************************************************/         
*/*              SWAP PLACEMENT DATA SECTION                         */         
*/* With MVS Release 13 this section is no longer filled.        @51C*/         
*/********************************************************************/         
*                                                                               
*   DCL SMF71NSR CONSTANT(18);      /* Reserved                  @51C*/         
*   %IF SMFARRAY = 'YES' %THEN %GOTO ARR71;                    /*@P1A*/         
*                                                                               
*   DCL 1 SMF71SWP BASED(ADDR(SMF71HDR)+SMF71SWS),             /*@P1A*/         
*       %GOTO CONT71;                                          /*@P1A*/         
*%ARR71:;                                                      /*@P1A*/         
*   DCL 1 SMF71SWP(1:SMF71NSR) BASED(ADDR(SMF71HDR)+SMF71SWS), /*@A2C*/         
*%CONT71:;                                                     /*@P1A*/         
*        3 SMF71TOT FIXED(32),      /* Reserved                  @51C*/         
*        3 SMF71AXD FIXED(32),      /* Reserved                  @51C*/         
*        3 SMF71LES FIXED(32),      /* Reserved                  @51C*/         
*        3 SMF71LAX FIXED(32),      /* Reserved                  @51C*/         
*        3 SMF71ESD FIXED(32),      /* Reserved                  @51C*/         
*        3 SMF71MIG FIXED(32);      /* Reserved                  @51C*/         
