.***AI.L01 CSVDYNEX FMT=LIST FILL IN PARAMETER AREA                     01905800
.******************************************************************     01905900
                      ACTR  10000                                       01906000
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01906100
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01906200
&ZCVAL                SETC  'LAE'          XAX MODE                     01906300
.ZIMFXA               ANOP ,                                            01906400
.*                                                                      01906500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01906600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01906700
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01906800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01906900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01907000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01907100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01907200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01907300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01907400
&ZOPN                 SETA  &ZOPN+1                                     01907500
&ZOPL(&ZOPN)          SETC  ' '                                         01907600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01907700
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01907800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01907900
                      AGO   .ZIMFNX                                     01908000
.*                                                                      01908100
.ZIMFN2               ANOP ,                                            01908200
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01908300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01908400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01908500
&ZOPN                 SETA  &ZOPN+1                                     01908600
&ZOPL(&ZOPN)          SETC  ' '                                         01908700
&ZOPO(&ZOPN)          SETC  'LAE'                                       01908800
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01908900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01909000
                      AGO   .ZIMFNX                                     01909100
.ZIMFNX1              ANOP ,                                            01909200
&ZOPN                 SETA  &ZOPN+1                                     01909300
&ZOPL(&ZOPN)          SETC  ' '                                         01909400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01909500
