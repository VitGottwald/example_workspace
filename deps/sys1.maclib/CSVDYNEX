.***AC.PLV01  PROCESS PLISTVER                                      *   00409100
.********************************************************************   00409200
&XPLISTVER      SETA  0                                                 00409300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00409400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00409500
&XPLISTVER      SETA  1                                                 00409600
.ZPLV0          ANOP  ,                                                 00409700
.*                                                                      00409800
                AIF   ('&SERVICEID' EQ '').ZPLV1E                       00409900
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00410000
&XPLISTVER      SETA  1                                                 00410100
.ZPLV1E         ANOP  ,                                                 00410200
.*                                                                      00410300
                AIF   ('&SERVICEID' EQ '').ZPLV2E                       00410400
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00410500
&XPLISTVER      SETA  1                                                 00410600
.ZPLV2E         ANOP  ,                                                 00410700
.*                                                                      00410800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00410900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00411000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00411100
.*-------------------------------------------------------------------   00411200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00411300
&ZMS            SETC  '"PLISTVER='                                      00411400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00411500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00411600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00411700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00411800
                AGO   .ZPVCK2                                           00411900
.ZPVCK3         ANOP  ,                                                 00412000
.*-------------------------------------------------------------------   00412100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00412200
&ZMS            SETC  '"PLISTVER='                                      00412300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00412400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00412500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00412600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00412700
                AGO   .ZPVCKE                                           00412800
.ZPVCK2         ANOP  ,                                                 00412900
.*-------------------------------------------------------------------   00413000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00413100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00413200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00413300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00413400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00413500
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00413600
.ZPVCK6         ANOP  ,                                                 00413700
&ZMS            SETC  '"PLISTVER='                                      00413800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00413900
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00414000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00414100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00414200
&XPLISTVER      SETA  1                                                 00414300
                AGO   .ZPVCKE                                           00414400
.*-------------------------------------------------------------------   00414500
.ZPVCK4A        ANOP  ,                                                 00414600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00414700
&ZMS            SETC  'PLISTVER='                                       00414800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00414900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00415000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00415100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00415200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00415300
                AGO   .ZPVCK5                                           00415400
.ZPVCK4         ANOP  ,                                                 00415500
&XPLISTVER      SETA  &PLISTVER(1)                                      00415600
.ZPVCK5         ANOP  ,                                                 00415700
.ZPVCKE         ANOP  ,                                                 00415800
.********************************************************************   00415900
