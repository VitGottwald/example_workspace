.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02151000
         AIF   ('&REQUEST' EQ 'QUERY').FPLINK                      @L1A 02151100
         AIF   ('&REQUEST' NE 'CALL').NOTFP                        @L1A 02151200
         AIF   ('&FASTPATH' EQ 'YES').FPLINK                       @L1A 02151300
.NOTFP   ANOP                          Not CALL FASTPATH=YES       @L1A 02151400
         AIF   ('&LINKAGE' NE 'BRANCH').PCLINK                     @L1A 02151500
         L     15,16(0,0)              GET CVT ADDRESS             @L1A 02151600
         L     15,140(15,0)            GET ECVT ADDRESS            @L1A 02151700
         L     15,228(15,0)                                        @L1A 02151800
         AIF   ('&LINKTEST(1)' NE 'YES').DOBASSM0                  @L1A 02151900
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 02152000
         BZ    C&SYSNDX                NO, DO NOT CALL             @L1A 02152100
         L     15,32(15,0)             GET SERVICE ROUTINE ADDR    @L1A 02152200
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 02152300
         BNZ   A&SYSNDX                YES, CALL IT                @L1A 02152400
C&SYSNDX DS    0H                      FUNCTION NOT AVAILABLE      @L1A 02152500
         LA    15,12                   INDICATE FUNCTION NOT AVAIL @L1A 02152600
         LA    0,X'0C01'               REASON CODE                 @L1A 02152700
         B     B&SYSNDX                                            @L1A 02152800
A&SYSNDX DS    0H                      NEED TO CALL                @L1A 02152900
         BASSM 14,15                   CALL ROUTINE                @L1A 02153000
B&SYSNDX DS    0H                                                  @L1A 02153100
         AGO   .ENDLINK                                            @L1A 02153200
.DOBASSM0 ANOP                                                     @L1A 02153300
         L     15,32(15,0)             GET SERVICE ROUTINE ADDR    @L1A 02153400
         BASSM 14,15                   CALL ROUTINE                @L1A 02153500
         AGO   .ENDLINK                                            @L1A 02153600
.PCLINK  ANOP                                                      @L1A 02153700
         L     14,16(0,0)              GET CVT ADDRESS             @L1A 02153800
         AIF   ('&LINKTEST(1)' NE 'YES').DOPC                      @L1A 02153900
         L     15,140(14,0)            GET ECVT ADDRESS            @L1A 02154000
         L     15,228(15,0)                                        @L1A 02154100
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 02154200
         BZ    C&SYSNDX                NO, NOT AVAILABLE           @L1A 02154300
         AIF   ('&SYSASCE' EQ 'P').ASCPRIM                         @L1A 02154400
         LAE   15,0(15,0)              SET AR for compare          @L1A 02154500
.ASCPRIM ANOP                                                      @L1A 02154600
         CLI   24(15),0                IS PC ROUTINE AVAILABLE     @L1A 02154700
         BNZ   A&SYSNDX                YES, CALL ROUTINE           @L1A 02154800
C&SYSNDX DS    0H                      NOT AVAILABLE               @L1A 02154900
         LA    15,12                   INDICATE FUNCTION NOT AVAIL @L1A 02155000
         LA    0,X'0C01'               REASON CODE                 @L1A 02155100
         B     B&SYSNDX                                            @L1A 02155200
A&SYSNDX DS    0H                      NEED TO CALL                @L1A 02155300
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 02155400
         L     14,368(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 02155500
         PC    0(14)                   CALL ROUTINE                @L1A 02155600
B&SYSNDX DS    0H                                                  @L1A 02155700
         AGO   .ENDLINK                                            @L1A 02155800
.DOPC    ANOP                                                      @L1A 02155900
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 02156000
         L     14,368(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 02156100
         PC    0(14)                                               @L1A 02156200
         AGO   .ENDLINK                                            @L1A 02156300
.FPLINK  ANOP                                                      @L1A 02156400
         L     15,16(0,0)              GET CVT ADDRESS             @L1A 02156500
         L     15,140(15,0)            GET ECVT ADDRESS            @L1A 02156600
         L     15,228(15,0)                                        @L1A 02156700
         AIF   ('&LINKTEST(1)' NE 'YES').DOBASSM                   @L1A 02156800
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 02156900
         BZ    C&SYSNDX                NO, DO NOT CALL             @L1A 02157000
         L     15,28(15,0)             GET EXIT FASTPATH CALL ADDR @L1A 02157100
         LTR   15,15                   SEE IF ROUTINE EXISTS       @L1A 02157200
         BNZ   A&SYSNDX                YES, CALL IT                @L1A 02157300
C&SYSNDX DS    0H                      NO NEED TO CALL             @L1A 02157400
         LA    15,12                   INDICATE FUNCTION NOT AVAIL @L1A 02157500
         LA    0,X'0C01'               REASON CODE                 @L1A 02157600
         B     B&SYSNDX                                            @L1A 02157700
A&SYSNDX DS    0H                      NEED TO CALL                @L1A 02157800
         BASSM 14,15                   CALL ROUTINE                @L1A 02157900
B&SYSNDX DS    0H                                                  @L1A 02158000
         AGO   .ENDLINK                                            @L1A 02158100
