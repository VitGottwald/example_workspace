*          5 E27TAIX   BIT     (1),    /* 1=AIX DATA SET         @PDC*/         
*          5 E27TVRRD  BIT     (1),    /* VRRDS DATASET          @PDC*/         
*          5 E27TRACF  BIT     (1),    /* 1=DATA SET IS PROTECTED BY            
*                                      DISCRETE RACF PROFILE @OA16588*/         
*          5 *         BIT     (5),    /* RESERVED           @OA16588*/         
*      3 EI27TRKS      CHAR    (8),    /* TOTAL TRACKS COPIED    @PDC*/         
*      3 EI27SVOL      CHAR    (6),    /* SOURCE VOLUME SERIAL   @PDC*/         
*      3 EI27TVOL      CHAR    (6),    /* TARGET VOLUME SERIAL   @PDC*/         
*      3 E27FLGS       CHAR    (1),    /* MISC FLAGS             @PDC*/         
*        4 EI27LVOL    BIT     (1),    /* LAST VOLUME INDICATOR  @PDC*/         
*                                      /* FROM SRC DATA SET      @PDC*/         
*        4 E27F9ATT    BIT     (1),    /* F9 ATTRS LOST          @P0A*/         
*        4 *           BIT     (6),    /* AVAILABLE              @P0C*/         
*      3 *             CHAR    (1),    /* RESERVED               @PDC*/         
*      3 EI27VLSQ      FIXED   (15),   /* VOLUME SEQUENCE NUMBER                
*                                         FROM THE SOURCE DATA                  
*                                         SET                    @PDC*/         
*      3 EI27DSHA      FIXED   (31),   /* VSAM DATA COMPONENT                   
*                                         ALLOCATED RBA COUNT    @PDC*/         
*      3 EI27ISHA      FIXED   (31),   /* VSAM INDEX COMPONENT                  
*                                         HIGH ALLOC RBA COUNT   @PDC*/         
*      3 EI27SCNT      FIXED   (15),   /* VSAM DATA SET STRIPE                  
*                                         COUNT                  @PDC*/         
*      3 E27DSTP#      FIXED   (15),   /* DATA COMPONENT STRIPE                 
*                                         COUNT                  @PDC*/         
*                                  /* THE EI27DRBA AND EI27IRBA FIELDS          
*                                     ARE CALCULATED BY SUBTRACTING             
*                                     THE STARTING RBA FROM THE                 
*                                     ENDING RBA                 @PDC*/         
*      3 EI27DRBA      FIXED   (32),   /* RBAS PROCESSED FOR                    
*                                         THE DATA COMPONENT     @PDC*/         
*      3 EI27IRBA      FIXED   (32),   /* RBAS PROCESSED FOR                    
*                                         THE INDEX COMPONENT    @PDC*/         
*      3 EI27DANM      CHAR    (44),   /* NEW DATA COMP NAME     @HRA*/         
*      3 EI27IXNM      CHAR    (44),   /* NEW INDEX COMP NAME    @HRA*/         
*      3 *             CHAR    (32);   /* RESERVED               @HRC*/         
*/********************************************************************/         
