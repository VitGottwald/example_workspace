*%GOTO PLSEID0;           /* GO TO THE PLS VERSION */                           
ADREIBID DC    C'EIDB'              /* BLOCK IDENTIFIER              */         
ADREIB@  DS    F'0'                                                             
EIOP00   DC    X'0000'              /* FUNCTION STARTUP ENTRY        */         
EIOP01   DC    X'0001'              /* READING SYSIN RECORD          */         
EIOP02   DC    X'0002'              /* PRINTING SYSPRINT RECORD      */         
EIOP03   DC    X'0003'              /* READING TAPE BLOCK RECORD     */         
EIOP04   DC    X'0004'              /* READING TAPE LOGICAL RECORD   */         
EIOP05   DC    X'0005'              /* WRITING TAPE LOGICAL RECORD   */         
EIOP06   DC    X'0006'              /* WRITING TAPE BLOCK RECORD     */         
EIOP07   DC    X'0007'              /* READING DISK TRACK            */         
EIOP08   DC    X'0008'              /* WRITING DISK TRACK            */         
EIOP09   DC    X'0009'              /* READING UTILITY SYSPRINT      */         
EIOP10   DC    X'000A'              /* WRITING UTILITY SYSPRINT      */         
EIOP11   DC    X'000B'              /* WRITING WTO MESSAGE           */         
EIOP12   DC    X'000C'              /* WRITING WTOR MESSAGE          */         
EIOP13   DC    X'000D'              /* PRESENTING ADRUFO RECORD      */         
EIOP14   DC    X'000E'              /* FUNCTION TERMINATION ENTRY    */         
EIOP15   DC    X'000F'              /* PRESENTING WTOR RESPONSE      */         
EIOP16   DC    X'0010'              /* TAPE VOL SEC/VERIFY       @V22*/         
EIOP17   DC    X'0011'              /* TAPE VOL MOUNT(NON-SPEC)  @V22*/         
EIOP18   DC    X'0012'              /* INSERT RESTORE RECORD     @V22*/         
EIOP19   DC    X'0013'              /* OUTPUT TAPE ERROR         @V22*/         
EIOP20   DC    X'0014'              /* VOLUME NOTIFICATION       @V24*/         
EIOP21   DC    X'0015'              /* DATA SET VERIFICATION     @V25*/         
EIOP22   DC    X'0016'              /* BYPASS VERIFICATION       @V25*/         
EIOP23   DC    X'0017'              /* DS PROCESSED NOTIFICATION @V25*/         
VOLSLST  DC    X'02C4'              /* MAX SIZE OF VOLSER LIST                  
*                                      FOR EIREC23 (118 * 6) @NA08383*/         
EIOP24   DC    X'0018'          /* CONCURRENT COPY INIT COMPLETE @V26*/         
EIOP25   DC    X'0019'              /* BACKSPACE INPUT TAPE  @OW13783*/         
EIOP26   DC    X'001A'              /* VOLUME OPEN FOR O/P   @OW15980*/         
EIOP27   DC    X'001B'              /* PHYSICAL DS PROCESSED     @PDC*/         
EIOP28   DC    X'001C'              /* TARGET ALLOCATION         @E2C*/         
EIOP29   DC    X'001D'              /* Reserved                  @P2C*/         
EIOP30   DC    X'001E'              /* PHYS COPY REST NEWVOL     @NVA*/         
EIOP31   DC    X'001F'              /* Application STORE         @MCA*/         
EIOP32   DC    X'0020'              /* Application RETREIVE      @MCA*/         
EIOP33   DC    X'0021'              /* OBJECT NAME NOTIFICATION  @MCA*/         
EIOP34   DC    X'0022'              /* Reserved                  @L5A*/         
EIOP35   DC    X'0023'              /* Cloud Function Notify     @L6A*/         
EIOP41   EQU   X'0029'              /* Unix Path Notification    @UXA*/         
EIOP42   EQU   X'002A'              /* Unix Path Bypass Verify   @UXA*/         
EIOP43   EQU   X'002B'              /* Unix Path Processed       @UXA*/         
EIOP44   EQU   X'002C'              /* Clone Initialization      @UXA*/         
EIOP45   EQU   X'002D'              /* Present Attributes        @UXA*/         
EIMAXOP  DC    X'002D'              /* MAX VALID EXIT NUMBER     @UXC*/         
EXITRECL DC    X'0030'              /* EXIT RECORD LENGTH    @NA08383*/         
EIRC00   DC    X'0000'              /* CONTINUE NORMAL PROCESSING    */         
EIRC04   DC    X'0004'              /* RECORD REPLACED               */         
EIRC08   DC    X'0008'              /* INSERT RECORD                 */         
EIRC12   DC    X'000C'              /* DELETE RECORD                 */         
EIRC16   DC    X'0010'              /* RECORD MODIFIED               */         
EIRC20   DC    X'0014'              /* DISCONNECT EXIT               */         
EIRC24   DC    X'0018'              /* SELECT USER STATISTICAL RECS  */         
EIRC28   DC    X'001C'              /* WTOR RESPONSE                 */         
EIRC32   DC    X'0020'              /* TERMINATE FUNCTION        @V22*/         
EIRC36   DC    X'0024'              /* TERMINATE DATA SET ONLY   @V25*/         
***********************************************************************         
*          Constants for Serialization error actions              @STA*         
***********************************************************************         
EISRLNONE  EQU   0                      Use None                  @STA          
EISRLDB2   EQU   1                      Use DB2                   @STA          
EISRLZFS   EQU   2                      Use ZFS                   @STA          
EISRLCICS  EQU   3                      Use CICS TS               @STA          
EISRLEXIT  EQU   4                      Use User Exit             @STA          
ADREIB   DSECT                                                                  
EIDLLEN  DS    H                    /* LENGTH OF ADREIB - 2      @V22*/         
EIID     DS    CL4                  /* BLOCK IDENTIFIER EBCDIC "EID0"*/         
EITSKID  DS    FL4                  /* TASK ID NUM OF CALLER @LA02135*/         
EIXALLOW DS    0XL4                 /* USER EXIT ALLOWANCE OPTIONS   */         
EIXALOW0 DS    X                    /* ALLOWANCE OPTIONS BYTE 1      */         
EIXREP   EQU   X'80'                /* ALLOW REPLACE OF RECORD       */         
EIXINS   EQU   X'40'                /* ALLOW INSERT OF RECORD        */         
EIXDEL   EQU   X'20'                /* ALLOW DELETE OF RECORD        */         
EIXMOD   EQU   X'10'                /* ALLOW MODIFICATION OF RECORD  */         
EIXDIS   EQU   X'08'                /* ALLOW DISCONNECT OF EXIT      */         
EIXWTOR  EQU   X'04'                /* ALLOW WTOR RESPONSE           */         
EIXSTAT  EQU   X'02'                /* ALLOW SELECTION OF USER STATS */         
EIXTERM  EQU   X'01'                /* ALLOW TERMINATION OF FUNC @V22*/         
EIXALOW1 DS    X                    /* ALLOWANCE OPTIONS BYTE 2      */         
EIXTDSET EQU   X'80'                /* ALLOW DATA SET TERMINATION@V25*/         
EIXALOW2 DS    X                    /* ALLOWANCE OPTIONS BYTE 3      */         
EIXALOW3 DS    X                    /* ALLOWANCE OPTIONS BYTE 4      */         
EIXERR   EQU   X'80'                /* DIS-ALLOWED OPTION    @V23N298*/         
EIOPTION DS    H                    /* PROCESSING OPTION             */         
EIRETCOD DS    H                    /* EXIT RETURN CODE              */         
EIRECALN DS    FL4                  /* RECORD AREA LENGTH    @LA02135*/         
EIRECLEN DS    FL4                  /* ORIGINAL RECORD LEN   @LA02135*/         
EIRECPTR DS    FL4                  /* ORIGINAL RECORD ADDR  @LA02135*/         
EIUSEPTR DS    FL4                  /* USER DATA AREA PTR    @V23N253*/         
EIDDID   DS    0FL4                 /* EIOP06 DDN/VOLID PTR  @OW36779*/         
EIXMWPL  DS    FL4                  /* EIOP11 WTO PLIST PTR  @OW36779*/         
EIXFLAGS DS    0XL4                 /* OTHER FLAGS           @LA09760*/         
EIXFLAG0 DS    X                    /* FLAG BYTE 0           @LA09760*/         
EIXABEND EQU   X'80'                /* ABEND MESSAGE FLAG    @LA09760*/         
EIXNTERR EQU   X'40'                /* NON-TAPE ERR MSG FLAG @LA09760*/         
EIXWNGOK EQU   X'20'                /* WARNINGS ARE OK  FLAG @LA54862*/         
EIXTRKER EQU   X'10'                /* track is in error     @OW07425*/         
EIXFLAG1 DS    X                    /* FLAG BYTE 1           @LA09760*/         
EIXTRC32 EQU   X'80'                /* UIM SET EIRETCOD 32   @OW33495*/         
EIXTDUMS EQU   X'20'                /* DUMMY SOURCE TAPE     @OW13783*/         
EIXTDUMT EQU   X'10'                /* DUMMY TARGET TAPE     @OW13783*/         
EIXTISXM EQU   X'08'                /* APPL USING XMAPI      @OW13783*/         
EIXDASYS EQU   X'04'                /* XMAPI ALLOCATION ERR  @OW21750*/         
EIXTWERR EQU   X'02'                /* XMAPI TAPE WRITE ERR  @OW22314*/         
EIXTRERR EQU   X'01'                /* XMAPI TAPE READ ERR   @OW22673*/         
EIXFLAG2 DS    X                    /* FLAG BYTE 2           @LA09760*/         
*                                   /* Remove Batch jcl bit      @03D*/         
EIXFLAG3 DS    X                    /* FLAG BYTE 3           @LA09760*/         
         DS    0F                   /* FORCE WORD BOUNDARY           */         
EIDBLEN  EQU   *-ADREIB             /* LENGTH OF THE EIB BLOCK       */         
*/********************************************************************/         
*/* RECORD PRESENTED BY EXIT 06 (WRITE PHYSICAL TAPE):       @OW34887*/         
*/* EIDDINFO IS BASED ON (EIDDID).                           @OW34887*/         
*/********************************************************************/         
EIDDINFO DSECT                                                                  
EIDDNAME DS    CL8                  /* DDNAME LEFT-JUSTIFIED  @V22AEG*/         
EIVOLID  DS    CL6                  /* VOLUME SERIAL NO.      @V22AEG*/         
EIRETC   DS    XL1                  /* RETURN CODE            @V22AEG*/         
EI06FLGS DS    XL1                  /* EXIT 06 FLAGS         @OW34887*/         
EI06FTRL EQU   X'80'                /* RESERVED FOR HSM USE  @OW34887*/         
EI06THWE EQU   X'40'                /* RESERVED FOR HSM USE      @TPE*/         
*                                   /* OUTPUT DEVICE ENCRYPTS    @TPE*/         
EI06DCB@ DS    AL4                  /* RESERVED FOR HSM USE  @OW34887*/         
*        DS    CL4                  /* RESERVED              @OW34887*/         
*/********************************************************************/         
*/* RECORD PRESENTED BY EXIT 00 (FUNCTION STARTUP):          @OY57120*/         
*/********************************************************************/         
EIREC00  DSECT                      /*                       @OY57120*/         
EI00SBPL DS    XL1                  /* SHARED SUBPOOL NUMBER @OY57120*/         
EI00FLGS DS    XL1                  /* startup flags         @OW20023*/         
EI00SENQ EQU   X'80'                /* short VTOC enqueue    @OW20023*/         
EI00NONF EQU   X'40'                /* 1 = Don't allow IGWNOTIF call,           
*                                    0 = Allow IGWNOTIF call @OW28223*/         
EI00NOLK EQU   X'20'                /* 1= NO ADRLOCK ENQ/DEQ @OW42942*/         
EI00NENQ EQU   X'10'                /* 1= NO INPUT VOLUME                       
*                                      SERIALIZATION.                           
*                                      VALID DURING PHYSICAL COPY AND           
*                                      DUMP FUNCTION STARTUPS    @ALF*/         
EI00BSEC EQU   X'08'                /* 1= BYPASS SECURITY VERIFICATION.         
*                                      VALID DURING COPY FULL/TRACKS,           
*                                      DUMP FULL/TRACKS, AND RESTORE            
*                                      FULL/TRACKS FUNCTION STARTUPS,           
*                                      UNIX PATH DUMP/RESTORE                   
*                                                                @UXC*/         
EI00MVOLRECOV EQU X'04'             /* 1=CALLER IS PERFORMING A MULTI           
*                                      VOLUME RECOVERY USING PHYSICAL           
*                                      DATA SET RESTORE AND IT IS               
*                                      ACCEPTABLE FOR DFSMSDSS TO               
*                                      SCRATCH PREALLOCATED TARGET              
*                                      DATA SETS THAT ARE NOT THE               
*                                      CORRECT SIZE              @PDC*/         
EI00BSER EQU   X'02'                /* 1=BYPASS SERIALIZATION OF THE            
*                                      DATA SETS BEING RESTORED DURING          
*                                      A PHYSICAL DATA SET RESTORE              
*                                      OPERATION                 @PDC*/         
EI00_NOEXCL_NSPLOCK EQU X'01'       /* 1=BYPASS EXCLUSIVE ADRLOCK               
*                                      NONSPEC ENQ               @A1C*/         
EI00SVOL DS    CL6                  /* SOURCE VOLSER. VALID DURING              
*                                      COPY FULL/TRACKS, DUMP                   
*                                      FULL/TRACKS, AND RESTORE                 
*                                      FULL/TRACKS FUNCTION STARTUPS.           
*                                      FOR RESTORE, THIS FIELD CONTAINS         
*                                      VOLSER OF THE INPUT VOLUME               
*                                      WHERE THE DUMP DATA SET                  
*                                      RESIDES                   @ALF*/         
EI00FLG2 DS    XL1                  /* 2ND FLAG AREA             @XES*/         
EI00SWNCRYPT EQU X'80'              /* SOFTWARE ENCRYPTION WAS   @XES*/         
*                                   /* REQUESTED (RSA OR         @XES*/         
*                                   /* KEYPASSWORD WAS           @XES*/         
*                                   /* SPECIFIED)                @XES*/         
EI00CANSFE   EQU X'40'              /* CANCEL SOFTWARE ENCRYPT   @XES*/         
EI00MIXDEV   EQU X'20'              /* MIXTURE OF DEVICES PERFORMING            
*                                      AND NOT PERFORMING HARDWARE              
*                                      ENCRYPTION                @XES*/         
EI00ZCOMP    EQU X'10'              /* ZCOMPRESSION IS ATTEMPTED @ZCA*/         
*            EQU X'0F'              /* RESERVED                  @ZCC*/         
              DS  CL3               /* RESERVED                  @P4A*/         
EI00CIRBA_DA  DS  XL4               /* RBA OF VVR CI DATA/NONVSM @NVA*/         
EI00CIRBA_IX  DS  XL4               /* RBA OF VVR CI INDEX       @NVA*/         
*/********************************************************************/         
*/* THE FOLLOWING MAPS THE RECORD PRESENTED BY EXIT 14:      @LA65381*/         
