.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00168700
.*******************************************************************    00168800
                      ACTR  10000                                       00168900
.*                                                                      00169000
                      LCLA  &ZLC           COLUMN POSITION              00169100
                      LCLA  &ZOPX          LIST TABLE INDEX             00169200
                      LCLC  &ZLL           LABEL                        00169300
                      LCLC  &ZLOP          OPERATION                    00169400
                      LCLC  &ZLV           VALUE                        00169500
                      LCLC  &ZLD           DESCRIPTION                  00169600
.*                                                                      00169700
&ZOPX                 SETA  1                                           00169800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00169900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00170000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00170100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00170200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00170300
                      AIF   (K'&ZLD LE 0).ZL5                           00170400
&ZLC                  SETA  35             COMMENT COLUMN               00170500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00170600
&ZLC                  SETA  33             COMMMENT COLUMN              00170700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00170800
&ZLV                  SETC  '&ZLV'.' '                                  00170900
                      AGO   .ZL3                                        00171000
.*                                                                      00171100
.ZL4                  ANOP  ,                                           00171200
&ZLV                  SETC  '&ZLV'.' '                                  00171300
.ZL5                  ANOP  ,                                           00171400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00171500
.*                                                                      00171600
&ZLL     &ZLOP &ZLV                                                     00171700
.*                                                                      00171800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00171900
                      AGO   .ZL2           CONTINUE LOOPING             00172000
.*                                                                      00172100
.ZLX                  ANOP  ,                                           00172200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00172300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00172400
         L     14,16(0,0)              GET CVT ADDRESS                  00172500
         L     14,772(14,0)            GET SFT ADDRESS                  00172600
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00172700
         LA    15,25                   GET EX FOR SERVICE ROUTINE       00172800
         OR    14,15                   GET PC NUMBER                    00172900
         PC    0(14)                                                    00173000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00173100
.ZLINKX               ANOP  ,                                           00173200
.*******************************************************************    00173300
.***AP.X01  FORMAT GENERATED CODE                                       00173400
