.***AC.PLV01  PROCESS PLISTVER                                      *   00062900
.********************************************************************   00063000
&XPLISTVER      SETA  0                                                 00063100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00063200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00063300
&XPLISTVER      SETA  2                                                 00063400
.ZPLV0          ANOP  ,                                                 00063500
.*                                                                      00063600
                AIF   ('&CPUSERVICE' EQ '').ZPLV1E                      00063700
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00063800
&XPLISTVER      SETA  1                                                 00063900
.ZPLV1E         ANOP  ,                                                 00064000
.*                                                                      00064100
                AIF   ('&SYSPLEXCPUSRV' EQ '').ZPLV2E                   00064200
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00064300
&XPLISTVER      SETA  2                                                 00064400
.ZPLV2E         ANOP  ,                                                 00064500
.*                                                                      00064600
                AIF   ('&CPUTIME' EQ '').ZPLV3E                         00064700
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00064800
&XPLISTVER      SETA  1                                                 00064900
.ZPLV3E         ANOP  ,                                                 00065000
.*                                                                      00065100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00065200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00065300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00065400
.*-------------------------------------------------------------------   00065500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00065600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00065700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00065900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00066000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066100
                AGO   .ZPVCK2                                           00066200
.ZPVCK3         ANOP  ,                                                 00066300
.*-------------------------------------------------------------------   00066400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00066500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00066600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00066800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00066900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067000
                AGO   .ZPVCKE                                           00067100
.ZPVCK2         ANOP  ,                                                 00067200
.*-------------------------------------------------------------------   00067300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00067400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00067500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00067600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00067700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00067800
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00067900
.ZPVCK6         ANOP  ,                                                 00068000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00068100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068200
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00068300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00068400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068500
&XPLISTVER      SETA  2                                                 00068600
                AGO   .ZPVCKE                                           00068700
.*-------------------------------------------------------------------   00068800
.ZPVCK4A        ANOP  ,                                                 00068900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00069000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00069100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00069200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00069300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00069400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00069500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069600
                AGO   .ZPVCK5                                           00069700
.ZPVCK4         ANOP  ,                                                 00069800
&XPLISTVER      SETA  &PLISTVER(1)                                      00069900
.ZPVCK5         ANOP  ,                                                 00070000
.ZPVCKE         ANOP  ,                                                 00070100
.********************************************************************   00070200
