.*******************************************************************    00118500
.***AE.01  START OF IWMEDELE MACRO CODE                                 00118600
.*******************************************************************    00118700
                   LCLC  &M0M0001                 NAME                  00118800
&M0M0001           SETC  '&XLABEL'                SET NAME              00118900
                   AIF   (K'&XLABEL NE 0).XLAB2                         00119000
&M0M0001           SETC  'M00M&SYSNDX'            DEFAULT NAME          00119100
.*                                                                      00119200
.XLAB2             ANOP  ,                                              00119300
                   AIF   ('&XMF' EQ 'L').XLAB3                          00119400
&M0M0001 DS    0H                             IWMEDELE-2                00119500
.XLAB3             ANOP  ,                                              00119600
.*******************************************************************    00119700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00119800
.*******************************************************************    00119900
              PUSH  PRINT                                               00120000
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     00120100
                   PRINT OFF         ++ PR01                            00120200
                   AGO   .ZPRNTXX                                       00120300
.ZPRINTX           PRINT ON                                             00120400
.ZPRNTXX           ANOP  ,                                              00120500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00120600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00120700
.********************************************************************   00120800
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00120900
.********************************************************************   00121000
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00121100
                      LCLA  &ZOPN1         INVOCATION INDEX             00121200
                      LCLC  &ZOPL(4095)    LABEL                        00121300
                      LCLC  &ZOPO(4095)    OPERATION                    00121400
                      LCLC  &ZOPV(4095)    VALUE                        00121500
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00121600
.*                                                                      00121700
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00121800
.******************************************************************     00121900
.***AL.L10 IWMEDELE FMT=LIST MF=S!L PARAMETERS                          00122000
.******************************************************************     00122100
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00122200
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00122300
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00122400
&XMFATTR              SETC  '0D'                                        00122500
.*                                                                      00122600
&ZOPN                 SETA  &ZOPN+1                                     00122700
&ZOPL(&ZOPN)          SETC  ' '                                         00122800
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00122900
&ZOPO(&ZOPN)          SETC  'B'                                         00123000
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00123100
                      AGO   .ZALVL6B                                    00123200
.ZALVL6A              ANOP                                              00123300
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00123400
&ZOPV(&ZOPN)          SETC  '4,8'                                       00123500
&ZOPN                 SETA  &ZOPN+1                                     00123600
&ZOPL(&ZOPN)          SETC  ' '                                         00123700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00123800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00123900
.ZALVL6B              ANOP                                              00124000
&ZOPD(&ZOPN)          SETC  '++ IWMEDELE ARG PARMS'                     00124100
.*                                                                      00124200
&ZOPN                 SETA  &ZOPN+1                                     00124300
&ZOPL(&ZOPN)          SETC  ' '                                         00124400
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00124500
&ZOPV(&ZOPN)          SETC  '1'                                         00124600
&ZOPD(&ZOPN)          SETC  ' '                                         00124700
.*                                                                      00124800
.ZLMFSL2              ANOP  ,                                           00124900
.*-------------------------------------------------------------------   00125000
.***AM.L00  IWMEDELE FMT=LIST name                                      00125100
