                   AIF   ('&XRSNCODE'(1,1) NE '(').ZG21R                00128200
&ZCGPR             SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                   00128300
                   AIF   ('&ZCGPR'(1,1) NE '0').ZG21P                   00128400
                   AIF   (K'&ZCGPR GT 2).ZG21Q                          00128500
                   AIF   ('&ZCGPR' EQ '0').ZG21Q                        00128600
&ZCGPR             SETC  '&ZCGPR'(2,1)                                  00128700
                   AIF   (T'&ZCGPR NE 'N').ZG21Q  INVALID REG NAME      00128800
.ZG21P             ANOP  ,                                              00128900
                   AIF   (T'&ZCGPR NE 'N').ZG21R  NON NUMERIC           00129000
                   AIF   ('&ZCGPR' LT '2').ZG21Q                        00129100
                   AIF   ('&ZCGPR' LE '12').ZG21R                       00129200
.ZG21Q             ANOP  ,                                              00129300
&ZMS               SETC  '"RSNCODE='                                    00129400
&ZMS               SETC  '&ZMS.&RSNCODE"'                               00129500
&ZMS               SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'      00129600
                   MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'               00129700
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00129800
.ZG21R             ANOP  ,                                              00129900
.******************************************************************     00130000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00130100
