.***AC.PLV01  PROCESS PLISTVER                                      *   00072400
.********************************************************************   00072500
&XPLISTVER      SETA  0                                                 00072600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00072700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00072800
&XPLISTVER      SETA  0                                                 00072900
.ZPLV0          ANOP  ,                                                 00073000
.*                                                                      00073100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00073200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00073300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00073400
.*-------------------------------------------------------------------   00073500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00073600
&ZMS            SETC  '"PLISTVER='                                      00073700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00073800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00073900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00074000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00074100
                AGO   .ZPVCK2                                           00074200
.ZPVCK3         ANOP  ,                                                 00074300
.*-------------------------------------------------------------------   00074400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00074500
&ZMS            SETC  '"PLISTVER='                                      00074600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00074700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00074800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00074900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00075000
                AGO   .ZPVCKE                                           00075100
.ZPVCK2         ANOP  ,                                                 00075200
.*-------------------------------------------------------------------   00075300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00075400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00075500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00075600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00075700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00075800
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00075900
.ZPVCK6         ANOP  ,                                                 00076000
&ZMS            SETC  '"PLISTVER='                                      00076100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00076200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00076300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00076400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00076500
&XPLISTVER      SETA  0                                                 00076600
                AGO   .ZPVCKE                                           00076700
.*-------------------------------------------------------------------   00076800
.ZPVCK4A        ANOP  ,                                                 00076900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00077000
&ZMS            SETC  'PLISTVER='                                       00077100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00077200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00077300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00077400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00077500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00077600
                AGO   .ZPVCK5                                           00077700
.ZPVCK4         ANOP  ,                                                 00077800
&XPLISTVER      SETA  &PLISTVER(1)                                      00077900
.ZPVCK5         ANOP  ,                                                 00078000
.ZPVCKE         ANOP  ,                                                 00078100
.********************************************************************   00078200
