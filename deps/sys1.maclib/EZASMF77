*                                              member responses  @Q1A*/         
*    3 SMF119AP_TSC6OutGrpMemRed FIXED(32); /* Outb ICMPv6 group                
*                                              member reductions @Q1A*/         
*                                                                               
*/********************************************************************/         
*/* Storage Statistics section portion of the record                 */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119AP_TSST BASED,                                      /*@E2A*/         
*    3 SMF119AP_TSSTECSACurrent    BIT(64),/* ECSA current usage @E2A*/         
*    3 SMF119AP_TSSTECSAFree       BIT(64),/* ECSA alloc & free  @E2A*/         
*    3 SMF119AP_TSSTPrivateCurrent BIT(64),/* Private usage      @E2A*/         
*    3 SMF119AP_TSSTPrivateFree    BIT(64),/* Pri alloc and free @E2A*/         
*    3 SMF119AP_TSSTSMCRFixedCurrent BIT(64), /* Cur 64bit SMCR fixed           
*                                                mem used        @V7A*/         
*    3 SMF119AP_TSSTSMCRFixedMax     BIT(64), /* Max 64bit SMCR fixed           
*                                                mem used        @V7A*/         
*    3 SMF119AP_TSSTSMCRSendCurrent  BIT(64), /* Cur 64bit SMCR send            
*                                                mem used        @V7A*/         
*    3 SMF119AP_TSSTSMCRSendMax      BIT(64), /* Max 64bit SMCR send            
*                                                mem used        @V7A*/         
*    3 SMF119AP_TSSTSMCRRecvCurrent  BIT(64), /* Cur 64bit SMCR recv            
*                                                mem used        @V7A*/         
*    3 SMF119AP_TSSTSMCRRecvMax      BIT(64), /* Max 64bit SMCR recv            
*                                                mem used        @V7A*/         
*    3 SMF119AP_TSSTSMCDFixedCurrent BIT(64), /* Cur 64bit SMCD fixed           
*                                                mem used        @41A*/         
*    3 SMF119AP_TSSTSMCDFixedMax     BIT(64), /* Max 64bit SMCD fixed           
*                                                mem used        @41A*/         
*    3 SMF119AP_TSSTZAGGCurrent      BIT(64), /* Cur 64bit ZERT AGG             
*                                                mem used        @63A*/         
*    3 SMF119AP_TSSTZAGGMax          BIT(64); /* Max 64bit ZERT AGG             
*                                                mem used        @63A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for TCP/IP Statistics section length                   */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119AP_TSTotalLen FIXED(15)                                               
*                       CONSTANT(LENGTH(SMF119AP_TSIP)+                         
*                                LENGTH(SMF119AP_TSTC)+                         
*                                LENGTH(SMF119AP_TSUD)+                         
*                                LENGTH(SMF119AP_TSIC)+                         
*                                LENGTH(SMF119AP_TSP6)+                         
*                                LENGTH(SMF119AP_TSC6)+                         
*                                LENGTH(SMF119AP_TSST));  /*@Q1C @E2C*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* Interface Statistics record (subtype 06)                         */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119IS_IF BASED,                                                         
*    3 SMF119IS_IFDuration BIT(64), /* Duration of recording (STCK              
*                                      interval)                     */         
*    3 SMF119IS_IFLnkHome  CHAR(16), /* Link Home address            */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119IS_IFLnkHomeFmt CHAR(2), /* IPv4 address format @L8A*/         
*      5 SMF119IS_IFLnkHomev4  CHAR(4), /* IPv4 piece of address     */         
*    3 SMF119IS_IFName   CHAR(16),  /* Link name                     */         
*    3 SMF119IS_IFDevName  CHAR(16), /* Device name              @L3C*/         
*    3 SMF119IS_IFDesc   CHAR(18),  /* Link description (TCPIP PROFILE          
*                                      keyword for LINK type)        */         
*    3 SMF119IS_IFFlags  BIT(8),    /* Flags                     @VCA*/         
*      5 SMF119IS_IFSMCRCFGED  BIT(1),  /* SMCR configured       @VCA*/         
*      5 SMF119IS_IFPNETIDFLG  BIT(1),  /* PNetID provided       @VCA*/         
*      5 SMF119IS_IFSMCDCFGED  BIT(1),  /* SMCD configured       @41A*/         
*      5 SMF119IS_IFIQDCFLAG   BIT(1),  /* IQDC flag indicates the              
*                                          associated name and the              
*                                          associated statistics                
*                                          are for IQDC,not IQDX @54A*/         
*      5 *                     BIT(4),  /* Reserved    @VCA @41C @54C*/         
*    3 *                 CHAR(1),   /* Reserved                  @VCC*/         
*    3 SMF119IS_IFActualMtu  FIXED(32), /* MTU Size                  */         
*    3 SMF119IS_IFSpeed  FIXED(32), /* Link Speed                    */         
*    3 SMF119IS_IFHSpeed FIXED(32), /* Link HSpeed                   */         
*    3 SMF119IS_IFInBytes  BIT(64), /* Number of inbound bytes during           
*                                      last SMF interval             */         
*    3 SMF119IS_IFInUniC BIT(64),   /* Inbound UniCast packets during           
*                                      last SMF interval             */         
*    3 SMF119IS_IFInBroadC BIT(64), /* Inbound broadcast packets during         
*                                      last SMF interval             */         
*    3 SMF119IS_IFInMultiC BIT(64), /* Inbound multicast packets during         
*                                      last SMF interval             */         
*    3 SMF119IS_IFInDisc FIXED(32), /* Inbound discarded packets during         
*                                      last SMF interval             */         
*    3 SMF119IS_IFInError  FIXED(32), /* Inbound packets in error               
*                                      during last SMF interval      */         
*    3 SMF119IS_IFInUProt  FIXED(32), /* Inbound protocol error packets         
*                                      during last SMF interval      */         
*    3 SMF119IS_IFOutBytes BIT(64), /* Number of outbound bytes during          
*                                      last SMF interval             */         
*    3 SMF119IS_IFOutUniC  BIT(64), /* Outbound UniCast packets during          
*                                      last SMF interval             */         
*    3 SMF119IS_IFOutBroadC  BIT(64), /* Outbound broadcast packets             
*                                      during last SMF interval      */         
*    3 SMF119IS_IFOutMultiC  BIT(64), /* Outbound multicast packets             
*                                      during last SMF interval      */         
*    3 SMF119IS_IFOutDisc  FIXED(32), /* Outbound discarded packets             
*                                      during last SMF interval      */         
*    3 SMF119IS_IFOutError FIXED(32), /* Outbound packets in error              
*                                      during last SMF interval      */         
*    3 SMF119IS_IFOQL    FIXED(32), /* Current output queue length   */         
*    3 SMF119IS_IFIQDXName CHAR(16), /* Associated IQDX interface               
*                                       name                     @M9A*/         
*      5 SMF119IS_IFIQDCName CHAR(16), /* Associated IQDC interface             
*                                       name                     @54A*/         
*    /* SMF IQDX/IQDC Stats area                            @M9A @54C*/         
*    3 SMF119IS_IFInIQDXBytes  BIT(64), /* Inbound bytes received over          
*                                      IQDX last SMF interval    @M9A*/         
*      5 SMF119IS_IFInIQDCBytes BIT(64), /* Inbound bytes received over         
*                                      IQDC last SMF interval    @54A*/         
*    3 SMF119IS_IFInIQDXUniC   BIT(64), /* Inbound pkts received over           
*                                      IQDX last SMF interval    @M9A*/         
*      5 SMF119IS_IFInIQDCUniC BIT(64), /* Inbound pkts received over           
*                                      IQDC last SMF interval    @54A*/         
*    3 SMF119IS_IFOutIQDXBytes BIT(64), /* Outbound bytes sent over             
*                                      IQDX last SMF interval    @M9A*/         
*      5 SMF119IS_IFOutIQDCBytes BIT(64), /* Outbound bytes sent over           
*                                      IQDC last SMF interval    @54A*/         
*    3 SMF119IS_IFOutIQDXUniC  BIT(64), /* Outbound pkts sent over              
*                                      IQDX last SMF interval    @M9A*/         
*      5 SMF119IS_IFOutIQDCUniC BIT(64), /* Outbound pkts sent over             
*                                      IQDC last SMF interval    @54A*/         
*    /*End of SMF IQDX/IQDC Stats area                      @M9A @54C*/         
*    3 SMF119IS_IFPNetID   CHAR(16); /* Physical network ID      @VCA*/         
*                                                                               
*/********************************************************************/         
*/* Constants for Link Description                                   */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119IS_IFLink_ATM    CHAR(18) CONSTANT('ATM               '),             
*   SMF119IS_IFLink_CDLC   CHAR(18) CONSTANT('CDLC              '),             
*   SMF119IS_IFLink_CTC    CHAR(18) CONSTANT('CTC               '),             
*   SMF119IS_IFLink_Enet   CHAR(18) CONSTANT('ETHERnet          '),             
*   SMF119IS_IFLink_Eor802 CHAR(18) CONSTANT('ETHEROR802.3      '),             
*   SMF119IS_IFLink_FDDI   CHAR(18) CONSTANT('FDDI              '),             
*   SMF119IS_IFLink_HCH    CHAR(18) CONSTANT('HCH               '),             
*   SMF119IS_IFLink_IBMTR  CHAR(18) CONSTANT('IBMTR             '),             
*   SMF119IS_IFLink_IP     CHAR(18) CONSTANT('IP                '),             
*   SMF119IS_IFLink_IPQEn  CHAR(18) CONSTANT('IPAQENET          '),             
*   SMF119IS_IFLink_IPQDio CHAR(18) CONSTANT('IPAQIDIO          '),             
*   SMF119IS_IFLink_IPQTr  CHAR(18) CONSTANT('IPAQTR            '),             
*   SMF119IS_IFLink_MPC    CHAR(18) CONSTANT('MPCPTP            '),             
*   SMF119IS_IFLink_OSAEn  CHAR(18) CONSTANT('OSAENET           '),             
*   SMF119IS_IFLink_OSAFd  CHAR(18) CONSTANT('OSAFDDI           '),             
*   SMF119IS_IFLink_Same   CHAR(18) CONSTANT('SAMEHOST          '),             
*   SMF119IS_IFLink_Unk    CHAR(18) CONSTANT('Unknown           '),             
*   SMF119IS_IFLink_802    CHAR(18) CONSTANT('802.3             '),             
*   SMF119IS_IFLink_IPQEn6 CHAR(18) CONSTANT('IPAQENET6         '),  /*         
*                                                                @Q1A*/         
*   SMF119IS_IFLink_MPC6   CHAR(18) CONSTANT('MPCPTP6           '),  /*         
*                                                                @Q1A*/         
*   SMF119IS_IFLink_IPQDio6 CHAR(18) CONSTANT('IPAQIDIO6         '),/*          
*                                                                @R1A*/         
*   SMF119IS_IFLink_IPQIQDX CHAR(18) CONSTANT('IPAQIQDX          '),/*          
*                                                                @M9A*/         
*   SMF119IS_IFLink_IPQIQX6 CHAR(18) CONSTANT('IPAQIQDX6         '),/*          
*                                                                @M9A*/         
*   SMF119IS_IFLink_RNIC   CHAR(18) CONSTANT('RNIC              '), /*          
*                                                                @V7A*/         
*   SMF119IS_IFLink_ISM    CHAR(18) CONSTANT('ISM               '), /*          
*                                                                @41A*/         
*   SMF119IS_IFLink_IPQIQDC CHAR(18) CONSTANT('IPAQIQDC          '),/*          
*                                                                @54A*/         
*   SMF119IS_IFLink_IPQIQC6 CHAR(18) CONSTANT('IPAQIQDC6         '),/*          
*                                                                @54A*/         
*   SMF119IS_IFLink_ZCX     CHAR(18) CONSTANT('ZCX               '),/*          
*                                                                @58A*/         
*   SMF119IS_IFLink_ZCX6    CHAR(18) CONSTANT('ZCX6              ');/*          
*                                                                @58A*/         
*                                                                               
*/********************************************************************/         
*/* Additional IP address section portion of the record              */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119IS_IFIP BASED,                                                       
*    3 SMF119IS_IFAddIntfName  CHAR(16), /* Link name            @Q1A*/         
*    3 SMF119IS_IFAddIntfHome  CHAR(16); /* IPv6 address         @Q1A*/         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* Server Port Statistics record (subtype 07). Two different type   */         
*/* of statistics can be reported: TCP or UDP. Each has a distinct   */         
*/* mapping.                                                         */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
*/********************************************************************/         
*/* TCP Server Port statistics                                       */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SP_TC BASED,                                                         
*    3 SMF119SP_TCDuration BIT(64), /* Duration of recording (STCK              
*                                      interval)                     */         
*    3 SMF119SP_TCRName  CHAR(8),   /* Server socket resource name   */         
*    3 SMF119SP_TCBindIP CHAR(16),  /* Local IP address, for Bind               
*                                    specific reservations           */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119SP_TCBindFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119SP_TCBindIPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119SP_TCPort   CHAR(2),   /* Port number                   */         
*    3 *                 CHAR(2),   /* Unused                        */         
*    3 SMF119SP_TCConn   FIXED(32), /* Successful connection                    
*                                      establishments this interval  */         
*    3 SMF119SP_TCBinds  FIXED(32), /* Socket binds received for this           
*                                      port during interval          */         
*    3 SMF119SP_TCBusySrv  FIXED(32), /* Connections rejected due to            
*                                      Server Busy conditions on this           
*                                      port during interval          */         
*    3 SMF119SP_TCSynAttack  FIXED(32), /* Connections rejected due to          
*                                      SYN attack detections on this            
*                                      port during interval          */         
*    3 SMF119SP_TCHighWater  FIXED(32), /* Highest number of active             
*                                      TCP connections for this port            
*                                      during interval               */         
*    3 SMF119SP_TCNumConns FIXED(32); /* Number of active connections           
*                                      on this port                  */         
*                                                                               
*/********************************************************************/         
*/* UDP Server Port statistics                                       */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SP_UD BASED,                                                         
*    3 SMF119SP_UDDuration BIT(64), /* Duration of recording (STCK              
*                                      interval)                     */         
*    3 SMF119SP_UDRName  CHAR(8),   /* Server socket resource name   */         
*    3 SMF119SP_UDBindIP CHAR(16),  /* Local IP address, for Bind               
*                                      specific reservations         */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119SP_UDBindFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119SP_UDBindIPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119SP_UDPort   CHAR(2),   /* Port number                   */         
*    3 *                 CHAR(2),   /* Unused                        */         
*    3 SMF119SP_UDIDgrams BIT(64),  /* Inbound UDP datagrams to this            
*                                      server port this interval     */         
*    3 SMF119SP_UDODgrams BIT(64),  /* Outbound UDP datagrams to this           
*                                      server port this interval     */         
*    3 SMF119SP_UDIBytes BIT(64),   /* Inbound bytes on this server             
*                                      port during interval          */         
*    3 SMF119SP_UDOBytes BIT(64);   /* Outbound bytes on this server            
*                                      port during interval          */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TCP/IP Stack Start/Stop record (subtype 08)                      */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119TC_ST BASED,                                                         
*    3 SMF119TC_STType   CHAR(1),   /* Event type                    */         
*    3 SMF119TC_STFlags  CHAR(1),   /* Flags                     @Q1A*/         
*      5 SMF119TC_STType_FlagsIPv6   BIT(1), /* IPv6 enabled stac@E1M*/         
*      5 SMF119TC_STType_FlagsIPSEC  BIT(1), /* IPSEC stack      @E1A*/         
*      5 SMF119TC_STType_FlagsIPSEC6 BIT(1), /* IPSEC6 stack     @E1A*/         
*      5 *                           BIT(5),                                    
*    3 *                 CHAR(2),   /* unused, available         @Q1C*/         
*    3 SMF119TC_STTime   BIT(32),   /* Event time of day             */         
*    3 SMF119TC_STDate   BIT(32),   /* Event date                    */         
*    3 SMF119TC_STECSAMax        BIT(64), /* ECSA high watermark @E2A*/         
*    3 SMF119TC_STECSALimit      BIT(64), /* ECSA config limit   @E2A*/         
*    3 SMF119TC_STPrivateMax     BIT(64), /* Private highwater   @E2A*/         
*    3 SMF119TC_STPrivateLimit   BIT(64); /* Private config limit@E2A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for Event Type                                         */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119TC_STType_Start    CHAR(1) CONSTANT('80'x), /* Stack start */         
*   SMF119TC_STType_NormTerm CHAR(1) CONSTANT('40'x), /* Stack normal           
*                                      termination                   */         
*   SMF119TC_STType_AbnTerm  CHAR(1) CONSTANT('20'x); /* Stack                  
*                                      unplanned termination         */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* UDP Socket Close records (subtype 10)                            */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119UD_UC BASED,                                                         
*    3 SMF119UD_UCRName  CHAR(8),   /* Socket resource name          */         
*    3 SMF119UD_UCConnID FIXED(32), /* UDP socket resource ID                   
*                                      (connection ID)               */         
*    3 SMF119UD_UCSubTask  FIXED(32), /* Subtask name                */         
*    3 SMF119UD_UCOTime  BIT(32),   /* Socket Open time (UTC)        */         
*    3 SMF119UD_UCODate  FIXED(32), /* Socket Open date              */         
*    3 SMF119UD_UCCTime  BIT(32),   /* Socket Close time (UTC)       */         
*    3 SMF119UD_UCCDate  FIXED(32), /* Socket Close date             */         
*    3 SMF119UD_UCRIP    CHAR(16),  /* Remote IP address (IPv6) of last         
*                                      datagram received         @A3C*/         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119UD_UCRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119UD_UCRIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119UD_UCLIP    CHAR(16),  /* Local IP address (IPv6) at time          
*                                      of socket close               */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119UD_UCLIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119UD_UCLIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119UD_UCRPort  CHAR(2),   /* Remote port number of last               
*                                      datagram received         @A3C*/         
*    3 SMF119UD_UCLPort  CHAR(2),   /* Local port number at time of             
*                                      socket close                  */         
*    3 SMF119UD_UCType   CHAR(1),   /* UDP Socket Type               */         
*    3 SMF119UD_UCReason CHAR(1),   /* UDP Socker Close reason       */         
*    3 *                 CHAR(2),   /* unused                        */         
*    3 SMF119UD_UCInDgrams BIT(64),  /* Inbound UDP datagrams        */         
*    3 SMF119UD_UCOutDgrams  BIT(64),  /* Outbound UDP datagrams     */         
*    3 SMF119UD_UCInBytes  BIT(64), /* Inbound bytes                 */         
*    3 SMF119UD_UCOutBytes BIT(64); /* Outbound bytes                */         
*                                                                               
*/********************************************************************/         
*/* Constants for Socket Type                                        */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119UD_UCType_NonEE CHAR(1) CONSTANT('01'X), /* Standard UDP   */         
*   SMF119UD_UCType_EE    CHAR(1) CONSTANT('02'X); /* Enterprise                
*                                      Extender UDP                  */         
*                                                                               
*/********************************************************************/         
*/* Constants for Socket Close Reason                                */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119UD_UCReason_Norm CHAR(1) CONSTANT('01'X), /* Normal Close  */         
*   SMF119UD_UCReason_Abnr CHAR(1) CONSTANT('02'X); /* Abnormal Close*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* zERT Security Session Attributes event record (subtype 11)       */         
*/*                                                              @47A*/         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SC_SA Based,             /*                           @47A*/         
*    3 SMF119SC_SAEvent_Type Fixed(8), /* Event type             @47A*/         
*    3 SMF119SC_SASecProtos Bit(8),    /* Security Protocol(s)   @47A*/         
*      5 SMF119SC_SASecProto_TLS Bit(1), /* TLS/SSL              @47A*/         
*      5 SMF119SC_SASecProto_SSH Bit(1), /* SSH                  @47A*/         
*      5 SMF119SC_SASecProto_IPSec Bit(1), /* IPSec              @47A*/         
*      5 *                Bit(5),   /* unused                    @47A*/         
*    3 SMF119SC_SAFlags          Bit(8), /* Flags                @47A*/         
*      5 SMF119SC_SAIPv6  Bit(1),   /* IPv6                      @47A*/         
*      5 SMF119SC_SAATTLS_Optimized Bit(1), /* AT-TLS optimized                 
*                                      security session          @55A*/         
*      5 SMF119SC_SAEnforcement Bit(1), /* Connection reset by zERT             
*                                           Enforcement action   @68A*/         
*      5 *                Bit(5),   /* unused          @47A @55C @68C*/         
*    3 SMF119SC_SASecFlags Bit(8),  /* IP security flags         @47A*/         
*      5 SMF119SC_SAIPSec_On Bit(1), /* IPSec enabled            @47A*/         
*      5 SMF119SC_SAIPSec_IPv6_On Bit(1), /* IPSec enabled                      
*                                      for IPv6                  @47A*/         
*      5 SMF119SC_SAIPFiltering Bit(1), /* IP Filtering performed               
*                                      for this connection       @47A*/         
*      5 *                     Bit(5), /* Reserved               @47A*/         
*    3 SMF119SC_SAIPProto Fixed(8), /* IP protocol               @47A*/         
*    3 *                 Char(3),   /* unused                    @47A*/         
*    3 SMF119SC_SAJobname    Char(8), /* Job name                @47A*/         
*    3 SMF119SC_SAJobID      Char(8), /* Job ID                  @47A*/         
*    3 SMF119SC_SAUserID     Char(8), /* z/OS user ID            @47A*/         
*    3 SMF119SC_SASTime  BIT(32),   /* Connection start time (UTC)              
*                                                                @47A*/         
*    3 SMF119SC_SASDate  FIXED(32), /* Connection start date     @47A*/         
*    3 SMF119SC_SAETime  BIT(32),   /* Connection end time (UTC) @47A*/         
*    3 SMF119SC_SAEDate  FIXED(32), /* Connection end date       @47A*/         
*    3 SMF119SC_SARIP Union,        /* Remote connection endpoint               
*                                      IP address                @47A*/         
*      5 SMF119SC_SARIP_IPv4  Char(4), /* IPv4                   @47A*/         
*      5 SMF119SC_SARIP_IPv6  Char(16), /* IPv6                  @47A*/         
*    3 SMF119SC_SALIP Union,        /* Local connection endpoint                
*                                      IP address                @47A*/         
*      5 SMF119SC_SALIP_IPv4  Char(4), /* IPv4                   @47A*/         
*      5 SMF119SC_SALIP_IPv6  Char(16), /* IPv6                  @47A*/         
*    3 SMF119SC_SARPort  Char(2),   /* Remote port number        @47A*/         
*    3 SMF119SC_SALPort  Char(2),   /* Local port number         @47A*/         
*    3 SMF119SC_SAConnID Char(4),   /* Transport layer connection ID            
*                                                                @47A*/         
*    3 SMF119SC_SAInBytes Bit(64),  /* Inbound bytes since start of             
*                                      connection                @47A*/         
*    3 SMF119SC_SAOutBytes Bit(64), /* Outbound bytes since start of            
*                                      connection                @47A*/         
*    3 SMF119SC_SAInSegDG Bit(64),  /* Inbound TCP segments or UDP              
*                                      datagrams since start of                 
*                                      connection                @47A*/         
*    3 SMF119SC_SAOutSegDG Bit(64), /* Outbound TCP segments or UDP             
*                                      datagrams since start of                 
*                                      connection                @47A*/         
*    3 *                 Char(8),   /* unused                    @47A*/         
*    3 SMF119SC_SAEnd    Char(0);   /* End of section            @47A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* zERT (subtype 11) TLS section                                    */         
*/*                                                              @47A*/         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SC_TLS_data Based,       /*                           @47A*/         
*    3 SMF119SC_TLS_Prot_Ver Fixed(16), /* SSL/TLS Prot                         
*                                      Version                   @47A*/         
*    3 SMF119SC_TLS_Source Fixed(8), /* Source of information    @47A*/         
*    3 SMF119SC_TLS_Handshake_Type Fixed(8), /* Handshake method @47A*/         
*    3 SMF119SC_TLS_Handshake_Role Fixed(8), /* Handshake role   @47A*/         
*    3 *                     Char(2), /* Reserved                @47A*/         
*    3 SMF119SC_TLS_Session_ID_Len Fixed(8),/* TLS Session ID                   
*                                           Length (in bytes)    @47A*/         
*    3 SMF119SC_TLS_Session_ID     Char(32), /* TLS Session ID                  
*                                                                @47A*/         
*    3 SMF119SC_TLS_Protocol_Provider Char(16), /* Cryptographic                
*                                        Protocol Provider (CPP) @47A*/         
*    3 SMF119SC_TLS_Neg_Cipher Union, /* Negotiated Cipher in                   
*                                        EBCDIC                  @47A*/         
