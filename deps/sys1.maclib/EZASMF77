*                                      /* LUNS Backup        0       */         
*    7 SMF119TN_TGresv3      Bit(6),   /* Reserved               @VDA*/         
*   5 SMF119TN_TGFlag4       Bit(8),   /* Flags                  @VDA*/         
*    7 SMF119TN_TGresv4      Bit(8),   /* Reserved               @VDA*/         
*                                                                               
*  3 SMF119TN_TGTCPName      Char(8),  /* TCPIP Jobname          @VDA*/         
*                                                                               
*  3 SMF119TN_TGSACacheTime  Fixed(31),/* TNSACONFIG CacheTime   @VDA*/         
*  3 SMF119TN_TGSACommName   Char(32), /* TNSACONFIG Community   @VDA*/         
*                                      /*   blank if in a SMF record */         
*                                      /*   Non blank in a NMI call  */         
*  3 SMF119TN_TGSAPort       Fixed(16),/* TNSACONFIG AGENT Port  @VDA*/         
*                                                                               
*  3 SMF119TN_TGresv5        Char(2),  /* Alignment              @VDA*/         
*  3 SMF119TN_TGXCFSubplex   Char(8),  /* XCF  Subplex name      @VDA*/         
*  3 SMF119TN_TGXCFMon       Fixed(31),/* XCF  XCFMonitor        @VDA*/         
*  3 SMF119TN_TGXCFConnTO    Fixed(31),/* XCF  ConnectionTimeoout@VDA*/         
*  3 SMF119TN_TGXCFRcvyTO    Fixed(31),/* XCF  RecoveryTimeoout  @VDA*/         
*                                                                               
*  3 SMF119TN_TGresv6        Char(2),  /* Alignment              @VDA*/         
*  3 SMF119TN_TGLUPort       Fixed(16),/* LUNS Port              @VDA*/         
*  3 SMF119TN_TGLUIpAddr     Char(16), /* LUNS Ip address        @VDA*/         
*  3 SMF119TN_TGLURank       Fixed(31),/* LUNS Rank              @VDA*/         
*                                                                               
*  /* SMF119TN_TGLDAPPort, SMF119TN_TGLDAPNameLen, and               */         
*  /* SMF119TN_TGLDAPNames are deprecated in V2R5.                   */         
*  /* SMF119TN_TGLDAPPort and SMF119TN_TGLDAPNameLen are set to      */         
*  /* 0. SMF119TN_TGLDAPNames is set to blanks                   @6DA*/         
*  3 SMF119TN_TGLDAPPort     Fixed(16),/* CRL LDAP Port number   @VKC*/         
*  3 SMF119TN_TGLDAPNameLen  Fixed(15),/* CRL LDAP Name Length   @VKC*/         
*  3 SMF119TN_TGLDAPNames    Char(256),/* CRL LDAP Names         @VKC*/         
*                                      /*  A set of up to five names */         
*                                      /*  separated by a space  @VDA*/         
*  3 SMF119TN_TG_End         Char(0)   Bdy(Word);             /* @VDA*/         
*                                                                               
*Declare                                                                        
*  SMF119TN_TG_X             Fixed     CONSTANT(3),           /* @VDA*/         
*                                       /* Section index             */         
*  SMF119TN_TGEyeC           Char(4) CONSTANT('TNTG');        /* @VDA*/         
*                                   /* SMF119TN_TGEye field constant */         
*                                                                               
* /*******************************************************************/         
* /* Telnet Parms parameters section                                 */         
* /*        SMF type 119 Subtype 24 Section  4 (SMF119S4)            */         
* /* Telnet ParmGroup parameters section                             */         
* /*        SMF type 119 Subtype 24 Section 19 (SMF119S19)           */         
* /*******************************************************************/         
*                                                                               
*Declare                                                                        
*1 SMF119TN_TP               Based,                           /* @VDA*/         
* 3 SMF119TN_TPEye           Char(4),  /* "TNTP" eyecatcher      @VDA*/         
* 3 SMF119TN_TPPortNum       Fixed(16),/* Port number            @VDA*/         
