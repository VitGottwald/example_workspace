*  5 SMF119TN_TPFlag08       Bit(8),   /* Flag 08                @VDA*/         
*   7 SMF119TN_TPTN3270E     Bit(1),   /* TN3270E             1  @VDA*/         
*                                      /* NOTN3270E           0      */         
*   7 SMF119TN_TPUnlKybd_B   Bit(1),   /* UNLOCK BeforeRead   1  @VDA*/         
*                                      /* UNLOCK AfterRead    0      */         
*   7 SMF119TN_TPTN3270Bind  Bit(1),   /* TN3270BIND          1  @VDA*/         
*                                      /* NOTN3270BINDRead    0      */         
*   7 SMF119TN_TPresv0810    Bit(1),   /* Reserved            1  @VDA*/         
*   7 SMF119TN_TPresv0808    Bit(1),   /* Reserved            1  @VDA*/         
*   7 SMF119TN_TPresv0804    Bit(1),   /* Reserved            1  @VDA*/         
*   7 SMF119TN_TPresv0802    Bit(1),   /* Reserved            1  @VDA*/         
*   7 SMF119TN_TPresv0801    Bit(1),   /* Reserved            1  @VDA*/         
*                                                                               
*  5 SMF119TN_TPFlag09       Bit(8),   /* Flag 09                @VDA*/         
*   7 SMF119TN_TPresv09FF    Bit(1),   /* Reserved            1  @VDA*/         
*                                                                               
*                                                                               
* 3 SMF119TN_TPProfId       Fixed(15), /* Profile Id             @VDA*/         
*                                                                               
* 3 SMF119TN_TPCConnSec     Fixed(31), /* CHECKCLIENTCONN sec    @VDA*/         
* 3 SMF119TN_TPCConnMax     Fixed(31), /* CHECKCLIENTCONN        @VDA*/         
*                                      /*   maxconns                 */         
* 3 SMF119TN_TPCodePage_A   Char(12),  /* Codepage ASCII         @VDA*/         
* 3 SMF119TN_TPCodePage_E   Char(12),  /* Codepage EBCDIC        @VDA*/         
*                                                                               
* /* SMF119TN_TPCipherCnt, SMF119TN_TPV3Cipher, and SMF119TN_TPCipher*/         
* /* are deprecated in V2R5. SMF119TN_TPCipherCnt is set to 0.       */         
* /* SMF119TN_TPV3Cipher/SMF119TN_TPCipher is set to blanks.     @6DA*/         
* 3 SMF119TN_TPCipherCnt    Fixed(15), /* Number of encryptions  @VDA*/         
* 3 SMF119TN_TPV3Cipher     Char(64),  /* encryption algorithms  @VDA*/         
*   5 SMF119TN_TPCipher(32) Char(2),   /* encryption algorithms  @VPC*/         
*                                                                               
* /* SMF119TN_TPKeyRingLen is deprecated in V2R5, set to 0       @6DA*/         
* 3 SMF119TN_TPKeyRingLen   Fixed(15), /* Length of TPKeyRing    @VDA*/         
* 3 SMF119TN_TPInactSec     Fixed(31), /* INACTIVE sec           @VDA*/         
* 3 SMF119TN_TPKeepInact    Fixed(31), /* KEEPINACTIVE sec       @VDA*/         
* 3 SMF119TN_TPKeepLUSec    Fixed(31), /* KEEPLU sec             @VDA*/         
* /* SMF119TN_TPKeyRing is deprecated in V2R5, set to blanks     @6DA*/         
* 3 SMF119TN_TPKeyRing      Char(256), /* KEYRING data set name  @VDA*/         
*                                      /*  See SMF119TN_TPKeyRingLen */         
*                                      /*  Last 255 bytes of name    */         
*                                      /*  if longer than 255        */         
* 3 SMF119TN_TPMaxRcv       Fixed(31), /* MAXRECEIVE bytes       @VDA*/         
* 3 SMF119TN_TPMaxReqSess   Fixed(31), /* MAXREQSESS num_req     @VDA*/         
* 3 SMF119TN_TPMax_RUChain  Fixed(31), /* MAXRUCHAIN num_RUs     @VDA*/         
* 3 SMF119TN_TPMaxTcpSendQ  Fixed(31), /* MAXTCPSENDQ bytes      @VDA*/         
* 3 SMF119TN_TPMaxVtamSend  Fixed(31), /* MAXVTAMSENDQ num_rpls  @VDA*/         
* 3 SMF119TN_TPNacUserId    Char(8),   /* NACUSERID NAC_name     @VDA*/         
* 3 SMF119TN_TPPrtInact     Fixed(31), /* PRTINACTIVE sec        @VDA*/         
* 3 SMF119TN_TPProfInact    Fixed(31), /* PROFILEINACTIVE sec    @VDA*/         
* 3 SMF119TN_TPScan_Sec     Fixed(31), /* SCANINTERVAL sec1      @VDA*/         
* 3 SMF119TN_TPTMark_Sec    Fixed(31), /* TIMEMARK sec2          @VDA*/         
* 3 SMF119TN_TPSMFInit118   Fixed(8),  /* SMFINIT nn             @VDA*/         
* 3 SMF119TN_TPSMFTerm118   Fixed(8),  /* SMFTERM nn             @VDA*/         
* 3 SMF119TN_TPresv20       Fixed(15), /* Alignment              @VDA*/         
* /* SMF119TN_TPSSL_Sec is deprecated in V2R5, set to 0          @6DA*/         
* 3 SMF119TN_TPSSL_Sec      Fixed(31), /* SSLTIMEOUT sec         @VDA*/         
* 3 SMF119TN_TPTelnetDevice,           /* TELNETDEVICE           @VDA*/         
*   5 SMF119TN_TPDevEntry(SMF119TN_TPTDCnt),                  /* @VDA*/         
*                                      /*  Device logmode entries@VDA*/         
*    7 SMF119TN_TPLogmode   Char(8),   /*   TN3270 Logmode       @VDA*/         
*    7 SMF119TN_TPeLogmode  Char(8),   /*   TN3270E Logmode      @VDA*/         
* 3 SMF119TN_TPTKOGLu_sec   Fixed(31), /* TKOGENLU  sec          @VDA*/         
* 3 SMF119TN_TPTKOSLu_sec   Fixed(31), /* TKOSPECLU  sec         @VDA*/         
*                                                                               
* 3 SMF119TN_TPDBG_Flags         Bit(64),    /* Flags for debug  @VDA*/         
*  4 SMF119TN_TPDBG_Connection    Bit(16),   /*                  @VDA*/         
*   5 SMF119TN_TPDBG_ConnFlg       Bit(8),   /* DEBUG CONN flags @VDA*/         
*    6 SMF119TN_TPDBG_ConnExc       Bit(1),  /* DEBUG CONN EXCE  @VDA*/         
*    6 SMF119TN_TPDBG_ConnSum       Bit(1),  /* DEBUG CONN SUMM  @VDA*/         
*    6 SMF119TN_TPDBG_ConnDet       Bit(1),  /* DEBUG CONN DETAIL@VDA*/         
*    6 SMF119TN_TPDBG_ConnOff       Bit(1),  /* DEBUG CONN OFF   @VDA*/         
*    6 SMF119TN_TPDBG_ConnVOff      Bit(1),  /* DEBUG CONN Vary off  */         
*    6 SMF119TN_TPDBG_resv05        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_ConnFlg_Route Bit(2),  /* CONSOLE!JOBLOG!CTRACE*/         
*                                                                               
*   5 SMF119TN_TPDBG_ConnTrace     Bit(8),   /* DEBUG CONN TRACE flag*/         
*    6 SMF119TN_TPDBG_ConnTrc       Bit(1),  /* DEBUG CONN TRACE @VDA*/         
*    6 SMF119TN_TPDBG_ConnTrcOff    Bit(1),  /* DEBUG CONN NOTRACE   */         
*    6 SMF119TN_TPDBG_ConnTrcVOff   Bit(1),  /* Vary Off         @VDA*/         
*    6 SMF119TN_TPDBG_resv14        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_ConnTrc_Route Bit(2),  /* CONSOLE!JOBLOG!CTRACE*/         
*    6 SMF119TN_TPDBG_resv16        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_resv17        Bit(1),  /* Reserved         @VDA*/         
*                                                                               
*  4 SMF119TN_TPDBG_Task          Bit(16),   /*                  @VDA*/         
*   5 SMF119TN_TPDBG_TaskFlg       Bit(8),   /* DEBUG TASK flags @VDA*/         
*    6 SMF119TN_TPDBG_TaskExc       Bit(1),  /* DEBUG TASK EXCEPTION */         
*    6 SMF119TN_TPDBG_resv21        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_TaskDet       Bit(1),  /* DEBUG TASK DETAIL@VDA*/         
*    6 SMF119TN_TPDBG_TaskOff       Bit(1),  /* DEBUG TASK OFF   @VDA*/         
*    6 SMF119TN_TPDBG_TaskVOff      Bit(1),  /* Vary Off         @VDA*/         
*    6 SMF119TN_TPDBG_resv25        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_TaskFlg_Route Bit(2),  /* CONSOLE!JOBLOG!CTRACE*/         
*   5 SMF119TN_TPDBG_resv3x        Bit(8),   /* Reserved         @VDA*/         
*                                                                               
*  4 SMF119TN_TPDBG_Config        Bit(16),   /*                  @VDA*/         
*   5 SMF119TN_TPDBG_ConfFlg       Bit(8),   /* DEBUG CONFIG flags   */         
*    6 SMF119TN_TPDBG_ConfExc       Bit(1),  /* DEBUG CONFIG EXCEPTIO*/         
*    6 SMF119TN_TPDBG_resv41        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_resv42        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_ConfOff       Bit(1),  /* DEBUG CONFIG OFF @VDA*/         
*    6 SMF119TN_TPDBG_ConfVOff      Bit(1),  /* Vary Off         @VDA*/         
*    6 SMF119TN_TPDBG_resv45        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_ConfFlg_Route Bit(2),  /* CONSOLE!JOBLOG!CTRACE*/         
*                                                                               
*   5 SMF119TN_TPDBG_ConfTrace     Bit(8),   /*                  @VDA*/         
*    6 SMF119TN_TPDBG_ConfTrc       Bit(1),  /* DEBUG CONFIG TRACE   */         
*    6 SMF119TN_TPDBG_ConfTrcOff    Bit(1),  /* DEBUG CONFIG NOTRACE */         
*    6 SMF119TN_TPDBG_ConfTrcVOff   Bit(1),  /* Vary Off         @VDA*/         
*    6 SMF119TN_TPDBG_resv54        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_ConfTrc_Route Bit(2),  /* CONSOLE!JOBLOG!CTRACE*/         
*    6 SMF119TN_TPDBG_resv56        Bit(1),  /* Reserved         @VDA*/         
*    6 SMF119TN_TPDBG_resv57        Bit(1),  /* Reserved         @VDA*/         
*                                                                               
*  4 SMF119TN_TPDBG_resv6x7x      Bit(16),   /* Reserved         @VDA*/         
*   5 SMF119TN_TPDBG_resv6x        Bit(8),   /* Reserved         @VDA*/         
*   5 SMF119TN_TPDBG_resv7x        Bit(8),   /* Reserved         @VDA*/         
*                                                                               
*                                                                               
* 3 SMF119TN_TP_End         Char(0)   Bdy(Word);              /* @VDA*/         
*                                                                               
*Declare                                                                        
*  SMF119TN_TP_X           Fixed     CONSTANT(4),             /* @VDA*/         
*                                       /* Section index             */         
*  SMF119TN_TPEyeC         Char(4) CONSTANT('TNTP');          /* @VDA*/         
*                                   /* SMF119TN_TPEye field constant */         
*                                                                               
* /* SMF119TN_TPDevEntry index values                            @VDA*/         
*Declare                                                                        
*  SMF119TN_TPTDCnt              Fixed Constant(21)           /* @VDA*/         
* ,SMF119TN_TP_IBM_3277          Fixed Constant( 1)           /* @VDA*/         
* ,SMF119TN_TP_IBM_3278_2_E      Fixed Constant( 2)           /* @VDA*/         
