*%dcl ezasmf77_inc fixed external;                                              
*%if ezasmf77_inc=0 %then %do;                                                  
*%ezasmf77_inc=1;                                                               
*%GOTO SMF771; /*                                                               
         MACRO                                                                  
         EZASMF77 &FTP=YES,       YES = GEN FTP SMF DSECTS             X        
               &TELNET=YES,       YES = GEN TELNET SMF DSECTS          X        
               &HEADER=YES,       YES = SMF HEADER DSECTS FOR ALL RECS X        
               &API=YES,          YES = GEN SMF API DSECTS             X        
               &STAT=YES,         YES = GEN INTERVAL STATISTICS DSECTS X        
               &IPSEC=YES,        YES = GEN IPSEC SMF DSECTS  @E1C @E4CX        
               &PROF=YES,         YES = GEN PROFILE SMF DSECTS     @F9AX        
               &DVIPA=YES,        YES = GEN DVIPA SMF DSECTS       @H8AX        
               &SMCD=YES,         YES = GEN SMCD SMF DSECTS        @41AX        
               &SMCR=YES,         YES = GEN SMCR SMF DSECTS        @V7AX        
               &VTAM=YES,         YES = GEN VTAM IDS 3270          @4BAX        
               &MAIL=YES          YES = GEN CSSMTP  SMF DSECTS     @H2A         
         AGO   .M010000      */                                                 
*%SMF771: ;                                                                     
