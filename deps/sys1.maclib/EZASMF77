*/*                                                                  */         
*/* zERT Security Session Summary event record (subtype 12)          */         
*/*                                                              @51A*/         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SS_SA Based,             /*                           @51A*/         
*    3 SMF119SS_SAIntervalDuration BIT(64), /* Duration of recording            
*                                      (STCK interval)           @51A*/         
*    3 SMF119SS_SAEvent_Type Fixed(8), /* Event type             @51A*/         
*    3 SMF119SS_SAFlags     Bit(8), /* Flags                     @51A*/         
*      5 SMF119SS_SAIPv6    Bit(1), /* IPv6                      @51A*/         
*      5 SMF119SS_SAServer Bit(1),  /* Local end of session is server           
*                                      (used only for sessions                  
*                                      protecting TCP traffic)   @51A*/         
*      5 SMF119SS_SAClient Bit(1),  /* Local end of session is client           
*                                      (used only for sessions                  
*                                      protecting TCP traffic)   @51A*/         
*      5 SMF119SS_SAEEConn Bit(1),  /* EE connection (used only for             
*                                      sessions protecting UDP)  @51A*/         
*      5 SMF119SS_SAActive_FTP Bit(1), /* Active FTP connections @51A*/         
*      5 SMF119SS_SAATTLS_Optimized Bit(1), /* AT-TLS optimized                 
                                       security session          @55A*/         
       5 SMF119SS_SATCPIP_INTVAL Bit(1),    /* INTVAL specified  @63A*/         
*      5 *                  Bit(1), /* unused           51A @55C @63C*/         
*    3 SMF119SS_SASecProtos Bit(8), /* Security Protocols        @51A*/         
*      5 SMF119SS_SASecProto_TLS Bit(1), /* TLS/SSL              @51A*/         
*      5 SMF119SS_SASecProto_SSH Bit(1), /* SSH                  @51A*/         
*      5 SMF119SS_SASecProto_IPSec Bit(1), /* IPSec              @51A*/         
*      5 *                  Bit(5), /* unused                    @51A*/         
*    3 SMF119SS_SAJobname   Char(8), /* Job name                 @51A*/         
*    3 SMF119SS_SAUserID    Char(8), /* z/OS user ID             @51A*/         
*    3 SMF119SS_SAIPProto   Fixed(8), /* IP protocol             @51A*/         
*    3 SMF119SS_SASrvIP Union,      /* Server connection endpoint               
*                                      IP address                @51A*/         
*      5 SMF119SS_SASrvIP_IPv4 Char(4),  /* IPv4                 @51A*/         
*      5 SMF119SS_SASrvIP_IPv6 Char(16), /* IPv6                 @51A*/         
*    3 SMF119SS_SACltIP Union,      /* Client connection endpoint               
*                                      IP address                @51A*/         
*      5 SMF119SS_SACltIP_IPv4 Char(4),  /* IPv4                 @51A*/         
*      5 SMF119SS_SACltIP_IPv6 Char(16), /* IPv6                 @51A*/         
*    3 SMF119SS_SASrvPortStart Char(2), /* Starting value of server             
*                                      port range                @51A*/         
*    3 SMF119SS_SASrvPortEnd Char(2), /* Ending value of server                 
*                                      port range                @51A*/         
*    3 SMF119SS_SASessionID  Char(42), /* Session ID             @51A*/         
*    3 *                     Char(2), /* Unused, alignment       @51A*/         
*    3 SMF119SS_SAInitLifeConnCnt Fixed(32), /* Interval starting               
*                                      value for connection count for           
*                                      the life of this session  @51A*/         
*    3 SMF119SS_SAInitLifePartialConnCnt Fixed(32), /* Interval                 
*                                      starting value for partial               
*                                      connection count          @51A*/         
*    3 SMF119SS_SAInitLifeShortConnCnt Fixed(32), /* Interval                   
*                                      starting value for short term            
*                                      connection count          @51A*/         
*    3 SMF119SS_SAInitActiveConnCnt Fixed(32), /* Interval starting             
*                                      value for active connection              
*                                      count for this session    @51A*/         
*    3 SMF119SS_SAInitLifeInBytes  Bit(64), /* Interval starting                
*                                      value for inbound byte count             
*                                      for this session          @51A*/         
*    3 SMF119SS_SAInitLifeOutBytes Bit(64), /* Interval starting                
*                                      value for outbound byte count            
*                                      for this session          @51A*/         
*    3 SMF119SS_SAInitLifeInSegDG  Bit(64), /* Interval starting                
*                                      value for inbound TCP segments           
*                                      or UDP datagrams          @51A*/         
*    3 SMF119SS_SAInitLifeOutSegDG Bit(64), /* Interval starting value          
*                                      for outbound TCP segments                
*                                      or UDP datagrams          @51A*/         
*    3 SMF119SS_SAEndLifeConnCnt Fixed(32), /* Interval ending                  
*                                      value for connection count for           
*                                      the life of this session  @51A*/         
*    3 SMF119SS_SAEndLifePartialConnCnt Fixed(32), /* Interval                  
*                                      ending value for partial                 
*                                      connection count          @51A*/         
*    3 SMF119SS_SAEndLifeShortConnCnt Fixed(32), /* Interval                    
*                                      ending value for short term              
*                                      connection count          @51A*/         
*    3 SMF119SS_SAEndActiveConnCnt Fixed(32), /* Interval ending                
*                                      value for active connection              
*                                      count for this session    @51A*/         
*    3 SMF119SS_SAEndLifeInBytes   Bit(64), /* Interval ending                  
*                                      value for inbound byte count             
*                                      for this session          @51A*/         
*    3 SMF119SS_SAEndLifeOutBytes  Bit(64), /* Interval ending                  
*                                      value for outbound byte count            
*                                      for this session          @51A*/         
*    3 SMF119SS_SAEndLifeInSegDG   Bit(64), /* Interval ending value            
*                                      for inbound TCP segments                 
*                                      or UDP datagrams          @51A*/         
*    3 SMF119SS_SAEndLifeOutSegDG  Bit(64), /* Interval ending                  
*                                      value for outbound TCP segments          
*                                      or UDP datagrams          @51A*/         
*    3 SMF119SS_SAEnd       Char(0); /* End of section           @51A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* zERT (subtype 12) TLS section                                    */         
*/*                                                              @51A*/         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SS_TLS_data Based,       /*                           @51A*/         
*    3 SMF119SS_TLS_Source  Fixed(8), /* Source of information   @51A*/         
*    3 SMF119SS_TLS_CryptoFlags Bit(8), /* Cryptographic operations             
*                                        flags                   @51A*/         
*      5 SMF119SS_TLS_Enc_Then_Mac Bit(1), /* Encrypt-then-MAC                  
*                                        processing order        @51A*/         
*      5 SMF119SS_TLS_Raw_Public_Key_Auth Bit(1), /* Raw public key             
*                                        authentication was used @5DA*/         
*      5 *                      Bit(1), /* Reserved              @5DA*/         
*      5 SMF119SS_TLS_Preshared_Key_Auth  Bit(1), /* Preshared key              
*                                        authentication was used @5DA*/         
*      5 *                      Bit(4), /* Reserved              @51A*/         
*    3 SMF119SS_TLS_Prot_Ver Fixed(16), /* SSL/TLS protocol                     
*                                      version                   @51A*/         
*    3 SMF119SS_TLS_Neg_Cipher Union, /* Negotiated Cipher in                   
*                                      EBCDIC                    @51A*/         
*      5 SMF119SS_TLS_Neg_Cipher_v2 Char(6), /* Cipher for SSLv2 @51A*/         
*      5 SMF119SS_TLS_Neg_Cipher_v3 Char(4), /* Cipher for SSLv3 or             
*                                        higher                  @51A*/         
*    3 SMF119SS_TLS_CS_Enc_Alg  Fixed(16), /* Cipher suite                      
*                                        encryption algorithm    @51A*/         
*    3 SMF119SS_TLS_CS_Msg_Auth Fixed(16), /* Cipher suite message              
*                                        authentication algorithm               
*                                                                @51A*/         
*    3 SMF119SS_TLS_CS_Kex_Alg  Fixed(16), /* Cipher suite key                  
*                                        exchange algorithm      @51A*/         
*    3 SMF119SS_TLS_SCert_Data,         /* Server Certificate    @51A*/         
*      5 SMF119SS_TLS_SCert_Signature_Method Fixed(16), /* Server               
*                                        Certificate signature                  
*                                        method                  @51A*/         
*      5 SMF119SS_TLS_SCert_Enc_Method Fixed(16), /* Server                     
*                                        Certificate encryption                 
*                                        method                  @51A*/         
*      5 SMF119SS_TLS_SCert_Digest_Alg Fixed(16), /* Server                     
*                                        Certificate digest                     
*                                        algorithm               @51A*/         
*      5 SMF119SS_TLS_SCert_Key_Type Fixed(16), /* Server Certificate           
*                                        Key type                @51A*/         
*      5 SMF119SS_TLS_SCert_Key_Len Fixed(16), /* Server Certificate            
*                                        Key length (in bits)    @51A*/         
*    3 SMF119SS_TLS_CCert_Data,        /* Client Certificate     @51A*/         
*      5 SMF119SS_TLS_CCert_Signature_Method Fixed(16), /* Client               
*                                        Certificate signature                  
*                                        method                  @51A*/         
*      5 SMF119SS_TLS_CCert_Enc_Method Fixed(16), /* Client                     
*                                        Certificate encryption                 
*                                        method                  @51A*/         
*      5 SMF119SS_TLS_CCert_Digest_Alg Fixed(16), /* Client                     
*                                        Certificate digest                     
*                                        algorithm               @51A*/         
*      5 SMF119SS_TLS_CCert_Key_Type Fixed(16), /* Client Certificate           
*                                        Key type                @51A*/         
*      5 SMF119SS_TLS_CCert_Key_Len Fixed(16), /* Client Certificate            
*                                        Key length (in bits)    @51A*/         
*    3 SMF119SS_TLS_Server_HS_Sig_Method Fixed(16), /* Server                   
*                                        handshake signature method,            
*                                        applicable to TLSv1.2                  
*                                        connections and later   @5EC*/         
*    3 SMF119SS_TLS_Client_HS_Sig_Method Fixed(16), /* Client                   
*                                        handshake signature method,            
*                                        applicable to TLSv1.2                  
*                                        connections and later   @5EA*/         
*    3 SMF119SS_TLS_Neg_Key_Share Fixed(16), /* Negotiated key                  
*                                        key share               @5DA*/         
*    3 SMF119SS_TLS_End     Char(0); /* End of section           @51A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* zERT (subtype 12) SSH section                                    */         
*/*                                                              @51A*/         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SS_SSH_data Based,       /*                           @51A*/         
*    3 SMF119SS_SSH_Source  Fixed(8), /* Source of information   @51A*/         
*    3 *                    Char(1), /* Unused, alignment        @51A*/         
*    3 SMF119SS_SSH_Prot_Ver Fixed(8), /* SSH protocol/version   @51A*/         
*    3 SMF119SS_SSH_CryptoFlags Bit(8), /* Cryptographic operations             
*                                      flags                     @51A*/         
*      5 SMF119SS_SSH_Enc_Then_Mac_In  Bit(1), /* Encrypt-then-MAC              
*                                      processing is used for                   
*                                      inbound traffic           @51A*/         
*      5 SMF119SS_SSH_Enc_Then_Mac_Out Bit(1), /* Encrypt-then-MAC              
*                                      processing is used for                   
*                                      outbound traffic          @51A*/         
*      5 *                  Bit(6),  /* Reserved                 @51A*/         
*    3 SMF119SS_SSH_Auth_Method  Fixed(16), /* First or only                    
*                                      authentication method used               
*                                                                @51A*/         
*    3 SMF119SS_SSH_Auth_Method2 Fixed(16), /* Last authentication              
*                                      method used if multiple are              
*                                      used                      @51A*/         
*    3 SMF119SS_SSH_In_Enc_Alg Fixed(16), /* Encryption algorithm               
*                                      for inbound traffic       @51A*/         
*    3 SMF119SS_SSH_In_Msg_Auth Fixed(16), /* Authentication                    
*                                      algorithm for inbound traffic            
*                                                                @51A*/         
*    3 SMF119SS_SSH_Kex_Method  Fixed(16), /* Key exchange method               
*                                                                @51A*/         
*    3 SMF119SS_SSH_Out_Enc_Alg Fixed(16), /* Encryption algorithm              
*                                      for outbound traffic      @51A*/         
*    3 SMF119SS_SSH_Out_Msg_Auth Fixed(16), /* Authentication                   
*                                      algorithm for outbound traffic           
*                                                                @51A*/         
*    3 SMF119SS_SSH_SKey_Type Fixed(16), /* Server raw key type  @51A*/         
*    3 SMF119SS_SSH_SKey_Len  Fixed(16), /* Server raw key length               
*                                      (in bits)                 @51A*/         
*    3 SMF119SS_SSH_CKey_Type Fixed(16), /* Client raw key type  @51A*/         
*    3 SMF119SS_SSH_CKey_Len  Fixed(16), /* Client raw key length               
*                                      (in bits)                 @51A*/         
*    3 SMF119SS_SSH_SCert_Data,     /* Server Certificate Data   @51A*/         
*      5 SMF119SS_SSH_SCert_Signature_Method Fixed(16), /* Server               
*                                      Certificate signature                    
*                                      method                    @51A*/         
*      5 SMF119SS_SSH_SCert_Enc_Method Fixed(16), /* Server                     
*                                      Certificate encryption                   
*                                      method                    @51A*/         
