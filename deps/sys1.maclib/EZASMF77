*    3 SMF119SM_LILclLnkId      Fixed(32), /* Local SMC link ID  @V7A*/         
*    3 SMF119SM_LIRmtLnkId      Fixed(32), /* Remote SMC link ID @V7A*/         
*    3 SMF119SM_LILnkGrpId      Bit(24),   /* Link group ID      @V7A*/         
*    3 *                        Char,      /* Reserved           @V7A*/         
