SMF119HDST_SMTPStats  EQU X'0034'                                @H2A           
SMF119HDST_FTPServer  EQU X'0046'                                               
SMF119HDST_FTPDConfig EQU X'0047'                                @V8A           
SMF119HDST_FTPLogonF  EQU X'0048'                                               
SMF119HDST_IKETunAct_Ref    EQU X'0049'                          @E1A           
SMF119HDST_IkeTunDeact_Exp  EQU X'004A'                          @E1A           
SMF119HDST_DynTunAct_Ref    EQU X'004B'                          @E1A           
SMF119HDST_DynTunDeact      EQU X'004C'                          @E1A           
SMF119HDST_DynTunAdd        EQU X'004D'                          @E3A           
SMF119HDST_DynTunRem        EQU X'004E'                     @E1A @E3C           
SMF119HDST_ManTunAct        EQU X'004F'                     @E1A @E3C           
SMF119HDST_ManTunDeact      EQU X'0050'                     @E1A @E3C           
SMF119HDST_DS3270     EQU X'0051'        VTAM 3270 IDS           @4BA           
SMF119HDST_SSH94      EQU X'005E'        OpenSSH                 @38A           
SMF119HDST_SSH95      EQU X'005F'        OpenSSH                 @38A           
