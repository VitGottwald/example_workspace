*                                                                               
*   SMF119FT_FxLoginMechPass  CHAR(1)  CONSTANT('P'),  /* Passwd @Q1A*/         
*   SMF119FT_FxLoginMechCert  CHAR(1)  CONSTANT('C'), /* Cert@Q1A@Y4C*/         
*   SMF119FT_FxLoginMechTick  CHAR(1)  CONSTANT('T'),  /* Tick   @Y3A*/         
*                                                                               
*   SMF119FT_FxFips140_Off    FIXED(8) CONSTANT(0),    /* Off    @FBA*/         
*   SMF119FT_FxFips140_On     FIXED(8) CONSTANT(1),    /*On @FBA @46C*/         
*   SMF119FT_FxFips140_Lvl1   FIXED(8) CONSTANT(2),    /* Level1 @46A*/         
*   SMF119FT_FxFips140_Lvl2   FIXED(8) CONSTANT(3),    /* Level2 @46A*/         
*   SMF119FT_FxFips140_Lvl3   FIXED(8) CONSTANT(4);    /* Level3 @46A*/         
*                                                                               
*/********************************************************************/         
