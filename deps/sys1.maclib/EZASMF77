*   7 SMF119TN_TPConnT_Any     Bit(1), /* CONNTYPE ANY           @VDA*/         
*   7 SMF119TN_TPConnT_Basic   Bit(1), /* CONNTYPE BASIC         @VDA*/         
*   7 SMF119TN_TPConnT_None    Bit(1), /* CONNTYPE NONE          @VDA*/         
