*                                             start              @V7A*/         
*    3 SMF119SM_LISSTCK         Bit(64),   /* STCK of SMC-R link                
*                                             start              @V7A*/         
*    3 SMF119SM_LISmcrV2Flags  Bit(8),    /* SMC-Rv2 Flags       @6BA*/         
*      5 SMF119SM_LISmcrVersion Bit(4),   /* SMC version         @6BA*/         
*      5 *                      Bit(4),   /* reserved            @6BA*/         
*    3 SMF119SM_LISmceid       Char(32),  /* SMC-Rv2 EID         @6BA*/         
*    3 SMF119SM_LIFlag1        Bit(8),                                          
*      5 SMF119SM_LIRmtOSType     Bit(4),  /* Remote peer's OS type             
*                                                                @6BA*/         
*      5 *                        Bit(4),  /* reserved           @6BA*/         
*    3 SMF119SM_LIRmtHostName     Char(32), /* Peer Host Name    @6BA*/         
*    3 SMF119SM_LISmcLnkFlags     Bit(8),    /* Flags for SMCv2  @6BA*/         
*      5 SMF119SM_LISmcLnkTypeDirect Bit(1), /* SMC link type is direct         
*                                               if bit on, indirect             
*                                               otherwise        @6BA*/         
*      5 SMF119SM_LISmcLnkRoleServer Bit(1), /* SMC link role is server         
*                                               if bit on, client               
*                                               otherwise        @6BA*/         
*      5 SMF119SM_LIIPAddr_IPv6      Bit(1), /* IPv6 address indicator          
*                                               for SMC-Rv2 IP                  
*                                               addresses in this               
