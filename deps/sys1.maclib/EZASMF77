*/*                                                                  */         
*/* SMF119ML_SS     CSSMTP Spool Statistics section                  */         
*/*                 Subtype 0x33 Section 4                           */         
*/*                                                                  */         
*/********************************************************************/         
*Declare                                                                        
*1 SMF119ML_SS_t          Type,     /* Spool Job statistics      @H2A*/         
*                                                                               
*  3 SMF119ML_SS_STime    Fixed(31),/* Time spool was read from  @H2A*/         
*                                   /*  JES - Hundredths of seconds  */         
*  3 SMF119ML_SS_SDate    Char(4),  /* Date spool was read from  @H2A*/         
*                                   /*  JES - 0CYYDDDF               */         
*  3 SMF119ML_SS_RTime    Fixed(31),/* Time spool read completed @H2A*/         
*                                   /*  Hundredths of seconds        */         
*  3 SMF119ML_SS_RDate    Char(4),  /* Date spool read completed @H2A*/         
*                                   /*  0CYYDDDF                     */         
*  3 SMF119ML_SS_RcdCnt   Fixed(32),/* Spool file record count   @H2A*/         
*  3 SMF119ML_SS_ETime    Fixed(31),/* Time spool was completed  @H2A*/         
*                                   /*  Hundredths of seconds        */         
*  3 SMF119ML_SS_EDate    Char(4),  /* Date spool was completed  @H2A*/         
*                                   /*  0CYYDDDF                     */         
*  3 SMF119ML_SS_MAIL     Fixed(32),/* Number of mail messages   @H2A*/         
*  3 SMF119ML_SS_Good     Fixed(32),/* Number of deliverable mail@H2A*/         
*  3 SMF119ML_SS_Udv      Fixed(32),/* Number of undeliverables  @H2A*/         
*  3 SMF119ML_SS_Dead     Fixed(32),/* Number of Deadletter mail @H2A*/         
*  3 SMF119ML_SS_Rcpt     Fixed(32),/* Number of RCPTs           @H2A*/         
*  3 SMF119ML_SS_CRcpt    Fixed(32),/* Total RCPTs sent completed@H2A*/         
*  3 SMF119ML_SS_URcpt    Fixed(32),/* Total RCPTs sent in       @H2A*/         
*                                   /*  undelivered                  */         
*  3 SMF119ML_SS_Skip     Fixed(32),/* Number of mail skipped        */         
*                                   /*  Due to user exit or restart  */         
*  3 SMF119ML_SS_Err      Fixed(32),/* Number of mail in error   @H2A*/         
*                                   /*  that were undeliverable      */         
*  3 SMF119ML_SS_Bsize    Fixed(64),/* Total size of mail bodies @H2A*/         
*  3 SMF119ML_SS_RtnCd    Fixed(32),/* Processing Return code    @H2A*/         
*                                                                               
*  3 SMF119ML_SS_Flags     Bit(32), /* Flags                     @H2A*/         
*    5 SMF119ML_SS_TLS     Bit(1),  /* Jes spool data set        @H2A*/         
*                                   /*  contained valid STARTTLS     */         
*                                   /*  command                      */         
*    5 SMF119ML_SS_Finis   Bit(1),  /* CSSMTP completed          @H2A*/         
*                                   /*  processing the spool data set*/         
*    5 SMF119ML_SS_ErrRpt  Bit(1),  /* This is an error report   @H2A*/         
*                                   /*   spool file created by CSSMTP*/         
*    5 SMF119ML_SS_Hold    Bit(1),  /* Final disposition of      @H2A*/         
*                                   /*   spool file                  */         
*                                   /*   1 - Hold  0- Delete         */         
*    5 SMF119ML_SS_Error   Bit(1),  /* There were one or more    @H2A*/         
*                                   /*  severity 5 errors found in   */         
*                                   /*  the spool file processing    */         
*    5 SMF119ML_SS_Rsv05   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv06   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv07   Bit(1),  /*                           @H2A*/         
*                                                                               
*    5 SMF119ML_SS_Xmit    Bit(1),  /* file was in NetData       @H2A*/         
*    5 SMF119ML_SS_RStrt   Bit(1),  /* file was restarted        @H2A*/         
*    5 SMF119ML_SS_Rsv12   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv13   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv14   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Reply   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv16   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv17   Bit(1),  /*                           @H2A*/         
*                                                                               
*    5 SMF119ML_SS_RStc    Bit(1),  /* Data set created by       @H2A*/         
*                                   /*  started task                 */         
*    5 SMF119ML_SS_RTsc    Bit(1),  /* Data set created by time  @H2A*/         
*                                   /*   sharing user                */         
*    5 SMF119ML_SS_RJob    Bit(1),  /* Data set created by batch @H2A*/         
*                                   /*   job                         */         
*    5 SMF119ML_SS_Rsv23   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv24   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv25   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv26   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv27   Bit(1),  /*                           @H2A*/         
*    5 SMF119ML_SS_Rsv3x   Bit(8),  /*                           @H2A*/         
*  3 SMF119ML_SS_EMail    Fixed(32),/* Number of mail saved for  @M1A*/         
*                                   /*  extended retry               */         
*  3 SMF119ML_SS_ERcpt    Fixed(32),/* Number of recipients to   @M1A*/         
*                                   /*  be retried in mail messages  */         
*                                   /*  saved for extended retry     */         
*                                                                               
*  SMF119ML_SS            Isa(SMF119ML_SS_t) Based;           /* @H2A*/         
*                                                                               
*Declare                            /* SMF119ML_SS_RtnCd             */         
* SMF119ML_SS_Ok          Fixed(31) Constant(00),             /* @H2A*/         
*                                   /* JES processing completed      */         
* SMF119ML_SS_Alloc       Fixed(31) Constant(07),             /* @H2A*/         
*                                   /* JES sysout allocation failed  */         
* SMF119ML_SS_OPEN        Fixed(31) Constant(08),             /* @H2A*/         
*                                   /* Open failed for sysout file   */         
* SMF119ML_SS_REQERR      Fixed(31) Constant(10),             /* @H2A*/         
*                                   /* IEFSSREQ failed               */         
* SMF119ML_SS_APIERR      Fixed(31) Constant(11),             /* @H2A*/         
*                                   /* IEFSSREQ SSS2 API failed      */         
* SMF119ML_SS_ICONV       Fixed(31) Constant(13),             /* @H2A*/         
*                                   /* Conversion table open error   */         
* SMF119ML_SS_EMPTY       Fixed(31) Constant(14),             /* @H2A*/         
*                                   /* Empty data set                */         
* SMF119ML_SS_JESSIZE     Fixed(31) Constant(15),             /* @H2A*/         
*                                   /* size exceeds JesJobSize       */         
* SMF119ML_SS_SAF         Fixed(31) Constant(16),             /* @H2A*/         
*                                   /* Access is not authorized      */         
* SMF119ML_SS_TRANSLATE   Fixed(31) Constant(18),             /* @H2A*/         
*                                   /* Translation error             */         
* SMF119ML_SS_NOEBCDIC    Fixed(31) Constant(19),             /* @H2A*/         
*                                   /* Unknown translation table     */         
* SMF119ML_SS_USEREXIT    Fixed(31) Constant(21),             /* @H2A*/         
*                                  /* The return code from the CSSMTP*/         
*                                  /* user exit indicates processing */         
*                                  /* of the spool file should stop  */         
* SMF119ML_SS_NOMAIL      Fixed(31) Constant(22),             /* @H2A*/         
*                                  /* The spool file does not        */         
*                                  /* contain any mail transactions  */         
* SMF119ML_SS_JESCLOSE    Fixed(31) Constant(23),             /* @H2A*/         
*                                  /* The JES spool file was not     */         
*                                  /* properly closed by JES.  File  */         
*                                  /* data may be incomplete.        */         
* SMF119ML_SS_IOERROR     Fixed(31) Constant(24),             /* @H2A*/         
*                                  /* An I/O error occurred          */         
*                                  /* reading the spool file         */         
* SMF119ML_SS_MAXERROR    Fixed(31) Constant(25);             /* @H2A*/         
*                                  /* There were too many errors in  */         
*                                  /* the spool file to continue     */         
*                                  /* processing                     */         
*                                                                               
*/********************************************************************/         
