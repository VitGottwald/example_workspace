*/*                                                                  */         
*/* SMF119ML_CF     CSSMTP Configuration                             */         
*/*                 Subtype (0x30) Section 2                         */         
*/*                                                                  */         
*/*                 General configuration parameters                 */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* Declare                                                     /* @H2A*/         
* 1 SMF119ML_CF_t            Type,   /* CSSMTP Configuration     @H2A*/         
*   3 SMF119ML_CF_Flags      Bit(32),/* Configuration flags      @H2A*/         
*     5 SMF119ML_CF_Targets  Bit(1), /* Target servers updated   @H2A*/         
*     5 SMF119ML_CF_Ntarget  Bit(1), /* Non target data updated  @H2A*/         
*     5 SMF119ML_CF_Warning  Bit(1), /* Warning issued on update @H2A*/         
*     5 SMF119ML_CF_Modify   Bit(1), /* Modify Refresh update    @H2A*/         
*     5 SMF119ML_CF_rsvd04   Bit(1), /* Reserved                 @H2A*/         
*     5 SMF119ML_CF_rsvd05   Bit(1), /* Reserved                 @H2A*/         
*     5 SMF119ML_CF_rsvd06   Bit(1), /* Reserved                 @H2A*/         
*     5 SMF119ML_CF_rsvd07   Bit(1), /* Reserved                 @H2A*/         
*     5 SMF119ML_CF_Mbcs     Bit(1), /* Mbcs support   0-No 1-Yes@56A*/         
*     5 SMF119ML_CF_IpV4ONly Bit(1), /* IPv4 stack               @H2A*/         
*     5 SMF119ML_CF_DateHdr  Bit(1), /* Header Date:   0-No 1-Yes@VEA*/         
*     5 SMF119ML_CF_UserInfo Bit(1), /* Header UserInfo0-No 1-Yes@A5A*/         
*     5 SMF119ML_CF_NullTrnc Bit(1), /* Truncate Nulls 0-No 1-Yes@A6A*/         
*     5 SMF119ML_CF_TestMode Bit(1), /* Testmode       0-No 1-Yes@37A*/         
*     5 SMF119ML_CF_DataTrnc Bit(1), /* DataLineTrunc  0-No 1-Yes@37A*/         
*     5 SMF119ML_CF_TLSEhlo  Bit(1), /* TLSEhlo        0-No 1-Yes@42A*/         
*     5 SMF119ML_CF_SmfConfg Bit(1), /* SMF119 Config  0-No 1-Yes@H2A*/         
*     5 SMF119ML_CF_SmfConn  Bit(1), /* SMF119 Connect 0-No 1-Yes@H2A*/         
*     5 SMF119ML_CF_SmfMail  Bit(1), /* SMF119 Mail    0-No 1-Yes@H2A*/         
*     5 SMF119ML_CF_SmfSpool Bit(1), /* SMF119 Spool   0-No 1-Yes@H2A*/         
*     5 SMF119ML_CF_SmfStats Bit(1), /* SMF119 Stats   0-No 1-Yes@H2A*/         
*     5 SMF119ML_CF_rsvd25   Bit(1), /*                          @H2A*/         
*     5 SMF119ML_CF_rsvd26   Bit(1), /*                          @H2A*/         
*     5 SMF119ML_CF_rsvd27   Bit(1), /*                          @H2A*/         
*     5 SMF119ML_CF_rsvd3x   Bit(8), /*                          @H2A*/         
*   3 SMF119ML_CF_CfgPidId   Fixed,  /* Process ID value         @H2A*/         
*   3 SMF119ML_CF_BadSpool   Fixed,  /* BadSpoolDisp             @H2A*/         
*                                    /*   0=HOLD 1=DELETE            */         
*   3 SMF119ML_CF_ChkPtSz    Fixed,  /* CkpPointSizeLimit        @H2A*/         
*   3 SMF119ML_CF_ExtWrt     Char(8),/* ExtWrtName               @H2A*/         
*   3 SMF119ML_CF_Tcpip      Char(8),/* Tcpip name parameter     @H2A*/         
*   3 SMF119ML_CF_JesJobSz   Fixed,  /* JESJobSize               @H2A*/         
*   3 SMF119ML_CF_JesMsgSz   Fixed,  /* JESMsgSize               @H2A*/         
*   3 SMF119ML_CF_LogLevel   Fixed,  /* LogLevel                 @H2A*/         
*   3 SMF119ML_CF_Report     Fixed,  /* Report                   @H2A*/         
*                                    /* 0=SYSOUT 1=NONE 2=ADMIN  @M2C*/         
*   3 SMF119ML_CF_RtyCount   Fixed,  /* Retry Count              @H2A*/         
*   3 SMF119ML_CF_RtyIntvl   Fixed,  /* Retry Interval           @H2A*/         
*   3 SMF119ML_CF_AnyCmd     Fixed,  /* Timeout AnyCmd           @H2A*/         
*   3 SMF119ML_CF_ConnRty    Fixed,  /* Timeout ConnectRetry     @H2A*/         
*   3 SMF119ML_CF_DataBlk    Fixed,  /* Timeout DataBlock        @H2A*/         
*   3 SMF119ML_CF_DataCmd    Fixed,  /* Timeout DATACmd          @H2A*/         
*   3 SMF119ML_CF_DataTRM    Fixed,  /* Timeout DataTerm         @H2A*/         
*   3 SMF119ML_CF_InitMsg    Fixed,  /* Timeout InitialMsg       @H2A*/         
*   3 SMF119ML_CF_MailCmd    Fixed,  /* Timeout MAILCmd          @H2A*/         
*   3 SMF119ML_CF_RCPTCmd    Fixed,  /* Timeout RCPTCmd          @H2A*/         
*   3 SMF119ML_CF_ChkPnt     Fixed,  /* Checkpoint option        @H2A*/         
*                                                                               
*   3 SMF119ML_CF_CfgCP      Char(20), /* Configuration code page@H2A*/         
*   3 SMF119ML_CF_CodePage   Char(20), /* TRANSLATE              @H2A*/         
*   3 SMF119ML_CF_RtnTo      Fixed,   /* Undeliverable           @H2A*/         
*                                     /*   ReturnToMailFrom          */         
*   3 SMF119ML_CF_DeadAct    Fixed,   /* Undeliverable           @H2A*/         
*                                     /*   DeadLetterAction          */         
*   3 SMF119ML_CF_UserExit   Fixed,   /* Userexit                @H2A*/         
*   3 SMF119ML_CF_ErtAge     Fixed,   /* Extended Retry Age      @M1A*/         
*   3 SMF119ML_CF_ErtIntvl   Fixed,   /* Extended Retry Interval @M1A*/         
*   3 SMF119ML_CF_JESSynMax  Fixed,   /* Maximum number of  @M6A @M8C*/         
*                                     /*syntax errors to be tolerated*/         
*                                     /*in a JES spool file          */         
*   3 SMF119ML_CF_ConnIdle   Fixed,   /* Timeout ConnectIdle     @37A*/         
*   3 SMF119ML_CF_AtSign     Char(2), /* AtSign                  @42A*/         
*                                                                               
*  SMF119ML_CF               Isa(SMF119ML_CF_t) Based;        /* @H2A*/         
*                                                                               
*Declare                                                                        
*ML_CF_BADSPOOLDISP_HOLD      Constant(0),                    /* @H2A*/         
*ML_CF_BADSPOOLDISP_DELETE    Constant(1),                    /* @H2A*/         
*                                                                               
*ML_CF_DEADLETTERACTION_STORE  Constant(0),                   /* @H2A*/         
*ML_CF_DEADLETTERACTION_DELETE Constant(1),                   /* @H2A*/         
*                                                                               
*ML_CF_REPORT_SYSOUT           Constant(0),                   /* @H2A*/         
*ML_CF_REPORT_NONE             Constant(1),                   /* @H2A*/         
*ML_CF_REPORT_ADMIN            Constant(2),                   /* @H2A*/         
*                                                                               
*ML_CF_RETURNTOMAILFROM_NO     Constant(0),                   /* @H2A*/         
*ML_CF_RETURNTOMAILFROM_YES    Constant(1),                   /* @H2A*/         
*                                                                               
*ML_CF_USEREXIT_NONE           Constant(0),                   /* @H2A*/         
*ML_CF_USEREXIT_VERSION2       Constant(2),                   /* @H2A*/         
*ML_CF_USEREXIT_VERSION3       Constant(3),                   /* @H2A*/         
*                                                                               
*ML_CF_CHK_WARMSTART           Constant(0),                   /* @H2A*/         
*ML_CF_CHK_COLDSTART           Constant(1),                   /* @H2A*/         
*ML_CF_CHK_NOTAVAILABLE        Constant(2);                   /* @H2A*/         
