*/* TN3270 Server SNA Session Initiation section (subtype 20)        */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119TN_NI BASED,                                                         
*    3 SMF119TN_NILU     CHAR(8),   /* Telnet LU name                */         
*    3 SMF119TN_NIAppl   CHAR(8),   /* Host application name         */         
*    3 SMF119TN_NILdev   CHAR(4),   /* Telnet Server logical device             
*                                      number                        */         
*    3 SMF119TN_NIRIP    CHAR(16),  /* Remote IP address (IPv6)      */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_NIRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_NIRIP_IPv4  CHAR(4), /* IPv4 piece of address  @L4C*/         
*    3 SMF119TN_NILIP    CHAR(16),  /* Local IP address (IPV6)       */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_NILIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_NILIP_IPv4  CHAR(4), /* IPv4 piece of address  @L4C*/         
*    3 SMF119TN_NIRPort  CHAR(2),   /* Remote port number            */         
*    3 SMF119TN_NILPort  CHAR(2),   /* Local port number             */         
*    3 SMF119TN_NITime   BIT(32),   /* SNA session initiation                   
*                                      Time of Day (UTC)             */         
*    3 SMF119TN_NIDate   BIT(32);   /* SNA session initiation date   */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TN3270 Server SNA Session Termination section (subtype 21)       */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119TN_NT BASED,                                                         
*    3 SMF119TN_NTLU     CHAR(8),   /* Telnet LU name                */         
*    3 SMF119TN_NTAppl   CHAR(8),   /* Host application name         */         
*    3 SMF119TN_NTLdev   CHAR(4),   /* Telnet server logical                    
*                                      device number                 */         
*    3 SMF119TN_NTRIP    CHAR(16),  /* Remote IP address (IPv6)      */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_NTRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_NTRIP_IPv4  CHAR(4), /* IPv4 piece of address  @L4C*/         
*    3 SMF119TN_NTLIP    CHAR(16),  /* Local IP address (IPv6)       */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_NTLIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_NTLIP_IPv4  CHAR(4), /* IPv4 piece of address  @L4C*/         
*    3 SMF119TN_NTRPort  CHAR(2),   /* Remote port                   */         
*    3 SMF119TN_NTLPort  CHAR(2),   /* Local port                    */         
*    3 SMF119TN_NTHostNm CHAR(8),   /* TCP/IP hostname               */         
*    3 SMF119TN_NTInByte BIT(64),   /* Server inbound bytes          */         
*    3 SMF119TN_NTOutByte  BIT(64), /* Server outbound bytes         */         
*    3 SMF119TN_NTiTime  BIT(32),   /* SNA session initiation                   
*                                      Time of Day (UTC)             */         
*    3 SMF119TN_NTiDate  BIT(32),   /* SNA session initiation date   */         
*    3 SMF119TN_NTtTime  BIT(32),   /* SNA session termination                  
*                                      Time of Day (UTC)             */         
*    3 SMF119TN_NTtDate  BIT(32),   /* SNA session termination date  */         
*    3 SMF119TN_NTDur    BIT(32),   /* SNA session duration          */         
*    3 SMF119TN_NTSType  CHAR(1),   /* Session type              @L1C*/         
*    3 SMF119TN_NTLUSel  CHAR(1),   /* LU selection method       @L1C*/         
*    3 SMF119TN_NTSSL    CHAR(1),   /* SSL status                @L1C*/         
*    3 *                 CHAR(1),   /* Reserved                      */         
*    3 SMF119TN_NTCopt   BIT(8),    /* Connection options            */         
*      5 SMF119TN_NTCopt3270E  BIT(1), /* TN3270E                    */         
*      5 SMF119TN_NTCoptTerm   BIT(1), /* Terminal type              */         
*      5 SMF119TN_NTCoptEOR    BIT(1), /* End of record              */         
*      5 SMF119TN_NTCoptBin    BIT(1), /* Transmit binary            */         
*      5 SMF119TN_NTCoptEcho   BIT(1), /* Echos                      */         
*      5 SMF119TN_NTCoptGoAh   BIT(1), /* Suppress go ahead          */         
*      5 SMF119TN_NTCoptTime   BIT(1), /* Timemark                   */         
*      5 SMF119TN_NTCoptNEnv   BIT(1), /* New environment            */         
*    3 *                 CHAR(1),   /* Reserved                      */         
*    3 SMF119TN_NT32opt  BIT(16),   /* TN3270E connection options    */         
*      5 SMF119TN_NT32optBind  BIT(1), /* Bind image                 */         
*      5 SMF119TN_NT32optSysRq BIT(1), /* SysRequest                 */         
*      5 SMF119TN_NT32optResp  BIT(1), /* Responses                  */         
*      5 SMF119TN_NT32optSCS   BIT(1), /* SCS control codes          */         
*      5 SMF119TN_NT32optDCS   BIT(1), /* DCS control codes          */         
*      5 SMF119TN_NT32optContR BIT(1), /* Contention resolution      */         
*      5 SMF119TN_NT32optFMH   BIT(1), /* FMH support                */         
*      5 SMF119TN_NT32optSNA   BIT(1), /* SNA sense support          */         
*      5 SMF119TN_NT32optBytDb BIT(1), /* Suppress header byte                  
*                                      doubling                      */         
*      5 *                     BIT(7), /* Reserved                   */         
*    3 SMF119TN_NTRCode  CHAR(8),   /* Session termination reason               
*                                      code                          */         
*    3 SMF119TN_NTLMode  CHAR(8),   /* SNA logmode                   */         
*    3 SMF119TN_NTDevt   CHAR(20),  /* Telnet device type            */         
*    3 SMF119TN_NTEnd    CHAR(0);   /* End of section                */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TN3270 Server SNA Session Termination Hostname section (subtype  */         
*/* 21). This section is optional and is present only if hostname    */         
*/* has been resolved.                                               */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119TN_NTH BASED,                                                        
*    3 SMF119TN_NTHostname CHAR(*); /* Hostname associated with                 
*                                      this session                  */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TN3270 Server SNA Session Termination Round Trip Performance     */         
*/* section (subtype 21).  This section is optional and is present   */         
*/* only if the connection maps to a MonitorGroup.                   */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119TN_NTR BASED,                                                        
*    3 SMF119TN_NTRRts         Fixed(32), /* Sum of round trip times            
*                                                                @X3A*/         
*    3 SMF119TN_NTRIPRts       Fixed(32), /* Sum of IP portion round            
*                                            trip time           @X3A*/         
*    3 SMF119TN_NTRCountTrans  Fixed(32), /* Number of transactions             
*                                            for round trip time @X3A*/         
*    3 SMF119TN_NTRCountIP     Fixed(32), /* Number of IP transactions          
*                                            for round trip time @X3A*/         
*    3 SMF119TN_NTRElapsRndTrpSq Bit(64), /* Sum of the square (SSQ)            
*                                            of round trip times @X3A*/         
*    3 SMF119TN_NTRElapsIpRtSq   Bit(64), /* SSQ of IP portion   @X3A*/         
*    3 SMF119TN_NTRElapsSnaRtSq  Bit(64), /* SSQ of SNA portion  @X3A*/         
*    3 SMF119TN_NTRGrpIndex    Fixed(32), /* Index of group in master           
*                                            table               @X3A*/         
*    3 SMF119TN_NTRDR          Bit(8),    /* Measurement method  @X3A*/         
*     4 SMF119TN_NTRDR_Used     Bit(1),   /* Def Response used   @X3A*/         
*     4 SMF119TN_NTRTM_Used     Bit(1),   /* Timemark used       @X3A*/         
*    3 *                       Char(3);   /* Not used - Available    */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TN3270 Server SNA Session Termination Time Bucket Performance    */         
*/* section (subtype 21).  This section is optional and is present   */         
*/* only if the connection maps to a MonitorGroup and Bucket data is */         
*/* requested.                                                       */         
*/* The upper boundary of one bucket is the lower boundary of the    */         
*/* next bucket.  A transaction is added to a bucket when its round  */         
*/* trip time falls within the bounds of that bucket.                */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119TN_NTB BASED,                                                        
*    3 SMF119TN_NTBucketBndry1 Fixed(32), /* Upper bndry bucket1 @X3A*/         
*    3 SMF119TN_NTBucketBndry2 Fixed(32), /* Upper bndry bucket2 @X3A*/         
*    3 SMF119TN_NTBucketBndry3 Fixed(32), /* Upper bndry bucket3 @X3A*/         
*    3 SMF119TN_NTBucketBndry4 Fixed(32), /* Upper bndry bucket4 @X3A*/         
*    3 SMF119TN_NTBucket1Rts   Fixed(32), /* Trans in bucket1    @X3A*/         
*    3 SMF119TN_NTBucket2Rts   Fixed(32), /* Trans in bucket2    @X3A*/         
*    3 SMF119TN_NTBucket3Rts   Fixed(32), /* Trans in bucket3    @X3A*/         
*    3 SMF119TN_NTBucket4Rts   Fixed(32), /* Trans in bucket4    @X3A*/         
*    3 SMF119TN_NTBucket5Rts   Fixed(32); /* Trans in bucket5    @X3A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for Session Type     SMF119TN_NTSType                  */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119TN_NTSTypeUnk      CHAR(1) CONSTANT('00'x), /* Unknown @L1A*/         
*   SMF119TN_NTSType3270     CHAR(1) CONSTANT('01'x), /* TN3270  @L1A*/         
*   SMF119TN_NTSType3270E    CHAR(1) CONSTANT('02'x), /* TN3270E @L1A*/         
*   SMF119TN_NTSTypeLine     CHAR(1) CONSTANT('03'x), /* Linemode               
*                                                                @L1A*/         
*   SMF119TN_NTSTypeDBCS     CHAR(1) CONSTANT('04'x), /* DBCSTransform          
*                                                                @L1A*/         
*   SMF119TN_NTSTypeBin      CHAR(1) CONSTANT('05'x); /* Binary  @L1A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for LU Selection Type SMF119TN_NTLUSel                 */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119TN_NTLUSelSrv      CHAR(1) CONSTANT('00'x), /* Chosen by              
*                                                        server  @L1A*/         
*   SMF119TN_NTLUSelCli      CHAR(1) CONSTANT('01'x); /* Chosen by              
*                                                        client  @L1A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for SSL Status        SMF119TN_NTSSL                   */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119TN_NTSSLNone       CHAR(1) CONSTANT('00'x), /* Non-SSL                
*                                      session                   @L1A*/         
*   SMF119TN_NTSSLServer     CHAR(1) CONSTANT('01'x), /* Server Auth            
*                                      only                      @L1A*/         
*   SMF119TN_NTSSLnoSAF      CHAR(1) CONSTANT('02'x), /* Server and             
*                                      client auth (SSL auth only)              
*                                      SSLCERT- No SAF cert                     
*                                      REQUIRED- SAF check done,                
*                                                no userid OK    @L1A*/         
*   SMF119TN_NTSSLSAF        CHAR(1) CONSTANT('03'x), /* Server and             
*                                      client auth (SSL auth and SAF            
*                                      cert/check requires Userid)              
*                                                                @L1A*/         
*   SMF119TN_NTSSLFULL       CHAR(1) CONSTANT('04'x), /* Server and             
*                                      optional client cert.  If                
*                                      present, SSL auth, SAF check             
*                                      done, no UserID OK        @Y2A*/         
*   SMF119TN_NTSSLPTHRU      CHAR(1) CONSTANT('05'x); /* Server and             
*                                      optional client cert.  If                
*                                      present, No SSL auth, SAF check          
*                                      done, no UserID OK        @Y2A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* NOTE]]] The FTP records and Telnet Client records are mapped     */         
*/* below, but are not used within TCP/IP. The code that builds the  */         
*/* records has its own mappings in C and PASCAL, respectively. The  */         
*/* mappings are included here to allow SVT test tools to correctly  */         
*/* run using the mappings.                                          */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Client Transfer Completion section (subtype 03)              */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FC BASED,                                                         
*    3 SMF119FT_FCCmd    CHAR(4),   /* FTP Command, per RFC959+      */         
*    3 SMF119FT_FCFType  CHAR(4),   /* File Type                     */         
*    3 SMF119FT_FCDRIP   CHAR(16),  /* Remote IP address (IPv6), data           
*                                      connection                    */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_FCDRIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FCDRIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FCDLIP   CHAR(16),  /* Local IP address (IPv6), data            
*                                      connection                    */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_FCDLIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FCDLIP_IPv4 CHAR(4), /* IPv4 piece of address  @L3C*/         
*    3 SMF119FT_FCDRPort CHAR(2),   /* Remote Port, data connection  */         
*    3 SMF119FT_FCDLPort CHAR(2),   /* Local Port, data connection   */         
*    3 SMF119FT_FCCRIP   CHAR(16),  /* Remote IP address (IPv6),                
*                                      control connection            */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_FCCRIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FCCRIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FCCLIP   CHAR(16),  /* Local IP address (IPv6),                 
*                                      control connection            */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_FCCLIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FCCLIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FCCRPort CHAR(2),   /* Remote Port, control connection          
*                                                                    */         
*    3 SMF119FT_FCCLPort CHAR(2),   /* Local Port, control connection           
*                                                                    */         
*    3 SMF119FT_FCRUser  CHAR(8),   /* User ID (logon name)          */         
*    3 SMF119FT_FCLUser  CHAR(8),   /* Local User ID                 */         
*    3 SMF119FT_FCType   CHAR(1),   /* Data Type                     */         
*    3 SMF119FT_FCMode   CHAR(1),   /* Transfer Mode                 */         
*    3 SMF119FT_FCStruct CHAR(1),   /* Data Structure                */         
*    3 SMF119FT_FCDsType CHAR(1),   /* Data Set Type                 */         
*    3 SMF119FT_FCSTime  BIT(32),   /* FTP Transmission start                   
*                                      Time of Day (UTC time)        */         
*    3 SMF119FT_FCSDate  BIT(32),   /* FTP Transmission start date   */         
*    3 SMF119FT_FCETime  BIT(32),   /* FTP Transmission end                     
*                                      Time of Day (UTC time)        */         
*    3 SMF119FT_FCEDate  BIT(32),   /* FTP Transmission end date     */         
*    3 SMF119FT_FCDur    BIT(32),   /* FTP Transmission duration     */         
*    3 SMF119FT_FCBytes  BIT(64),   /* 64-bit transmission byte                 
*                                      count                     @L7C*/         
*    3 SMF119FT_FCLReply CHAR(4),   /* Last reply from server        */         
*    3 SMF119FT_FCM1     CHAR(8),   /* PDS Member name               */         
*    3 SMF119FT_FCHostname CHAR(8), /* Host name                     */         
*    3 SMF119FT_FCRS     CHAR(8),   /* Reserved, abnormal end info   */         
*    3 SMF119FT_FCBytesFloat BIT(64), /* Transmission Byte Count                
*                                      floating point format     @X1C*/         
*    3 SMF119FT_FCCConnID BIT(32),  /* TCP connection ID - control              
*                                      connection                @X1A*/         
*    3 SMF119FT_FCDConnID BIT(32);  /* TCP connection ID - data                 
*                                      connection                @X1A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for File Type                                          */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCFTypeSEQ  CHAR(4) CONSTANT('SEQ '),                              
*   SMF119FT_FCFTypeJES  CHAR(4) CONSTANT('JES '),                              
*   SMF119FT_FCFTypeSQL  CHAR(4) CONSTANT('SQL ');                              
*                                                                               
*/********************************************************************/         
*/* Constants for Data Type                                          */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCTypeASCII   CHAR(1) CONSTANT('A'),                               
*   SMF119FT_FCTypeEBCDIC  CHAR(1) CONSTANT('E'),                               
*   SMF119FT_FCTypeImage   CHAR(1) CONSTANT('I'),                               
*   SMF119FT_FCTypeDByte   CHAR(1) CONSTANT('B'),                               
*   SMF119FT_FCTypeUCS2    CHAR(1) CONSTANT('U');                               
*                                                                               
*/********************************************************************/         
*/* Constants for Transmission Mode                                  */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCModeBlock   CHAR(1) CONSTANT('B'),                               
*   SMF119FT_FCModeCmpress CHAR(1) CONSTANT('C'),                               
*   SMF119FT_FCModeStream  CHAR(1) CONSTANT('S');                               
*                                                                               
*/********************************************************************/         
*/* Constants for Data Structure                                     */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCStructFile  CHAR(1) CONSTANT('F'),                               
*   SMF119FT_FCStructRec   CHAR(1) CONSTANT('R');                               
*                                                                               
*/********************************************************************/         
*/* Constants for Data Set Type                                      */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCDsTypeSEQ   CHAR(1) CONSTANT('S'),                               
*   SMF119FT_FCDsTypePDS   CHAR(1) CONSTANT('P'),                               
*   SMF119FT_FCDsTypeHFS   CHAR(1) CONSTANT('H');                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Client Transfer Complete First Associated Data Set Name      */         
*/* section (subtype 03). This section represents an MVS or HFS      */         
*/* dataset name associated the file transfer. The type of filename  */         
*/* contained is found in SMF119FT_FCDsType.                         */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FCAN BASED,                                                       
*    3 SMF119FT_FCFileName   CHAR(*); /* Filename associated with this          
*                                      FTP transfer                  */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Client Transfer Complete SOCKS section (subtype 03).         */         
*/* FTP Client Transfer Initialization SOCKS section (subtype 101).  */         
*/* FTP Client Login Failure SOCKS section (subtype 102).            */         
*/* FTP Client Session SOCKS section (subtype 103).                  */         
*/* This section is present when the FTP operation traverses a SOCKS */         
*/* server on the path between the z/OS client and the FTP server.   */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FCSO BASED,                                                       
*    3 SMF119FT_FCCIP    CHAR(16),  /* SOCKS server IP address (IPv6),          
*                                      control connection            */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_FCCIPFmt CHAR(2), /* IPv4 address format     @L8A*/         
*      5 SMF119FT_FCCIP_IPv4 CHAR(4), /* IPv4 piece of address       */         
*    3 SMF119FT_FCCPort  CHAR(2),   /* SOCKS Port, control connection           
*                                                                    */         
*    3 SMF119FT_FCCProt  CHAR(1);   /* SOCKS Protocol version, control          
*                                      connection                    */         
*                                                                               
*/********************************************************************/         
*/* Constants for SOCKS Protocol version                             */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCCProtV4   CHAR(1) CONSTANT('01'X),                               
*   SMF119FT_FCCProtV5   CHAR(1) CONSTANT('02'X);                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Client Transfer Complete Security information subsection     */         
*/* (subtype 03).                                                    */         
*/* FTP Client Transfer Initialization Security information          */         
*/* subsection (subtype 101).                                        */         
*/* FTP Client Login Failure Security information subsection         */         
*/* (subtype 102).                                                   */         
*/* FTP Client Session Security information subsection               */         
*/* (subtype 103).                                                   */         
*/* This section reports security information for an FTP file        */         
*/* transfer.                                                        */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FCSC BASED,                                      /*@Q1A*/         
*    3 SMF119FT_FCMechanism  CHAR(1), /* Protection mechanism    @Q1A*/         
*    3 SMF119FT_FCCProtect   CHAR(1), /* Control connection protection          
*                                        level                   @Q1A*/         
*    3 SMF119FT_FCDProtect   CHAR(1), /* Data connection protection             
*                                        level                   @Q1A*/         
*    3 SMF119FT_FCLoginMech  CHAR(1), /* Login method            @Q1A*/         
*    3 SMF119FT_FCProtoLevel CHAR(8), /* Protocol level          @Q1A*/         
*    3 SMF119FT_FCCipherSpec CHAR(20),/* Cipher suite            @Q1A*/         
*    3 SMF119FT_FCProtBuffSize BIT(32),  /* Negotiated protection               
*                                           buffer size          @Q1A*/         
*    3 SMF119FT_FCCipher     CHAR(2), /* Cipher value            @Y5A*/         
*    3 SMF119FT_FCFips140    FIXED(8), /* FIPS 140 compliance    @F5A*/         
*    3 SMF119FT_FCCipher4    CHAR(4), /* 4 byte cipher value@V1A @34C*/         
*    3 SMF119FT_FCSessReuse  CHAR(1), /* SSL session reuse flag  @34A*/         
*    3 SMF119FT_FCCSSLSessIDLen FIXED(16), /* Ctrl conn SSL session             
*                                             ID length          @34A*/         
*    3 SMF119FT_FCCSSLSessID CHAR(32),/* Ctrl conn SSL session ID@34A*/         
*    3 SMF119FT_FCDSSLSessIDLen FIXED(16), /* Data conn SSL session             
*                                             ID length          @34A*/         
*    3 SMF119FT_FCDSSLSessID CHAR(32),/* Data conn SSL session                  
*                                        ID                 @34A @5CC*/         
*    3 SMF119FT_FCKeyShare   CHAR(4); /* 4 byte key share   @5BA @5CM*/         
*                                                                               
*/********************************************************************/         
*/* Constants for SSL session reuse flag                             */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FCSessReuseNone  CHAR(1)  CONSTANT('N'), /* None    @34A*/         
*   SMF119FT_FCSessReuseAllow CHAR(1)  CONSTANT('A'), /* Allowed @34A*/         
*   SMF119FT_FCSessReuseReq   CHAR(1)  CONSTANT('R'); /* Required@34A*/         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Client Transfer Complete User Name subsection (subtype 03).  */         
*/* This section reports the user name used to log into the FTP      */         
*/* server.  This section is always present.                         */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119FT_FCUID BASED,                                                      
*    3 SMF119FT_FCIUserID CHAR(*); /* USER id associated with                   
*                                    this FTP session            @X1A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Client Transfer Complete load module name section            */         
*/* (subtype 03)                                                     */         
*/* This section reports the load module or program object library   */         
*/* name and member name associated with this transfer. It's optional*/         
*/* and presented only for load module transfer.                     */         
*/********************************************************************/         
*/* The length of this section is variable.  Its length is indicated */         
*/* by the subsection length in the self-defining section.           */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119FT_FCLM BASED,                                                       
*    3 SMF119FT_FCMemNum FIXED(32), /* Member num in this record @45C*/         
*    3 SMF119FT_FCLibNameLen FIXED(8), /* Library name length    @45C*/         
*    3 SMF119FT_FCLibName CHAR(44), /* Library name              @45C*/         
*    3 SMF119FT_FCMemName CHAR(8);  /* Name of member #1         @44A*/         
*/* Additional SMF119FT_FCMemName member name fields follow          */         
*/* the first name field, for a count of SMF119FT_FCMemNum - 1.      */         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TSO Telnet Client Session Initiation section (subtype 22)        */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119TN_CI BASED,                                                         
*    3 SMF119TN_CIRIP    CHAR(16),  /* Remote IP address (IPv6)      */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_CIRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_CIRIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119TN_CILIP    CHAR(16),  /* Local IP address (IPv6)       */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_CILIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_CILIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119TN_CIRPort  CHAR(2),   /* Remote port                   */         
*    3 SMF119TN_CILPort  CHAR(2),   /* Local port                    */         
*    3 SMF119TN_CITime   BIT(32),   /* Telnet session initiation                
*                                      Time of Day (local time)      */         
*    3 SMF119TN_CIDate   BIT(32);   /* Telnet session initiation date*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TSO Telnet Client Session Termination section (subtype 23)       */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119TN_CT BASED,                                                         
*    3 SMF119TN_CTRIP    CHAR(16),  /* Remote IP address (IPv6)      */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_CTRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_CTRIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119TN_CTLIP    CHAR(16),  /* Local IP address (IPv6)       */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119TN_CTLIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119TN_CTLIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119TN_CTRPort  CHAR(2),   /* Remote port                   */         
*    3 SMF119TN_CTLPort  CHAR(2),   /* Local port                    */         
*    3 SMF119TN_CTNJENode  CHAR(8), /* NJE Node Name                 */         
*    3 SMF119TN_CTInBytes  BIT(64), /* Telnet Client inbound bytes   */         
*    3 SMF119TN_CTOutBytes BIT(64), /* Telnet Client outbound bytes  */         
*    3 SMF119TN_CTiTime  BIT(32),   /* Telnet session initiation                
*                                      Time of Day (local time)      */         
*    3 SMF119TN_CTiDate  BIT(32),   /* Telnet session initiation date*/         
*    3 SMF119TN_CTtTime  BIT(32),   /* Telnet session termination               
*                                      Time of Day (local time)      */         
*    3 SMF119TN_CTtDate  BIT(32),   /* Telnet session termination               
*                                      date                          */         
*    3 SMF119TN_CTDur    BIT(32),   /* Telnet session duration       */         
*    3 SMF119TN_CTCopt   BIT(8),    /* Connection options            */         
*      5 *                     BIT(1), /* reserved                   */         
*      5 SMF119TN_CTCoptTerm   BIT(1), /* Terminal type              */         
*      5 SMF119TN_CTCoptEOR    BIT(1), /* End of record              */         
*      5 SMF119TN_CTCoptBin    BIT(1), /* Transmit binary            */         
*      5 SMF119TN_CTCoptEcho   BIT(1), /* Echos                      */         
*      5 SMF119TN_CTCoptGoAh   BIT(1), /* Suppress go ahead          */         
*      5 *                     BIT(2), /* reserved                   */         
*    3 *                 CHAR(3),   /* Unused, available             */         
*    3 SMF119TN_CTDevt   CHAR(20);  /* Telnet Device type            */         
*                                                                               
*                                                                               
*/********************************************************************/         
*/* TELNET Profile information (Subtype 24)                          */         
*/*                                                                  */         
*/* The following structures map TelNet Profile information.  These  */         
*/* structures can be provided in SMF 119 subtype 24 records,        */         
*/* or can be returned by the GetTmProfile Callable NMI.             */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
*/********************************************************************/         
*/* Telnet Profile Information section                               */         
*/*        SMF type 119 Subtype 24 Section 1  (SMF119S1)             */         
*/********************************************************************/         
*                                                                               
*Declare                                                                        
*1 SMF119TN_PI               Based,     /* Profile Information   @VDA*/         
*  3 SMF119TN_PIEye          Char(4),   /* "TNPI" eyecatcher     @VDA*/         
*  3 *                       Fixed(31), /* Reserved              @VDA*/         
*  3 SMF119TN_PIStartStck    Char(8),   /* Stck time when Telnet @VDA*/         
*                                       /*   started                 */         
*  3 SMF119TN_PIStartTime    Fixed(31), /* Time when Telnet      @VDA*/         
*                                       /*   started in 100ths of sec*/         
*  3 SMF119TN_PIStartDate    Char(4),   /* Date when Telnet      @VDA*/         
*                                       /*   started in 0CYYDDDF     */         
*  3 SMF119TN_PIProfStck     Char(8),   /* Stck time when Telnet @VDA*/         
*                                       /*  profile processed        */         
*  3 SMF119TN_PIProfTime     Fixed(31), /* Time when Telnet      @VDA*/         
*                                       /*  profile created in       */         
*                                       /*  hundredths of seconds    */         
*  3 SMF119TN_PIProfDate     Char(4),   /* Date when Telnet      @VDA*/         
*                                       /*  profile created          */         
*                                       /*   in 0CYYDDDF             */         
*                                                                               
*  3 SMF119TN_PIFlags        Bit(16),   /* flags                 @VDA*/         
*    5 SMF119TN_PIInitProfile Bit(1),   /* This is the initial   @VDA*/         
*                                       /*  profile                  */         
*                                       /*  0 - Obeyfile             */         
*    5 *                     Bit(7),    /* Reserved              @VDA*/         
*    5 *                     Bit(8),    /* Reserved              @VDA*/         
*  3 SMF119TN_PIresv3        Bit(16),   /* Reserved              @VDA*/         
*  3 SMF119TN_PIresv4        Fixed(31), /* Reserved              @VDA*/         
*  3 SMF119TN_PIConsName     Char(8),   /* Console name          @VDA*/         
*  3 SMF119TN_PIUserToken    Char(80),  /* Security user token -                
*                                          mapped by ICHRUTKN in                
*                                          MACLIB                @VDA*/         
*  3 SMF119TN_PI_End         Char(0)    Bdy(Word);            /* @VDA*/         
*                                                                               
*Declare                                                                        
*  SMF119TN_PI_X             Fixed     CONSTANT(1),           /* @VDA*/         
*                                       /* Section index             */         
*  SMF119TN_PIEyeC           Char(4)   CONSTANT('TNPI');      /* @VDA*/         
*                                   /* SMF119TN_PIEye field constant */         
*                                                                               
*/********************************************************************/         
*/* Profile Information Data Set Name section                        */         
*/*        SMF type 119 Subtype 24 Section 2  (SMF119S2)             */         
*/*        The first entry is for the base profile data set          */         
*/*        The rest are for any include files.                       */         
*/********************************************************************/         
*                                                                               
*Declare                                                                        
*1 SMF119TN_DS               Based,                           /* @VDA*/         
*  3 SMF119TN_DSEye          Char(4),  /* "TNDS" eyecatcher      @VDA*/         
*  3 SMF119TN_DSFlag         Bit(8),   /* flags                  @VDA*/         
*    5 SMF119TN_DSresv1      Bit(1),                          /* @VDA*/         
*    5 SMF119TN_DSInclude    Bit(1),   /* If set, INCLUDE data   @VDA*/         
*                                      /* set. else main data set    */         
*    5 SMF119TN_DSresv2      Bit(6),                          /* @VDA*/         
*  3 SMF119TN_DSresv3        Char(1),                         /* @VDA*/         
*  3 SMF119TN_DSName         Char(54), /* Dataset(Member) name   @VDA*/         
*  3 SMF119TN_DS_End         Char(0)   Bdy(Word);             /* @VDA*/         
*                                                                               
*Declare                                                                        
*  SMF119TN_DS_X             Fixed     CONSTANT(2),           /* @VDA*/         
*                                       /* Section index             */         
*  SMF119TN_DSEyeC           Char(4)    CONSTANT('TNDS');     /* @VDA*/         
*                                   /* SMF119TN_DSEye field constant */         
*                                                                               
*/********************************************************************/         
*/* Telnet Global parameters section                                 */         
*/*        SMF type 119 Subtype 24 Section 3 (SMF119S3)              */         
*/********************************************************************/         
*                                                                               
*Declare                                                                        
*1 SMF119TN_TG               Based,                           /* @VDA*/         
*  3 SMF119TN_TGEye          Char(4),  /* "TNTG" eyecatcher      @VDA*/         
*  3 SMF119TN_TGFlags        Bit(32),  /* Flags                  @VDA*/         
*   5 SMF119TN_TGFlag1       Bit(8),   /* Flags                  @VDA*/         
*    7 SMF119TN_TGFmt_Long   Bit(1),   /* FORMAT LONG        1   @VDA*/         
*                                      /* FORMAT SHORT       0       */         
