*    7 SMF119TN_AGExit       Bit(1),   /* EXIT defined with group@VDA*/         
*   5 SMF119TN_AGFlag1       Bit(8),   /* Flag byte 1            @VDA*/         
*   5 SMF119TN_AGFlag2       Bit(8),   /* Flag byte 2            @VDA*/         
*   5 SMF119TN_AGCapacity    Fixed(8), /* Capacity percentage    @VDA*/         
* 3 SMF119TN_AGRngCnt        Fixed(31),/* Total number of ranges @VMC*/         
*                                      /*   in the group             */         
* 3 SMF119TN_AGRngIdx        Fixed(31),/* Index into ranges      @VDA*/         
*                                      /*   defined in this entry    */         
* 3 SMF119TN_AGRngNum        Fixed(31),/* Total number of ranges @VMC*/         
*                                      /*   returned in this entry   */         
* 3 SMF119TN_AGCount         Fixed(31),/* Number of LUs defined  @VDA*/         
* 3 SMF119TN_AGRng(1:SMF119TN_AGRngMax),                      /* @VDA*/         
*                                                                               
*   5 SMF119TN_AGRngLow      Char(8),  /* Low value of range     @VDA*/         
*   5 SMF119TN_AGRngHigh     Char(8),  /* High value of range    @VDA*/         
*   5 SMF119TN_AGRngRule     Char(8),  /* Rule defining the range@VDA*/         
*                                      /*  If the value is c'FFFFFFFF*/         
*                                      /*  then it is a single LU    */         
* 3 SMF119TN_AG_End         Char(0)   Bdy(Word);              /* @VDA*/         
*                                                                               
*Declare                                                                        
*  SMF119TN_AGRngMax       Fixed     CONSTANT(10),            /* @VDA*/         
*                                       /* Maximum number of ranges  */         
*                                       /*  defined in an entry      */         
