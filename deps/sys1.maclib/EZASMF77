*                                                                               
* DCL                                                                           
*  1 SMF119IS_IPDynamicStack BASED,                                             
*    3 *                              FIXED(32),                                
*      5 SMF119IS_IPDynStackShadow  BIT(1),                                     
*      5 *                          BIT(31),                                    
*    3 SMF119IS_IPDynStackLifesizeCur BIT(64),                                  
*    3 SMF119IS_IPDynStackOutPkt      BIT(64),                                  
*    3 SMF119IS_IPDynStackInPkt       BIT(64),                                  
*    3 SMF119IS_IPDynStackOutBytes    BIT(64),                                  
*    3 SMF119IS_IPDynStackInBytes     BIT(64);                /* @E1A*/         
*                                                                               
*/********************************************************************/         
*/* This section reports stack specific info about an IPSec tunnel.  */         
*/* It is used by the following records:                             */         
*/* IPSec Manual Tunnel Deactivation record (subtype 80)             */         
*/********************************************************************/         
*/********************************************************************/         
*/* IP manual tunnel specific data                                   */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119IS_IPManualTunnel BASED,                                             
*    3 SMF119IS_IPManTunOutPkt   BIT(64),                                       
*    3 SMF119IS_IPManTunInPkt    BIT(64),                                       
*    3 SMF119IS_IPManTunOutBytes BIT(64),                                       
*    3 SMF119IS_IPManTunInBytes  BIT(64),                                       
*    3 SMF119IS_IPManTunEnd      CHAR(0);                     /* @E1A*/         
*                                                                               
* Declare /* Exchange mode  SMF119IS_IKETunExchangeMode              */         
*   SMF119IS_IKETUN_EXCHAGGRESSIVE FIXED(8) CONSTANT(4),                        
*   SMF119IS_IKETUN_EXCHMAIN       FIXED(8) CONSTANT(2);                        
*                                                                               
* Declare /* Tunnel states SMF119IS_IKETunState & SMF119IS_IPTunState*/         
*   SMF119IS_SASTATE_DEACT   FIXED(8) CONSTANT(1), /* Tunnel         */         
*   SMF119IS_SASTATE_ACTIVE  FIXED(8) CONSTANT(2), /* Active         */         
*   SMF119IS_SASTATE_EXPIRED FIXED(8) CONSTANT(3); /* Expired    @E1A*/         
*                                                                               
* Declare /* Authentication algorithm for:                                      
*                                      SMF119IS_IKETunAuthAlg                   
*                                      SMF119IS_IKETunPseudoRandomFunc          
*                                      SMF119IS_IPTunAuthAlg         */         
*   SMF119IS_AUTH_NULL              FIXED(8) CONSTANT(0),      /*@H4A*/         
*   SMF119IS_AUTH_AES_GMAC_128      FIXED(8) CONSTANT(4),      /*@H4A*/         
*   SMF119IS_AUTH_AES_GMAC_256      FIXED(8) CONSTANT(6),      /*@H4A*/         
*   SMF119IS_AUTH_HMAC_SHA2_256_128 FIXED(8) CONSTANT(7),      /*@H4A*/         
*   SMF119IS_AUTH_AES128_XCBC_96    FIXED(8) CONSTANT(9),      /*@H4A*/         
*   SMF119IS_AUTH_HMAC_SHA2_384_192 FIXED(8) CONSTANT(13),     /*@H4A*/         
*   SMF119IS_AUTH_HMAC_SHA2_512_256 FIXED(8) CONSTANT(14),     /*@H4A*/         
*   SMF119IS_AUTH_HMAC_SHA2_256     FIXED(8) CONSTANT(15),     /*@H7A*/         
*   SMF119IS_AUTH_HMAC_SHA2_384     FIXED(8) CONSTANT(16),     /*@H7A*/         
*   SMF119IS_AUTH_HMAC_SHA2_512     FIXED(8) CONSTANT(17),     /*@H7A*/         
*   SMF119IS_AUTH_AES128_XCBC       FIXED(8) CONSTANT(18),     /*@H7A*/         
*   SMF119IS_AUTH_HMAC_MD5          FIXED(8) CONSTANT(38),                      
*   SMF119IS_AUTH_HMAC_SHA1         FIXED(8) CONSTANT(39),     /*@E1A*/         
*   SMF119IS_AUTH_HMAC_MD5_96       FIXED(8) CONSTANT(40),     /*@D3A*/         
*   SMF119IS_AUTH_HMAC_SHA1_96      FIXED(8) CONSTANT(41);     /*@D3A*/         
*                                                                               
* Declare /* Encryption algorithm SMF119IS_IKETunEncryptAlg and                 
*                                 SMF119IS_IPTunEncryptAlg           */         
*   SMF119IS_ENCR_NONE       FIXED(8) CONSTANT(0),  /*No encryption  */         
*   SMF119IS_ENCR_NULL       FIXED(8) CONSTANT(11), /*NULL encryption*/         
*   SMF119IS_ENCR_DES        FIXED(8) CONSTANT(18), /*DES encryption */         
*   SMF119IS_ENCR_3DES       FIXED(8) CONSTANT(3),  /*3DES encryption*/         
*   SMF119IS_ENCR_AES        FIXED(8) CONSTANT(12), /*AES encr.      */         
*   SMF119IS_ENCR_AES_CBC    FIXED(8) CONSTANT(12), /*AES encr.  @H4A*/         
*   SMF119IS_ENCR_AES_GCM_16 FIXED(8) CONSTANT(20); /*AES-GCM-16 @H4A*/         
*                                                                               
* Declare /* Peer authentication method SMF119IS_IKETunPeerAuthMethod*/         
*   SMF119IS_IKETUN_PRESHAREDKEY FIXED(8) CONSTANT(3), /* Pre-shared */         
*   SMF119IS_IKETUN_RSASIGNATURE FIXED(8) CONSTANT(2), /*   @E1A @H5C*/         
*   SMF119IS_IKETUN_ECDSA_256    FIXED(8) CONSTANT(4), /*        @H5A*/         
*   SMF119IS_IKETUN_ECDSA_384    FIXED(8) CONSTANT(5), /*        @H5A*/         
*   SMF119IS_IKETUN_ECDSA_521    FIXED(8) CONSTANT(6), /*        @H5A*/         
*/* Local authentication method (all of the above plus the following)*/         
*   SMF119IS_IKETUN_DS           Fixed(8) CONSTANT(7); /*        @HDA*/         
*                                                                               
* Declare /* Role SMF119IS_IKETunRole                                */         
*   SMF119IS_IKETUN_INITIATOR FIXED(8) CONSTANT(1), /* Initiator     */         
*   SMF119IS_IKETUN_RESPONDER FIXED(8) CONSTANT(2); /* Responder @E1A*/         
*                                                                               
* Declare /* NAT traversal support level  SMF119IS_IKETunNATTLevel   */         
*   SMF119IS_IKETUN_NATTNONE  FIXED(8) CONSTANT(0), /* No NAT        */         
*   SMF119IS_IKETUN_NATTRFCD2 FIXED(8) CONSTANT(1), /* RFC draft 2   */         
*   SMF119IS_IKETUN_NATTRFCD3 FIXED(8) CONSTANT(3), /* RFC draft 3   */         
*   SMF119IS_IKETUN_NATTRFC   FIXED(8) CONSTANT(4), /* RFC support   */         
*   SMF119IS_IKETUN_NATTZOS   FIXED(8) CONSTANT(5), /* RFC z/OS  @M7C*/         
*   SMF119IS_IKETUN_NATTV2    FIXED(8) CONSTANT(6), /* IKEv2     @M7A*/         
*   SMF119IS_IKETUN_NATTV2ZOS FIXED(8) CONSTANT(7); /* IKEv2 z/OS@M7A*/         
*                                                                               
* Declare /* Tunnel Extended state info SMF119IS_IKETunExtState and             
*                                       SMF119IS_IPDynIKEExtState    */         
*   SMF119IS_EXTSASTATE_ACTIVATE FIXED(8) CONSTANT(1), /*Phase 1/2act*/         
*   SMF119IS_EXTSASTATE_REFRESH  FIXED(8) CONSTANT(2), /*Phase 1/2ref*/         
*   SMF119IS_EXTSASTATE_DEACT    FIXED(8) CONSTANT(3), /*Deact w/out            
*                                                     neg failure    */         
*   SMF119IS_EXTSASTATE_PROPOSAL FIXED(8) CONSTANT(4), /* Neg failure           
*                                                    - no prop chosen*/         
*   SMF119IS_EXTSASTATE_RETRANS  FIXED(8) CONSTANT(5), /* Neg failure           
*                                                 - retransmit limit */         
*   SMF119IS_EXTSASTATE_POLICY   FIXED(8) CONSTANT(6), /* Neg failure           
*                                                  - Policy mismatch */         
*   SMF119IS_EXTSASTATE_OTHER    FIXED(8) CONSTANT(7), /* Neg failure           
*                                                 - Invalid data/err */         
*   SMF119IS_EXTSASTATE_NOINS    FIXED(8) CONSTANT(8); /* Stack err, P2         
*                                                   SA not installed */         
*                                                                               
* Declare /* Identity types SMF119IS_IKETunLIDType and                          
*                           SMF119IS_IKETunRIDType                   */         
*   SMF119IS_IDTYPE_IPV4        FIXED(8) CONSTANT(1), /* IPv4 addr ID*/         
*   SMF119IS_IDTYPE_FQDN        FIXED(8) CONSTANT(2), /* Fully-qualDN*/         
*   SMF119IS_IDTYPE_EMAIL       FIXED(8) CONSTANT(3), /* Email addrID*/         
*   SMF119IS_IDTYPE_IPV4_SUBNET FIXED(8) CONSTANT(4), /* IPv4subnetID*/         
*   SMF119IS_IDTYPE_IPV6        FIXED(8) CONSTANT(5), /* IPv6 addr ID*/         
*   SMF119IS_IDTYPE_IPV6_SUBNET FIXED(8) CONSTANT(6), /* IPv6subnetID*/         
*   SMF119IS_IDTYPE_IPV4_RANGE  FIXED(8) CONSTANT(7), /* IPv4 addr ID*/         
*   SMF119IS_IDTYPE_IPV6_RANGE  FIXED(8) CONSTANT(8), /* IPv6 addr ID*/         
*   SMF119IS_IDTYPE_X500DN      FIXED(8) CONSTANT(9); /* X.500distnam*/         
*                                                                               
* Declare /* Tunnel type SMF119IS_IPTunType */                                  
*   SMF119IS_IPTUN_MANUAL FIXED(8) CONSTANT(1), /* Manual tunnel     */         
*   SMF119IS_IPTUN_STACK  FIXED(8) CONSTANT(2), /* Dynamic tunnel stk*/         
*   SMF119IS_IPTUN_IKE    FIXED(8) CONSTANT(3); /* Dynamic tunnel IKE*/         
*                                                                               
* Declare /* Encapsulation mode SMF119IS_IPTunEncapMode              */         
*   SMF119IS_IPTUN_TUNNELMODE    FIXED(8) CONSTANT(1), /* Tunnelencap*/         
*   SMF119IS_IPTUN_TRANSPORTMODE FIXED(8) CONSTANT(2); /* Transport             
*                                                         encap mode */         
*                                                                               
* Declare /* Tunnel activation method SMF119IS_IPDynActMethod        */         
*   SMF119IS_DYNTUN_USER     FIXED(8) CONSTANT(1), /* User activation*/         
*   SMF119IS_DYNTUN_REMOTE   FIXED(8) CONSTANT(2), /* Remote activati*/         
*   SMF119IS_DYNTUN_ONDEMAND FIXED(8) CONSTANT(3), /* On-demand activ*/         
*   SMF119IS_DYNTUN_TAKEOVER FIXED(8) CONSTANT(5), /* SWSA takeover a*/         
*   SMF119IS_DYNTUN_AUTOACT  FIXED(8) CONSTANT(6); /* Auto-activation*/         
*                                                                               
*                                                             /*7@F3D*/         
*                                                                               
* /* Maximum SMF 119 record size                                 @F3A*/         
* Dcl SMF_MAX_RECLEN  Fixed(16)  Constant(32756);              /*@F3A*/         
*                                                                               
* /* Maximum SMF 119 record size for SMF records which are supported */         
* /* on the real-time SMF NMI, SYSTCPSM.  The SMF records written    */         
* /* to SYSTCPSM are written like CTRACE records.  So some of the    */         
* /* SMF record storage size needs to be reserved for CTRACE record  */         
* /* fields.  The space remaining for the SMF record is calculated   */         
* /* as follows:                                                     */         
* /*   32768  The maximum length of a CTRACE record.                 */         
* /*   -  16  The length of the CTE prologue header                  */         
* /*   -   2  The length of the CTE epilogue                         */         
* /*   -   4  Reserved bytes for IPCS problem which may be fixed     */         
* /* -------                                                         */         
* /*   32746  Max size of SMF record                             @FCA*/         
* Dcl SMF_MAX_RECLEN_NMI  Fixed(16)  Constant(32746);          /*@FCA*/         
*                                                                               
*%end;                                                                          
