*%GOTO SMF772; /*                                                               
.M010000 ANOP                                                                   
         AIF   ('&HEADER' NE 'YES').M020000                                     
*---------------------------------------------------------------------*         
*                                                                               
* SMF Type 119 Record                                                           
*                                                                               
SMF119Header   DSECT ,          SMF Record Header                               
SMF119HDLength  DS  BL2         Record length                                   
SMF119HDSegDesc DS  BL2         Segment descriptor                              
SMF119HDFlags   DS  BL1         Header flags                                    
SMF119HDSUB    EQU X'40'        Subtypes are valid                              
SMF119HDSP4    EQU X'10'        MVS/SP Version 4 and above                      
SMF119HDSP3    EQU X'08'        MVS/SP Version 3 and above                      
SMF119HDSP2    EQU X'04'        MVS/SP Version 2 and above                      
SMF119HDVS2    EQU X'02'        MVS/VS2                                         
SMF119HDType    DS  BL1         Record type (119, x'77')                        
SMF119HDTime    DS  CL4         Time record was moved to SMF buffer             
SMF119HDDate    DS  PL4         Date record was moved to SMF buffer             
SMF119HDSID     DS  CL4         System ID                                       
SMF119HDSSI     DS  CL4         Subsystem ID                                    
SMF119HDSubType DS  BL2         Record subtype                                  
SMF119HDST_TCPInit    EQU X'0001'                                               
SMF119HDST_TCPTerm    EQU X'0002'                                               
SMF119HDST_FTPClient  EQU X'0003'                                               
SMF119HDST_ProfEvent  EQU X'0004'                                @F9A           
SMF119HDST_TCPStats   EQU X'0005'                                               
SMF119HDST_IFStats    EQU X'0006'                                               
SMF119HDST_PortStats  EQU X'0007'                                               
SMF119HDST_StackSS    EQU X'0008'                                               
SMF119HDST_UDPClose   EQU X'000A'                                               
SMF119HDST_ZertDetail EQU X'000B'                           @47A @49C           
SMF119HDST_ZertSummary  EQU X'000C'                              @51A           
SMF119HDST_TNSvrInit  EQU X'0014'                                               
SMF119HDST_TNSvrTerm  EQU X'0015'                                               
SMF119HDST_TSOCInit   EQU X'0016'                                               
SMF119HDST_TSOCTerm   EQU X'0017'                                               
