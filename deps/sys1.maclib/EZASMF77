*********************************************************************           
* Constants used in IPSec SMF records subtypes 73-80                *           
*********************************************************************           
* Exchange mode                                                                 
SMF119IS_IKETUN_EXCHMAIN       EQU 2   Main mode                 @E1A           
SMF119IS_IKETUN_EXCHAGGRESSIVE EQU 4   Aggressive mode           @E1A           
*                                                                               
* Tunnel states                                                                 
SMF119IS_SASTATE_DEACT         EQU 1   Tunnel is deactivated     @E1A           
SMF119IS_SASTATE_ACTIVE        EQU 2   Tunnel is active          @E1A           
SMF119IS_SASTATE_EXPIRED       EQU 3   Tunnel is expired         @E1A           
*                                                                               
* Authentication algorithm for:                                                 
*                             SMF119IS_IKETunAuthAlg                            
*                             SMF119IS_IKETunPseudoRandomFunc                   
*                             SMF119IS_IPTunAuthAlg                             
SMF119IS_AUTH_NULL              EQU 0   NULL auth                @H4A           
SMF119IS_AUTH_AES_GMAC_128      EQU 4   AES-GMAC-128 auth        @H4A           
SMF119IS_AUTH_AES_GMAC_256      EQU 6   AES-GMAC-256 auth        @H4A           
SMF119IS_AUTH_HMAC_SHA2_256_128 EQU 7   HMAC-SHA2-256-128 auth   @H4A           
SMF119IS_AUTH_AES128_XCBC_96    EQU 9   AES128-XCBC-96 auth      @H4A           
SMF119IS_AUTH_HMAC_SHA2_384_192 EQU 13  HMAC-SHA2-384-192 auth   @H4A           
SMF119IS_AUTH_HMAC_SHA2_512_256 EQU 14  HMAC-SHA2-512-256 auth   @H4A           
SMF119IS_AUTH_HMAC_SHA2_256     EQU 15  HMAC-SHA2-256 auth       @H7A           
SMF119IS_AUTH_HMAC_SHA2_384     EQU 16  HMAC-SHA2-384 auth       @H7A           
SMF119IS_AUTH_HMAC_SHA2_512     EQU 17  HMAC-SHA2-512 auth       @H7A           
SMF119IS_AUTH_AES128_XCBC       EQU 18  AES128-XCBC auth         @H7A           
SMF119IS_AUTH_HMAC_MD5          EQU 38  HMAC-MD5 authentication  @E1A           
SMF119IS_AUTH_HMAC_SHA1         EQU 39  HMAC-SHA1 authentication @E1A           
SMF119IS_AUTH_HMAC_MD5_96       EQU 40  HMAC-MD5-96 auth         @D3A           
SMF119IS_AUTH_HMAC_SHA1_96      EQU 41  HMAC-SHA1-96 auth        @D3A           
*                                                                               
* Encryption algorithm for:                                                     
*                         SMF119IS_IKETunEncryptAlg                             
*                         SMF119IS_IPTunEncryptAlg                              
SMF119IS_ENCR_NONE             EQU 0   No encryption             @E1A           
SMF119IS_ENCR_3DES             EQU 3   3DES encryption      @E1A @HBM           
SMF119IS_ENCR_NULL             EQU 11  NULL encryption           @E1A           
SMF119IS_ENCR_AES              EQU 12  AES encryption            @E1A           
SMF119IS_ENCR_AES_CBC          EQU 12  AES encryption            @H4A           
SMF119IS_ENCR_DES              EQU 18  DES encryption       @E1A @HBM           
SMF119IS_ENCR_AES_GCM_16       EQU 20  AES-GCM-16 encryption     @H4A           
*                                                                               
* Peer authentication method                                                    
SMF119IS_IKETUN_PRESHAREDKEY   EQU 3   Pre-shared key            @E1A           
SMF119IS_IKETUN_RSASIGNATURE   EQU 2   RSA signature             @E1A           
SMF119IS_IKETUN_ECDSA_256      EQU 4   ECDSA-256                 @H5A           
SMF119IS_IKETUN_ECDSA_384      EQU 5   ECDSA-384                 @H5A           
SMF119IS_IKETUN_ECDSA_521      EQU 6   ECDSA-521                 @H5A           
* Local authentication method (all of the above plus the following)             
SMF119IS_IKETUN_DS             EQU 7   DigitalSignature          @HDA           
*                                                                               
* Role                                                                          
SMF119IS_IKETUN_INITIATOR      EQU 1   Initiator                 @E1A           
SMF119IS_IKETUN_RESPONDER      EQU 2   Responder                 @E1A           
*                                                                               
* NAT traversal support level                                                   
SMF119IS_IKETUN_NATTNONE       EQU 0   No NAT traversal support  @E1A           
SMF119IS_IKETUN_NATTRFCD2      EQU 1   RFC draft 2 support       @E1A           
SMF119IS_IKETUN_NATTRFCD3      EQU 3   RFC draft 3 support       @E1A           
SMF119IS_IKETUN_NATTRFC        EQU 4   RFC support               @E1A           
SMF119IS_IKETUN_NATTZOS        EQU 5   RFC support with z/OS peer@E1A           
SMF119IS_IKETUN_NATTV2         EQU 6   IKEv2                     @M7A           
SMF119IS_IKETUN_NATTV2ZOS      EQU 7   IKEv2 with z/OS peer      @M7A           
*                                                                               
* Extended state information                                                    
SMF119IS_EXTSASTATE_ACTIVATE   EQU 1 Phase 1/2 activation        @E1A           
SMF119IS_EXTSASTATE_REFRESH    EQU 2 Phase 1/2 refresh           @E1A           
SMF119IS_EXTSASTATE_DEACT      EQU 3 Deactivation w/out neg failu@E1A           
SMF119IS_EXTSASTATE_PROPOSAL   EQU 4 Neg failure -no prop chosen @E1A           
SMF119IS_EXTSASTATE_RETRANS    EQU 5 Neg failure -retransmit limi@E1A           
SMF119IS_EXTSASTATE_POLICY     EQU 6 Neg failure -Policy mismatch@E1A           
SMF119IS_EXTSASTATE_OTHER      EQU 7 Neg failure -Invalid data/er@E1A           
SMF119IS_EXTSASTATE_NOINS      EQU 8 Stack err, P2 SA not install@E1A           
*                                                                               
* Identity types                                                                
SMF119IS_IDTYPE_IPV4           EQU 1  IPv4 address ID            @E1A           
SMF119IS_IDTYPE_FQDN           EQU 2  Fully-qualified domain name@E1A           
SMF119IS_IDTYPE_EMAIL          EQU 3  Email address ID           @E1A           
SMF119IS_IDTYPE_IPV4_SUBNET    EQU 4  IPv4 subnet ID             @E1A           
SMF119IS_IDTYPE_IPV6           EQU 5  IPv6 address ID            @E1A           
SMF119IS_IDTYPE_IPV6_SUBNET    EQU 6  IPv6 subnet ID             @E1A           
SMF119IS_IDTYPE_IPV4_RANGE     EQU 7  IPv4 address range ID      @E1A           
SMF119IS_IDTYPE_IPV6_RANGE     EQU 8  IPv6 address range ID      @E1A           
SMF119IS_IDTYPE_X500DN         EQU 9  X.500 distinguished nam    @E1A           
*                                                                               
* Tunnel type                                                                   
SMF119IS_IPTUN_MANUAL          EQU 1  Manual tunnel              @E1A           
SMF119IS_IPTUN_STACK           EQU 2  Dynamic tunnel, stack view @E1A           
SMF119IS_IPTUN_IKE             EQU 3  Dynamic tunnel, IKE view   @E1A           
*                                                                               
* Encapsulation mode                                                            
SMF119IS_IPTUN_TUNNELMODE      EQU 1  Tunnel encap mode          @E1A           
SMF119IS_IPTUN_TRANSPORTMODE   EQU 2  Transport encap mode       @E1A           
*                                                                               
* Tunnel activation method                                                      
SMF119IS_DYNTUN_USER           EQU 1  User activation            @E1A           
SMF119IS_DYNTUN_REMOTE         EQU 2  Remote activation          @E1A           
SMF119IS_DYNTUN_ONDEMAND       EQU 3  On-demand activation       @E1A           
SMF119IS_DYNTUN_TAKEOVER       EQU 5  SWSA takeover activation   @E1A           
SMF119IS_DYNTUN_AUTOACT        EQU 6  Auto-activation                           
*                                                                               
.M070000 ANOP                                                    @E1A           
         AIF   ('&PROF' NE 'YES').M080000                        @FAA           
*---------------------------------------------------------------------*         
* TCP/IP Profile record                                                         
*                                                                               
* The TCP/IP Profile record is created as an SMF 119 event record               
* with a subtype of 4.    It provides TCP/IP stack profile                      
* information.                                                                  
* The record is comprised, potentially, of 24 separate sections.                
* The first section is always the TCP/IP Identification section.                
* Macro EZBNMMPA maps the sections which follow the TCP/IP                      
* Identification section.                                          @H6C         
*---------------------------------------------------------------------*         
* The following constant value is set in field SMF119SD_TRN for                 
* this record.                                                @F9A @FAC         
*---------------------------------------------------------------------*         
SMF119SD_TRN_TP       EQU 24    TCP/IP Profile record # of triplets@6BC         
*---------------------------------------------------------------------*         
* Include mapping macro EZBNMMPA                                   @FAA         
         EZBNMMPA                                                  @FAA         
*---------------------------------------------------------------------*         
.M080000 ANOP                                                      @FAA         
         AIF   ('&VTAM' NE 'YES').M090000                          @4BA         
*---------------------------------------------------------------------*         
* Include mapping macro ISTSMF77                                   @4BA         
        ISTSMF77                                                   @4BA         
*---------------------------------------------------------------------*         
.M090000 ANOP                                                      @4BA         
        MEND       */                                                           
*%SMF772: ;                                                                     
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* Mapping for the Common SMF record header that is part of all     */         
*/* TCP/IP SMF records. The only fields that need to be set by the   */         
*/* SMF processing are the Length, Flags, Type, and Subtype values.  */         
*/* The rest are filled in by MVS SMF processing.                    */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119Header BASED BDY(WORD),  /* Header overlay                */         
*    3 SMF119HDLength    FIXED(15), /* Length of record              */         
*    3 SMF119HDSegDesc   FIXED(15), /* Segment Descriptor            */         
*    3 SMF119HDFlags     BIT(8),    /* System Indicator              */         
*      5 *               BIT(1),    /* Reserved                      */         
*      5 SMF119HDSub     BIT(1),    /* Subtypes are valid.           */         
*      5 *               BIT(1),    /* Reserved                      */         
*      5 SMF119HDSP4     BIT(1),    /* MVS/SP Version 4 and above    */         
*      5 SMF119HDSP3     BIT(1),    /* MVS/SP Version 3 and above    */         
*      5 SMF119HDSP2     BIT(1),    /* MVS/SP Version 2 and above    */         
*      5 SMF119HDVS2     BIT(1),    /* MVS/VS2                       */         
*      5 *               BIT(1),    /* Reserved                      */         
*    3 SMF119HDType      FIXED(8),  /* Record type number is 119,               
*                                      or hex 77.                    */         
*    3 SMF119HDTime      FIXED(31) BDY(HWORD), /* TOD, Using format             
*                                      from TIME macro with BINARY   */         
*    3 SMF119HDDate      CHAR(4),   /* DATE Record written CCYYDDDS  */         
*    3 SMF119HDSID       CHAR(4),   /* SYSTEM Id from Installation   */         
*    3 SMF119HDSSI       CHAR(4),   /* Subsystem identification      */         
*    3 SMF119HDSubType   FIXED(16), /* SMF sub type field            */         
*    3 SMF119HDEnd       CHAR(0);   /* End of header mapping         */         
*                                                                               
*/********************************************************************/         
*/* Constants for SMF119HDType Field                                 */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119Type             FIXED(8) CONSTANT(119);                              
*                                                                               
*/********************************************************************/         
*/* Constants for SMF119HDFlags Field                                */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FlagsValue       BIT(8) CONSTANT('50'X); /* Subtypes are              
*                                      valid and MVS V4 and above    */         
*                                                                               
*/********************************************************************/         
*/* Constants for SMF119HDSubType field                              */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119HDST_TCPInit     FIXED(16) CONSTANT(1),                               
*   SMF119HDST_TCPTerm     FIXED(16) CONSTANT(2),                               
*   SMF119HDST_FTPClient   FIXED(16) CONSTANT(3),                               
*   SMF119HDST_ProfEvent   FIXED(16) CONSTANT(4),             /* @F1A*/         
*   SMF119HDST_TCPStats    FIXED(16) CONSTANT(5),                               
*   SMF119HDST_IFStats     FIXED(16) CONSTANT(6),                               
*   SMF119HDST_PortStats   FIXED(16) CONSTANT(7),                               
*   SMF119HDST_StackSS     FIXED(16) CONSTANT(8),                               
*   SMF119HDST_UDPClose    FIXED(16) CONSTANT(10),                              
*   SMF119HDST_ZertDetail  FIXED(16) CONSTANT(11),       /* @47A @49C*/         
*   SMF119HDST_ZertSummary FIXED(16) CONSTANT(12),            /* @51A*/         
*   SMF119HDST_TNSvrInit   FIXED(16) CONSTANT(20),                              
*   SMF119HDST_TNSvrTerm   FIXED(16) CONSTANT(21),                              
*   SMF119HDST_TSOCInit    FIXED(16) CONSTANT(22),                              
*   SMF119HDST_TSOCTerm    FIXED(16) CONSTANT(23),                              
*   SMF119HDST_TnProfile   FIXED(16) CONSTANT(24),            /* @VDA*/         
*   SMF119HDST_DVIPAStChg  FIXED(16) CONSTANT(32),            /* @H8A*/         
*   SMF119HDST_DVIPARem    FIXED(16) CONSTANT(33),            /* @H8A*/         
*   SMF119HDST_TgtAdd      FIXED(16) CONSTANT(34),            /* @H8A*/         
*   SMF119HDST_TgtRem      FIXED(16) CONSTANT(35),            /* @H8A*/         
*   SMF119HDST_TgtSvrStrt  FIXED(16) CONSTANT(36),            /* @H8A*/         
*   SMF119HDST_TgtSvrEnd   FIXED(16) CONSTANT(37),            /* @H8A*/         
*   SMF119HDST_SmcdLnkStats FIXED(16) CONSTANT(38),           /* @41A*/         
*   SMF119HDST_SmcdLnkStart FIXED(16) CONSTANT(39),           /* @41A*/         
*   SMF119HDST_SmcdLnkEnd   FIXED(16) CONSTANT(40),           /* @41A*/         
*   SMF119HDST_SmcrGrpstats FIXED(16) CONSTANT(41),           /* @V7A*/         
*   SMF119HDST_SmcrLnkStart FIXED(16) CONSTANT(42),           /* @V7A*/         
*   SMF119HDST_SmcrLnkEnd  FIXED(16) CONSTANT(43),            /* @V7A*/         
*   SMF119HDST_RnicStats   FIXED(16) CONSTANT(44),            /* @VCA*/         
*   SMF119HDST_IsmStats    FIXED(16) CONSTANT(45),            /* @41A*/         
*   SMF119HDST_SMTPConfig  Fixed(16) Constant(48),            /* @H2A*/         
*   SMF119HDST_SMTPConn    Fixed(16) Constant(49),            /* @H2A*/         
*   SMF119HDST_SMTPMail    Fixed(16) Constant(50),            /* @H2A*/         
*   SMF119HDST_SMTPSpool   Fixed(16) Constant(51),            /* @H2A*/         
*   SMF119HDST_SMTPStats   Fixed(16) Constant(52),            /* @H2A*/         
*   SMF119HDST_FTPServer   FIXED(16) CONSTANT(70),                              
*   SMF119HDST_FTPDConfig  FIXED(16) CONSTANT(71),            /* @V8A*/         
*   SMF119HDST_FTPLogonF   FIXED(16) CONSTANT(72),                              
*   SMF119HDST_IKETunAct_Ref   FIXED(16) CONSTANT(73),                          
*   SMF119HDST_IkeTunDeact_Exp FIXED(16) CONSTANT(74),                          
*   SMF119HDST_DynTunAct_Ref   FIXED(16) CONSTANT(75),                          
*   SMF119HDST_DynTunDeact     FIXED(16) CONSTANT(76),                          
*   SMF119HDST_DynTunAdd       FIXED(16) CONSTANT(77),                          
*   SMF119HDST_DynTunRem       FIXED(16) CONSTANT(78),                          
*   SMF119HDST_ManTunAct       FIXED(16) CONSTANT(79),                          
*   SMF119HDST_ManTunDeact     FIXED(16) CONSTANT(80),    /*@F1C @F6C*/         
*   SMF119HDST_DS3270      FIXED(16) CONSTANT(81), /* VTAM 3270 IDS             
*                                                                @4BA*/         
*   SMF119HDST_SSH94       FIXED(16) CONSTANT(94), /* OpenSSH x'5E'             
*                                                                @38A*/         
*   SMF119HDST_SSH95       FIXED(16) CONSTANT(95), /* OpenSSH x'5F'             
*                                                                @38A*/         
*   SMF119HDST_SSH96       FIXED(16) CONSTANT(96), /* OpenSSH x'60'             
*                                                           @F6A @38C*/         
*   SMF119HDST_SSH97       FIXED(16) CONSTANT(97), /* OpenSSH x'61'             
*                                                           @F6A @38C*/         
*   SMF119HDST_SSH98       FIXED(16) CONSTANT(98); /* OpenSSH x'62'             
*                                                           @F6A @38C*/         
*/*------------------------------------------------------------------*/         
*/*SMF record subtypes of 100-104 (x'64' - x'68') are provided by    */         
*/*FTP but are only available from the real-time SMF NMI interface,  */         
*/*SYSTCPSM.  They are not written to the MVS SMF data sets.         */         
*/*The subtype constants and mapping structures for these records    */         
*/*are defined in macro EZANMFTA in SEZANMAC.                    @VFA*/         
*/*------------------------------------------------------------------*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* Mappings for the Self Defining Section triplets. Each Type 119   */         
*/* record will have at least two triplets, so the basic mapping     */         
*/* will include at least that many triplets. Additional mappings    */         
*/* will be defined for records that have more triplets.             */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119SDefSect BASED(ADDR(SMF119HDEnd)),                                   
*    3 SMF119SD_TRN      FIXED(16), /* Number of triplets            */         
*    3 *                 CHAR(2),   /* Unused, available             */         
*                                                                               
*    /* The first triplet points to the TCP/IP Common Stack          */         
*    /* Identification section used by all Type 119 records.         */         
*                                                                               
*    3 SMF119IDOff       FIXED(32), /* Offset to Common section      */         
*    3 SMF119IDLen       FIXED(16), /* Length of Common section      */         
*    3 SMF119IDNum       FIXED(16), /* Number of Common sections     */         
*                                                                               
*    /* The next triplet defines the first of the record specific    */         
*    /* sections for a given record.                                 */         
*                                                                               
*    3 SMF119S1Off       FIXED(32), /* Offset, first record section  */         
*    3 SMF119S1Len       FIXED(16), /* Length, first record section  */         
*    3 SMF119S1Num       FIXED(16), /* Number, first record sections */         
*    3 SMF119S1End       CHAR(0);   /* End of Self Defining Section             
*                                      with two triplets             */         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (third) triplet, counting the Common Stack section.  */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDThreeTriplets BASED(ADDR(SMF119S1End)),                            
*    3 SMF119S2Off       FIXED(32), /* Offset, second record section */         
*    3 SMF119S2Len       FIXED(16), /* Length, second record section */         
*    3 SMF119S2Num       FIXED(16), /* Number, second record sections*/         
*    3 SMF119S2End       CHAR(0);   /* End of Self Defining Section             
*                                      with three triplets           */         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (fourth) triplet, counting the Common Stack section. */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDFourTriplets BASED(ADDR(SMF119S2End)),                             
*    3 SMF119S3Off       FIXED(32), /* Offset, third record section  */         
*    3 SMF119S3Len       FIXED(16), /* Length, third record section  */         
*    3 SMF119S3Num       FIXED(16), /* Number, third record sections */         
*    3 SMF119S3End       CHAR(0);   /* End of Self Defining Section             
*                                      with four triplets            */         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (fifth) triplet, counting the Common Stack section.  */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDFiveTriplets BASED(ADDR(SMF119S3End)),                             
*    3 SMF119S4Off       FIXED(32), /* Offset, fourth record section */         
*    3 SMF119S4Len       FIXED(16), /* Length, fourth record section */         
*    3 SMF119S4Num       FIXED(16), /* Number, fourth record sections*/         
*    3 SMF119S4End       CHAR(0);   /* End of Self Defining Section             
*                                      with five triplets            */         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (sixth) triplet, counting the Common Stack section.  */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDSixTriplets BASED(ADDR(SMF119S4End)),             /*@Q1A*/         
*    3 SMF119S5Off       FIXED(32), /* Offset, fifth section     @Q1A*/         
*    3 SMF119S5Len       FIXED(16), /* Length, fifth section     @Q1A*/         
*    3 SMF119S5Num       FIXED(16), /* Number, fifth sections    @Q1A*/         
*    3 SMF119S5End       CHAR(0);   /* End of Self Defining Section             
*                                      with six triplets         @Q1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (seventh) triplet, counting the Common Stack section */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDSevenTriplets BASED(ADDR(SMF119S5End)),           /*@Q1A*/         
*    3 SMF119S6Off       FIXED(32), /* Offset, sixth section     @Q1A*/         
*    3 SMF119S6Len       FIXED(16), /* Length, sixth section     @Q1A*/         
*    3 SMF119S6Num       FIXED(16), /* Number, sixth sections    @Q1A*/         
*    3 SMF119S6End       CHAR(0);   /* End of Self Defining Section             
*                                      with seven triplets       @Q1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (eighth) triplet, counting the Common Stack section  */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDEightTriplets BASED(ADDR(SMF119S6End)),           /*@E2A*/         
*    3 SMF119S7Off       FIXED(32), /* Offset, seventh section   @E2A*/         
*    3 SMF119S7Len       FIXED(16), /* Length, seventh section   @E2A*/         
*    3 SMF119S7Num       FIXED(16), /* Number, seventh sections  @E2A*/         
*    3 SMF119S7End       CHAR(0);   /* End of Self Defining Section             
*                                      with eight triplets       @E2A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (9th) triplet, counting the Common Stack section     */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD9Triplets BASED(ADDR(SMF119S7End)),               /*@F1A*/         
*    3 SMF119S8Off       FIXED(32), /* Offset, 8th section       @F1A*/         
*    3 SMF119S8Len       FIXED(16), /* Length, 8th section       @F1A*/         
*    3 SMF119S8Num       FIXED(16), /* Number, 8th sections      @F1A*/         
*    3 SMF119S8End       CHAR(0);   /* End of Self Defining Section             
*                                      with 9 triplets           @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (10th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD10Triplets BASED(ADDR(SMF119S8End)),              /*@F1A*/         
*    3 SMF119S9Off       FIXED(32), /* Offset, 9th section       @F1A*/         
*    3 SMF119S9Len       FIXED(16), /* Length, 9th section       @F1A*/         
*    3 SMF119S9Num       FIXED(16), /* Number, 9th sections      @F1A*/         
*    3 SMF119S9End       CHAR(0);   /* End of Self Defining Section             
*                                      with 10 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (11th) triplet, counting the Common Stack            */         
* /* section                                                         */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD11Triplets BASED(ADDR(SMF119S9End)),              /*@F1A*/         
*    3 SMF119S10Off      FIXED(32), /* Offset, 10th section      @F1A*/         
*    3 SMF119S10Len      FIXED(16), /* Length, 10th section      @F1A*/         
*    3 SMF119S10Num      FIXED(16), /* Number, 10th sections     @F1A*/         
*    3 SMF119S10End      CHAR(0);   /* End of Self Defining Section             
*                                      with 11 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (12th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD12Triplets BASED(ADDR(SMF119S10End)),             /*@F1A*/         
*    3 SMF119S11Off      FIXED(32), /* Offset, 11th section      @F1A*/         
*    3 SMF119S11Len      FIXED(16), /* Length, 11th section      @F1A*/         
*    3 SMF119S11Num      FIXED(16), /* Number, 11th sections     @F1A*/         
*    3 SMF119S11End      CHAR(0);   /* End of Self Defining Section             
*                                      with 12 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (13th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD13Triplets BASED(ADDR(SMF119S11End)),             /*@F1A*/         
*    3 SMF119S12Off      FIXED(32), /* Offset, 12th section      @F1A*/         
*    3 SMF119S12Len      FIXED(16), /* Length, 12th section      @F1A*/         
*    3 SMF119S12Num      FIXED(16), /* Number, 12th sections     @F1A*/         
*    3 SMF119S12End      CHAR(0);   /* End of Self Defining Section             
*                                      with 13 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (14th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD14Triplets BASED(ADDR(SMF119S12End)),             /*@F1A*/         
*    3 SMF119S13Off      FIXED(32), /* Offset, 13th section      @F1A*/         
*    3 SMF119S13Len      FIXED(16), /* Length, 13th section      @F1A*/         
*    3 SMF119S13Num      FIXED(16), /* Number, 13th sections     @F1A*/         
*    3 SMF119S13End      CHAR(0);   /* End of Self Defining Section             
*                                      with 14 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (15th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD15Triplets BASED(ADDR(SMF119S13End)),             /*@F1A*/         
*    3 SMF119S14Off      FIXED(32), /* Offset, 14th section      @F1A*/         
*    3 SMF119S14Len      FIXED(16), /* Length, 14th section      @F1A*/         
*    3 SMF119S14Num      FIXED(16), /* Number, 14th sections     @F1A*/         
*    3 SMF119S14End      CHAR(0);   /* End of Self Defining Section             
*                                      with 15 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (16th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD16Triplets BASED(ADDR(SMF119S14End)),             /*@F1A*/         
*    3 SMF119S15Off      FIXED(32), /* Offset, 15th section      @F1A*/         
*    3 SMF119S15Len      FIXED(16), /* Length, 15th section      @F1A*/         
*    3 SMF119S15Num      FIXED(16), /* Number, 15th sections     @F1A*/         
*    3 SMF119S15End      CHAR(0);   /* End of Self Defining Section             
*                                      with 16 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (17th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD17Triplets BASED(ADDR(SMF119S15End)),             /*@F1A*/         
*    3 SMF119S16Off      FIXED(32), /* Offset, 16th section      @F1A*/         
*    3 SMF119S16Len      FIXED(16), /* Length, 16th section      @F1A*/         
*    3 SMF119S16Num      FIXED(16), /* Number, 16th sections     @F1A*/         
*    3 SMF119S16End      CHAR(0);   /* End of Self Defining Section             
*                                      with 17 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (18th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD18Triplets BASED(ADDR(SMF119S16End)),             /*@F1A*/         
*    3 SMF119S17Off      FIXED(32), /* Offset, 17th section      @F1A*/         
*    3 SMF119S17Len      FIXED(16), /* Length, 17th section      @F1A*/         
*    3 SMF119S17Num      FIXED(16), /* Number, 17th sections     @F1A*/         
*    3 SMF119S17End      CHAR(0);   /* End of Self Defining Section             
*                                      with 18 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (19th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD19Triplets BASED(ADDR(SMF119S17End)),             /*@F1A*/         
*    3 SMF119S18Off      FIXED(32), /* Offset, 18th section      @F1A*/         
*    3 SMF119S18Len      FIXED(16), /* Length, 18th section      @F1A*/         
*    3 SMF119S18Num      FIXED(16), /* Number, 18th sections     @F1A*/         
*    3 SMF119S18End      CHAR(0);   /* End of Self Defining Section             
*                                      with 19 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (20th) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD20Triplets BASED(ADDR(SMF119S18End)),             /*@F1A*/         
*    3 SMF119S19Off      FIXED(32), /* Offset, 19th section      @F1A*/         
*    3 SMF119S19Len      FIXED(16), /* Length, 19th section      @F1A*/         
*    3 SMF119S19Num      FIXED(16), /* Number, 19th sections     @F1A*/         
*    3 SMF119S19End      CHAR(0);   /* End of Self Defining Section             
*                                      with 20 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (21st) triplet, counting the Common Stack section    */         
*                                                                               
* DCL                                                                           
*  1 SMF119SD21Triplets BASED(ADDR(SMF119S19End)),             /*@F1A*/         
*    3 SMF119S20Off      FIXED(32), /* Offset, 20th section      @F1A*/         
*    3 SMF119S20Len      FIXED(16), /* Length, 20th section      @F1A*/         
*    3 SMF119S20Num      FIXED(16), /* Number, 20th sections     @F1A*/         
*    3 SMF119S20End      CHAR(0);   /* End of Self Defining Section             
*                                      with 21 triplets          @F1A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (22nd) triplet, counting the Common Stack section@H6A*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD22Triplets BASED(ADDR(SMF119S20End)),             /*@H6A*/         
*    3 SMF119S21Off      FIXED(32), /* Offset, 21th section      @H6A*/         
*    3 SMF119S21Len      FIXED(16), /* Length, 21th section      @H6A*/         
*    3 SMF119S21Num      FIXED(16), /* Number, 21th sections     @H6A*/         
*    3 SMF119S21End      CHAR(0);   /* End of Self Defining Section             
*                                      with 22 triplets          @H6A*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (23nd) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD23Triplets BASED(ADDR(SMF119S21End)),             /*@VDA*/         
*    3 SMF119S22Off      FIXED(32), /* Offset, 22th section      @VDA*/         
*    3 SMF119S22Len      FIXED(16), /* Length, 22th section      @VDA*/         
*    3 SMF119S22Num      FIXED(16), /* Number, 22th sections     @VDA*/         
*    3 SMF119S22End      CHAR(0);   /* End of Self Defining Section             
*                                      with 23 triplets          @VDA*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (24th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD24Triplets BASED(ADDR(SMF119S22End)),             /*@VDA*/         
*    3 SMF119S23Off      FIXED(32), /* Offset, 23rd section      @VDA*/         
*    3 SMF119S23Len      FIXED(16), /* Length, 23rd section      @VDA*/         
*    3 SMF119S23Num      FIXED(16), /* Number, 23rd sections     @VDA*/         
*    3 SMF119S23End      CHAR(0);   /* End of Self Defining Section             
*                                      with 24 triplets          @VDA*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (25th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD25Triplets BASED(ADDR(SMF119S23End)),             /*@VDA*/         
*    3 SMF119S24Off      FIXED(32), /* Offset, 24th section      @VDA*/         
*    3 SMF119S24Len      FIXED(16), /* Length, 24th section      @VDA*/         
*    3 SMF119S24Num      FIXED(16), /* Number, 24th sections     @VDA*/         
*    3 SMF119S24End      CHAR(0);   /* End of Self Defining Section             
*                                      with 25 triplets          @VDA*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (26th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD26Triplets BASED(ADDR(SMF119S24End)),             /*@VDA*/         
*    3 SMF119S25Off      FIXED(32), /* Offset, 25th section      @VDA*/         
*    3 SMF119S25Len      FIXED(16), /* Length, 25th section      @VDA*/         
*    3 SMF119S25Num      FIXED(16), /* Number, 25th sections     @VDA*/         
*    3 SMF119S25End      CHAR(0);   /* End of Self Defining Section             
*                                      with 26 triplets          @VDA*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (27th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD27Triplets BASED(ADDR(SMF119S24End)),             /*@VDA*/         
*    3 SMF119S26Off      FIXED(32), /* Offset, 26th section      @VDA*/         
*    3 SMF119S26Len      FIXED(16), /* Length, 26th section      @VDA*/         
*    3 SMF119S26Num      FIXED(16), /* Number, 26th sections     @VDA*/         
*    3 SMF119S26End      CHAR(0);   /* End of Self Defining Section             
*                                      with 27 triplets          @VDA*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (28th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD28Triplets BASED(ADDR(SMF119S26End)),             /*@VDA*/         
*    3 SMF119S27Off      FIXED(32), /* Offset, 27th section      @VDA*/         
*    3 SMF119S27Len      FIXED(16), /* Length, 27th section      @VDA*/         
*    3 SMF119S27Num      FIXED(16), /* Number, 27th sections     @VDA*/         
*    3 SMF119S27End      CHAR(0);   /* End of Self Defining Section             
*                                      with 28 triplets          @VDA*/         
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (29th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD29Triplets BASED(ADDR(SMF119S27End)),             /*@VDA*/         
*    3 SMF119S28Off      FIXED(32), /* Offset, 28th section      @VDA*/         
*    3 SMF119S28Len      FIXED(16), /* Length, 28th section      @VDA*/         
*    3 SMF119S28Num      FIXED(16), /* Number, 28th sections     @VDA*/         
*    3 SMF119S28End      CHAR(0);   /* End of Self Defining Section             
*                                      with 29 triplets          @VDA*/         
*                                                                               
*                                                                               
* /* The next mapping extends the self defining section to have an   */         
* /* additional (30th) triplet, counting the Common Stack section@VDA*/         
*                                                                               
* DCL                                                                           
*  1 SMF119SD30Triplets BASED(ADDR(SMF119S28End)),             /*@VDA*/         
*    3 SMF119S29Off      FIXED(32), /* Offset, 29th section      @VDA*/         
*    3 SMF119S29Len      FIXED(16), /* Length, 29th section      @VDA*/         
*    3 SMF119S29Num      FIXED(16), /* Number, 29th sections     @VDA*/         
*    3 SMF119S29End      CHAR(0);   /* End of Self Defining Section             
*                                      with 30 triplets          @VDA*/         
*                                                                               
*                                                              /*@F8D*/         
*                                                                               
*/********************************************************************/         
*/* TCP/IP Profile record                                            */         
*/*                                                                  */         
*/* The TCP/IP Profile record is created as an SMF 119 event record  */         
*/* with a subtype of 4.  It provides TCP/IP stack profile           */         
*/* information.                                                     */         
*/* The record is comprised, potentially, of 24 separate sections.   */         
*/* The first section is always the TCP/IP Identification section.   */         
*/*                                                              @H6C*/         
*/*                                                                  */         
*/* The following constant can be used to process the Self-defining  */         
*/* section triplets for this record.                       @F3M @F8C*/         
*/********************************************************************/         
* DCL SMF119SD_TRN_TP    Fixed(16) CONSTANT(24);  /* TCP/IP Profile             
*                                 record # of triplets @F8C @H6C @6BC*/         
*                                                                               
* /* Mapping of a triplet in the Self-defining section               */         
*                                                                               
* DCL                                                                           
*  1 SMF119SDTriplet    BASED,                                 /*@F1A*/         
*    3 SMF119Off         FIXED(32), /* Offset of section         @F1A*/         
*    3 SMF119Len         FIXED(16), /* Length of section         @F1A*/         
*    3 SMF119Num         FIXED(16), /* Number of sections        @F1A*/         
*    3 SMF119End         CHAR(0);   /* End of triplet            @F1A*/         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* Mapping for the TCP/IP Common Stack Identification Section that  */         
*/* is included in all TCP/IP Type 119 SMF records. The section is   */         
*/* the first one included in the record, after the last of the      */         
*/* self defining triplets.                                          */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119Ident BASED,                                                         
*    3 SMF119TI_SYSName  CHAR(8),   /* System name                   */         
*    3 SMF119TI_SysplexName  CHAR(8), /* Sysplex name                */         
*    3 SMF119TI_Stack    CHAR(8),   /* Stack name                    */         
*    3 SMF119TI_ReleaseID  CHAR(8), /* CS/390 release identifier     */         
*    3 SMF119TI_Comp     CHAR(8),   /* TCP/IP subcomponent           */         
*    3 SMF119TI_ASName   CHAR(8),   /* Address space name of writer  */         
*    3 SMF119TI_UserID   CHAR(8),   /* User ID of security context   */         
*    3 SMF119TI_ASID     FIXED(32), /* ASID of writer            @L5C*/         
*      5 *               CHAR(2),   /* Filler                        */         
*      5 SMF119TI_ASID2  FIXED(16), /* ASID currently just 2 bytes   */         
*    3 SMF119TI_Reason   CHAR(1),   /* Reason for writing record     */         
*    3 SMF119TI_RecordID FIXED(8),  /* ID value used for correlating */         
*                                   /* records                   @F7A*/         
*    3 *                 CHAR(2),   /* Padding, available        @F7C*/         
*    3 SMF119TI_End      CHAR(0);   /* End of section mapping        */         
*                                                                               
*/********************************************************************/         
*/* Constants for SMF119TI_Comp field                                */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119TI_CompTCP     CHAR(8) CONSTANT('TCP     '),                          
*   SMF119TI_CompUDP     CHAR(8) CONSTANT('UDP     '),                          
*   SMF119TI_CompIP      CHAR(8) CONSTANT('IP      '),                          
*   SMF119TI_CompSTACK   CHAR(8) CONSTANT('STACK   '),                          
*   SMF119TI_CompTN3270S CHAR(8) CONSTANT('TN3270S '),                          
*   SMF119TI_CompTN3270C CHAR(8) CONSTANT('TN3270C '),                          
*   SMF119TI_CompFTPS    CHAR(8) CONSTANT('FTPS    '),                          
*   SMF119TI_CompFTPC    CHAR(8) CONSTANT('FTPC    '),                          
*   SMF119TI_CompIKE     CHAR(8) CONSTANT('IKE     '),        /* @E1C*/         
*   SMF119TI_CompCSSMTP  CHAR(8) CONSTANT('CSSMTP  '),        /* @H2C*/         
*   SMF119TI_CompSMCR    CHAR(8) CONSTANT('SMCR    '),        /* @V7A*/         
*   SMF119TI_CompSMCD    CHAR(8) CONSTANT('SMCD    '),        /* @41A*/         
*   SMF119TI_Comp3270    CHAR(8) CONSTANT('IDS3270 '),        /* @4BA*/         
*   SMF119TI_CompFTPD    CHAR(8) CONSTANT('FTPD    ');        /* @VBC*/         
*                                                                               
*/********************************************************************/         
*/* Constants for SMF119TI_Reason field                              */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119TI_ReasonIntInc CHAR(1) CONSTANT('C0'X), /* Interval record,          
*                                      incomplete reporting          */         
*   SMF119TI_ReasonInt  CHAR(1) CONSTANT('80'X), /* Interval record,            
*                                      completed reporting           */         
*   SMF119TI_ReasonIEndInc  CHAR(1) CONSTANT('60'X), /* Interval record         
*                                      ending statistics, incomplete            
*                                      recording                     */         
*   SMF119TI_ReasonIEnd CHAR(1) CONSTANT('20'X), /* Interval record             
*                                      ending statistics, completed             
*                                      recording                     */         
*   SMF119TI_ReasonIShtInc  CHAR(1) CONSTANT('50'X), /* Interval record         
*                                      shutdown statistics, incomplete          
*                                      recording                     */         
*   SMF119TI_ReasonISht CHAR(1) CONSTANT('10'X), /* Interval record             
*                                      shutdown statistics, completed           
*                                      recording                     */         
*   SMF119TI_ReasonEvtInc  CHAR(1) CONSTANT('48'X), /* Event record,            
*                                      incomplete reporting      @F1A*/         
*   SMF119TI_ReasonEvt  CHAR(1) CONSTANT('08'X), /* Event record,               
*                                      completed reporting       @F1C*/         
*   SMF119TI_ReasonStart  CHAR(1) CONSTANT('FF'X), /* Start value,              
*                                      used as interface value to the           
*                                      IF Stats, Port Stats, and                
*                                      SMC-R Group Stats record                 
*                                      builder routines     @V7C @69C*/         
*   SMF119TI_GarbCollect CHAR(1) CONSTANT('04'X); /* Internal request           
*                                      to perform zAGG early garbage            
*                                      collection. Never reported in            
*                                      SMF type 119 record.      @69C*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TCP Connection Initiation section (subtype 01)                   */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119AP_TI BASED,                                                         
*    3 SMF119AP_TIRName  CHAR(8),   /* TCP socket resource name      */         
*    3 SMF119AP_TIConnID CHAR(4),   /* TCP socket resource ID                   
*                                      (connection ID)          @A1C */         
*    3 SMF119AP_TIRSV1   CHAR(4),   /* Reserved                 @A1C */         
*    3 SMF119AP_TISubTask  FIXED(32), /* Subtask name-address of the */         
*                                   /* MVS TCB that owns this connect*/         
*    3 SMF119AP_TIRIP    CHAR(16),  /* Remote IP address (IPv6)      */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119AP_TIRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119AP_TIRIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119AP_TILIP    CHAR(16),  /* Local IP address (IPv6)       */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119AP_TILIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119AP_TILIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119AP_TIRPort  CHAR(2),   /* Remote port number            */         
*    3 SMF119AP_TILPort  CHAR(2),   /* Local port number             */         
*    3 SMF119AP_TITime   BIT(32),   /* Connection start time (UTC)   */         
*    3 SMF119AP_TIDate   FIXED(32), /* Connection start date         */         
*    3 SMF119AP_TISTCK   CHAR(8);   /* STCK of connection start  @Q3A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* TCP Connection Termination section (subtype 02)                  */         
*/*                                                                  */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119AP_TT BASED,                                                         
*    3 SMF119AP_TTRName  CHAR(8),   /* TCP socket resource name      */         
*    3 SMF119AP_TTConnID CHAR(4),   /* TCP socket resource ID                   
*                                      (connection ID)          @A1C */         
*    3 SMF119AP_TTTTLSCS FIXED(8),  /* TTLS Connection Status   @W1A */         
*    3 SMF119AP_TTTTLSPS FIXED(8),  /* TTLS Policy Status            */         
*    3 SMF119AP_TTTermCode FIXED(8),/* TCP connection term code @X2A */         
*    3 SMF119AP_TTSMCStatus Fixed(8), /* SMC-R status            @V7A*/         
*    3 SMF119AP_TTSubTask  FIXED(32), /* Subtask name-address of the */         
*                                   /* MVS TCB that owns this connect*/         
*    3 SMF119AP_TTSTime  BIT(32),   /* Connection start time (UTC)   */         
*    3 SMF119AP_TTSDate  FIXED(32), /* Connection start date         */         
*    3 SMF119AP_TTETime  BIT(32),   /* Connection end time (UTC)     */         
*    3 SMF119AP_TTEDate  FIXED(32), /* Connection end date           */         
*    3 SMF119AP_TTRIP    CHAR(16),  /* Remote IP address (IPv6)      */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119AP_TTRIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119AP_TTRIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119AP_TTLIP    CHAR(16),  /* Local IP address (IPv6)       */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119AP_TTLIPFmt  CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119AP_TTLIP_IPv4  CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119AP_TTRPort  CHAR(2),   /* Remote port number            */         
*    3 SMF119AP_TTLPort  CHAR(2),   /* Local port number             */         
*    3 SMF119AP_TTInBytes  BIT(64), /* Inbound byte count            */         
*    3 SMF119AP_TTOutBytes BIT(64), /* Outbound byte count           */         
*    3 SMF119AP_TTSWS    FIXED(32), /* Send window size at time of              
*                                      connection close              */         
*    3 SMF119AP_TTMSWS   FIXED(32), /* Max send window size at time             
*                                      of connection close           */         
*    3 SMF119AP_TTCWS    FIXED(32), /* Congestion window size at time           
*                                      of connection close           */         
*    3 SMF119AP_TTSMS    FIXED(32), /* Send segment size at time of             
*                                      connection close              */         
*    3 SMF119AP_TTRTT    FIXED(32), /* Round trip time at time of               
*                                      connection close (ms)         */         
*    3 SMF119AP_TTRVA    FIXED(32), /* RTT variance at time of                  
*                                      connection close (ms*ms)      */         
*    3 SMF119AP_TTStatus FIXED(8),  /* Status flags                  */         
*    3 SMF119AP_TTTOS    FIXED(8),  /* Type of service               */         
*    3 SMF119AP_TTXRT    FIXED(16), /* Number of retransmits         */         
*                                   /* This field has a maximum value*/         
*                                   /* of 65535. If the actual count */         
*                                   /* exceeds 65535, this value will*/         
*                                   /* not wrap. It will remain at   */         
*                                   /* 65535.Field SMF119AP_TTXRT32  */         
*                                   /* contains the actual count.@A9C*/         
*    3 SMF119AP_TTProf   CHAR(32),  /* Service profile name          */         
*    3 SMF119AP_TTPol    CHAR(32),  /* Service policy name at time of           
*                                      connection close              */         
*    3 SMF119AP_TTInSeg  BIT(64),   /* Inbound segment count         */         
*    3 SMF119AP_TTOutSeg BIT(64),   /* Outbound segment count        */         
*    3 SMF119AP_TTSSTCK  CHAR(8),   /* STCK of connection start  @Q3A*/         
*    3 SMF119AP_TTESTCK  CHAR(8),   /* STCK of connection end    @Q3A*/         
*    3 SMF119AP_TTDupAcksRcvd fixed(32), /* total dup acks recvd                
*                                                 on connection  @X2A*/         
*    3 SMF119AP_TTLclSMCLinkId  Fixed(32), /* Lcl SMC link ID    @V7A           
*                                                                @41C*/         
*    3 SMF119AP_TTRmtSMCLinkId  Fixed(32), /* Rmt SMC link ID    @V7A           
*                                                                @41C*/         
*    3 SMF119AP_TTSMCReason     Fixed(32), /* SMC-R reason       @V7A*/         
*    3 SMF119AP_TTSMCFlags      Bit(8),    /* SMC flags     @V7A @41C*/         
*      5 SMF119AP_TTSMCRsnRmt   Bit(1),    /* SMC-R rsn set by peer             
*                                                                @V7A*/         
*      5 SMF119AP_TTSMCCached   Bit(1),    /* This route cached not to          
*                                             use SMC-R          @33A*/         
*      5 SMF119AP_TTSMCDRSNRMT  Bit(1),    /* SMC-D rsn set by peer             
*                                                                @41A*/         
*      5 SMF119AP_TTSMCDCACHED  Bit(1),    /* This route cached not to          
*                                             use SMC-D          @41A*/         
*      5 *                      Bit(4),    /* Available     @V7A @41C*/         
*    3 SMF119AP_TTSMCDStatus  Fixed(8),    /* SMC-D status       @41A*/         
*    3 SMF119AP_TTSMCFlags2     Bit(8),    /* SMC flags          @64A*/         
*      5 SMF119AP_TTSMCVersion  Bit(4),    /* SMC version                       
*                                                      @V7A @41C @64C*/         
*      5 *                      Bit(4),    /* reserved           @64A*/         
*    3 SMF119AP_TTIPsecurityFlags    Bit(8), /* IP Security Status              
*                                                 @V7A @41C @47C @48C*/         
*      5 SMF119AP_TTIPsecurityEnbl   Bit(1), /* IPSECURITY                      
*                                               enabled          @48A*/         
*      5 SMF119AP_TTIPv6securityEnbl Bit(1), /* IPv6 IPSECURITY                 
*                                               enabled          @48A*/         
*      5 SMF119AP_TTIPFltrIncl       Bit(1), /* IP filtering done               
*                                               for connection   @48A*/         
*      5 *                           Bit(5), /* Available        @48A*/         
*    3 SMF119AP_TTLclSMCBufSz Fixed(32),   /* Lcl RMB/DMB buf size in           
*                                             KB            @33A @41C*/         
*    3 SMF119AP_TTRmtSMCBufSz Fixed(32),   /* Rmt RMB/DMB buf size in           
*                                             KB            @33A @41C*/         
