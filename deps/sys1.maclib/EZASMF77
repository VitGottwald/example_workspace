*  3 SMF119ML_IP_RcvdBytes Fixed(64),  /* Received bytes         @H2A*/         
*  3 SMF119ML_IP_SentBytes Fixed(64),  /* Sent bytes             @H2A*/         
*                                                                               
*  3 SMF119ML_IP_Flags      Bit(32),   /*                        @H2A*/         
*    5 SMF119ML_IP_ESMTP    Bit(1),    /* ESMTP supported        @H2A*/         
*    5  SMF119ML_IP_Rsv0x   Bit(7),    /* Reserved               @H2A*/         
*    5  SMF119ML_IP_Rsv1x   Bit(8),    /* Reserved               @H2A*/         
*    5  SMF119ML_IP_Rsv2x   Bit(8),    /* Reserved               @H2A*/         
*    5  SMF119ML_IP_Rsv3x   Bit(8),    /* Reserved               @H2A*/         
*                                                                               
* SMF119ML_IP              Isa(SMF119ML_IP_t) Based;          /* @H2A*/         
                                                                                
*Declare                            /* SMF119ML_IP_ConnState values  */         
*SMF119ML_IP_New        Constant(1),/* target server is new so   @H2A*/         
*                                   /* its capabilities are unknown but         
*                                      it is in the configured address          
*                                      list.                         */         
*SMF119ML_IP_Active     Constant(2),/* the target server is      @H2A*/         
*                                   /*  usable.                      */         
*SMF119ML_IP_Monitoring Constant(3),/* The target server is being@H2A*/         
*                                   /* monitored for a successful               
*                                      connection open and reply from           
*                                      the server - unusable server  */         
*SMF119ML_IP_NonActive  Constant(4);/* The target server is      @H2A*/         
*                                   /* unusable.                     */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Server Transfer Completion section (subtype 70)              */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FS BASED,                                                         
*    3 SMF119FT_FSOper   CHAR(1),   /* FTP operation subtype         */         
*    3 *                 CHAR(3),   /* Unused, available             */         
*    3 SMF119FT_FSCmd    CHAR(4),   /* FTP Command, per RFC959+      */         
*    3 SMF119FT_FSFType  CHAR(4),   /* File Type                     */         
*    3 SMF119FT_FSDRIP   CHAR(16),  /* Remote IP address (IPv6), data           
*                                      connection                    */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119FT_FSDRIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FSDRIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FSDLIP   CHAR(16),  /* Local IP address (IPv6), data            
*                                      connection                    */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119FT_FSDLIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FSDLIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FSDRPort CHAR(2),   /* Remote Port, data connection,            
*                                      client                        */         
*    3 SMF119FT_FSDLPort CHAR(2),   /* Local Port, data connection,             
*                                      server                        */         
*    3 SMF119FT_FSCRIP   CHAR(16),  /* Remote IP address (IPv6),                
*                                      control connection            */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119FT_FSCRIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FSCRIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FSCLIP   CHAR(16),  /* Local IP address (IPv6),                 
*                                      control connection            */         
*      5 *               CHAR(12),                                              
*        7 *             CHAR(10),                            /* @L8A*/         
*        7 SMF119FT_FSCLIPFmt CHAR(2), /* IPv4 address format    @L8A*/         
*      5 SMF119FT_FSCLIP_IPv4 CHAR(4), /* IPv4 piece of address      */         
*    3 SMF119FT_FSCRPort CHAR(2),   /* Remote Port, control connection,         
*                                      client                        */         
*    3 SMF119FT_FSCLPort CHAR(2),   /* Local Port, control connection,          
*                                      server                        */         
*    3 SMF119FT_FSSUser  CHAR(8),   /* Client User ID on server      */         
*    3 SMF119FT_FSType   CHAR(1),   /* Data Type                     */         
*    3 SMF119FT_FSMode   CHAR(1),   /* Transmission Mode             */         
*    3 SMF119FT_FSStruct CHAR(1),   /* Data Structure                */         
*    3 SMF119FT_FSDsType CHAR(1),   /* Data Set Type                 */         
*    3 SMF119FT_FSSTime  BIT(32),   /* FTP Transmission start                   
*                                      Time of Day (UTC time)        */         
*    3 SMF119FT_FSSDate  BIT(32),   /* FTP Transmission start date   */         
*    3 SMF119FT_FSETime  BIT(32),   /* FTP Transmission end                     
*                                      Time of Day (UTC time)        */         
*    3 SMF119FT_FSEDate  BIT(32),   /* FTP Transmission end date     */         
*    3 SMF119FT_FSDur    BIT(32),   /* FTP Transmission duration     */         
*    3 SMF119FT_FSBytes  BIT(64),   /* 64-bit transmission byte                 
*                                      count                     @L7C*/         
*    3 SMF119FT_FSLReply CHAR(4),   /* Last reply to client          */         
*    3 SMF119FT_FSM1     CHAR(8),   /* PDS Member name               */         
*    3 SMF119FT_FSRS     CHAR(8),   /* Reserved, abnormal end info   */         
*    3 SMF119FT_FSM2     CHAR(8),   /* Second PDS Member name, for              
*                                      rename operations             */         
*    3 SMF119FT_FSBytesFloat BIT(64), /* Transmission Byte Count                
*                                        floating point format   @X1C*/         
*    3 SMF119FT_FSCConnID    BIT(32), /* TCP connection ID - control            
*                                        connection              @X1A*/         
*    3 SMF119FT_FSDConnID    BIT(32), /* TCP connection ID - data               
*                                        connection              @X1A*/         
*    3 SMF119FT_FSSessionID  CHAR(15), /* activity logging session              
*                                         ID                     @X1A*/         
*    3 *                     CHAR(1);  /* available              @X1A*/         
*                                                                               
*/********************************************************************/         
*/* Constants for FTP Operation                                      */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSOperAPPE  CHAR(1) CONSTANT('01'X), /* Append          */         
*   SMF119FT_FSOperDELE  CHAR(1) CONSTANT('02'X), /* Delete          */         
*   SMF119FT_FSOperRENM  CHAR(1) CONSTANT('03'X), /* Rename          */         
*   SMF119FT_FSOperRETR  CHAR(1) CONSTANT('04'X), /* Retrieve        */         
*   SMF119FT_FSOperSTOR  CHAR(1) CONSTANT('05'X), /* Store           */         
*   SMF119FT_FSOperSTOU  CHAR(1) CONSTANT('06'X); /* Store Unique    */         
*                                                                               
*/********************************************************************/         
*/* Constants for File Type                                          */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSFTypeSEQ  CHAR(4) CONSTANT('SEQ '),                              
*   SMF119FT_FSFTypeJES  CHAR(4) CONSTANT('JES '),                              
*   SMF119FT_FSFTypeSQL  CHAR(4) CONSTANT('SQL ');                              
*                                                                               
*/********************************************************************/         
*/* Constants for Data Type                                          */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSTypeASCII   CHAR(1) CONSTANT('A'),                               
*   SMF119FT_FSTypeEBCDIC  CHAR(1) CONSTANT('E'),                               
*   SMF119FT_FSTypeImage   CHAR(1) CONSTANT('I'),                               
*   SMF119FT_FSTypeDByte   CHAR(1) CONSTANT('B'),                               
*   SMF119FT_FSTypeUCS2    CHAR(1) CONSTANT('U');                               
*                                                                               
*/********************************************************************/         
*/* Constants for Transmission Mode                                  */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSModeBlock   CHAR(1) CONSTANT('B'),                               
*   SMF119FT_FSModeCmpress CHAR(1) CONSTANT('C'),                               
*   SMF119FT_FSModeStream  CHAR(1) CONSTANT('S');                               
*                                                                               
*/********************************************************************/         
*/* Constants for Data Structure                                     */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSStructFile  CHAR(1) CONSTANT('F'),                               
*   SMF119FT_FSStructRec   CHAR(1) CONSTANT('R');                               
*                                                                               
*/********************************************************************/         
*/* Constants for Data Set Type                                      */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSDsTypeSEQ   CHAR(1) CONSTANT('S'),                               
*   SMF119FT_FSDsTypePDS   CHAR(1) CONSTANT('P'),                               
*   SMF119FT_FSDsTypeHFS   CHAR(1) CONSTANT('H');                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Server Transfer Complete Hostname section (subtype 70).      */         
*/* FTP Server Transfer Initialization Hostname-section (subtype 100)*/         
*/* This section is optional and present if gethostbyaddr performed. */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FSHN BASED,                                                       
*    3 SMF119FT_FSHostname CHAR(*); /* Hostname associated with                 
*                                      this FTP transfer             */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Server Transfer Complete First Associated Data Set Name      */         
*/* section (subtype 70).                                            */         
*/* FTP Server Transfer Complete First Associated Data Set Name      */         
*/* section (subtype 100).                                           */         
*/* This section represents an MVS or HFS dataset name associated    */         
*/* the file transfer. The type of filename contained is found in    */         
*/* SMF119FT_FSDsType.                                               */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FSA1 BASED,                                                       
*    3 SMF119FT_FSFileName1  CHAR(*); /* First filename associated              
*                                      with this FTP transfer        */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Server Transfer Complete Second Associated Data Set Name     */         
*/* section (subtype 70).                                            */         
*/* FTP Server Transfer Initialization Second Associated Data Set    */         
*/* Name section (subtype 100).                                      */         
*/* This section represents an MVS or HFS  dataset name associated   */         
*/* the file transfer, and is only present for Rename operations.    */         
*/* The type of filename contained in this  section is found in      */         
*/* SMF119FT_FSDsType.                                               */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FSA2 BASED,                                                       
*    3 SMF119FT_FSFileName2  CHAR(*); /* Second filename associated             
*                                      with this FTP transfer        */         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Server Transfer Complete Security information subsection     */         
*/* (subtype 70).                                                    */         
*/* FTP Server Transfer Initialization Security Information section  */         
*/* (subtype 100).                                                   */         
*/* FTP Server Session Security Information section                  */         
*/* (subtype 104).                                                   */         
*/* This section reports security information for an FTP file        */         
*/* transfer.                                                        */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FSSC BASED,                                      /*@Q1A*/         
*    3 SMF119FT_FSMechanism  CHAR(1), /* Protection mechanism    @Q1A*/         
*    3 SMF119FT_FSCProtect   CHAR(1), /* Control connection protection          
*                                        level                   @Q1A*/         
*    3 SMF119FT_FSDProtect   CHAR(1), /* Data connection protection             
*                                        level                   @Q1A*/         
*    3 SMF119FT_FSLoginMech  CHAR(1), /* Login method            @Q1A*/         
*    3 SMF119FT_FSProtoLevel CHAR(8), /* Protocol level          @Q1A*/         
*    3 SMF119FT_FSCipherSpec CHAR(20),/* Cipher suite            @Q1A*/         
*    3 SMF119FT_FSProtBuffSize BIT(32),  /* Negotiated protection               
*                                           buffer size          @Q1A*/         
*    3 SMF119FT_FSCipher     CHAR(2), /* Cipher value            @Y5A*/         
*    3 SMF119FT_FSFips140    FIXED(8), /* FIPS 140 compliance    @F5A*/         
*    3 SMF119FT_FSCipher4    CHAR(4), /* 4 byte cipher value@V1A @34C*/         
*    3 SMF119FT_FSSessReuse  CHAR(1), /* SSL session reuse flag  @34A*/         
*    3 SMF119FT_FSCSSLSessIDLen FIXED(16), /* Ctrl conn SSL session             
*                                             ID length          @34A*/         
*    3 SMF119FT_FSCSSLSessID CHAR(32),/* Ctrl conn SSL session ID@34A*/         
*    3 SMF119FT_FSDSSLSessIDLen FIXED(16), /* Data conn SSL session             
*                                             ID length          @34A*/         
*    3 SMF119FT_FSDSSLSessID CHAR(32),/* Data conn SSL session                  
*                                        ID                 @34A @5CC*/         
*    3 SMF119FT_FSKeyShare   CHAR(4); /* 4 byte key share   @5BA @5CM*/         
*                                                                               
*/********************************************************************/         
*/* Constants for SSL session reuse flag                             */         
*/********************************************************************/         
* Declare                                                                       
*   SMF119FT_FSSessReuseAllow CHAR(1)  CONSTANT('A'), /* Allowed @34A*/         
*   SMF119FT_FSSessReuseReq   CHAR(1)  CONSTANT('R'); /* Required@34A*/         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Server Transfer Complete load module name section            */         
*/* (subtype 70)                                                     */         
*/* This section reports the load module or program object library   */         
*/* name and member name associated with this transfer. It's optional*/         
*/* and presented only for load module transfer.                     */         
*/********************************************************************/         
*/* The length of this section is variable.  Its length is indicated */         
*/* by the subsection length in the self-defining section.           */         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119FT_FSLM BASED,                                                       
*    3 SMF119FT_FSMemNum FIXED(32), /* Member num in this record @45C*/         
*    3 SMF119FT_FSLibNameLen FIXED(8), /* Library name length    @45C*/         
*    3 SMF119FT_FSLibName CHAR(44), /* Library name              @45C*/         
*    3 SMF119FT_FSMemName CHAR(8);  /* Name of member #1         @44A*/         
*/* Additional SMF119FT_FSMemName member name fields follow          */         
*/* the first name field, for a count of SMF119FT_FSMemNum - 1.      */         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* FTP Daemon Configuration (subtype 71) section 1                  */         
*/* This section reports the identification information of FTPD      */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* DCL                                                                           
*  1 SMF119FT_FD BASED,                                       /* @V8A*/         
*    3 SMF119FT_FDIdent      Char(4),   /* Ident of Section 1    @VAA*/         
*    3 SMF119FT_FDJobName    CHAR(8),   /* Jobname of the FTPD                  
*                                                                @V8A*/         
*    3 SMF119FT_FDASID       FIXED(16), /* ASID of the FTPD level               
*                                                                @V8A*/         
*    3 SMF119FT_FDrsvd1      FIXED(16), /* Unused, available     @V8A*/         
*    3 SMF119FT_FDStartTime  BIT(32),   /* Start time of the FTPD               
*                                                                @V8A*/         
*    3 SMF119FT_FDStartDate  FIXED(32), /* Start date of the FTPD               
*                                                                @V8A*/         
*    3 SMF119FT_FDUserID     CHAR(8),   /* User ID that started FTPD            
*                                                                @V8A*/         
*    3 SMF119FT_FDUToken     CHAR(80);  /* User token of the user ID            
*                                            started the FTPD    @V8A*/         
*                                                                               
*/********************************************************************/         
