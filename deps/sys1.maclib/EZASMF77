*  3 SMF119ML_SJ_PNAM     Char(20), /* Programmer name from Job  @H2A*/         
*  3 SMF119ML_SJ_NOTN     Char(8),  /* Job Notify node           @H2A*/         
*  3 SMF119ML_SJ_NOTU     Char(8),  /* Job notify userid         @H2A*/         
*  3 SMF119ML_SJ_CLAR     Char(1),  /* Sysout class of data set  @H2A*/         
*  3 SMF119ML_SJ_LSAB     Fixed(24),/* Last ABEND code           @H2A*/         
*  3 SMF119ML_SJ_DSN      Char(44), /* Data set name             @H2A*/         
*  3 SMF119ML_SJ_NACT     Char(8),  /* Network accounting number @H2A*/         
*  3 SMF119ML_SJ_UserExit Fixed(31),/* User Exit version         @H2A*/         
*  3 SMF119ML_SJ_QTime    Fixed(31),/* Time spool was queued to  @H2A*/         
