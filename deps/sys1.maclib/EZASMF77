*SMF119ML_CS_TRM_CONNERR Constant(08), /* Connect failed         @H2A*/         
*SMF119ML_CS_TRM_SECURE  Constant(09), /* StartTLS command failed@H2A*/         
*SMF119ML_CS_TRM_MAXMSG  Constant(10), /* Maximum number of      @H2A*/         
*                                      /*   messages                 */         
*SMF119ML_CS_TRM_CONNECT Constant(11), /* Connection wait timeout@H2A*/         
*SMF119ML_CS_TRM_INITMSG Constant(12), /* Initial message timeout@H2A*/         
*SMF119ML_CS_TRM_13      Constant(13), /* Reserved               @H2A*/         
*SMF119ML_CS_TRM_MAILCMD Constant(14), /* Mail command time out  @H2A*/         
*SMF119ML_CS_TRM_RCPTCMD Constant(15), /* RCPT command time out  @H2A*/         
*SMF119ML_CS_TRM_DATACMD Constant(16), /* DATA command time out  @H2A*/         
*SMF119ML_CS_TRM_DATABUF Constant(17), /* Data buffer time out   @H2A*/         
*SMF119ML_CS_TRM_DATATRM Constant(18), /* End of message time out@H2A*/         
*SMF119ML_CS_TRM_ANYCMD  Constant(19); /* Any command timeout    @H2A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* SMF119ML_SI     CSSMTP Spool Identification Section              */         
*/*                 Subtype (0x32, 0x33) Section 2                   */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
* Declare                                                                       
* 1 SMF119ML_SI_t         Type,     /* Spool Identification      @H2A*/         
*   3 SMF119ML_SI_JMR,              /* Job Management Record     @H2A*/         
*                                   /*  format in "Standard SMF      */         
*                                   /*  Record Header" in "MVS       */         
*                                   /*  System Management Facilities */         
*                                   /*  (SMF)" for a detail          */         
*                                   /*  description.                 */         
*     5 SMF119ML_SI_Job   Char(8),  /* Jobname                   @H2A*/         
*     5 SMF119ML_SI_Entry Fixed(32),/* Reader Entry Time         @H2A*/         
*     5 SMF119ML_SI_EDate Char(4),  /* Reader Entry Date 0CYYDDDF@H2A*/         
*     5 SMF119ML_SI_USEID Char(8),  /* User-defined              @H2A*/         
*                                   /* identification field (taken   */         
*                                   /* from common exit parameter    */         
*                                   /* area, not from USER=parameter */         
*                                   /* on Job statement).            */         
*                                                                               
*   3 SMF119ML_SI_JobId   Char(8),  /* Job Id of selected Job    @H2A*/         
*   3 SMF119ML_SI_SYS     Char(8),  /* System name of the MVS    @H2A*/         
*                                   /*  image where the Job output   */         
*                                   /*  was created                  */         
*   3 SMF119ML_SI_XEQ     Char(8),  /* Node where Job executed   @H2A*/         
*   3 SMF119ML_SI_CRER    Char(8),  /* Owning userid of data set @H2A*/         
*   3 SMF119ML_SI_TKID    Fixed,    /* JES task id               @H2A*/         
*   3 SMF119ML_SI_Jnum    Fixed(32),/* JES Jobnumber in binary   @H2A*/         
*   3 SMF119ML_SI_Dsky    Fixed(32),/* JES Dataset key (binary)  @H2A*/         
*   3 SMF119ML_SI_Dsnm    Fixed(32),/* JES Dataset number        @H2A*/         
*                                                                               
*   SMF119ML_SI           Isa(SMF119ML_SI_t) Based;           /* @H2A*/         
*                                                                               
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* SMF119ML_MI     CSSMTP Mail Identification section               */         
*/*                 Subtype (0x32) Section 3                         */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
*Declare                                                                        
*1 SMF119ML_MI_t            Type,      /* Mail Identification    @H2A*/         
*  3 SMF119ML_MI_STime     Fixed(31),  /* Time mail was read from@H2A*/         
*                                      /*  JES -Hundredths of seconds*/         
*                                      /*  since midnight            */         
*  3 SMF119ML_MI_SDate     Char(4),    /* Date mail was read from@H2A*/         
*                                      /*  JES - 0CYYDDDF            */         
*  3 SMF119ML_MI_ETime     Fixed(31),  /* Time mail was completed@H2A*/         
*                                      /*  Hundredths of seconds     */         
*                                      /*  since midnight            */         
*  3 SMF119ML_MI_EDate     Char(4),    /* Date mail was completed@H2A*/         
*                                      /*  0CYYDDDF                  */         
*  3 SMF119ML_MI_Dur       Fixed(31),  /* Mail was in progress   @H2A*/         
*                                      /*  Hundredths of seconds     */         
*  3 SMF119ML_MI_ID        Fixed,      /* Mail Id                @H2A*/         
*  3 SMF119ML_MI_Type      Fixed,      /* Type of mail message   @H2A*/         
*  3 SMF119ML_MI_Rsvd1     Fixed,      /* Reserved               @H2A*/         
*  3 SMF119ML_MI_BYCT      Fixed(64),  /* Body byte count        @H2A*/         
*  3 SMF119ML_MI_RLoc      Fixed,      /* Record location of     @H2A*/         
*                                      /*  MAIL command in spool file*/         
*  3 SMF119ML_MI_Rcpts     Fixed,      /* Number of total RCPTs  @H2A*/         
*  3 SMF119ML_MI_FRcpts    Fixed,      /* Number of failed RCPTs @H2A*/         
*  3 SMF119ML_MI_Retry     Fixed,      /* Number of retry attempt@H2A*/         
*  3 SMF119ML_MI_Flags     Bit(32),    /* Flags                  @H2A*/         
*    5 SMF119ML_MI_ESMTP   Bit(1),     /* EHLO(RFC 2821) command @H2A*/         
*    5 SMF119ML_MI_TLS     Bit(1),     /* STARTTLS       command @H2A*/         
*    5 SMF119ML_MI_Finis   Bit(1),     /* Mail was completed     @H2A*/         
*    5 SMF119ML_MI_Error   Bit(1),     /* An connection error    @H2A*/         
*                                      /*  occurred                  */         
*    5 SMF119ML_MI_ERetry  Bit(1),     /* Mail copied for        @M1C*/         
*                                      /*  extended retry            */         
*    5 SMF119ML_MI_Rsv05   Bit(1),     /* Reserved               @H2A*/         
*    5 SMF119ML_MI_Rsv06   Bit(1),     /* Reserved               @H2A*/         
*    5 SMF119ML_MI_MHFul   Bit(1),     /* The SMF record is full @H2A*/         
*                                      /*  Data in the MH section    */         
*                                      /*   was truncated            */         
*    5 SMF119ML_MI_From    Bit(1),     /* mail contains a From:  @H2A*/         
*    5 SMF119ML_MI_To      Bit(1),     /* mail contains a To:    @H2A*/         
*    5 SMF119ML_MI_Date    Bit(1),     /* mail contains a Date:  @H2A*/         
*    5 SMF119ML_MI_MsgID   Bit(1),     /* mail contains a msg-id @H2A*/         
*    5 SMF119ML_MI_Subj    Bit(1),     /* mail contains a subject@H2A*/         
*    5 SMF119ML_MI_Rsv15   Bit(1),     /* Reserved               @H2A*/         
*    5 SMF119ML_MI_Rsv16   Bit(1),     /* Reserved               @H2A*/         
*    5 SMF119ML_MI_Rsv17   Bit(1),     /* Reserved               @H2A*/         
*                                                                               
*    5 SMF119ML_MI_Rsv2x   Bit(8),     /* Reserved               @H2A*/         
*    5 SMF119ML_MI_Rsv3x   Bit(8),     /* Reserved               @H2A*/         
*                                                                               
*                                                                               
*SMF119ML_MI               Isa(SMF119ML_MI_t) Based;          /* @H2A*/         
*                                                                               
*Declare                                                                        
*SMF119ML_MI_TYPE_REG     Constant(1), /* A regular mail message @H2A*/         
*SMF119ML_MI_TYPE_UNDEL   Constant(2), /* A undeliverable message@H2A*/         
*SMF119ML_MI_TYPE_REPORT  Constant(3), /* An error report message@H2A*/         
*SMF119ML_MI_TYPE_UMNOTIF Constant(4); /* an Undeliverable notify@H2A*/         
*                                                                               
*                                                                               
* /*******************************************************************/         
* /*                                                                 */         
* /* SMF119ML_MH     CSSMTP Mail Header section                      */         
* /*                 Subtype (0x32) Section 4                        */         
* /*                                                                 */         
* /*******************************************************************/         
*                                                                               
*Declare                                                                        
*1 SMF119ML_MH_t            Type,      /* Mail Header            @H2A*/         
*  3 SMF119ML_MH_Len        Fixed(15), /* Mail header length     @H2A*/         
*  3 SMF119ML_MH_Key        Fixed(15), /* Mail header type value @H2A*/         
*  3 SMF119ML_MH_Data       Char(*),   /* Mail header data string@H2A*/         
*                                                                               
*SMF119ML_MH                Isa(SMF119ML_MH_t) Based;         /* @H2A*/         
*                                                                               
*Declare                                                                        
*SMF119ML_MH_FROM      Constant( 1),   /* Mail From:<mailbox>    @H2A*/         
*SMF119ML_MH_RCPT      Constant( 2),   /* RCPT TO:<mailbox>      @H2A*/         
*SMF119ML_MH_RCPTRPY   Constant( 3),   /* Reply to RCPT TO:      @H2A*/         
*SMF119ML_MH_SUBJ      Constant( 4),   /* Subject: subject       @H2A*/         
*SMF119ML_MH_DATE      Constant( 5),   /* Date: date             @H2A*/         
*                                                                               
*SMF119ML_MH_MSGID     Constant( 7),   /* Message-id:            @H2A*/         
*SMF119ML_MH_CMDTXT    Constant( 8),   /* Text of cmd in error   @H2A*/         
*SMF119ML_MH_RPYTXT    Constant( 9),   /* Reply to command in err@H2A*/         
*SMF119ML_MH_ERRTXT    Constant(10);   /* Text of error message  @H2A*/         
*                                                                               
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* SMF119ML_SS     CSSMTP Spool Job data section                    */         
