*                                      Certificate digest                       
*                                      algorithm                 @51A*/         
*      5 SMF119SS_SSH_CCert_Key_Type Fixed(16), /* Client Certificate           
*                                      Key type                  @51A*/         
*      5 SMF119SS_SSH_CCert_Key_Len Fixed(16), /* Client Certificate            
*                                      Key length (in bits)      @51A*/         
*    3 SMF119SS_SSH_End    Char(0); /* End of section            @51A*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/* zERT (subtype 12) IPSec section                                  */         
*/*                                                              @51A*/         
*/********************************************************************/         
* DCL                                                                           
*  1 SMF119SS_IPSec_data Based,     /*                           @51A*/         
*   2 SMF119SS_IPSec_IKEPhase1Info, /* IKE phase 1 tunnel data   @51A*/         
*    3 SMF119SS_IPSec_IKEMajVer Fixed(8), /* Major version of IKE               
*                                      protocol in use           @51A*/         
*    3 SMF119SS_IPSec_IKEMinVer Fixed(8), /* Minor version of IKE               
*                                      protocol in use           @51A*/         
*    3 SMF119SS_IPSec_IKETunLclEndpt Union, /* Local IP address of              
*                                      tunnel endpoint           @51A*/         
*      5 SMF119SS_IPSec_IKETunLclEP_IPv4 Char(4),   /* IPv4      @51A*/         
*      5 SMF119SS_IPSec_IKETunLclEP_IPv6 Char(16),  /* IPv6      @51A*/         
*    3 SMF119SS_IPSec_IKETunRmtEndpt Union, /* Remote IP address of             
*                                      tunnel endpoint           @51A*/         
*      5 SMF119SS_IPSec_IKETunRmtEP_IPv4 Char(4),   /* IPv4      @51A*/         
*      5 SMF119SS_IPSec_IKETunRmtEP_IPv6 Char(16),  /* IPv6      @51A*/         
*    3 SMF119SS_IPSec_IKETunLclAuthMeth  Fixed(16), /* Authentication           
*                                      method for the local endpoint            
*                                                                @51A*/         
*    3 SMF119SS_IPSec_IKETunRmtAuthMeth  Fixed(16), /* Authentication           
*                                      method for the remote endpoint           
*                                                                @51A*/         
*    3 SMF119SS_IPSec_IKETunAuthAlg Fixed(16),/* Tunnel authentication          
*                                      algorithm                 @51A*/         
*    3 SMF119SS_IPSec_IKETunEncAlg  Fixed(16), /* Tunnel encryption             
*                                      algorithm                 @51A*/         
*    3 SMF119SS_IPSec_IKETunDHGroup Fixed(16), /* Diffie-Hellman group          
*                                      used to generate the                     
*                                      keying material for this                 
*                                      IKE tunnel                @51A*/         
*    3 SMF119SS_IPSec_IKETunPseudoRandFunc Fixed(16), /* Pseudo-random          
*                                      function used for seeding                
*                                      keying material           @51A*/         
*    3 SMF119SS_IPSec_LclCertInfo,  /* IKE local certificate                    
*                                      information (will be set if              
*                                      SMF119SS_IPSec_IKETunLclAuthMeth         
*                                      is not set to "preshared key",           
*                                      otherwise, all fields set                
*                                      to zero)                  @51A*/         
*      5 SMF119SS_IPSec_LclCert_Sign_Meth Fixed(16), /* Local IKE               
*                                      certificate signature method             
*                                                                @51A*/         
*      5 SMF119SS_IPSec_LclCert_Enc_Meth Fixed(16), /* Local IKE                
*                                      certificate encryption                   
*                                      method                    @51A*/         
*      5 SMF119SS_IPSec_LclCert_Digest_Alg Fixed(16), /* Local IKE              
*                                      certificate digest algorithm             
*                                                                @51A*/         
*      5 SMF119SS_IPSec_LclCert_Key_Type Fixed(16), /* Local IKE                
*                                      certificate key type      @51A*/         
*      5 SMF119SS_IPSec_LclCert_Key_Len Fixed(16), /* Local IKE                 
*                                      certificate key length                   
*                                      (in bits)                 @51A*/         
*    3 SMF119SS_IPSec_RmtCertInfo,  /* IKE remote certificate                   
*                                      information (will be set if              
*                                      SMF119SS_IPSec_IKETunRmtAuthMeth         
*                                      is not set to "preshared key",           
*                                      otherwise, all fields set                
*                                      to zero)                  @51A*/         
*      5 SMF119SS_IPSec_RmtCert_Sign_Meth Fixed(16), /* Remote IKE              
*                                      certificate signature method             
*                                                                @51A*/         
*      5 SMF119SS_IPSec_RmtCert_Enc_Meth Fixed(16), /* Remote IKE               
*                                      certificate encryption                   
*                                      method                    @51A*/         
*      5 SMF119SS_IPSec_RmtCert_Digest_Alg Fixed(16), /* Remote IKE             
*                                      certificate digest algorithm             
*                                                                @51A*/         
*      5 SMF119SS_IPSec_RmtCert_Key_Type Fixed(16), /* Remote IKE               
*                                      certificate key type      @51A*/         
*      5 SMF119SS_IPSec_RmtCert_Key_Len Fixed(16), /* Remote IKE                
*                                      certificate key length                   
*                                      (in bits)                 @51A*/         
*   2 SMF119SS_IPSec_IKEPhase2Info, /* IKE phase 2 tunnel data   @51A*/         
*    3 SMF119SS_IPSec_PFSGroup Fixed(16), /* Diffie-Hellman group               
*                                      used for perfect forward                 
*                                      secrecy                   @51A*/         
*    3 SMF119SS_IPSec_EncapMode Fixed(8), /* Tunnel encapsulation               
*                                      mode                      @51A*/         
*    3 SMF119SS_IPSec_AuthProto Fixed(8), /* Protocol used for                  
*                                      message authentication    @51A*/         
*    3 SMF119SS_IPSec_AuthAlg Fixed(16), /* Authentication algorithm            
*                                                                @51A*/         
*    3 SMF119SS_IPSec_EncAlg Fixed(16), /* Encryption algorithm  @51A*/         
*    3 SMF119SS_IPSec_End  Char(0); /* End of section            @51A*/         
*                                                                               
*/********************************************************************/         
