                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00638900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00639000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00639100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00639200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00639300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00639400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00639500
&ZOPN                 SETA  &ZOPN+1                                     00639600
&ZOPL(&ZOPN)          SETC  ' '                                         00639700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00639800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00639900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00640000
                      AGO   .ZIMFNX                                     00640100
.*                                                                      00640200
.ZIMFN2               ANOP ,                                            00640300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00640400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00640500
&ZOPN                 SETA  &ZOPN+1                                     00640600
&ZOPL(&ZOPN)          SETC  ' '                                         00640700
