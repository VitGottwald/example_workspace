.********************************************************************   00237000
.***AC.PLV01  PROCESS PLISTVER                                      *   00237100
.********************************************************************   00237200
&XPLISTVER      SETA  1                                                 00237300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00237400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00237500
&XPLISTVER      SETA  2                                                 00237600
.ZPLV0          ANOP  ,                                                 00237700
.*                                                                      00237800
                AIF   ('&USERDATAMATCH' EQ '').ZPLV1E                   00237900
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00238000
&XPLISTVER      SETA  2                                                 00238100
.ZPLV1E         ANOP  ,                                                 00238200
.*                                                                      00238300
                AIF   ('&USERDATA' EQ '').ZPLV2E                        00238400
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00238500
&XPLISTVER      SETA  2                                                 00238600
.ZPLV2E         ANOP  ,                                                 00238700
.*                                                                      00238800
                AIF   ('&USERDATA' EQ '').ZPLV3E                        00238900
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00239000
&XPLISTVER      SETA  2                                                 00239100
.ZPLV3E         ANOP  ,                                                 00239200
.*                                                                      00239300
                AIF   ('&USERDATALEN' EQ '').ZPLV4E                     00239400
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00239500
&XPLISTVER      SETA  2                                                 00239600
.ZPLV4E         ANOP  ,                                                 00239700
.*                                                                      00239800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00239900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00240000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00240100
.*-------------------------------------------------------------------   00240200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00240300
&ZMS            SETC  '"PLISTVER='                                      00240400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00240500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00240600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00240700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00240800
                AGO   .ZPVCK2                                           00240900
.ZPVCK3         ANOP  ,                                                 00241000
.*-------------------------------------------------------------------   00241100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00241200
&ZMS            SETC  '"PLISTVER='                                      00241300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00241400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00241500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00241600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00241700
                AGO   .ZPVCKE                                           00241800
.ZPVCK2         ANOP  ,                                                 00241900
.*-------------------------------------------------------------------   00242000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00242100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00242200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00242300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00242400
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00242500
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00242600
.ZPVCK6         ANOP  ,                                                 00242700
&ZMS            SETC  '"PLISTVER='                                      00242800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00242900
