                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00243100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00243200
&XPLISTVER      SETA  2                                                 00243300
                AGO   .ZPVCKE                                           00243400
.*-------------------------------------------------------------------   00243500
.ZPVCK4A        ANOP  ,                                                 00243600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00243700
&ZMS            SETC  'PLISTVER='                                       00243800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00243900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00244000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00244100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00244200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00244300
                AGO   .ZPVCK5                                           00244400
