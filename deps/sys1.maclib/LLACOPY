.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00132100
.*******************************************************************    00132200
                      ACTR  10000                                       00132300
.*                                                                      00132400
                      LCLA  &ZLC           COLUMN POSITION              00132500
                      LCLA  &ZOPX          LIST TABLE INDEX             00132600
                      LCLC  &ZLL           LABEL                        00132700
                      LCLC  &ZLOP          OPERATION                    00132800
                      LCLC  &ZLV           VALUE                        00132900
                      LCLC  &ZLD           DESCRIPTION                  00133000
.*                                                                      00133100
&ZOPX                 SETA  1                                           00133200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00133300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00133400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00133500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00133600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00133700
                      AIF   (K'&ZLD LE 0).ZL5                           00133800
&ZLC                  SETA  35             COMMENT COLUMN               00133900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00134000
&ZLC                  SETA  33             COMMMENT COLUMN              00134100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00134200
&ZLV                  SETC  '&ZLV'.' '                                  00134300
                      AGO   .ZL3                                        00134400
.*                                                                      00134500
.ZL4                  ANOP  ,                                           00134600
&ZLV                  SETC  '&ZLV'.' '                                  00134700
.ZL5                  ANOP  ,                                           00134800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00134900
.*                                                                      00135000
&ZLL     &ZLOP &ZLV                                                     00135100
.*                                                                      00135200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00135300
                      AGO   .ZL2           CONTINUE LOOPING             00135400
.*                                                                      00135500
.ZLX                  ANOP  ,                                           00135600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00135700
