.***AC.PLV01  PROCESS PLISTVER                                      *   00043000
.********************************************************************   00043100
&XPLISTVER      SETA  0                                                 00043200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00043300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00043400
&XPLISTVER      SETA  0                                                 00043500
.ZPLV0          ANOP  ,                                                 00043600
.*                                                                      00043700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00043800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00043900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00044000
.*-------------------------------------------------------------------   00044100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00044200
&ZMS            SETC  '"PLISTVER='                                      00044300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00044400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00044500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00044600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00044700
                AGO   .ZPVCK2                                           00044800
.ZPVCK3         ANOP  ,                                                 00044900
.*-------------------------------------------------------------------   00045000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00045100
&ZMS            SETC  '"PLISTVER='                                      00045200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00045300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00045400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00045500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00045600
                AGO   .ZPVCKE                                           00045700
.ZPVCK2         ANOP  ,                                                 00045800
.*-------------------------------------------------------------------   00045900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00046000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00046100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00046200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00046300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00046400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00046500
