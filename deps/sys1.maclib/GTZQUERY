.***AG.KCK02 NAME, OPT INPUT, KEYS -GPR OUT OF RANGE                    00217000
                    AIF   (K'&ZXOWNER LE 0).ZG9R  NOT I/O               00217100
         AIF   ('&XOWNER' EQ '*').ZG9R                                  00217200
                    AIF   ('&XOWNER'(1,1) NE '(').ZG9R                  00217300
