                    AIF   ('&ZCGPR' LT '2').ZG9Q                        00218200
                    AIF   ('&ZCGPR' LE '12').ZG9R                       00218300
.ZG9Q               ANOP  ,                                             00218400
&ZMS                SETC  '"OWNER='                                     00218500
