.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00142600
.********************************************************************   00142700
                    GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL           00142800
                    GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE    00142900
                    GBLA  &SYSALVL    SYSSTATE ARCHLVL                  00143000
                    GBLC  &SYSAM64    SYSSTATE AMODE64                  00143100
.*                                                                      00143200
                    AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED    00143300
                    AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED     00143400
                    SPLEVEL  TEST                                       00143500
.ZSYS2              ANOP  ,                                             00143600
                    AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1         00143700
                    AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1         00143800
                    SYSSTATE TEST                                       00143900
                    AGO   .ZSYS4                                        00144000
.ZSYS3              ANOP  ,                                             00144100
&SYSASCE            SETC  'P'         ASSUME SYSSTATE PRIMARY           00144200
.ZSYS4              ANOP  ,                                             00144300
                    AIF   ('&SYSAM64' NE 'YES').ZSYSN64                 00144400
&ZMAC_LLGT          SETC 'LLGT'                                         00144500
&ZMAC_LGR           SETC 'LGR'                                          00144600
&ZMAC_LG            SETC 'LG'                                           00144700
&ZMAC_STG           SETC 'STG'                                          00144800
.ZSYSN64            ANOP  ,                                             00144900
&ZMAC_G             SETC 'G'                                            00145000
&ZMAC_LR            SETC 'LGR'                                          00145100
&ZMAC_L             SETC 'LG'                                           00145200
&ZMAC_ST            SETC 'STG'                                          00145300
.*******************************************************************    00145400
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00145500
.*******************************************************************    00145600
                    LCLC  &XREQUEST   ++ VALUE                          00145700
                    LCLC  &XOWNER     ++ NAME                           00145800
                    LCLC  &XSOURCETYPE          ++ VALUE                00145900
                    LCLC  &XSOURCE    ++ NAME                           00146000
                    LCLC  &XSOURCEPATHLEN       ++ NAME                 00146100
                    LCLC  &XSOURCEPATH          ++ NAME                 00146200
                    LCLC  &XEVENTDESCLEN        ++ NAME                 00146300
                    LCLC  &XEVENTDESC           ++ NAME                 00146400
                    LCLC  &XEVENTDATA           ++ NAME                 00146500
                    LCLC  &XEVENTASID           ++ NAME                 00146600
                    LCLC  &XEVENTJOB  ++ NAME                           00146700
                    LCLC  &XPROGRAMTYPE         ++ VALUE                00146800
                    LCLC  &XPROGRAM   ++ NAME                           00146900
                    LCLC  &XPROGRAMPATHLEN      ++ NAME                 00147000
                    LCLC  &XPROGRAMPATH         ++ NAME                 00147100
                    LCLC  &XPROGRAMOFFSET       ++ NAME                 00147200
                    LCLC  &XHOMEASID  ++ NAME                           00147300
                    LCLC  &XHOMEJOB   ++ NAME                           00147400
                    LCLC  &XANSAREA   ++ NAME                           00147500
                    LCLC  &XANSLEN    ++ NAME                           00147600
                    LCLC  &XSECCHECK  ++ VALUE                          00147700
                    LCLC  &XRETCODE   ++ NAME                           00147800
                    LCLC  &XRSNCODE   ++ NAME                           00147900
                    LCLA  &XPLISTVER  ++ VALUE                          00148000
                    LCLC  &XMF        ++ VALUE                          00148100
                    LCLC  &XMFCTRL    ++ NAME                           00148200
                    LCLC  &XMFATTR    ++ VALUE                          00148300
                    LCLC  &XMFSCK     ++ VALUE                          00148400
.*******************************************************************    00148500
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00148600
.*******************************************************************    00148700
                    LCLC  &ZXREQUEST  ++ INPUT VALUE                    00148800
                    LCLC  &ZXOWNER    ++ INPUT NAME                     00148900
                    LCLC  &ZXSOURCETYPE         ++ INPUT VALUE          00149000
                    LCLC  &ZXSOURCE   ++ INPUT NAME                     00149100
                    LCLC  &ZXSOURCEPATHLEN      ++ INPUT NAME           00149200
                    LCLC  &ZXSOURCEPATH         ++ INPUT NAME           00149300
                    LCLC  &ZXEVENTDESCLEN       ++ INPUT NAME           00149400
                    LCLC  &ZXEVENTDESC          ++ INPUT NAME           00149500
                    LCLC  &ZXEVENTDATA          ++ INPUT NAME           00149600
                    LCLC  &ZXEVENTASID          ++ INPUT NAME           00149700
                    LCLC  &ZXEVENTJOB           ++ INPUT NAME           00149800
                    LCLC  &ZXPROGRAMTYPE        ++ INPUT VALUE          00149900
                    LCLC  &ZXPROGRAM  ++ INPUT NAME                     00150000
                    LCLC  &ZXPROGRAMPATHLEN     ++ INPUT NAME           00150100
                    LCLC  &ZXPROGRAMPATH        ++ INPUT NAME           00150200
                    LCLC  &ZXPROGRAMOFFSET      ++ INPUT NAME           00150300
                    LCLC  &ZXHOMEASID           ++ INPUT NAME           00150400
                    LCLC  &ZXHOMEJOB  ++ INPUT NAME                     00150500
                    LCLC  &ZXANSAREA  ++ OUTPUT NAME                    00150600
                    LCLC  &ZXANSLEN   ++ INPUT NAME                     00150700
                    LCLC  &ZXSECCHECK           ++ INPUT VALUE          00150800
                    LCLC  &ZXRETCODE  ++ OUTPUT NAME                    00150900
                    LCLC  &ZXRSNCODE  ++ OUTPUT NAME                    00151000
                    LCLC  &ZXPLISTVER           ++ INPUT VALUE          00151100
                    LCLC  &ZXMF       ++ INPUT VALUE                    00151200
                    LCLC  &ZXMFCTRL   ++ INPUT NAME                     00151300
                    LCLC  &ZXMFATTR   ++ INPUT VALUE                    00151400
                    LCLC  &ZXMFSCK    ++ INPUT VALUE                    00151500
.********************************************************************   00151600
.***AC.PLV01  PROCESS PLISTVER                                      *   00151700
.********************************************************************   00151800
&XPLISTVER      SETA  0                                                 00151900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00152000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00152100
&XPLISTVER      SETA  0                                                 00152200
.ZPLV0          ANOP  ,                                                 00152300
.*                                                                      00152400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00152500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00152600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00152700
.*-------------------------------------------------------------------   00152800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00152900
&ZMS            SETC  '"PLISTVER='                                      00153000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00153100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00153200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00153300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00153400
                AGO   .ZPVCK2                                           00153500
.ZPVCK3         ANOP  ,                                                 00153600
.*-------------------------------------------------------------------   00153700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00153800
&ZMS            SETC  '"PLISTVER='                                      00153900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00154000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00154100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00154200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00154300
                AGO   .ZPVCKE                                           00154400
.ZPVCK2         ANOP  ,                                                 00154500
.*-------------------------------------------------------------------   00154600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00154700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00154800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00154900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00155000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00155100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00155200
.ZPVCK6         ANOP  ,                                                 00155300
&ZMS            SETC  '"PLISTVER='                                      00155400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00155500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00155600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00155700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00155800
&XPLISTVER      SETA  0                                                 00155900
                AGO   .ZPVCKE                                           00156000
.*-------------------------------------------------------------------   00156100
.ZPVCK4A        ANOP  ,                                                 00156200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00156300
&ZMS            SETC  'PLISTVER='                                       00156400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00156500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00156600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00156700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00156800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00156900
                AGO   .ZPVCK5                                           00157000
.ZPVCK4         ANOP  ,                                                 00157100
&XPLISTVER      SETA  &PLISTVER(1)                                      00157200
.ZPVCK5         ANOP  ,                                                 00157300
.ZPVCKE         ANOP  ,                                                 00157400
.********************************************************************   00157500
