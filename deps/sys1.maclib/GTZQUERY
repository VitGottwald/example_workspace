.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00493000
&ZPLLEN               SETA 176                                          00493100
.*                                                                      00493200
.******************************************************************     00493300
.***AI.L01 GTZQUERY FMT=LIST FILL IN PARAMETER AREA                     00493400
.******************************************************************     00493500
                      ACTR  10000                                       00493600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00493700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00493800
&ZCVAL                SETC  'LAE'          XAX MODE                     00493900
.ZIMFXA               ANOP ,                                            00494000
.*                                                                      00494100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00494200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00494300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00494400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00494500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00494600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00494700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00494800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00494900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00495000
&ZOPN                 SETA  &ZOPN+1                                     00495100
&ZOPL(&ZOPN)          SETC  ' '                                         00495200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00495300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00495400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00495500
                      AGO   .ZIMFNX                                     00495600
.*                                                                      00495700
.ZIMFN2               ANOP ,                                            00495800
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00495900
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00496000
&ZOPN                 SETA  &ZOPN+1                                     00496100
&ZOPL(&ZOPN)          SETC  ' '                                         00496200
&ZOPO(&ZOPN)          SETC  'LAE'                                       00496300
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00496400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00496500
                      AGO   .ZIMFNX                                     00496600
.ZIMFNX1              ANOP ,                                            00496700
&ZOPN                 SETA  &ZOPN+1                                     00496800
&ZOPL(&ZOPN)          SETC  ' '                                         00496900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00497000
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00497100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00497200
.ZIMFNX               ANOP ,                                            00497300
.********************************************************************   00497400
