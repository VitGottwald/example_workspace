.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00609200
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00609300
.*******************************************************************    00609400
.***  PROCESS GTZQUERY OUTPUT PARAMETERS                                00609500
.*******************************************************************    00609600
                      ACTR  10000                                       00609700
.*******************************************************************    00609800
.***AO.X02  RETURN CODE PROCESSING                                      00609900
.*******************************************************************    00610000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00610100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00610200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00610300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00610400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00610500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00610600
&ZOPN                 SETA  &ZOPN+1                                     00610700
&ZOPL(&ZOPN)          SETC  ' '                                         00610800
&ZOPO(&ZOPN)          SETC  'ST'                                        00610900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00611000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00611100
                      AGO   .ZMRETX                                     00611200
.*                                                                      00611300
.ZMRET2               ANOP  ,                                           00611400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00611500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00611600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00611700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00611800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00611900
&ZOPN                 SETA  &ZOPN+1                                     00612000
&ZOPL(&ZOPN)          SETC  ' '                                         00612100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00612200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00612300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00612400
.ZMRETX               ANOP  ,                                           00612500
.*******************************************************************    00612600
.***AO.X03  REASON CODE PROCESSING                                      00612700
.*******************************************************************    00612800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00612900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00613000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00613100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00613200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00613300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00613400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00613500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00613600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00613700
&ZOPN                 SETA  &ZOPN+1                                     00613800
&ZOPL(&ZOPN)          SETC  ' '                                         00613900
&ZOPO(&ZOPN)          SETC  'ST'                                        00614000
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00614100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00614200
                      AGO   .ZMRSNX                                     00614300
.*                                                                      00614400
.ZMRSN2               ANOP  ,                                           00614500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00614600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00614700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00614800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00614900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00615000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00615100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00615200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00615300
&ZOPN                 SETA  &ZOPN+1                                     00615400
&ZOPL(&ZOPN)          SETC  ' '                                         00615500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00615600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00615700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00615800
.ZMRSNX               ANOP  ,                                           00615900
.*******************************************************************    00616000
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00616100
.*******************************************************************    00616200
.ZMFLX                ANOP  ,              MF=L PROCESSING              00616300
.*******************************************************************    00616400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00616500
.*******************************************************************    00616600
                      ACTR  10000                                       00616700
.*                                                                      00616800
                      LCLA  &ZLC           COLUMN POSITION              00616900
                      LCLA  &ZOPX          LIST TABLE INDEX             00617000
                      LCLC  &ZLL           LABEL                        00617100
                      LCLC  &ZLOP          OPERATION                    00617200
                      LCLC  &ZLV           VALUE                        00617300
                      LCLC  &ZLD           DESCRIPTION                  00617400
.*                                                                      00617500
&ZOPX                 SETA  1                                           00617600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00617700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00617800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00617900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00618000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00618100
                      AIF   (K'&ZLD LE 0).ZL5                           00618200
&ZLC                  SETA  35             COMMENT COLUMN               00618300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00618400
&ZLC                  SETA  33             COMMMENT COLUMN              00618500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00618600
&ZLV                  SETC  '&ZLV'.' '                                  00618700
                      AGO   .ZL3                                        00618800
.*                                                                      00618900
.ZL4                  ANOP  ,                                           00619000
&ZLV                  SETC  '&ZLV'.' '                                  00619100
.ZL5                  ANOP  ,                                           00619200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00619300
.*                                                                      00619400
&ZLL     &ZLOP &ZLV                                                     00619500
.*                                                                      00619600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00619700
                      AGO   .ZL2           CONTINUE LOOPING             00619800
.*                                                                      00619900
.ZLX                  ANOP  ,                                           00620000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00620100
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00620200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00620300
         &ZMAC_LLGT 15,16(0,0)      Get CVT address                     00620400
         AIF   ('&SYSASCE' EQ 'P').NOT_AR_1                             00620500
         LAE   15,0(15,0)           Clear AR 15                         00620600
.NOT_AR_1 ANOP                                                          00620700
         &ZMAC_LLGT 15,140(15,0)    Get ECVT address                    00620800
         ICM   15,B'1111',924(15)   Get GTZTABLE address                00620900
         JZ    GTZQUERYNA_&SYSNDX   No GTZTABLE, get out now            00621000
         TM    20(15),B'10000000'   Check QueryAvail                    00621100
         JZ    GTZQUERYNA_&SYSNDX   Query not available                 00621200
         TM    18(15),B'01000000'   Check Stopping                      00621300
         JNZ   GTZQUERYNA_&SYSNDX   Query not available                 00621400
         LMH   15,15,100(15)        LX SEQ#                             00621500
         ICM   15,B'1111',108(15)   Get GTZQUERY PC NUM                 00621600
         JZ    GTZQUERYNA_&SYSNDX   No PC Num, get out now              00621700
         PC    0(15)                PC to handle GTZQUERY               00621800
         J     GTZQUERYEND_&SYSNDX  Branch around setting bad RC        00621900
GTZQUERYNA_&SYSNDX DS 0H                                                00622000
         LA    15,X'0C'             Set RC (Environment error)          00622100
         LA    0,X'C90'             Set RSN (Not ready)                 00622200
GTZQUERYEND_&SYSNDX DS 0H                                               00622300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00622400
.ZLINKX               ANOP  ,                                           00622500
.*******************************************************************    00622600
.***AP.X01  FORMAT GENERATED CODE                                       00622700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00622800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00622900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00623000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00623100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00623200
                      AIF   (K'&ZLD LE 0).ZL51                          00623300
&ZLC                  SETA  35             COMMENT COLUMN               00623400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00623500
&ZLC                  SETA  33             COMMENT COLUMN               00623600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00623700
&ZLV                  SETC  '&ZLV'.' '                                  00623800
                      AGO   .ZL31                                       00623900
.*                                                                      00624000
.ZL41                 ANOP  ,                                           00624100
&ZLV                  SETC  '&ZLV'.' '                                  00624200
.ZL51                 ANOP  ,                                           00624300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00624400
.*                                                                      00624500
&ZLL     &ZLOP &ZLV                                                     00624600
.*                                                                      00624700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00624800
                      AGO   .ZL21          CONTINUE LOOPING             00624900
.*                                                                      00625000
.ZLX1                 ANOP  ,                                           00625100
.*******************************************************************    00625200
.***AP.X02  END OF GTZQUERY MACRO                                       00625300
&ZGTZQUERY            SETC  'YES'          SET GTZQUERY USED            00625400
         POP   PRINT                                                    00625500
*                                            GTZQUERY-0                 00625600
         SPACE 1                                                        00625700
.ZLX2                 ANOP  ,                                           00625800
         MEND                                                           00625900
**********************************************************************/ 00626000
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00626100
*%GTZQUERY: MACRO KEYS(REQUEST,OWNER,SOURCETYPE,SOURCE,SOURCEPATHLEN,   00626200
*                 SOURCEPATH,EVENTDESCLEN,EVENTDESC,EVENTDATA,          00626300
*                 EVENTASID,EVENTJOB,PROGRAMTYPE,PROGRAM,               00626400
*                 PROGRAMPATHLEN,PROGRAMPATH,PROGRAMOFFSET,HOMEASID,    00626500
*                 HOMEJOB,ANSAREA,ANSLEN,SECCHECK,RETCODE,RSNCODE,      00626600
*                 PLISTVER,MF);                                         00626700
*  ANS('?'!!MACLABEL!!' '!!'GTZQUERP '!!MACLIST!!                       00626800
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00626900
*%END GTZQUERY;                                                         00627000
