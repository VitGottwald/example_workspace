*                                      disabled supported        @71A*/         
*       3 *        BIT(5),          /* Reserved                  @71C*/         
*      2 R79ECPAT FIXED(8),         /* Path attribute:                          
*                                      0 = No attributes specified,             
*                                      1 = Preferred path,                      
*                                      2 = Non-preferred path    @L1A*/         
*      2 *        CHAR(2),          /* Reserved for future use   @71C*/         
*      2 R79ECPCX,                  /* Channel path measurement                 
*                                      data extended             @71A*/         
*       3 R79ECTMW FIXED(32),       /* Transport mode write count               
*                                      valid if R79ECPTD=1       @71A*/         
*       3 R79ECTRD FIXED(32),       /* First-transfer-ready-                    
*                                      disabled write count                     
*                                      valid if R79ECPTD=1       @71A*/         
*       3 *        CHAR(8);         /* Reserved for future use   @71A*/         
*   @SPACE;                                                                     
*   /* I/O QUEUING DATA SECTION */                                              
*   @SPACE;                                                                     
*   DECLARE                                                                     
*   @SPACE;                                                                     
*     1 R79EDS                      /* I/O QUEUING DATA SECTION  @P4A*/         
*         BASED(ADDR(SMF79HDR)+SMF79ASS) BDY(WORD),                             
*       3 R79EID2 CHAR(2),          /* LOGICAL CONTROL UNIT                     
*                                      IDENTIFIER                @P4A*/         
*       3 R79EDST BIT(8),           /* DATA STATUS               @P4A*/         
*         5 * BIT(1),               /* RESERVED                  @P4A*/         
*         5 R79ENHMA BIT(1),        /* =1 IF NO HARDWARE MEASUREMENTS           
*                                      AVAILABLE                 @P4A*/         
*         5 * BIT(2),               /* Reserved                  @I3A*/         
*         5 R79ELDCM BIT(1),        /* =1 LCU contains DCM managed              
*                                      channels                  @I3A*/         
*         5 R79ELPAT BIT(1),        /* X'04' Path attributes are                
*                                      valid                     @L1A*/         
*         5 *        BIT(2),        /* Reserved                  @L1C*/         
*       3 * CHAR(1),                /* FOR ALIGNMENT ONLY        @P4A*/         
*       3 R79EQSM FIXED(31),        /* ACCUMULATION OF CU-HDR Q LENGTH          
*                                                                @P4A*/         
*       3 R79EQCT FIXED(31),        /* Count of entries placed on               
*                                      the CU-HDR queue          @I3C*/         
*       3 R79EMCMN FIXED(16),       /* Minimum number of DCM managed            
*                                      channels used             @I3A*/         
*       3 R79EMCMX FIXED(16),       /* Maximum number of DCM managed            
*                                      channels used             @I3A*/         
*       3 R79EMCDF FIXED(16),       /* Defined number of DCM managed            
*                                      channels                  @I3A*/         
*       3 * FIXED(16),              /* Reserved                  @I3A*/         
*       3 R79EDCMA,                 /* DCM managed channel accumulated          
*                                      measurements              @I3A*/         
*         5 R79EPTM FIXED(32),      /* Accumulated path taken count             
*                                      for DCM managed channels  @I3A*/         
*         5 R79EDPBM FIXED(32),     /* Accumulated director port busy           
*                                      count for DCM managed channels           
*                                                                @I3A*/         
*         5 R79ECUBM FIXED(32),     /* Accumulated control unit busy            
*                                      count for DCM managed channels           
*                                                                @K3C*/         
*         5 R79ECBTM FIXED(32),     /* Accumulated control unit busy            
*                                      delay time for DCM                       
*                                      managed channels          @K3A*/         
*         5 R79ECMRM FIXED(32),     /* Accumulated initial                      
*                                      CoMmand Response time for                
*                                      DCM managed channels      @K3A*/         
*         5 R79ESBSM FIXED(32),     /* Accumulated switch busy                  
*                                      count summation for                      
*                                      DCM managed channels      @K3A*/         
*         5 R79ETMWM FIXED(32),     /* Accumulated transport mode               
*                                      write count for DCM                      
*                                      managed channels valid if                
*                                      R79ECPTD=1                @71C*/         
*         5 R79ETRDM FIXED(32),     /* Accumulated first-transfer-              
*                                      ready-disabled write count               
*                                      for DCM managed channels                 
*                                      valid if R79ECPTD=1       @71C*/         
*       3 R79ECSST   FIXED(32);     /* Channel subsystem wait time in           
*                                      units of 128 microseconds @K3A*/         
*                                                                               
*   @SPACE;                                                                     
*   DECLARE                                                                     
*   @SPACE;                                                                     
*     R79E_FIX_LEN FIXED(31) CONSTANT(                                          
*       LENGTH(SMF79HDR) + LENGTH(SMF79PRO) + LENGTH(R79CHL) +                  
*       LENGTH(R79EGD)),            /*                           @E1A*/         
*     R79E_VAR_LEN FIXED(31) CONSTANT(                                          
*       LENGTH(R79ECS) + (8*LENGTH(R79ECPD)) +                                  
*       LENGTH(R79EDS));            /*                           @E1A*/         
*%LR79F: IF IFAR79F='' %THEN %GOTO LR79END;                 /*   @F1A*/         
*/********************************************************************/         
*/*                                                              @F1A*/         
*/* SMF Type 79 Subtype 15 Data Section Mapping R79FIRLM         @F1A*/         
*/*                                                              @F1A*/         
*/********************************************************************/         
*   @SPACE;                                                 /*   @F1A*/         
*   Declare                                                                     
*     1 R79FIRLM Based(Addr(SMF79HDR)+SMF79FPO) BDY(BYTE),  /*   @F1A*/         
*       3 R79FISTN Char(16),        /* IRLM Lock Structure Name  @F1A*/         
*       3 R79FDLKC Fixed(32)                                                    
*                  Boundary(BYTE),  /* Dead Lock Cycle Number    @F1A*/         
*       3 R79FETYP Char(1),         /* 'B'=Blocker / 'W'=Waiter  @F1A*/         
*       3 *        Char(3),         /* Reserved                  @F1A*/         
*       3 R79FWAIT,                 /* Start of variable section:               
*                                      Waiter data               @F1A*/         
*         5 R79FBLKR,               /* Blocker data              @F1A*/         
*           7 R79FIMSI Char(8),     /* IMS Subsystem ID          @F1A*/         
*           7 R79FPSTN Char(2),     /* PST Number                @F1A*/         
*           7 R79FPSBN Char(8),     /* PSB Name                  @F1A*/         
*           7 R79FRGTY Char(1),     /* Region Type               @F1A*/         
*           7 R79FRCVT Char(16),    /* Recovery Token            @F1A*/         
*           7 R79FCTID Char(8),     /* CICS Task ID (If CICS)    @F1A*/         
*           7 R79FLHTI Char(8),     /* Scheduled elapsed time    @F1A*/         
*           7 R79FLHCN Char(4),     /* Max Lock Held-PSTLKHLD    @F1A*/         
*           7 R79FLKNA Char(16),    /* Lock Name                 @F1A*/         
*           7 R79FTRNM Char(8),     /* Trans name / Job name @WA27252*/         
*         5 R79FRSNA Char(8);       /* Resource (DB/Area) Name   @F1A*/         
*   @SPACE;                                                 /*   @F1A*/         
*   Declare                                                                     
*     R79FIRLL Fixed(31) Constant(Length(R79FIRLM));        /*   @F1A*/         
*   @SPACE;                                                 /*   @F1A*/         
*   Declare                                                                     
*     R79FRBAT Char(1) Constant('80'X),  /* Region type = BATCH  @F1A*/         
*     R79FRBMP Char(1) Constant('40'X),  /* Region type = BMP    @F1A*/         
*     R79FRIFP Char(1) Constant('20'X),  /* Region type = IFP    @F1A*/         
*     R79FRCIC Char(1) Constant('10'X),  /* Region type = CICS   @F1A*/         
*     R79FRMPP Char(1) Constant('08'X),  /* Region type = MPP    @F1A*/         
*     R79FRDBC Char(1) Constant('04'X),  /* Region type = DBCTL  @F1A*/         
*     R79FRSYS Char(1) Constant('02'X);  /* Region type = SYPST                 
*                                                            @OA28931*/         
*%LR79END: ;                                                /*   @F1C*/         
