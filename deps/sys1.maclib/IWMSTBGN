.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00157400
.*******************************************************************    00157500
                      ACTR  10000                                       00157600
.*                                                                      00157700
                      LCLA  &ZLC           COLUMN POSITION              00157800
                      LCLA  &ZOPX          LIST TABLE INDEX             00157900
                      LCLC  &ZLL           LABEL                        00158000
                      LCLC  &ZLOP          OPERATION                    00158100
                      LCLC  &ZLV           VALUE                        00158200
                      LCLC  &ZLD           DESCRIPTION                  00158300
.*                                                                      00158400
&ZOPX                 SETA  1                                           00158500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00158600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00158700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00158800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00158900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159000
                      AIF   (K'&ZLD LE 0).ZL5                           00159100
&ZLC                  SETA  35             COMMENT COLUMN               00159200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00159300
&ZLC                  SETA  33             COMMMENT COLUMN              00159400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00159500
&ZLV                  SETC  '&ZLV'.' '                                  00159600
                      AGO   .ZL3                                        00159700
.*                                                                      00159800
.ZL4                  ANOP  ,                                           00159900
&ZLV                  SETC  '&ZLV'.' '                                  00160000
.ZL5                  ANOP  ,                                           00160100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00160200
.*                                                                      00160300
&ZLL     &ZLOP &ZLV                                                     00160400
.*                                                                      00160500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00160600
                      AGO   .ZL2           CONTINUE LOOPING             00160700
.*                                                                      00160800
.ZLX                  ANOP  ,                                           00160900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00161000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00161100
     L     14,16(0,0)              GET CVT ADDRESS                      00161200
     L     14,772(14,0)            GET SFT ADDRESS                      00161300
     L     14,380(14,0)            GET LX FOR SERVICE ROUTINE           00161400
     LA    15,47                   GET EX FOR SERVICE ROUTINE           00161500
     OR    14,15                   GET PC NUMBER                        00161600
     PC    0(14)                                                        00161700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00161800
.ZLINKX               ANOP  ,                                           00161900
.*******************************************************************    00162000
.***AP.X01  FORMAT GENERATED CODE                                       00162100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00162200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00162300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00162400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00162500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00162600
                      AIF   (K'&ZLD LE 0).ZL51                          00162700
&ZLC                  SETA  35             COMMENT COLUMN               00162800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00162900
&ZLC                  SETA  33             COMMENT COLUMN               00163000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00163100
&ZLV                  SETC  '&ZLV'.' '                                  00163200
                      AGO   .ZL31                                       00163300
.*                                                                      00163400
