              AIF   ('&XMF' EQ 'L').XLAB3                               00120800
&M0M0001 DS    0H                             IWMSTBGN-1                00120900
.XLAB3        ANOP  ,                                                   00121000
.*******************************************************************    00121100
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00121200
.*******************************************************************    00121300
              PUSH  PRINT                                               00121400
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00121500
              PRINT OFF         ++ PR01                                 00121600
              AGO   .ZPRNTXX                                            00121700
.ZPRINTX      PRINT ON                                                  00121800
.ZPRNTXX      ANOP  ,                                                   00121900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00122000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00122100
.********************************************************************   00122200
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00122300
.********************************************************************   00122400
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00122500
                      LCLA  &ZOPN1         INVOCATION INDEX             00122600
                      LCLC  &ZOPL(4095)    LABEL                        00122700
                      LCLC  &ZOPO(4095)    OPERATION                    00122800
                      LCLC  &ZOPV(4095)    VALUE                        00122900
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00123000
.*                                                                      00123100
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00123200
.******************************************************************     00123300
.***AL.L10 IWMSTBGN FMT=LIST MF=S!L PARAMETERS                          00123400
.******************************************************************     00123500
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00123600
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00123700
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00123800
&XMFATTR              SETC  '0D'                                        00123900
.*                                                                      00124000
&ZOPN                 SETA  &ZOPN+1                                     00124100
&ZOPL(&ZOPN)          SETC  ' '                                         00124200
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00124300
&ZOPO(&ZOPN)          SETC  'B'                                         00124400
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00124500
                      AGO   .ZALVL6B                                    00124600
.ZALVL6A              ANOP                                              00124700
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00124800
&ZOPV(&ZOPN)          SETC  '4,8'                                       00124900
&ZOPN                 SETA  &ZOPN+1                                     00125000
&ZOPL(&ZOPN)          SETC  ' '                                         00125100
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00125200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00125300
.ZALVL6B              ANOP                                              00125400
&ZOPD(&ZOPN)          SETC  '++ IWMSTBGN ARG PARMS'                     00125500
.*                                                                      00125600
&ZOPN                 SETA  &ZOPN+1                                     00125700
&ZOPL(&ZOPN)          SETC  ' '                                         00125800
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00125900
&ZOPV(&ZOPN)          SETC  '1'                                         00126000
&ZOPD(&ZOPN)          SETC  ' '                                         00126100
.*                                                                      00126200
.ZLMFSL2              ANOP  ,                                           00126300
