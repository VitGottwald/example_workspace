.ZSYS2        ANOP  ,                                                   00069800
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00069900
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00070000
              SYSSTATE TEST                                             00070100
              AGO   .ZSYS4                                              00070200
.ZSYS3        ANOP  ,                                                   00070300
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00070400
.ZSYS4        ANOP  ,                                                   00070500
