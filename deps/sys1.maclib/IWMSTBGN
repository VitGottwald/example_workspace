.***AC.PLV01  PROCESS PLISTVER                                      *   00073100
.********************************************************************   00073200
&XPLISTVER      SETA  0                                                 00073300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00073400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00073500
&XPLISTVER      SETA  1                                                 00073600
.ZPLV0          ANOP  ,                                                 00073700
.*                                                                      00073800
                AIF   ('&ETOKEN' EQ '').ZPLV1E                          00073900
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00074000
&XPLISTVER      SETA  1                                                 00074100
.ZPLV1E         ANOP  ,                                                 00074200
.*                                                                      00074300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00074400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00074500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00074600
.*-------------------------------------------------------------------   00074700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00074800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00074900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00075000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00075100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00075200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00075300
                AGO   .ZPVCK2                                           00075400
.ZPVCK3         ANOP  ,                                                 00075500
.*-------------------------------------------------------------------   00075600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00075700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00075800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00075900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00076000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00076100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00076200
                AGO   .ZPVCKE                                           00076300
.ZPVCK2         ANOP  ,                                                 00076400
.*-------------------------------------------------------------------   00076500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00076600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00076700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00076800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00076900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00077000
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00077100
.ZPVCK6         ANOP  ,                                                 00077200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00077300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00077400
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00077500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00077600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00077700
&XPLISTVER      SETA  1                                                 00077800
                AGO   .ZPVCKE                                           00077900
.*-------------------------------------------------------------------   00078000
.ZPVCK4A        ANOP  ,                                                 00078100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00078200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00078300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00078400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00078500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00078600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00078700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00078800
                AGO   .ZPVCK5                                           00078900
.ZPVCK4         ANOP  ,                                                 00079000
&XPLISTVER      SETA  &PLISTVER(1)                                      00079100
.ZPVCK5         ANOP  ,                                                 00079200
.ZPVCKE         ANOP  ,                                                 00079300
.********************************************************************   00079400
.***AG.MF01 PROCESS MF MACRO FORMS                                      00079500
.********************************************************************   00079600
