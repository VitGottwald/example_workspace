.***AC.PLV01  PROCESS PLISTVER                                      *   00076500
.********************************************************************   00076600
&XPLISTVER      SETA  0                                                 00076700
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00076800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00076900
&XPLISTVER      SETA  0                                                 00077000
.ZPLV0          ANOP  ,                                                 00077100
.*                                                                      00077200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00077300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00077400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00077500
.*-------------------------------------------------------------------   00077600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00077700
&ZMS            SETC  '"PLISTVER='                                      00077800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00077900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00078000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00078100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00078200
                AGO   .ZPVCK2                                           00078300
.ZPVCK3         ANOP  ,                                                 00078400
.*-------------------------------------------------------------------   00078500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00078600
&ZMS            SETC  '"PLISTVER='                                      00078700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00078800
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00078900
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00079000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00079100
                AGO   .ZPVCKE                                           00079200
.ZPVCK2         ANOP  ,                                                 00079300
.*-------------------------------------------------------------------   00079400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00079500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00079600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00079700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00079800
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00079900
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00080000
.ZPVCK6         ANOP  ,                                                 00080100
&ZMS            SETC  '"PLISTVER='                                      00080200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00080300
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00080400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00080500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00080600
&XPLISTVER      SETA  0                                                 00080700
                AGO   .ZPVCKE                                           00080800
.*-------------------------------------------------------------------   00080900
.ZPVCK4A        ANOP  ,                                                 00081000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00081100
&ZMS            SETC  'PLISTVER='                                       00081200
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00081300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00081400
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00081500
                MNOTE 8,' &ZMS IS REQUIRED.'                            00081600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00081700
                AGO   .ZPVCK5                                           00081800
.ZPVCK4         ANOP  ,                                                 00081900
&XPLISTVER      SETA  &PLISTVER(1)                                      00082000
.ZPVCK5         ANOP  ,                                                 00082100
.ZPVCKE         ANOP  ,                                                 00082200
.********************************************************************   00082300
.***AG.MF01 PROCESS MF MACRO FORMS                                      00082400
.********************************************************************   00082500
              ACTR  10000                                               00082600
