&ZOPV(&ZOPN)          SETC  '2(2,1),=AL2(&ZPLLEN)'                      00154200
&ZOPD(&ZOPN)          SETC  '++ INPUT XPLISTLEN XL2'                    00154300
.*-------------------------------------------------------------------   00154400
.***AI.L25  XWLMEUTKN CL8                                               00154500
                      AIF   ('&ZXWLMEUTKN' EQ '').ZI7E                  00154600
                      AIF   ('&ZXWLMEUTKN' EQ 'OUTPUT').ZI7E            00154700
