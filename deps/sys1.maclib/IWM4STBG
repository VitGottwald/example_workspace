.ZIMFLX               ANOP  ,                                           00150700
.*-------------------------------------------------------------------   00150800
.***AI.L05 XOPTIONS BL1                                                 00150900
&ZW                   SETC  ''                                          00151000
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00151100
.*-------------------------------------------------------------------   00151200
.***AI.L08 XOPTIONS "YES"                                               00151300
.***      BL1'1XXXXXXX'                                                 00151400
         AIF   ('&XSUBTASKS' NE 'YES').ZI3A                             00151500
&ZW                   SETC  '&ZW.1'                                     00151600
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00151700
                      AGO   .ZI3C                                       00151800
.*                                                                      00151900
.ZI3A                 ANOP  ,                                           00152000
&ZW                   SETC  '&ZW.0'                                     00152100
.ZI3C                 ANOP  ,                                           00152200
.*-------------------------------------------------------------------   00152300
.***AI.L08 XOPTIONS "IWM4STBG"                                          00152400
.***      BL1'XXXXXXX1'                                                 00152500
&ZW                   SETC  '&ZW.0000001'                               00152600
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00152700
.*-------------------------------------------------------------------   00152800
.***AI.L11 XOPTIONS BL1                                                 00152900
                      AIF   ('&ZCLNG' NE 'YES').ZI4E  ALL ZERO BITS     00153000
&ZOPN                 SETA  &ZOPN+1                                     00153100
&ZOPL(&ZOPN)          SETC  ' '                                         00153200
&ZOPO(&ZOPN)          SETC  'OI'                                        00153300
&ZOPV(&ZOPN)          SETC  '1(1),B''&ZW'''                             00153400
&ZOPD(&ZOPN)          SETC  '++ 39 BL1'                                 00153500
.ZI4E                 ANOP  ,                                           00153600
.*-------------------------------------------------------------------   00153700
.***AI.L04 XPLISTLEN XL2                                                00153800
&ZOPN                 SETA  &ZOPN+1                                     00153900
&ZOPL(&ZOPN)          SETC  ' '                                         00154000
