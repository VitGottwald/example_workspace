.*-------------------------------------------------------------------   00142700
.***AM.L10 END OF IWM4STBG LIST PARAMETERS                              00142800
&ZOPN                 SETA &ZOPN+1                                      00142900
&ZOPL(&ZOPN)          SETC '&XMFCTRL.L'                                 00143000
&ZOPO(&ZOPN)          SETC 'EQU'                                        00143100
&ZOPV(&ZOPN)          SETC '*-&XMFCTRL'                                 00143200
&ZOPD(&ZOPN)          SETC '++ LENGTH OF PLIST'                         00143300
.*                                                                      00143400
&ZOPN                 SETA &ZOPN+1                                      00143500
&ZOPL(&ZOPN)          SETC ' '                                          00143600
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00143700
&ZOPV(&ZOPN)          SETC '1'                                          00143800
&ZOPD(&ZOPN)          SETC ' '                                          00143900
.*-------------------------------------------------------------------   00144000
.***AL.L12 IWM4STBG FMT=LIST MF=S!L PARAMETERS                          00144100
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00144200
.*                                                                      00144300
&ZOPN                 SETA  &ZOPN+1                                     00144400
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00144500
