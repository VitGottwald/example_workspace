.***AE.01  START OF IWM4STBG MACRO CODE                                 00129500
.*******************************************************************    00129600
              LCLC  &M0M0001                 NAME                       00129700
&M0M0001      SETC  '&XLABEL'                SET NAME                   00129800
              AIF   (K'&XLABEL NE 0).XLAB2                              00129900
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00130000
.*                                                                      00130100
.XLAB2        ANOP  ,                                                   00130200
              AIF   ('&XMF' EQ 'L').XLAB3                               00130300
&M0M0001 DS    0H                             IWM4STBG-0                00130400
.XLAB3        ANOP  ,                                                   00130500
.*******************************************************************    00130600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00130700
.*******************************************************************    00130800
              PUSH  PRINT                                               00130900
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00131000
              PRINT OFF         ++ PR01                                 00131100
              AGO   .ZPRNTXX                                            00131200
.ZPRINTX      PRINT ON                                                  00131300
.ZPRNTXX      ANOP  ,                                                   00131400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00131500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00131600
.********************************************************************   00131700
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00131800
.********************************************************************   00131900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00132000
                      LCLA  &ZOPN1         INVOCATION INDEX             00132100
                      LCLC  &ZOPL(4095)    LABEL                        00132200
                      LCLC  &ZOPO(4095)    OPERATION                    00132300
                      LCLC  &ZOPV(4095)    VALUE                        00132400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00132500
.*                                                                      00132600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00132700
.******************************************************************     00132800
.***AL.L10 IWM4STBG FMT=LIST MF=S!L PARAMETERS                          00132900
.******************************************************************     00133000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00133100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00133200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00133300
&XMFATTR              SETC  '0D'                                        00133400
.*                                                                      00133500
&ZOPN                 SETA  &ZOPN+1                                     00133600
&ZOPL(&ZOPN)          SETC  ' '                                         00133700
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00133800
&ZOPV(&ZOPN)          SETC  '4,8'                                       00133900
&ZOPN                 SETA  &ZOPN+1                                     00134000
&ZOPL(&ZOPN)          SETC  ' '                                         00134100
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00134200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00134300
&ZOPD(&ZOPN)          SETC  '++ IWM4STBG ARG PARMS'                     00134400
.*                                                                      00134500
&ZOPN                 SETA  &ZOPN+1                                     00134600
&ZOPL(&ZOPN)          SETC  ' '                                         00134700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00134800
&ZOPV(&ZOPN)          SETC  '1'                                         00134900
&ZOPD(&ZOPN)          SETC  ' '                                         00135000
.*                                                                      00135100
.ZLMFSL2              ANOP  ,                                           00135200
.*-------------------------------------------------------------------   00135300
.***AM.L00  IWM4STBG FMT=LIST name                                      00135400
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00135500
&ZOPN                 SETA &ZOPN+1                                      00135600
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00135700
&ZOPO(&ZOPN)          SETC 'EQU'                                        00135800
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00135900
&ZOPD(&ZOPN)          SETC '++ IWM4STBG NAME'                           00136000
