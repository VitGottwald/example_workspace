.*                                                                      00155900
.ZI7E                 ANOP  ,                                           00156000
.*-------------------------------------------------------------------   00156100
&ZOPN1                SETA &ZOPN                                        00156200
.*******************************************************************    00156300
.***  PROCESS IWM4STBG OUTPUT PARAMETERS                                00156400
.*******************************************************************    00156500
                      ACTR  10000                                       00156600
.*------------------------------------------------------------------    00156700
.***AO.L02 XETOKEN CL8                                                  00156800
         AIF   ('&ZXETOKEN' EQ 'OUTPUT').ZO8A  OUTPUT                   00156900
         AIF   ('&ZXETOKEN' NE 'INOUT').ZO8C  INOUT                     00157000
.ZO8A    AIF   ('&XETOKEN'(1,1) NE '(').ZO8B  ADDR IN GPR               00157100
&ZCGPR   SETC  '&XETOKEN'(2,K'&XETOKEN-2)                               00157200
&ZOPN                 SETA  &ZOPN+1                                     00157300
&ZOPL(&ZOPN)          SETC  ' '                                         00157400
&ZOPO(&ZOPN)          SETC  'MVC'                                       00157500
&ZOPV(&ZOPN)          SETC  '0(8,&ZCGPR),16(1)'                         00157600
&ZOPD(&ZOPN)          SETC  '++ XETOKEN CL8'                            00157700
                      AGO   .ZO8C                                       00157800
.*                                                                      00157900
.ZO8B                 ANOP  ,                                           00158000
&ZAWRK   SETA  K'&XETOKEN                                               00158100
         AIF   ('&XETOKEN'(&ZAWRK,1) NE ')').ZO8G                       00158200
&ZN      SETA  2                                                        00158300
.ZO8H    ANOP                                                           00158400
         AIF   (&ZN GE &ZAWRK).ZO8G                                     00158500
         AIF   ('&XETOKEN'(&ZN,1) NE '(').ZO8I                          00158600
&ZC      SETC  '&XETOKEN'(1,&ZN).'8'.','                                00158700
&ZC      SETC  '&ZC'.'&XETOKEN'(&ZN+1,&ZAWRK-&ZN).',16(1)'              00158800
         AGO   .ZO8J                                                    00158900
.ZO8I    ANOP                                                           00159000
&ZN      SETA  &ZN+1                                                    00159100
         AGO   .ZO8H                                                    00159200
.ZO8G    ANOP                                                           00159300
&ZC      SETC  '&XETOKEN.(8),16(1)'                                     00159400
.ZO8J    ANOP                                                           00159500
&ZOPN                 SETA  &ZOPN+1                                     00159600
&ZOPL(&ZOPN)          SETC  ' '                                         00159700
&ZOPO(&ZOPN)          SETC  'MVC'                                       00159800
&ZOPV(&ZOPN)          SETC  '&ZC'                                       00159900
&ZOPD(&ZOPN)          SETC  '++ XETOKEN CL8'                            00160000
.ZO8C                 ANOP  ,                                           00160100
.*******************************************************************    00160200
.***AO.X02  RETURN CODE PROCESSING                                      00160300
.*******************************************************************    00160400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00160500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00160600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00160700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00160800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00160900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00161000
&ZOPN                 SETA  &ZOPN+1                                     00161100
&ZOPL(&ZOPN)          SETC  ' '                                         00161200
&ZOPO(&ZOPN)          SETC  'ST'                                        00161300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00161400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00161500
                      AGO   .ZMRETX                                     00161600
.*                                                                      00161700
.ZMRET2               ANOP  ,                                           00161800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00161900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00162000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00162100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00162200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00162300
&ZOPN                 SETA  &ZOPN+1                                     00162400
&ZOPL(&ZOPN)          SETC  ' '                                         00162500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00162600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00162700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00162800
.ZMRETX               ANOP  ,                                           00162900
.*******************************************************************    00163000
.***AO.X03  REASON CODE PROCESSING                                      00163100
.*******************************************************************    00163200
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00163300
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00163400
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00163500
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00163600
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00163700
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00163800
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00163900
