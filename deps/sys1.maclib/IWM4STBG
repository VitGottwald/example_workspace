&ZOPD(&ZOPN)          SETC '++ FIELD_LABEL'                             00138200
.*                                                                      00138300
&ZOPN                 SETA  &ZOPN+1                                     00138400
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._XSUBTASKS_YES'                   00138500
&ZOPO(&ZOPN)          SETC  'EQU'                                       00138600
&ZOPV(&ZOPN)          SETC  'B''10000000'''                             00138700
&ZOPD(&ZOPN)          SETC  '++ XSUBTASKS.'                             00138800
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).YES'                          00138900
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00139000
.*                                                                      00139100
&ZOPN                 SETA  &ZOPN+1                                     00139200
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._IWM4STBG'                        00139300
&ZOPO(&ZOPN)          SETC  'EQU'                                       00139400
&ZOPV(&ZOPN)          SETC  'B''00000001'''                             00139500
&ZOPD(&ZOPN)          SETC  '++ MACUSED.'                               00139600
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).IWM4STBG'                     00139700
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00139800
.*-------------------------------------------------------------------   00139900
.***AM.L09  XPLISTLEN XL2                                               00140000
