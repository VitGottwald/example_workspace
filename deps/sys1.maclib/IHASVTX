* %DCL ASAXMAC_ISITPLX CHAR EXT;                            /* 061218*/         
* ?ASAXMAC IsItPLX;                                         /* 061218*/         
* %IF ASAXMAC_ISITPLX = 'YES' &                             /* 061218*/         
*     IHASVTX_NOEDCL ^= 'YES' %THEN                           /* @05A*/         
* %DO;                                                      /* 061218*/         
* DCL IEAVWUQP ENTRY(Fixed(31) Input ByValue InReg(0))                          
*       Based(SVTXWUQP) Valrg(*);  /* No AMODE, as it wants the AMODE           
*                          to match the caller's module's AMODE  @PBC*/         
* DCL IEAVWUQP_SET Fixed(31) Constant(1);                   /* 061218*/         
* DCL IEAVWUQP_RESET Fixed(31) Constant(0);                 /* 061218*/         
* DCL IEAVSPGW ENTRY Options(AMODE(31))                                         
*       Based(SVTXSPGW) Valrg(*);                             /* @LQA*/         
* DCL IEAVSPFW ENTRY Options(AMODE(31))                                         
*       Based(SVTXSPFW) Valrg(*);                             /* @LQA*/         
* DCL IEAVJAON ENTRY(Fixed ByValue Inreg(1)) Options(AMODE(31))                 
*       Based(SVTXJAON) Valrg(*);                             /* @05A*/         
* DCL IEAVJAON_Basic Constant(0) Fixed(31);                   /* @05A*/         
* DCL IEAVJA_KF Constant(1) Fixed(31);                        /* @05A*/         
* %END;                                                     /* 061218*/         
