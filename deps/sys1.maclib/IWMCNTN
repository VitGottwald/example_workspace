.***AO.X04  LABEL FOR MF=L PROCESSING                                   00219200
.*******************************************************************    00219300
.ZMFLX                ANOP  ,              MF=L PROCESSING              00219400
.*******************************************************************    00219500
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00219600
.*******************************************************************    00219700
                      ACTR  10000                                       00219800
.*                                                                      00219900
                      LCLA  &ZLC           COLUMN POSITION              00220000
                      LCLA  &ZOPX          LIST TABLE INDEX             00220100
                      LCLC  &ZLL           LABEL                        00220200
                      LCLC  &ZLOP          OPERATION                    00220300
                      LCLC  &ZLV           VALUE                        00220400
                      LCLC  &ZLD           DESCRIPTION                  00220500
.*                                                                      00220600
&ZOPX                 SETA  1                                           00220700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00220800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00220900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00221000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00221100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00221200
                      AIF   (K'&ZLD LE 0).ZL5                           00221300
&ZLC                  SETA  35             COMMENT COLUMN               00221400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00221500
&ZLC                  SETA  33             COMMMENT COLUMN              00221600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00221700
&ZLV                  SETC  '&ZLV'.' '                                  00221800
                      AGO   .ZL3                                        00221900
.*                                                                      00222000
.ZL4                  ANOP  ,                                           00222100
&ZLV                  SETC  '&ZLV'.' '                                  00222200
.ZL5                  ANOP  ,                                           00222300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00222400
.*                                                                      00222500
&ZLL     &ZLOP &ZLV                                                     00222600
.*                                                                      00222700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00222800
                      AGO   .ZL2           CONTINUE LOOPING             00222900
.*                                                                      00223000
.ZLX                  ANOP  ,                                           00223100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00223200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00223300
         L     14,16(0,0)              GET CVT ADDRESS                  00223400
         L     14,772(14,0)            GET SFT ADDRESS                  00223500
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00223600
         LA    15,107                  GET EX FOR SERVICE ROUTINE       00223700
         OR    14,15                   GET PC NUMBER                    00223800
         PC    0(14)                                                    00223900
END&SYSNDX DS    0H                    END LABEL                        00224000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00224100
