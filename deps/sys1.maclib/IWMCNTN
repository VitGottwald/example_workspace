&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00215400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00215500
.ZMRETX               ANOP  ,                                           00215600
.*******************************************************************    00215700
.***AO.X03  REASON CODE PROCESSING                                      00215800
.*******************************************************************    00215900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00216000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00216100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00216200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00216300
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00216400
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00216500
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00216600
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00216700
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00216800
&ZOPN                 SETA  &ZOPN+1                                     00216900
&ZOPL(&ZOPN)          SETC  ' '                                         00217000
&ZOPO(&ZOPN)          SETC  'ST'                                        00217100
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00217200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00217300
                      AGO   .ZMRSNX                                     00217400
.*                                                                      00217500
.ZMRSN2               ANOP  ,                                           00217600
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00217700
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00217800
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00217900
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00218000
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00218100
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00218200
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00218300
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00218400
