.***AC.PLV01  PROCESS PLISTVER                                      *   00092800
.********************************************************************   00092900
&XPLISTVER      SETA  0                                                 00093000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00093100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00093200
&XPLISTVER      SETA  0                                                 00093300
.ZPLV0          ANOP  ,                                                 00093400
.*                                                                      00093500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00093600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00093700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00093800
.*-------------------------------------------------------------------   00093900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00094000
&ZMS            SETC  '"PLISTVER='                                      00094100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00094200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00094300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00094400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00094500
                AGO   .ZPVCK2                                           00094600
.ZPVCK3         ANOP  ,                                                 00094700
.*-------------------------------------------------------------------   00094800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00094900
&ZMS            SETC  '"PLISTVER='                                      00095000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00095100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00095200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00095300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00095400
                AGO   .ZPVCKE                                           00095500
.ZPVCK2         ANOP  ,                                                 00095600
.*-------------------------------------------------------------------   00095700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00095800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00095900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00096000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00096100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00096200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00096300
.ZPVCK6         ANOP  ,                                                 00096400
&ZMS            SETC  '"PLISTVER='                                      00096500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00096600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00096700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00096800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00096900
&XPLISTVER      SETA  0                                                 00097000
                AGO   .ZPVCKE                                           00097100
.*-------------------------------------------------------------------   00097200
.ZPVCK4A        ANOP  ,                                                 00097300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00097400
&ZMS            SETC  'PLISTVER='                                       00097500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00097600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00097700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00097800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00097900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00098000
                AGO   .ZPVCK5                                           00098100
.ZPVCK4         ANOP  ,                                                 00098200
&XPLISTVER      SETA  &PLISTVER(1)                                      00098300
.ZPVCK5         ANOP  ,                                                 00098400
.ZPVCKE         ANOP  ,                                                 00098500
.********************************************************************   00098600
.***AG.MF01 PROCESS MF MACRO FORMS                                      00098700
.********************************************************************   00098800
                     ACTR  10000                                        00098900
.*------------------------------------------------------------------    00099000
