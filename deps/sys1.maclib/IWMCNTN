                      AIF   ('&ZXRESOURCEID_LEN' EQ '').ZI10E           00209900
                      AIF   ('&ZXRESOURCEID_LEN' EQ 'OUTPUT').ZI10E     00210000
&ZCGPR                SETC  '&XRESOURCEID_LEN'                          00210100
.*                                                                      00210200
                      AIF   ('&XRESOURCEID_LEN'(1,1) NE '(').ZI10B      00210300
&ZCGPR   SETC  '0('.'&XRESOURCEID_LEN'(2,K'&XRESOURCEID_LEN-2).')'      00210400
                      AGO   .ZI10D                                      00210500
.ZI10B                ANOP  ,                                           00210600
.*                                                                      00210700
&ZC                   SETC  '&XRESOURCEID_LEN'(1,1)                     00210800
                      AIF   (T'&ZC NE 'N').ZI10D                        00210900
&ZC                   SETC  '&XRESOURCEID_LEN'                          00211000
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI10D               00211100
&ZCGPR                SETC  '=AL4(&XRESOURCEID_LEN)'                    00211200
.*                                                                      00211300
.ZI10D                ANOP  ,                                           00211400
.*                                                                      00211500
&ZOPN                 SETA  &ZOPN+1                                     00211600
&ZOPL(&ZOPN)          SETC  ' '                                         00211700
&ZOPO(&ZOPN)          SETC  'MVC'                                       00211800
&ZOPV(&ZOPN)          SETC  '40(4,1),&ZCGPR'                            00211900
&ZOPD(&ZOPN)          SETC  '++ &ZXRESOURCEID_LEN XRESOURCEID_LEN'      00212000
.*                                                                      00212100
.ZI10E                ANOP  ,                                           00212200
.*-------------------------------------------------------------------   00212300
&ZOPN1                SETA &ZOPN                                        00212400
.*******************************************************************    00212500
.***  PROCESS IWMCNTN OUTPUT PARAMETERS                                 00212600
.*******************************************************************    00212700
                      ACTR  10000                                       00212800
.*******************************************************************    00212900
.***AO.X02  RETURN CODE PROCESSING                                      00213000
.*******************************************************************    00213100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00213200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00213300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00213400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00213500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00213600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00213700
&ZOPN                 SETA  &ZOPN+1                                     00213800
&ZOPL(&ZOPN)          SETC  ' '                                         00213900
&ZOPO(&ZOPN)          SETC  'ST'                                        00214000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00214100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00214200
                      AGO   .ZMRETX                                     00214300
.*                                                                      00214400
.ZMRET2               ANOP  ,                                           00214500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00214600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00214700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00214800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00214900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00215000
&ZOPN                 SETA  &ZOPN+1                                     00215100
