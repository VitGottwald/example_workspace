.***AC.PLV01  PROCESS PLISTVER                                      *   00171400
.********************************************************************   00171500
&XPLISTVER      SETA  2                                                 00171600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00171700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00171800
&XPLISTVER      SETA  3                                                 00171900
.ZPLV0          ANOP  ,                                                 00172000
.*                                                                      00172100
                AIF   ('&GROUP' EQ '').ZPLV1E                           00172200
                AIF   (&XPLISTVER GE 3).ZPLV1E                          00172300
&XPLISTVER      SETA  3                                                 00172400
.ZPLV1E         ANOP  ,                                                 00172500
.*                                                                      00172600
                AIF   ('&PAVINFOSUM' EQ '').ZPLV2E                      00172700
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00172800
&XPLISTVER      SETA  3                                                 00172900
.ZPLV2E         ANOP  ,                                                 00173000
.*                                                                      00173100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00173200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00173300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00173400
.*-------------------------------------------------------------------   00173500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00173600
&ZMS            SETC  '"PLISTVER='                                      00173700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00173800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00173900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00174000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00174100
                AGO   .ZPVCK2                                           00174200
.ZPVCK3         ANOP  ,                                                 00174300
.*-------------------------------------------------------------------   00174400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00174500
&ZMS            SETC  '"PLISTVER='                                      00174600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00174700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00174800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00174900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00175000
                AGO   .ZPVCKE                                           00175100
.ZPVCK2         ANOP  ,                                                 00175200
.*-------------------------------------------------------------------   00175300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00175400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00175500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00175600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00175700
                AIF   (&ZN LT 2).ZPVCK6    TOO LOW                      00175800
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00175900
.ZPVCK6         ANOP  ,                                                 00176000
&ZMS            SETC  '"PLISTVER='                                      00176100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00176200
&ZMS            SETC  '&ZMS THE RANGE IS "2-3"'                         00176300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00176400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00176500
&XPLISTVER      SETA  3                                                 00176600
                AGO   .ZPVCKE                                           00176700
.*-------------------------------------------------------------------   00176800
.ZPVCK4A        ANOP  ,                                                 00176900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00177000
&ZMS            SETC  'PLISTVER='                                       00177100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00177200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00177300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00177400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00177500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00177600
                AGO   .ZPVCK5                                           00177700
.ZPVCK4         ANOP  ,                                                 00177800
&XPLISTVER      SETA  &PLISTVER(1)                                      00177900
.ZPVCK5         ANOP  ,                                                 00178000
.ZPVCKE         ANOP  ,                                                 00178100
