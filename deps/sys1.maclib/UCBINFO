                      LCLA  &ZOPX          LIST TABLE INDEX             00723600
                      LCLC  &ZLL           LABEL                        00723700
                      LCLC  &ZLOP          OPERATION                    00723800
                      LCLC  &ZLV           VALUE                        00723900
                      LCLC  &ZLD           DESCRIPTION                  00724000
.*                                                                      00724100
&ZOPX                 SETA  1                                           00724200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00724300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00724400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00724500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00724600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00724700
                      AIF   (K'&ZLD LE 0).ZL5                           00724800
&ZLC                  SETA  35             COMMENT COLUMN               00724900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00725000
&ZLC                  SETA  33             COMMMENT COLUMN              00725100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00725200
&ZLV                  SETC  '&ZLV'.' '                                  00725300
                      AGO   .ZL3                                        00725400
.*                                                                      00725500
.ZL4                  ANOP  ,                                           00725600
&ZLV                  SETC  '&ZLV'.' '                                  00725700
.ZL5                  ANOP  ,                                           00725800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00725900
.*                                                                      00726000
&ZLL     &ZLOP &ZLV                                                     00726100
.*                                                                      00726200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00726300
                      AGO   .ZL2           CONTINUE LOOPING             00726400
.*                                                                      00726500
.ZLX                  ANOP  ,                                           00726600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00726700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00726800
.*-----------BAL invocation code-------------                           00726900
.LNKFC3  LA    0,3                ++ SET UCBINFO FUNCTION CODE          00727000
         AIF    ('&XLINKAGE' NE 'BRANCH').LNKPC1                        00727100
.*********************************************************************  00727200
.*                                                                   *  00727300
.*  Get addressability to CVT                                        *  00727400
.*                                                                   *  00727500
.*********************************************************************  00727600
         L     15,16(0,0)                                               00727700
.*********************************************************************  00727800
.*                                                                   *  00727900
.*  Get addressability to IOCOM                                      *  00728000
.*                                                                   *  00728100
.*********************************************************************  00728200
         L     15,124(15,0)                                             00728300
.*********************************************************************  00728400
.*                                                                   *  00728500
.*  Get addressability to IOVT                                       *  00728600
.*                                                                   *  00728700
.*********************************************************************  00728800
         L     15,208(15,0)                                             00728900
         L     15,12(15,0)        ++ SET ADDR OF UCB SERVICES           00729000
         BALR  14,15              ++ BRANCH TO UCB SERVICES             00729100
         AGO   .DONE1                                                   00729200
.LNKPC1  ANOP                                                           00729300
         L     14,16(0,0)         ++ GET CVT ADDRESS                    00729400
         L     14,772(14,0)       ++ GET SFT ADDRESS                    00729500
.*********************************************************************  00729600
.*                                                                   *  00729700
.*  Get LX/EX for UCB Services                                       *  00729800
.*                                                                   *  00729900
.*  Note: LX/EX is calculated by multiplying the PC number (79)      *  00730000
.*        times the length of a PC/AUTH table entry (4) and          *  00730100
.*        then subtracting 4.                                        *  00730200
.*                                                                   *  00730300
.*********************************************************************  00730400
         L     14,312(14,0)       ++ GET LX/EX FOR UCB SERVICES         00730500
         PC    0(14)              ++ PC TO UCB SERVICES                 00730600
.DONE1   ANOP                                                           00730700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00730800
.ZLINKX               ANOP  ,                                           00730900
.*******************************************************************    00731000
.***AP.X01  FORMAT GENERATED CODE                                       00731100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00731200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00731300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00731400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00731500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00731600
                      AIF   (K'&ZLD LE 0).ZL51                          00731700
&ZLC                  SETA  35             COMMENT COLUMN               00731800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00731900
&ZLC                  SETA  33             COMMENT COLUMN               00732000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00732100
&ZLV                  SETC  '&ZLV'.' '                                  00732200
                      AGO   .ZL31                                       00732300
.*                                                                      00732400
.ZL41                 ANOP  ,                                           00732500
&ZLV                  SETC  '&ZLV'.' '                                  00732600
.ZL51                 ANOP  ,                                           00732700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00732800
.*                                                                      00732900
&ZLL     &ZLOP &ZLV                                                     00733000
.*                                                                      00733100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00733200
                      AGO   .ZL21          CONTINUE LOOPING             00733300
.*                                                                      00733400
.ZLX1                 ANOP  ,                                           00733500
.*******************************************************************    00733600
.***AP.X02  END OF UCBINFO MACRO                                        00733700
