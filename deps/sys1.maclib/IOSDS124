*/* SMF Record Type and Subtype Constants                            */ 00841000
*/*                                                                  */ 00842000
*/********************************************************************/ 00843000
*Dcl SMF124RecordType Fixed(8) Constant(124);                           00844000
*                                   /* SMF record type               */ 00845000
*Dcl SMF124RecordSubTypeLinkDiag Fixed(16) Constant(1);                 00846000
*                                   /* Subtype 1 - Link Diagnostic      00847000
*                                      Information                   */ 00848000
*Dcl SMF124RecordSubTypeEPSecStat Fixed(16) Constant(2);                00849000
*                                   /* Subtype 2 - Endpoint security    00850000
*                                      Status                    @03A*/ 00851000
*Dcl SMF124RecordSubTypeEpSecAuthKeyUpd Fixed(16) Constant(3);          00852000
*                                   /* Subtype 3 - Endpoint security    00853000
*                                      authentication key update @03A*/ 00854000
*Dcl SMF124RecordSubTypeEpSecEncrKeyUpd Fixed(16) Constant(4);          00855000
*                                   /* Subtype 4 - Endpoint security    00856000
*                                      encryption key update     @03A*/ 00857000
*Dcl SMF124RecordSubTypeExtKeyMgr Fixed(16) Constant(5);                00858000
*                                   /* Subtype 5 - External key         00859000
*                                      manager event record      @03A*/ 00860000
*                                                                       00861000
*/********************************************************************/ 00862000
*/*                                                                  */ 00863000
*/* SMF 124 Record Header                                            */ 00864000
*/*                                                                  */ 00865000
*/********************************************************************/ 00866000
*Dcl                                                                    00867000
*  1 SMF124Rec Bdy(Word) Based                                          00868000
*   ,3 SMF124HdrPfx                 /* Standard SMF header w/subtype */ 00869000
*     ,5 SMF124Len Fixed(16)        /* Record length                 */ 00870000
*     ,5 SMF124Seg Fixed(16)        /* Segment descriptor            */ 00871000
*     ,5 SMF124Flg Bit(8)           /* Flags                         */ 00872000
*       ,7 SMF124Flg_Subsys Bit(1)  /* Subsystem id specified        */ 00873000
*       ,7 SMF124Flg_Subtype Bit(1) /* Subtypes are used             */ 00874000
*     ,5 SMF124Rty Fixed(8)         /* Record Type (=124 '7C'X)      */ 00875000
*     ,5 SMF124Tme Fixed(32) Bdy(HWord)                                 00876000
*                                   /* Time since midnight in           00877000
*                                      hundredths of a second that      00878000
*                                      the record was moved into        00879000
*                                      the SMF buffer                */ 00880000
*     ,5 SMF124Dte Bit(32)          /* Date when record was moved       00881000
*                                      into the SMF buffer.             00882000
*                                      Format: 0cyydddF              */ 00883000
*     ,5 SMF124SID Char(4)          /* System identification         */ 00884000
*     ,5 SMF124SSI Char(4)          /* Subsystem identification      */ 00885000
*     ,5 SMF124Sty Fixed(16)        /* Record subtype. See              00886000
*                                      Smf124RecordSubType above     */ 00887000
*                                                                       00888000
*   ,3 SMF124Hdr                    /* Self-defining header          */ 00889000
*     ,5 SMF124Hdr_Len Fixed(16)    /* Length of header              */ 00890000
*     ,5 SMF124Hdr_TrplCnt Fixed(16)/* Number of triplets in header  */ 00891000
*     ,5 SMF124Hdr_Flags Bit(16)    /* Header flags                  */ 00892000
*     ,5 * Char(2)                  /* Reserved                      */ 00893000
*     ,5 SMF124HdrTriplets Char(0)  /* Start of subtype triplets.       00894000
*                                      See the individual subtypes      00895000
*                                      for the definition of the        00896000
*                                      triplets for the particular      00897000
*                                      subtype.                      */ 00898000
* %If IBM_CBGENC ^= 'YES' %Then                                         00899000
* %Do;                                                                  00900000
*     ;                                                                 00901000
* %End;                                                                 00902000
* %Else %Do;                                                            00903000
