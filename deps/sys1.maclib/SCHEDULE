         AGO   .CHKSPT3                                            @D2A 03590000
.*                                                                      03600000
.SPTERR4 ANOP                                                      @D2C 03610000
         MNOTE 8,'*** SPTOKEN PARAMETER NOT VALID: ''&SPTOKEN''.'  @D2C 03620000
&IHBERR  SETA  8                        SAVE ERROR CODE            @D2A 03630000
         AGO   .ENDSPT                                             @D2A 03640000
.*                                                                      03650000
.FEAERR1 ANOP                                                      @01A 03660000
         MNOTE 8,'*** EXCESSIVE PARAMETERS SPECIFIED FOR ''FEATURE'' KEX03670000
               YWORD.'                                             @01A 03680000
&IHBERR  SETA  8                        SAVE ERROR CODE            @01A 03690000
         AGO   .CHKFEA1                                            @01A 03700000
.*                                                                      03710000
.FEAERR2 ANOP                                                      @01A 03720000
         MNOTE 8,'*** INVALID FEATURE OPTION, SPECIFY CRYPTO.'         X03730000
                                                                   @01A 03740000
&IHBERR  SETA  8                        SAVE ERROR CODE            @01A 03750000
         AGO   .ENDFEAT                                            @01A 03760000
.*                                                                      03770000
.********************************************************************** 03780000
.*       DELETE LABEL .LEVEL2 WITH CALL TO SCHEDUL7 THAT SET @YA11561 * 03790000
.*       COMPAT KEYWORD TO YES                               @YA11561 * 03800000
.********************************************************************** 03810000
.*                                                                      03820000
.LEVEL1  ANOP                                                           03830000
&NAME    SCHEDUL7 SRB=&SRB,SCOPE=&SCOPE,LLOCK=&LLOCK,FRR=&FRR,         X03840000
               CLEAR=&CLEAR,COMPAT=&COMPAT,&DISABLE                     03850000
.*                                                                      03860000
.*                                                                      03870000
.ENDSCHD ANOP                                                      @L2A 03880000
         MEXIT                                                          03890000
         MEND                                                           03900000
* */                                                                    03910000
