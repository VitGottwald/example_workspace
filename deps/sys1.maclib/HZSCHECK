.***AC.PLV01  PROCESS PLISTVER                                      *   00282600
.********************************************************************   00282700
&XPLISTVER      SETA  0                                                 00282800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00282900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00283000
&XPLISTVER      SETA  2                                                 00283100
.ZPLV0          ANOP  ,                                                 00283200
.*                                                                      00283300
                AIF   ('&REXXTIMELIM' EQ '').ZPLV1E                     00283400
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00283500
&XPLISTVER      SETA  1                                                 00283600
.ZPLV1E         ANOP  ,                                                 00283700
.*                                                                      00283800
                AIF   ('&REXXHLQ' EQ '').ZPLV2E                         00283900
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00284000
&XPLISTVER      SETA  2                                                 00284100
.ZPLV2E         ANOP  ,                                                 00284200
.*                                                                      00284300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00284400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00284500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00284600
.*-------------------------------------------------------------------   00284700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00284800
&ZMS            SETC  '"PLISTVER='                                      00284900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00285000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00285100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00285200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00285300
                AGO   .ZPVCK2                                           00285400
.ZPVCK3         ANOP  ,                                                 00285500
.*-------------------------------------------------------------------   00285600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00285700
&ZMS            SETC  '"PLISTVER='                                      00285800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00285900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00286000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00286100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00286200
                AGO   .ZPVCKE                                           00286300
.ZPVCK2         ANOP  ,                                                 00286400
.*-------------------------------------------------------------------   00286500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00286600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00286700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00286800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00286900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00287000
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00287100
.ZPVCK6         ANOP  ,                                                 00287200
&ZMS            SETC  '"PLISTVER='                                      00287300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00287400
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00287500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00287600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00287700
&XPLISTVER      SETA  2                                                 00287800
                AGO   .ZPVCKE                                           00287900
.*-------------------------------------------------------------------   00288000
.ZPVCK4A        ANOP  ,                                                 00288100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00288200
&ZMS            SETC  'PLISTVER='                                       00288300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00288400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00288500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00288600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00288700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00288800
                AGO   .ZPVCK5                                           00288900
.ZPVCK4         ANOP  ,                                                 00289000
&XPLISTVER      SETA  &PLISTVER(1)                                      00289100
.ZPVCK5         ANOP  ,                                                 00289200
.ZPVCKE         ANOP  ,                                                 00289300
.********************************************************************   00289400
.***AG.MF01 PROCESS MF MACRO FORMS                                      00289500
.********************************************************************   00289600
                    ACTR  10000                                         00289700
