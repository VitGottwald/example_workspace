.*                                                                      01478300
.******************************************************************     01478400
.***AI.L01 HZSCHECK FMT=LIST FILL IN PARAMETER AREA                     01478500
.******************************************************************     01478600
                      ACTR  10000                                       01478700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01478800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01478900
&ZCVAL                SETC  'LAE'          XAX MODE                     01479000
.ZIMFXA               ANOP ,                                            01479100
.*                                                                      01479200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01479300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01479400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01479500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01479600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01479700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01479800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01479900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01480000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01480100
&ZOPN                 SETA  &ZOPN+1                                     01480200
&ZOPL(&ZOPN)          SETC  ' '                                         01480300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01480400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01480500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01480600
                      AGO   .ZIMFNX                                     01480700
.*                                                                      01480800
.ZIMFN2               ANOP ,                                            01480900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01481000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01481100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01481200
&ZOPN                 SETA  &ZOPN+1                                     01481300
