*                    !   !   Ð ,CHECKNAME=xcheckname                  * 00031700
*                    !   !   ¿ ,NAME=xname                            * 00031800
*                    !   !    Ý,EXITRTN=»xexitrtn!ANY_EXITRTNº¨       * 00031900
*                    !   !    Ý,CATEGORY=»xcategory!ANY_CATEGORYº     * 00032000
*                    !   !         ,NUMCAT=xnumcat                    * 00032100
*                    !   !       Ð ,CATRULE=DEFAULT                   * 00032200
