.******************************************************************     00123900
.***AI.L01 IWMELEAV FMT=LIST FILL IN PARAMETER AREA                     00124000
.******************************************************************     00124100
                      ACTR  10000                                       00124200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00124300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00124400
&ZCVAL                SETC  'LAE'          XAX MODE                     00124500
.ZIMFXA               ANOP ,                                            00124600
.*                                                                      00124700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00124800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00124900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00125000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00125100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00125200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00125300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00125400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00125500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00125600
&ZOPN                 SETA  &ZOPN+1                                     00125700
&ZOPL(&ZOPN)          SETC  ' '                                         00125800
