.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00112600
.********************************************************************   00112700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00112800
                      LCLA  &ZOPN1         INVOCATION INDEX             00112900
                      LCLC  &ZOPL(4095)    LABEL                        00113000
                      LCLC  &ZOPO(4095)    OPERATION                    00113100
                      LCLC  &ZOPV(4095)    VALUE                        00113200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00113300
.*                                                                      00113400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00113500
.******************************************************************     00113600
.***AL.L10 IWMELEAV FMT=LIST MF=S!L PARAMETERS                          00113700
.******************************************************************     00113800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00113900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00114000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00114100
&XMFATTR              SETC  '0D'                                        00114200
.*                                                                      00114300
&ZOPN                 SETA  &ZOPN+1                                     00114400
&ZOPL(&ZOPN)          SETC  ' '                                         00114500
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00114600
&ZOPO(&ZOPN)          SETC  'B'                                         00114700
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00114800
                      AGO   .ZALVL6B                                    00114900
.ZALVL6A              ANOP                                              00115000
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00115100
&ZOPV(&ZOPN)          SETC  '4,8'                                       00115200
