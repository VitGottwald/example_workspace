*     3  JDYOWPTY  FIXED(8),       /* Output priority                */ 76910000
*     3  JDYOWFLG  BIT(8),         /* Work selection flags           */ 76920000
*       5  JDYOWBRS  BIT(1),       /* select SYSOUT with BURST=YES      76930000
*                                     (if not set - select              76940000
*                                     BURST=NO)                      */ 76950000
*       5  JDYOWHLD  BIT(1),       /* select output which is held       76960000
*                                     (if not set - select output       76970000
*                                     which is not held)             */ 76980000
*       5  JDYOWODH  BIT(1),       /* select output with                76990000
*                                     OUTDISP=HOLD                   */ 77000000
*       5  JDYOWODK  BIT(1),       /* select output with                77010000
*                                     OUTDISP=KEEP                   */ 77020000
*       5  JDYOWODL  BIT(1),       /* select output with                77030000
