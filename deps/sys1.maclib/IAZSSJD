*/*------------------------------------------------------------------*/ 49750000
* DCL JDDCLCL  FIXED(8) CONSTANT('01'X);  /* Local devices           */ 49760000
* DCL JDDCIFC  FIXED(8) CONSTANT('02'X);  /* Interface devices       */ 49770000
* DCL JDDCINT  FIXED(8) CONSTANT('03'X);  /* Internal devices        */ 49780000
* DCL JDDCNJE  FIXED(8) CONSTANT('04'X);  /* NJE devices             */ 49790000
* DCL JDDCOFLD FIXED(8) CONSTANT('05'X);  /* Offload devices         */ 49800000
