* DCL JDWSUCSN FIXED(8) CONSTANT('0A'X);  /* UCS name                */ 50470000
* DCL JDWSBRST FIXED(8) CONSTANT('21'X);  /* Burst setting           */ 50480000
* DCL JDWSHLDI FIXED(8) CONSTANT('22'X);  /* Hold indicator          */ 50490000
