*% /*                                                                   00400000
.P0      ANOP                                                           00401000
         AIF   ('&ZCBPRINT' EQ 'NO').P1                                 00402000
         AIF   ('&LIST' EQ 'YES').P2                                    00403000
.P1      ANOP                                                           00404000
         PUSH  PRINT                                                    00405000
         PRINT OFF                                                      00406000
.P2      ANOP                                                           00407000
         AIF   ('&SMF98_1_DAT' EQ 'NO').L0002                           00408000
         AIF   ('&DSECT' EQ 'YES').L0003                                00409000
         AIF   ('&SMF98_1_DAT' NE 'YES').L0002                          00410000
.L0003   ANOP                                                           00411000
*                                                                       00412000
*  ------------------------------------------------------------------   00413000
*    Data Section for Supervisor dispatcher data (subtype 1)            00414000
*                                                                       00415000
*    Its location in the SMF record is calculated by (Address of        00416000
*    SMFR98 record + offset value in SMF98DOF).                         00417000
*  ------------------------------------------------------------------   00418000
*                                                                       00419000
*                                                                       00420000
         AIF   ('&DSECT' EQ 'YES').L0004                                00421000
SMF98_1_DAT DS 0F        +00 Subtype 1 record control data              00422000
         AGO   .L0005                                                   00423000
.L0004   ANOP                                                           00424000
SMF98_1_DAT DSECT        +00 Subtype 1 record control data              00425000
.L0005   ANOP                                                           00426000
SMF98_1_DataTripletsNum DS F +00 Number of data triplets that follows   00427000
SMF98_1_DataTripletsLen DS F +04 Length of data triplets area          *00428000
                         SMF98_1_DataTripletsArea                       00429000
SMF98_1_DataTripletsArea DS CL136 +08 Data triplets area                00430000
*                                                                       00431000
*  ------------------------------------------------------------------   00432000
*    Environmental Section                                              00433000
*  ------------------------------------------------------------------   00434000
*                                                                       00435000
         ORG   SMF98_1_DataTripletsArea                                 00436000
SMF98_1_EnvOF DS F       Offset to environmental section from          *00437000
                         beginning of record. Data mapped by:          *00438000
                         SMF98_1_EnvInfo                                00439000
SMF98_1_EnvLN DS H       Length of environmental section                00440000
SMF98_1_EnvON DS H       Number of environmental sections               00441000
*                                                                       00442000
*  ------------------------------------------------------------------   00443000
*    Counter Sections                                                   00444000
*                                                                       00445000
*    - SIGP  Group counters   < z/OS internal IBM counters only >       00446000
