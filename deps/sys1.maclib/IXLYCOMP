*                                                                       00001000
* %DCL IXLYCOMP_INCLUDED CHAR EXT;                                      00002000
* %DEACTIVATE IXLYCOMP_INCLUDED;                                        00003000
* %IF IXLYCOMP_INCLUDED = '' %THEN                                      00004000
* %DO;                                                                  00005000
*/* Macro made bi-lingual on 18014. CBGEN compile date 15295         */ 00006000
*% /*                                                                   00007000
         MACRO                                                          00008000
         IXLYCOMP &DSECT=YES,&LIST=YES,&TITLE=YES,&COMPDATANAME=YES,   *00009000
               &COMPINDEX=COND,&COMPSTRTRL=COND,&COMPSTRHDR=COND,      *00010000
               &COMPSTROBJMAPINDEX=COND,&COMPSTROBJMAP=COND,           *00011000
               &COMPHASHTABLEHDR=COND,&COMPHASHSLOTARRAY=COND,         *00012000
               &COMPHASHELEM=COND,&COMPENTRYCNTL=COND                   00013000
         GBLC  &IXLYCOMP_INCLUDED                                       00014000
         GBLC  &ZCBPRINT                                                00015000
&IXLYCOMP_INCLUDED SETC 'YES'                                           00016000
         AIF   ('&LIST' EQ 'NONE').P0                                   00017000
         AIF   ('&TITLE' EQ 'NO').P5                                    00018000
 TITLE   'IXLYCOMP  - CF Dumping Compdata Record Format Mappings     '  00019000
