.******************************************************************     00304000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00304100
.******************************************************************     00304200
                   ACTR  10000                                          00304300
.*----------------------------------------------------------------***   00304400
.***AH.P02 EXTRA POSITIONALS                                            00304500
                   AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS           00304600
                   AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL           00304700
                   AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL       00304800
                   AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL        00304900
.ZGLISTX1          ANOP  ,                                              00305000
&ZCVAL             SETC  ''          ERROR FIELD                        00305100
&ZAWRK             SETA  0           POSITIONAL NUMBER                  00305200
.ZGLIST2           ANOP  ,                                              00305300
&ZAWRK             SETA  &ZAWRK+1    NEXT POSITIONAL                    00305400
                   AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE           00305500
&ZCVAL             SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                00305600
                   AGO   .ZGLIST2                                       00305700
.*                                                                      00305800
.ZGLIST3           ANOP  ,                                              00305900
&ZCVAL             SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA             00306000
&ZMS               SETC  'POSITIONAL ARGUMENT(S),'                      00306100
&ZMS               SETC  '&ZMS "&ZCVAL"'                                00306200
&ZMS               SETC  '&ZMS CANNOT BE USED.'                         00306300
&ZMS               SETC  '&ZMS NO POSITIONAL ARGUMENTS'                 00306400
                   MNOTE 8,' &ZMS ARE ALLOWED.'                         00306500
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00306600
.ZGLISTX           ANOP  ,                                              00306700
&ZASYSLNDX         SETA  0                                              00306800
.*-------------------------------------------------------------------   00306900
.***AG.MF04 MF=(SLE)                                                    00307000
.ZGMFX             ANOP  ,           END OF FULL SYNTAX CHECK           00307100
.*******************************************************************    00307200
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00307300
.*******************************************************************    00307400
                   AIF   (&ZMACRET NE 0).ZLX2   ERROR DETECTED          00307500
.*******************************************************************    00307600
.***AE.01  START OF IWM4RPT MACRO CODE                                  00307700
.*******************************************************************    00307800
                   LCLC  &M0M0001                 NAME                  00307900
&M0M0001           SETC  '&XLABEL'                SET NAME              00308000
                   AIF   (K'&XLABEL NE 0).XLAB2                         00308100
&M0M0001           SETC  'M00M&SYSNDX'            DEFAULT NAME          00308200
.*                                                                      00308300
.XLAB2             ANOP  ,                                              00308400
                   AIF   ('&XMF' EQ 'L').XLAB3                          00308500
&M0M0001 DS    0H                             IWM4RPT-1                 00308600
.XLAB3             ANOP  ,                                              00308700
.*******************************************************************    00308800
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00308900
.*******************************************************************    00309000
              PUSH  PRINT                                               00309100
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     00309200
                   PRINT OFF         ++ PR01                            00309300
                   AGO   .ZPRNTXX                                       00309400
.ZPRINTX           PRINT ON                                             00309500
.ZPRNTXX           ANOP  ,                                              00309600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00309700
         LCLC  &T1                                                  @P2 00309800
         AIF   ('&XEWLM_CHCORR' EQ '').EWLM1                        @P2 00309900
         AIF   ('&XEWLM_CHCORR' EQ 'NO_EWLM_CHCORR').EWLM1          @P2 00310000
         AIF   ('&XEWLM_CHCTKN' EQ '').EWLM1                        @P2 00310100
         AIF   ('&XEWLM_CHCTKN' EQ 'NO_EWLM_CHCTKN').EWLM1          @P2 00310200
&T1      SETC  '"EWLM_CHCORR" AND "EWLM_CHCTKN"'                    @P2 00310300
&T1      SETC  ' &T1 ARE MUTUALLY EXCLUSIVE.'                       @P2 00310400
         MNOTE 8,'&T1'                                              @P2 00310500
         MEXIT                                                      @P2 00310600
.EWLM1   ANOP  ,                                                    @P2 00310700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00310800
.********************************************************************   00310900
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00311000
.********************************************************************   00311100
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00311200
                      LCLA  &ZOPN1         INVOCATION INDEX             00311300
                      LCLC  &ZOPL(4095)    LABEL                        00311400
                      LCLC  &ZOPO(4095)    OPERATION                    00311500
                      LCLC  &ZOPV(4095)    VALUE                        00311600
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00311700
.*                                                                      00311800
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00311900
.******************************************************************     00312000
.***AL.L10 IWM4RPT FMT=LIST MF=S!L PARAMETERS                           00312100
.******************************************************************     00312200
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00312300
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00312400
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00312500
&XMFATTR              SETC  '0D'                                        00312600
.*                                                                      00312700
&ZOPN                 SETA  &ZOPN+1                                     00312800
&ZOPL(&ZOPN)          SETC  ' '                                         00312900
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00313000
&ZOPV(&ZOPN)          SETC  '4,8'                                       00313100
&ZOPN                 SETA  &ZOPN+1                                     00313200
&ZOPL(&ZOPN)          SETC  ' '                                         00313300
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00313400
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00313500
&ZOPD(&ZOPN)          SETC  '++ IWM4RPT ARG PARMS'                      00313600
.*                                                                      00313700
&ZOPN                 SETA  &ZOPN+1                                     00313800
&ZOPL(&ZOPN)          SETC  ' '                                         00313900
