.*******************************************************************    00425300
.***  PROCESS IWM4RPT OUTPUT PARAMETERS                                 00425400
.*******************************************************************    00425500
                      ACTR  10000                                       00425600
.*******************************************************************    00425700
.***AO.X02  RETURN CODE PROCESSING                                      00425800
.*******************************************************************    00425900
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00426000
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00426100
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00426200
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00426300
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00426400
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00426500
&ZOPN                 SETA  &ZOPN+1                                     00426600
&ZOPL(&ZOPN)          SETC  ' '                                         00426700
&ZOPO(&ZOPN)          SETC  'ST'                                        00426800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00426900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00427000
                      AGO   .ZMRETX                                     00427100
.*                                                                      00427200
.ZMRET2               ANOP  ,                                           00427300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00427400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00427500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00427600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00427700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00427800
&ZOPN                 SETA  &ZOPN+1                                     00427900
&ZOPL(&ZOPN)          SETC  ' '                                         00428000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00428100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00428200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00428300
.ZMRETX               ANOP  ,                                           00428400
.*******************************************************************    00428500
.***AO.X03  REASON CODE PROCESSING                                      00428600
.*******************************************************************    00428700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00428800
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00428900
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00429000
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00429100
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00429200
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00429300
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00429400
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00429500
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00429600
&ZOPN                 SETA  &ZOPN+1                                     00429700
&ZOPL(&ZOPN)          SETC  ' '                                         00429800
&ZOPO(&ZOPN)          SETC  'ST'                                        00429900
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00430000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00430100
                      AGO   .ZMRSNX                                     00430200
.*                                                                      00430300
.ZMRSN2               ANOP  ,                                           00430400
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00430500
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00430600
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00430700
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00430800
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00430900
