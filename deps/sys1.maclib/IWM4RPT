.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00347000
&ZPLLEN               SETA 112                                          00347100
.*                                                                      00347200
                      AIF  (&XPLISTVER LT 1).ZLLEN2Z                    00347300
&ZPLLEN               SETA 136                                          00347400
.ZLLEN2Z              ANOP  ,                                           00347500
.*                                                                      00347600
.******************************************************************     00347700
.***AI.L01 IWM4RPT FMT=LIST FILL IN PARAMETER AREA                      00347800
.******************************************************************     00347900
                      ACTR  10000                                       00348000
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00348100
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00348200
&ZCVAL                SETC  'LAE'          XAX MODE                     00348300
.ZIMFXA               ANOP ,                                            00348400
.*                                                                      00348500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00348600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00348700
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00348800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00348900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00349000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00349100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00349200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00349300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00349400
&ZOPN                 SETA  &ZOPN+1                                     00349500
&ZOPL(&ZOPN)          SETC  ' '                                         00349600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00349700
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00349800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00349900
                      AGO   .ZIMFNX                                     00350000
.*                                                                      00350100
.ZIMFN2               ANOP ,                                            00350200
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00350300
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00350400
&ZOPN                 SETA  &ZOPN+1                                     00350500
&ZOPL(&ZOPN)          SETC  ' '                                         00350600
&ZOPO(&ZOPN)          SETC  'LAE'                                       00350700
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00350800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00350900
