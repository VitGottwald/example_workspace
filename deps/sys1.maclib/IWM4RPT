.***AC.PLV01  PROCESS PLISTVER                                      *   00119300
.********************************************************************   00119400
&XPLISTVER      SETA  0                                                 00119500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00119600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00119700
&XPLISTVER      SETA  1                                                 00119800
.ZPLV0          ANOP  ,                                                 00119900
.*                                                                      00120000
                AIF   ('&CPUTIME' EQ '').ZPLV1E                         00120100
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00120200
&XPLISTVER      SETA  1                                                 00120300
.ZPLV1E         ANOP  ,                                                 00120400
.*                                                                      00120500
                AIF   ('&TIMEONCP' EQ '').ZPLV2E                        00120600
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00120700
&XPLISTVER      SETA  1                                                 00120800
.ZPLV2E         ANOP  ,                                                 00120900
.*                                                                      00121000
                AIF   ('&OFFLOADONCP' EQ '').ZPLV3E                     00121100
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00121200
&XPLISTVER      SETA  1                                                 00121300
.ZPLV3E         ANOP  ,                                                 00121400
.*                                                                      00121500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00121600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00121700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00121800
.*-------------------------------------------------------------------   00121900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00122000
&ZMS            SETC  '"PLISTVER='                                      00122100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00122200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00122300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00122400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00122500
                AGO   .ZPVCK2                                           00122600
.ZPVCK3         ANOP  ,                                                 00122700
.*-------------------------------------------------------------------   00122800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00122900
&ZMS            SETC  '"PLISTVER='                                      00123000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00123100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00123200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00123300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00123400
                AGO   .ZPVCKE                                           00123500
.ZPVCK2         ANOP  ,                                                 00123600
.*-------------------------------------------------------------------   00123700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00123800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00123900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00124000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00124100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00124200
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00124300
.ZPVCK6         ANOP  ,                                                 00124400
&ZMS            SETC  '"PLISTVER='                                      00124500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00124600
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00124700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00124800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00124900
&XPLISTVER      SETA  1                                                 00125000
                AGO   .ZPVCKE                                           00125100
.*-------------------------------------------------------------------   00125200
.ZPVCK4A        ANOP  ,                                                 00125300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00125400
&ZMS            SETC  'PLISTVER='                                       00125500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00125600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00125700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00125800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00125900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00126000
                AGO   .ZPVCK5                                           00126100
.ZPVCK4         ANOP  ,                                                 00126200
&XPLISTVER      SETA  &PLISTVER(1)                                      00126300
.ZPVCK5         ANOP  ,                                                 00126400
.ZPVCKE         ANOP  ,                                                 00126500
.********************************************************************   00126600
