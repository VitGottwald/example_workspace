                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00431100
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00431200
&ZOPN                 SETA  &ZOPN+1                                     00431300
&ZOPL(&ZOPN)          SETC  ' '                                         00431400
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00431500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00431600
