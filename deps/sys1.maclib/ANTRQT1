.***AC.PLV01  PROCESS PLISTVER                                      *   00554400
.********************************************************************   00554500
&XPLISTVER      SETA  0                                                 00554600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00554700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00554800
&XPLISTVER      SETA  4                                                 00554900
.ZPLV0          ANOP  ,                                                 00555000
.*                                                                      00555100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00555200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00555300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00555400
.*-------------------------------------------------------------------   00555500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00555600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00555700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00555800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00555900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00556000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00556100
                AGO   .ZPVCK2                                           00556200
.ZPVCK3         ANOP  ,                                                 00556300
.*-------------------------------------------------------------------   00556400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00556500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00556600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00556700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00556800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00556900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00557000
                AGO   .ZPVCKE                                           00557100
.ZPVCK2         ANOP  ,                                                 00557200
.*-------------------------------------------------------------------   00557300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00557400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00557500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00557600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00557700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00557800
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00557900
.ZPVCK6         ANOP  ,                                                 00558000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00558100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00558200
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00558300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00558400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00558500
&XPLISTVER      SETA  4                                                 00558600
                AGO   .ZPVCKE                                           00558700
.*-------------------------------------------------------------------   00558800
.ZPVCK4A        ANOP  ,                                                 00558900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00559000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00559100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00559200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00559300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00559400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00559500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00559600
                AGO   .ZPVCK5                                           00559700
.ZPVCK4         ANOP  ,                                                 00559800
&XPLISTVER      SETA  &PLISTVER(1)                                      00559900
.ZPVCK5         ANOP  ,                                                 00560000
.ZPVCKE         ANOP  ,                                                 00560100
.********************************************************************   00560200
