.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               02813400
.*******************************************************************    02813500
                      ACTR  10000                                       02813600
.*                                                                      02813700
                      LCLA  &ZLC           COLUMN POSITION              02813800
                      LCLA  &ZOPX          LIST TABLE INDEX             02813900
                      LCLC  &ZLL           LABEL                        02814000
                      LCLC  &ZLOP          OPERATION                    02814100
                      LCLC  &ZLV           VALUE                        02814200
                      LCLC  &ZLD           DESCRIPTION                  02814300
.*                                                                      02814400
&ZOPX                 SETA  1                                           02814500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            02814600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              02814700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              02814800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              02814900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              02815000
                      AIF   (K'&ZLD LE 0).ZL5                           02815100
&ZLC                  SETA  35             COMMENT COLUMN               02815200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    02815300
&ZLC                  SETA  33             COMMMENT COLUMN              02815400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   02815500
&ZLV                  SETC  '&ZLV'.' '                                  02815600
                      AGO   .ZL3                                        02815700
.*                                                                      02815800
.ZL4                  ANOP  ,                                           02815900
&ZLV                  SETC  '&ZLV'.' '                                  02816000
.ZL5                  ANOP  ,                                           02816100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               02816200
.*                                                                      02816300
&ZLL     &ZLOP &ZLV                                                     02816400
.*                                                                      02816500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               02816600
                      AGO   .ZL2           CONTINUE LOOPING             02816700
.*                                                                      02816800
.ZLX                  ANOP  ,                                           02816900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                02817000
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                02817100
