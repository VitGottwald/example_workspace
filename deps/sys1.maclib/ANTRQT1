*01* EXAMPLES:                                                        * 00491900
*                                                                     * 00492000
*    Operation:  Getting the current consistency time for             * 00492100
*    XRC session TUCSON in assembler.                                 * 00492200
*                                                                     * 00492300
*               TITLE 'TESTER: Test ANTRQST'                          * 00492400
*      TESTER   CSECT ,                                               * 00492500
*               USING *,15                                            * 00492600
*               B     @PROLOG                                         * 00492700
*               DC    AL1(16)                                         * 00492800
*               DC    C'TESTER    97.133'                             * 00492900
*               DROP  15                                              * 00493000
*      @PROLOG  STM   14,12,12(13)                                    * 00493100
*               LR    12,15                                           * 00493200
*      @PSTART  EQU   TESTER                                          * 00493300
*               USING @PSTART,12                                      * 00493400
*               LA    15,0                                            * 00493500
*               L     0,@SIZDATD+4                                    * 00493600
*               GETMAIN  RU,LV=(0),SP=(15)                            * 00493700
*               LR    15,13                                           * 00493800
*               LR    13,01                                           * 00493900
*               USING @DATD,13                                        * 00494000
*               ST    15,4(,13)                                       * 00494100
*               ST    13,8(,15)                                       * 00494200
*               LM    15,01,16(15)                                    * 00494300
*               MVC   XRCSID,=CL8'TUCSON  '                           * 00494400
*               ANTRQST ILK=VERSION1,                X                * 00494500
*                     REQUEST=XCONTIME,              X                * 00494600
*                     SID=XRCSID,                    X                * 00494700
*                     TIME=CONTIME,                  X                * 00494800
*                     RETINFO=XRETINFO,              X                * 00494900
*                     RETCODE=RTNCD,                 X                * 00495000
*                     RSNCODE=RSNCD,                 X                * 00495100
*                     MF=(E,P_LIST)                                   * 00495200
*               CLC   RTNCD,=F'0'                                     * 00495300
*               BE    SKIP1                                           * 00495400
*               MVC   RTCD,RTNCD                                      * 00495500
*               MVC   RSCD,RSNCD                                      * 00495600
*               B     SKIP2                                           * 00495700
*      SKIP1    DS    0H                                              * 00495800
*               MVC   RTCD,RTC                                        * 00495900
*               MVC   RSCD,RSC                                        * 00496000
*      SKIP2    DS    0H                                              * 00496100
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 00496200
*               BNE   SKIP3                                           * 00496300
*               WTO   'ANTAS000 NOT ACTIVE'                           * 00496400
*      SKIP3    DS    0H                                              * 00496500
*               L     2,RTCD                                          * 00496600
*               L     3,RSCD                                          * 00496700
*               LR    1,13                                            * 00496800
*               L     13,4(,13)                                       * 00496900
*               LA    15,0                                            * 00497000
*               L     0,@SIZDATD+4                                    * 00497100
*               FREEMAIN RU,LV=(0),A=(1),SP=(15)                      * 00497200
*               LR    15,2                                            * 00497300
*               LR    0,3                                             * 00497400
*               L     14,12(,13)                                      * 00497500
*               LM    1,12,24(13)                                     * 00497600
*               BR    14                                              * 00497700
*               LTORG                                                 * 00497800
*      @SIZDATD DS    0A                                              * 00497900
*               DC    AL1(0)                                          * 00498000
*               DC    AL3(@DYNSIZE)                                   * 00498100
*      @DATD    DSECT                                                 * 00498200
*               DS    18F                                             * 00498300
*      RTNCD    DS    F                                               * 00498400
*      RSNCD    DS    F                                               * 00498500
*      RTCD     DS    F                                               * 00498600
*      RSCD     DS    F                                               * 00498700
*      XRCSID   DS    CL8                                             * 00498800
*      CONTIME  DS    CL8                                             * 00498900
