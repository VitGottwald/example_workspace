.***AL.L12 ANTRQT1 FMT=LIST MF=S!L PARAMETERS                           02594000
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         02594100
.*                                                                      02594200
&ZOPN                 SETA  &ZOPN+1                                     02594300
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                02594400
&ZOPO(&ZOPN)          SETC  'DC'                                        02594500
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   02594600
&ZOPD(&ZOPN)          SETC  '++ ANTRQT1 ARG PARMS'                      02594700
.*                                                                      02594800
.ZLMFSLX              ANOP  ,                                           02594900
.*-------------------------------------------------------------------   02595000
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          02595100
&ZPLLEN               SETA 320                                          02595200
.*                                                                      02595300
.******************************************************************     02595400
.***AI.L01 ANTRQT1 FMT=LIST FILL IN PARAMETER AREA                      02595500
.******************************************************************     02595600
                      ACTR  10000                                       02595700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               02595800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    02595900
&ZCVAL                SETC  'LAE'          XAX MODE                     02596000
.ZIMFXA               ANOP ,                                            02596100
.*                                                                      02596200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    02596300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         02596400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 02596500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                02596600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 02596700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                02596800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   02596900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    02597000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   02597100
&ZOPN                 SETA  &ZOPN+1                                     02597200
&ZOPL(&ZOPN)          SETC  ' '                                         02597300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    02597400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              02597500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       02597600
                      AGO   .ZIMFNX                                     02597700
.*                                                                      02597800
.ZIMFN2               ANOP ,                                            02597900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     02598000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     02598100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  02598200
&ZOPN                 SETA  &ZOPN+1                                     02598300
&ZOPL(&ZOPN)          SETC  ' '                                         02598400
&ZOPO(&ZOPN)          SETC  'LAE'                                       02598500
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  02598600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       02598700
                      AGO   .ZIMFNX                                     02598800
.ZIMFNX1              ANOP ,                                            02598900
&ZOPN                 SETA  &ZOPN+1                                     02599000
&ZOPL(&ZOPN)          SETC  ' '                                         02599100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    02599200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                02599300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       02599400
.ZIMFNX               ANOP ,                                            02599500
.********************************************************************   02599600
.***AI.L02 ANTRQT1 FMT=LIST INITIALIZE PARAMETER AREA                   02599700
.********************************************************************   02599800
.***AI.L03 MF=(SLME)                                                    02599900
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      02600000
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            02600100
&ZAWRK                SETA  0                                           02600200
&ZN                   SETA  &ZPLLEN                                     02600300
.ZCLRCONT             AIF   (&ZN LE 256).ZCLRX                          02600400
.*                                                                      02600500
&ZOPN                 SETA  &ZOPN+1                                     02600600
&ZOPL(&ZOPN)          SETC  ' '                                         02600700
&ZOPO(&ZOPN)          SETC  'XC'                                        02600800
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(256,1),&ZAWRK.(1)'                 02600900
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             02601000
.*                                                                      02601100
&ZAWRK                SETA &ZAWRK+256                                   02601200
&ZN                   SETA  &ZN-256                                     02601300
                      AGO   .ZCLRCONT                                   02601400
.*                                                                      02601500
.ZCLRX                ANOP  ,                                           02601600
.*                                                                      02601700
&ZOPN                 SETA  &ZOPN+1                                     02601800
&ZOPL(&ZOPN)          SETC  ' '                                         02601900
&ZOPO(&ZOPN)          SETC  'XC'                                        02602000
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(&ZN,1),&ZAWRK.(1)'                 02602100
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             02602200
.ZIMFLX               ANOP  ,                                           02602300
.*-------------------------------------------------------------------   02602400
.***AI.L60 XEYECATCH CL8                                                02602500
&ZOPN                 SETA  &ZOPN+1                                     02602600
&ZOPL(&ZOPN)          SETC  ' '                                         02602700
&ZOPO(&ZOPN)          SETC  'MVC'                                       02602800
&ZOPV(&ZOPN)          SETC  '0(8,1),=CL8''ANTRQSTL'''                   02602900
&ZOPD(&ZOPN)          SETC  '++ CL8 CONSTANT'                           02603000
.*-------------------------------------------------------------------   02603100
.***AI.L60 XEYEVERSN CL4                                                02603200
&ZOPN                 SETA  &ZOPN+1                                     02603300
&ZOPL(&ZOPN)          SETC  ' '                                         02603400
