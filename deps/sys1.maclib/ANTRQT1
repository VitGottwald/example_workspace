                      AIF   (&SYSALVL NE 0).ZALVL6A                     02512600
&ZOPO(&ZOPN)          SETC  'B'                                         02512700
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                02512800
                      AGO   .ZALVL6B                                    02512900
.ZALVL6A              ANOP                                              02513000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      02513100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              02513200
.ZALVL6B              ANOP                                              02513300
&ZOPD(&ZOPN)          SETC  '++ ANTRQT1 ARG PARMS'                      02513400
.*                                                                      02513500
