.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00189100
.*******************************************************************    00189200
                      ACTR  10000                                       00189300
.*                                                                      00189400
                      LCLA  &ZLC           COLUMN POSITION              00189500
                      LCLA  &ZOPX          LIST TABLE INDEX             00189600
                      LCLC  &ZLL           LABEL                        00189700
                      LCLC  &ZLOP          OPERATION                    00189800
                      LCLC  &ZLV           VALUE                        00189900
                      LCLC  &ZLD           DESCRIPTION                  00190000
.*                                                                      00190100
&ZOPX                 SETA  1                                           00190200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00190300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00190400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00190500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00190600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00190700
                      AIF   (K'&ZLD LE 0).ZL5                           00190800
&ZLC                  SETA  35             COMMENT COLUMN               00190900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00191000
&ZLC                  SETA  33             COMMMENT COLUMN              00191100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00191200
&ZLV                  SETC  '&ZLV'.' '                                  00191300
                      AGO   .ZL3                                        00191400
.*                                                                      00191500
.ZL4                  ANOP  ,                                           00191600
&ZLV                  SETC  '&ZLV'.' '                                  00191700
.ZL5                  ANOP  ,                                           00191800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00191900
.*                                                                      00192000
&ZLL     &ZLOP &ZLV                                                     00192100
.*                                                                      00192200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00192300
                      AGO   .ZL2           CONTINUE LOOPING             00192400
.*                                                                      00192500
.ZLX                  ANOP  ,                                           00192600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00192700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00192800
         LCLC  &T1                                                      00192900
         AIF   ('&XRESOURCE_TYPE' EQ 'NULL').FINIS                      00193000
.*                                                                      00193100
&T1      SETC  '&XRES_DATA_EXIT@'                                       00193200
         AIF   ('&XRES_DATA_EXIT@'(1,1) NE '(').EXITRS                  00193300
&T1      SETC  '&XRES_DATA_EXIT@'(2,K'&XRES_DATA_EXIT@-2)               00193400
&T1      SETC  '0(&T1.)'                                                00193500
.EXITRS    ANOP                                                         00193600
.* &T1 IS NOW A VALID BASE-DISPLACEMENT EXPRESSION                      00193700
.*                                                                      00193800
         L     15,&T1                  FETCH DATA COLLECTION EXIT @     00193900
         BALR  14,15                                                    00194000
.*                                                                      00194100
.FINIS   ANOP                                                           00194200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00194300
.ZLINKX               ANOP  ,                                           00194400
.*******************************************************************    00194500
.***AP.X01  FORMAT GENERATED CODE                                       00194600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00194700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00194800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00194900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00195000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00195100
                      AIF   (K'&ZLD LE 0).ZL51                          00195200
&ZLC                  SETA  35             COMMENT COLUMN               00195300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00195400
&ZLC                  SETA  33             COMMENT COLUMN               00195500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00195600
&ZLV                  SETC  '&ZLV'.' '                                  00195700
                      AGO   .ZL31                                       00195800
.*                                                                      00195900
.ZL41                 ANOP  ,                                           00196000
&ZLV                  SETC  '&ZLV'.' '                                  00196100
.ZL51                 ANOP  ,                                           00196200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00196300
.*                                                                      00196400
&ZLL     &ZLOP &ZLV                                                     00196500
.*                                                                      00196600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00196700
                      AGO   .ZL21          CONTINUE LOOPING             00196800
.*                                                                      00196900
.ZLX1                 ANOP  ,                                           00197000
