**/%IECENF64_PLX2:;                                                     03050001
* Declare                                                               03060001
*%IF E64LSS ^= '' %THEN                                                 03070001
*  %GOTO E64LBL1;                                                       03080001
*   1 Enf64 Char(128)  Based Bdy(Word), /* DFSMS ENF 64 Record       */ 03090001
* %GOTO E64LBL2;                                                        03100001
*%E64LBL1:;                                                             03110001
*   1 Enf64 Char(5708) Based Bdy(Word), /* DFSMS ENF 64 Record   @MTc*/ 03120001
*%E64LBL2:;                                                             03130001
*    2 Enf64_Event_Header,                                    /* @05a*/ 03140001
*      3 Enf64_Id        Char(5),   /* Eyecatcher 'ENF64'            */ 03150001
*      3 Enf64_Version   Fixed(8),  /* Version number                */ 03160001
*      3 Enf64_Component Char(5),   /* Component acronym 'DF111'     */ 03170001
*      3 *               Char(1),   /* Reserved                      */ 03180001
*      3 Enf64_Qual      Bit(32),   /* Qualifier of this ENF event   */ 03190001
*        4 Enf64_Qual1   Fixed(8),  /* High level qualifier          */ 03200001
*        4 Enf64_Qual2   Fixed(8),  /* Reserved                      */ 03210001
*        4 Enf64_Qual3   Fixed(8),  /* Reserved                      */ 03220001
*        4 Enf64_Qual4   Fixed(8),  /* Low  level qualifier          */ 03230001
*      3 Enf64_Len       Fixed(16), /* Length of this area           */ 03240001
*      3 Enf64_Flag1     Fixed(8),  /* Flags                         */ 03250001
*        4 Enf64_SFI_Serial_Short Bit(1), /* When this bit = 1, the     03260001
*                                   SFI Serial fields CTYP, MANU, MANL, 03270001
*                                   and 7 left-most digits of SEQN      03280001
*                                   are set to x'00'.  Only the 5 right 03290001
*                                   most SEQN digits are valid.         03300001
*                                   When this bit = 0, the entire SFI   03310001
*                                   Serial number is valid.      @06a*/ 03320001
*      3 *               Char(11),  /* Reserved                  @07c*/ 03330001
*      3 Enf64_SubSysID CHAR(2),    /* Subsystem ID              @07a*/ 03340001
*      3 Enf64_Volser Char(6),      /* volume serial                 */ 03350001
*                                   /* Note: Not valid for LSS events*/ 03360001
*      /* The combination of the SFI_Serial, LSS, and Volume number  */ 03370001
*      /* provides a world-wide unique volume identifier.            */ 03380001
*      3 Enf64_SFI_Serial Char(23), /* Stg Facility Image serial     */ 03390001
*        4 Enf64_SFI_Serial_CTYP Char(6), /* Machine Type            */ 03400001
*        4 Enf64_SFI_Serial_MANU Char(3), /* Manufacturer ID         */ 03410001
*        4 Enf64_SFI_Serial_MANL Char(2), /* Manufacturer loc        */ 03420001
*        4 Enf64_SFI_Serial_SEQN Char(12),/* Sequence number         */ 03430001
*      3 Enf64_LSS    Char(1),      /* Logical Subsystem ID          */ 03440001
*      3 *            Char(1),      /* reserved                      */ 03450001
*      3 Enf64_VOLN   Char(1),      /* volume number within Lss      */ 03460001
*    2 Enf64_EventData UNION,     /* Identify source of event        */ 03470001
*      3 Enf64_OutofSpaceEvents,    /* Out of Space events       @SCA*/ 03480001
*                                   /* Valid if QUAL1 = 03       @SCA*/ 03490001
*          5 Enf64_PercentEmpty Fixed(8), /* Percentage empty of @SCA*/ 03500001
*                                         /* the SGC backup vol  @SCA*/ 03510001
*          5 *                  Fixed(8), /* Reserved            @SCA*/ 03520001
*          5 Enf64_ExtPoolID   Fixed(16), /* Extent Pool ID      @SCA*/ 03530001
*          5 Enf64_Token       Fixed(16), /* Event id token      @SCA*/ 03540001
*          5 Enf64_SrcVolAddr  Fixed(16), /* Source Volume Addr  @SCA*/ 03550001
*                                         /* of SGC backup volume@SCA*/ 03560001
*          5 Enf64_SeqNum Char(4),  /* Sequence # of consistency @SCA*/ 03570001
*                                   /* group which was removed   @SCA*/ 03580001
*        /* End of information for Out of Space events.          @SCA*/ 03590001
*      3 Enf64_VolumeEvents,      /* Volume-specific events.         */ 03600001
*                                 /* Valid if QUAL1 = 01             */ 03610001
*          5 Enf64_VX_Cyls Fixed(32),  /* new number of cylinders    */ 03620001
*          5 *              Char(12),  /* Reserved               @02a*/ 03630001
*          /* End of common volume data.                             */ 03640001
*          5 Enf64_VolEventData UNION, /* event data unique to volume*/ 03650001
*           6 Enf64_DSFEvents,         /* Events issued by ICKDSF@02a*/ 03660001
*             7 Enf64_DSF_Flags Bit(32), /* Reserved             @02a*/ 03670001
*             7 Enf64_VTOC_Loc Char(4),  /* VTOC Location        @02a*/ 03680001
*          /* End of information for ICKDSF events.              @02a*/ 03690001
*                                                                       03700001
*%IF E64LSS ^= '' %THEN                                                 03710001
*  %GOTO E64LBL3;                                                       03720001
*    2 * ;                        /* End of ENF64                    */ 03730001
*%GOTO E64LBL4;                                                         03740001
*%E64LBL3:;                                                   /* @SEA*/ 03750001
*      3 Enf64_LssEvents,           /* Lss events                    */ 03760001
*                                   /* Valid if QUAL1 = 02           */ 03770001
*        4 Enf64_UCBArea,                                     /* @05a*/ 03780001
*          5 Enf64_UCBInfo(256) Char(4) /* UCB Info in subsystem @03c*/ 03790001
*                Bdy(Word),                                             03800001
*           6 Enf64_UCBDevNum Fixed(16), /* binary UCB device number */ 03810001
*           6 Enf64_UCBInfoValid Bit(1), /* DevNum and SSID valid@03c*/ 03820001
*           6 *                  Bit(5), /* Reserved             @03c*/ 03830001
