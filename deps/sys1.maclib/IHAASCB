*      2 ASCBGXL  PTR(31),        /* ADDRESS OF GLOBALLY LOADED                 
*                                    MODULE EXTENT INFORMATION                  
*                                    CONTROL BLOCK.                             
*                                    SERIALIZATION - LOCAL LOCK                 
*                                    OWNERSHIP - CONTENTS SUPERVISION           
*                                                  (MDC388)  @G381P9A*/         
*      2 ASCBEATT CHAR(8) BDY(DWORD), /* EXPENDED AND ACCOUNTED                 
*                                    TASK TIME.                                 
*                                    SERIALIZATION - N/A.                       
*                                    OWNERSHIP - SCHEDULER.                     
*                                                  (MDC387)  @G381P9A*/         
*      2 ASCBINTS CHAR(8) BDY(DWORD), /* JOB SELECTION TIME STAMP.              
*                                    SERIALIZATION - N/A.                       
*                                    OWNERSHIP - SCHEDULER.                     
*                                                  (MDC387)  @G381P9A*/         
*      2 ASCBFW4 FIXED(32),       /* FULLWORD LABEL TO ADDRESS BITS             
*                                    IN THIS WORD.                              
*                                    SERIALIZATION - LOCAL LOCK.                
*                                                  (MDC389)  @G381P9A*/         
*       3 ASCBLL1 BIT(8),         /* FIRST BYTE OF FLAGS.                       
*                                    SERIALIZATION - LOCAL LOCK.                
*                                                  (MDC389)  @G381P9A*/         
*        4 ASCBSSPC BIT(1),       /* STATUS STOP TASKS PENDING A                
*                                    CML LOCK RELEASE.                          
*                                                  (MDC389)  @G381P9A*/         
*        4 *        BIT(7),       /* RESERVED.                   @D6C*/         
*       3 ASCBLL2 BIT(8),         /* SECOND BYTE OF FLAGS.                      
*                                    SERIALIZATION - LOCAL LOCK.                
*                                                  (MDC389)  @G381P9A*/         
*       3 ASCBLL3 BIT(8),         /* THIRD  BYTE OF FLAGS.                      
*                                    SERIALIZATION - LOCAL LOCK.                
*                                                  (MDC389)  @G381P9A*/         
*       3 ASCBLL4 BIT(8),         /* FOURTH BYTE OF FLAGS.                      
*                                    SERIALIZATION - LOCAL LOCK.                
*                                                  (MDC389)  @G381P9A*/         
*        4 ASCBNRLL BIT(1),       /* No Release of Local Lock OK. Set           
*                                    only after obtaining LL.    @LRA*/         
*        4 *        BIT(5),       /* RESERVED.                   @LRC*/         
*        4 ASCBTYP1 BIT(1),       /* TYPE 1 SVC HAS CONTROL.  THIS              
*                                    OFFSET FIXED BY ARCHITECTURE.              
*                                                            @G860PXK*/         
*        4 *        BIT(1),       /* RESERVED.               @G860PXK*/         
*      2 ASCBRCMS PTR(31),        /* ADDRESS OF THE REQUESTED CMS               
*                                    CLASS LOCK FOR WHICH THE LOCAL             
*                                    LOCK HOLDER IS SUSPENDED.                  
*                                    SERIALIZATION - LOCAL LOCK.                
*                                    OWNERSHIP - SUPERVISOR.                    
*                                                  (MDC391)  @G381P9A*/         
*      2 ASCBIOSC FIXED(32),      /* I/O SERVICE MEASURE.                       
*                                    SERIALIZATION - CS.                        
*                                    UPDATED BY JES2,JES3,SMF.                  
*                                    READ BY RMF,SMF,SRM.    @G381PXU*/         
*      2 ASCBPKML FIXED(16),      /* PKM OF LAST TASK DISPATCHED                
*                                    IN THIS ADDRESS SPACE.                     
*                                    SERIALIZATION - NONE.                      
*                                    OWNERSHIP - DISPATCHER. @G387PXH*/         
*      2 ASCBXCNT FIXED(15),      /* EXCP COUNT FIELD.                          
*                                    SERIALIZATION - LOCAL LOCK.                
*                                    OWNERSHIP - EXCP.           @P1C*/         
*      2 ASCBNSQA PTR(31),        /* ADDRESS OF THE SQA RESIDENT                
*                                    NSSA CHAIN.                                
*                                    SERIALIZATION - DISPATCHER LOCK.           
*                                    OWNERSHIP - DISPATCHER  @G38BPXH*/         
*      2 ASCBASM  PTR(31),        /* ADDRESS OF THE ASM HEADER.                 
*                                    SERIALIZATION - NONE.                      
*                                    OWNERSHIP - ASM.            @L2A*/         
*      2 ASCBASSB PTR(31),        /* POINTER TO ADDRESS SPACE                   
*                                    SECONDARY BLOCK (ASSB).     @D6C*/         
*      2 ASCBTCME PTR(31),        /* POINTER TO TCXTB.                          
*                                    OWNERSHIP - TCAM.           @D1A*/         
*      2 ASCBGQIR PTR(31),        /* ISGQSCAN INFORMATION ROUTINE               
*                                    ADDRESS.                                   
*                                    SERIALIZATION - COMPARE AND SWAP           
*                                    OWNERSHIP - GRS             @O3A*/         
*       3 *      BIT(8),          /* BYTE 0 OF ASCBGQIR          @O3A*/         
*        4 ASCBGQAB    BIT(1),    /* ISGQSCAN INFORMATION ROUTINE               
*                                    ABENDED FLAG. IF ON, ISGQSCAN              
*                                    INFORMATION ROUTINE UNEXPECTEDLY           
*                                    ABENDED.                    @O3A*/         
*        4 *           BIT(7),    /*                             @O3A*/         
*       3 *      BIT(16),         /* BYTES 1 AND 2 OF ASCBGQIR                  
*                                                                @O3A*/         
*       3 ASCBGQI3 BIT(8),        /* BYTE 3 OF ASCBGQIR          @O3A*/         
*        4 *           BIT(7),    /*                             @O3A*/         
*        4 ASCBGQDS BIT(1),       /* ISGQSCAN INFORMATION ROUTINE               
*                                    DISABLED FLAG. IF ON, ISGQSCAN             
*                                    WILL NOT INVOKE THE ISGQSCAN               
*                                    INFORMATION ROUTINE.        @O3A*/         
*      2 ASCBLSQE  FIXED(31),     /* Number of Enclave TCBs that are on         
*                                    a Local lock suspend queue.                
*                                    Ownership: Task Management                 
*                                    Serialization: Compare and Swap            
*                                                                @LCA*/         
*      2 ASCBIOSX CHAR(8),        /* I/O service measure extended.              
*                                    This is like ASCBIOSC but it               
*                                    is extended to 8 bytes, so                 
*                                    its value continues to grow                
*                                    past the 4GB ASCBIOSC maximum              
*                                    capacity.                                  
*                                    Serialization - CSG         @0AC*/         
*      2 ASCBR168 CHAR(1),        /* RESERVED.                   @0IC*/         
*      2 * CHAR(1),                                           /* @0IA*/         
* %IF SYSVERSION = '' %THEN %GOTO ASCB16A;                    /* @0IA*/         
* %IF SYSRULESMOD='PL/S-III ' %THEN %GOTO ASCB16A;            /* @0IA*/         
*                                                                               
*        3 ASCBDiag169 Char(1),   /* IBM use only                @0IA*/         
* %ASCB16A:;                                                  /* @0IA*/         
*      2 ASCBSVCN CHAR(2),        /* SVC number for type 1 SVC.                 
*                                    Serialization - local lock  @LOA*/         
*      2 ASCBRSME PTR(31),        /* POINTER TO RSM ADDRESS SPACE               
*                                    BLOCK EXTENSION.                           
*                                    SERIALIZATION: RSMAD LOCK                  
*                                    OWNERSHIP: RSM              @D5A*/         
*      2 ASCBAVM  BIT(32) BDY(WORD), /* AVAILABILITY MANAGER ADDRESS            
*                                    SPACE RELATED DATA.                        
*                                    SERIALIZATION: CS.                         
*                                    OWNERSHIP: AVM.             @01C*/         
*        3 ASCBAVM1 BIT(8),       /* FIRST BYTE OF ASCBAVM. - RESERVED.         
*                                                                @01A*/         
*        3 ASCBAVM2 BIT(8),       /* SECOND BYTE OF ASCBAVM.                    
*                                    RESERVED.                   @01A*/         
*        3 ASCBAGEN FIXED(16),    /* AVM ASID REUSE GENERATION NUMBER.          
*                                                                @01A*/         
*      2 ASCBARC  FIXED(32),      /* REASON CODE ON MEMTERM.                    
*                                    SERIALIZATION - N/A.                       
*                                    OWNERSHIP - RTM.        @G860PXK*/         
*      2 ASCBRSM  PTR(31),        /* ADDRESS OF RSM'S CONTROL BLOCK             
*                                    HEADER.                 @G860PVB*/         
*       3 ASCBRSMA PTR(31),       /* ADDRESS OF RSM'S CONTROL BLOCK             
*                                    HEADER.                 @G860PVB*/         
*      2 ASCBDCTI FIXED(31),      /* ACCUMULATED CHANNEL CONNECT TIME           
*                                    INCURRED BY THIS MEMORY.@G860PVB*/         
*      2 ASCBEND  CHAR(0) BDY(DWORD); /* END OF ASCB                 */         
*DCL ASCBVS00 FIXED(8) CONSTANT( 0); /* HBB2102 (NOT IN BASE)@ZA68643*/         
*DCL ASCBVS01 FIXED(8) CONSTANT( 1); /* JBB2110                  @D3A*/         
*DCL ASCBVS02 FIXED(8) CONSTANT( 2); /* JBB2133                  @H1A*/         
*DCL ASCBVS03 FIXED(8) CONSTANT( 3); /* HBB4410                  @L7A*/         
*DCL ASCBVERS FIXED(8) CONSTANT( 3); /* LEVEL OF THIS MAPPING    @L7C*/         
*DCL ASCBDSTZ FIXED(8) CONSTANT(64); /*  Bit constant (x'40') for bit           
*                                    position ASCBDSTK.  PL/X cannot            
*                                    map bit positions in generated             
*                                    positions in generated code, so            
*                                    this bit constant allows PL/X to           
*                                    set the bit in generated                   
*                                    assembler.  Note that the decimal          
*                                    value for the constant (64) is             
*                                    used here instead of x'40',                
*                                    because PL/AS does not tolerate            
*                                    a hex constant here.        @P8C*/         
*  @LISTPOP;                                                   /*@D6A*/         
