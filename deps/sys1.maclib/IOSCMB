.*******************************************************************    00157800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00157900
.*******************************************************************    00158000
                      ACTR  10000                                       00158100
.*                                                                      00158200
                      LCLA  &ZLC           COLUMN POSITION              00158300
                      LCLA  &ZOPX          LIST TABLE INDEX             00158400
                      LCLC  &ZLL           LABEL                        00158500
                      LCLC  &ZLOP          OPERATION                    00158600
                      LCLC  &ZLV           VALUE                        00158700
                      LCLC  &ZLD           DESCRIPTION                  00158800
.*                                                                      00158900
&ZOPX                 SETA  1                                           00159000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00159100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00159200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00159300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00159400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159500
                      AIF   (K'&ZLD LE 0).ZL5                           00159600
&ZLC                  SETA  35             COMMENT COLUMN               00159700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00159800
&ZLC                  SETA  33             COMMMENT COLUMN              00159900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00160000
&ZLV                  SETC  '&ZLV'.' '                                  00160100
                      AGO   .ZL3                                        00160200
.*                                                                      00160300
.ZL4                  ANOP  ,                                           00160400
&ZLV                  SETC  '&ZLV'.' '                                  00160500
.ZL5                  ANOP  ,                                           00160600
