.***AE.01  START OF IOSCMB MACRO CODE                                   00109600
.*******************************************************************    00109700
              LCLC  &M0M0001                 NAME                       00109800
&M0M0001      SETC  '&XLABEL'                SET NAME                   00109900
              AIF   (K'&XLABEL NE 0).XLAB2                              00110000
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00110100
.*                                                                      00110200
.XLAB2        ANOP  ,                                                   00110300
              AIF   ('&XMF' EQ 'L').XLAB3                               00110400
&M0M0001 DS    0H                             IOSCMB-0                  00110500
.XLAB3        ANOP  ,                                                   00110600
.*******************************************************************    00110700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00110800
.*******************************************************************    00110900
              PUSH  PRINT                                               00111000
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00111100
              PRINT OFF         ++ PR01                                 00111200
              AGO   .ZPRNTXX                                            00111300
.ZPRINTX      PRINT ON                                                  00111400
.ZPRNTXX      ANOP  ,                                                   00111500
.********************************************************************   00111600
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00111700
.********************************************************************   00111800
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00111900
                      LCLA  &ZOPN1         INVOCATION INDEX             00112000
                      LCLC  &ZOPL(4095)    LABEL                        00112100
                      LCLC  &ZOPO(4095)    OPERATION                    00112200
                      LCLC  &ZOPV(4095)    VALUE                        00112300
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00112400
.*                                                                      00112500
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00112600
.******************************************************************     00112700
.***AL.L10 IOSCMB FMT=LIST MF=S!L PARAMETERS                            00112800
.******************************************************************     00112900
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00113000
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00113100
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00113200
&XMFATTR              SETC  '0D'                                        00113300
.*                                                                      00113400
&ZOPN                 SETA  &ZOPN+1                                     00113500
&ZOPL(&ZOPN)          SETC  ' '                                         00113600
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00113700
&ZOPO(&ZOPN)          SETC  'B'                                         00113800
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00113900
                      AGO   .ZALVL6B                                    00114000
.ZALVL6A              ANOP                                              00114100
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00114200
&ZOPV(&ZOPN)          SETC  '4,8'                                       00114300
&ZOPN                 SETA  &ZOPN+1                                     00114400
&ZOPL(&ZOPN)          SETC  ' '                                         00114500
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00114600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00114700
.ZALVL6B              ANOP                                              00114800
&ZOPD(&ZOPN)          SETC  '++ IOSCMB ARG PARMS'                       00114900
.*                                                                      00115000
&ZOPN                 SETA  &ZOPN+1                                     00115100
&ZOPL(&ZOPN)          SETC  ' '                                         00115200
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00115300
