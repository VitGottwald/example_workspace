*DCL 1 SMF32CDS BASED (ADDR(SMFRCD32)+SMF32COF), /*          @G743PJW*/ 53490009
*     3 SMF32BSC,                 /* BASIC SECTION           @G743PJW*/ 53500009
*      5 SMF32CMD CHAR(8),        /* COMMAND NAME            @G743PJW*/ 53510009
*      5 SMF32CNT FIXED(31),      /* COMMAND COUNT           @G743PJW*/ 53520009
*     3 SMF32DTL,                 /* DETAIL SECTION          @G743PJW*/ 53530009
*      5 SMF32TCB FIXED(31),      /* TCB TIME FOR COMMAND    @G743PJW*/ 53540009
*      5 SMF32SRB FIXED(31),      /* SRB TIME FOR COMMAND    @G743PJW*/ 53550009
*      5 SMF32TGT FIXED(31),      /* TGETS FOR COMMAND       @G743PJW*/ 53560009
*      5 SMF32TPT FIXED(31),      /* TPUTS FOR COMMAND       @VS49784*/ 53570009
*      5 SMF32TRN FIXED(31),      /* TRANSACTIONS FOR CMD    @VS49784*/ 53580009
*      5 SMF32EXP FIXED(32),      /* EXCPS for command                  53590009
*                                    (valid up to 'FFFFFFFE'x,          53600009
*                                     zero and invalid when             53610009
*                                     SMF32EXF is set)           @0EC*/ 53620009
*      5 SMF32TCT FIXED(31),      /* DEVICE CONNECT TIME     @G860P27*/ 53630009
*      5 SMF32DSF CHAR(4),        /* Detail Section Flags        @07A*/ 53640009
*        7 SMF32TCF BIT(1),       /* SMF32TRN is not valid       @07A*/ 53650009
