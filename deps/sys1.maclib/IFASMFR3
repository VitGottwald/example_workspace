*%IFAL33:;                                                    /* @M4C*/ 53740009
*%IF IBM_CBGENC = 'YES' %THEN %GOTO IFAL33A;                  /* @M4A*/ 53750009
*        %IF IFAR33='' %THEN %GOTO IFAL34;                    /* @M4C*/ 53760009
