*   5 IFASMFR3_PFX%%TIVUID CHAR(8),                                     57110009
*  3 IFASMFR3_PFX%%TIVONTME FIXED(31) BDY(HWORD), /*                    57120009
*                                            TOD LOGON(.01 SEC)      */ 57130009
*  3 IFASMFR3_PFX%%TIVONDTE CHAR(4), /* LOGON DATE                   */ 57140009
*  3 IFASMFR3_PFX%%TIVUDATA CHAR(8), /*                                 57150009
*                                    User-defined identification field  57160009
