*  3 IFASMFR3_PFX%%TIVRFLG BIT(8), /* OPERATING ENVIRONMENT          */ 57040009
*  3 IFASMFR3_PFX%%TIVRCDTY FIXED(8), /* RECORD TYPE 34              */ 57050009
*  3 IFASMFR3_PFX%%TIVRCDTS FIXED(31) BDY(HWORD), /*                    57060009
*                                            TOD RECORD WRITTEN      */ 57070009
*  3 IFASMFR3_PFX%%TIVRCDTE CHAR(4), /* DATE RECORD WRITTEN          */ 57080009
