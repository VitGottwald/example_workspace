.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00281200
.*******************************************************************    00281300
                      ACTR  10000                                       00281400
.*                                                                      00281500
                      LCLA  &ZLC           COLUMN POSITION              00281600
                      LCLA  &ZOPX          LIST TABLE INDEX             00281700
                      LCLC  &ZLL           LABEL                        00281800
                      LCLC  &ZLOP          OPERATION                    00281900
                      LCLC  &ZLV           VALUE                        00282000
                      LCLC  &ZLD           DESCRIPTION                  00282100
.*                                                                      00282200
&ZOPX                 SETA  1                                           00282300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00282400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00282500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00282600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00282700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00282800
                      AIF   (K'&ZLD LE 0).ZL5                           00282900
&ZLC                  SETA  35             COMMENT COLUMN               00283000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00283100
&ZLC                  SETA  33             COMMMENT COLUMN              00283200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00283300
&ZLV                  SETC  '&ZLV'.' '                                  00283400
                      AGO   .ZL3                                        00283500
.*                                                                      00283600
.ZL4                  ANOP  ,                                           00283700
&ZLV                  SETC  '&ZLV'.' '                                  00283800
.ZL5                  ANOP  ,                                           00283900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00284000
.*                                                                      00284100
&ZLL     &ZLOP &ZLV                                                     00284200
.*                                                                      00284300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00284400
                      AGO   .ZL2           CONTINUE LOOPING             00284500
.*                                                                      00284600
.ZLX                  ANOP  ,                                           00284700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00284800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00284900
         L     14,16(0,0)              GET CVT ADDRESS                  00285000
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00285100
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00285200
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00285300
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00285400
         B     END&SYSNDX              BRANCH TO END LABEL              00285500
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00285600
         L     14,772(14,0)            GET SFT ADDRESS                  00285700
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00285800
         LA    15,19                   GET EX FOR SERVICE ROUTINE       00285900
         OR    14,15                   GET PC NUMBER                    00286000
         PC    0(14)                                                    00286100
END&SYSNDX DS    0H                    END LABEL                        00286200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00286300
.ZLINKX               ANOP  ,                                           00286400
.*******************************************************************    00286500
.***AP.X01  FORMAT GENERATED CODE                                       00286600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00286700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00286800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00286900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00287000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00287100
                      AIF   (K'&ZLD LE 0).ZL51                          00287200
&ZLC                  SETA  35             COMMENT COLUMN               00287300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00287400
&ZLC                  SETA  33             COMMENT COLUMN               00287500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00287600
&ZLV                  SETC  '&ZLV'.' '                                  00287700
                      AGO   .ZL31                                       00287800
.*                                                                      00287900
.ZL41                 ANOP  ,                                           00288000
&ZLV                  SETC  '&ZLV'.' '                                  00288100
