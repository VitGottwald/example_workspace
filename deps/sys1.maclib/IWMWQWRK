&ZOPV(&ZOPN)          SETC  '0(4,&ZCGPR),60(1)'                         00271900
&ZOPD(&ZOPN)          SETC  '++ XSKIP_REGIONS F'                        00272000
                      AGO   .ZO20C                                      00272100
.*                                                                      00272200
.ZO20B                ANOP  ,                                           00272300
&ZAWRK   SETA  K'&XSKIP_REGIONS                                         00272400
         AIF   ('&XSKIP_REGIONS'(&ZAWRK,1) NE ')').ZO20G                00272500
&ZN      SETA  2                                                        00272600
.ZO20H    ANOP                                                          00272700
         AIF   (&ZN GE &ZAWRK).ZO20G                                    00272800
         AIF   ('&XSKIP_REGIONS'(&ZN,1) NE '(').ZO20I                   00272900
&ZC      SETC  '&XSKIP_REGIONS'(1,&ZN).'4'.','                          00273000
&ZC      SETC  '&ZC'.'&XSKIP_REGIONS'(&ZN+1,&ZAWRK-&ZN).',60(1)'        00273100
         AGO   .ZO20J                                                   00273200
.ZO20I    ANOP                                                          00273300
