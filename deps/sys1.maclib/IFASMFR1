 /* RECORD TYPE 9 - VARY ONLINE                                      */ 03220000
 /* WRITTEN WHEN A DEVICE IS VARIED ONLINE                           */ 03230000
    DCL 1 SMFRCD09 BASED(SMF09PTR) BDY(WORD),                           03231006
         3 SMF9LEN FIXED(15),     /* RDW RECORD LENGTH               */ 03240000
         3 SMF9SEG FIXED(15),     /* RDW SEGMENT DESCRIPTOR          */ 03250000
         3 SMF9FLG BIT(8),        /* OPERATING ENVIRONMENT           */ 03260000
         3 SMF9RTY FIXED(8),      /* RECORD TYPE 9                   */ 03270000
         3 SMF9TME FIXED(31) BDY(HWORD), /* TOD RECORD WRITTEN          03280000
                                                             @ZMC1210*/ 03290000
         3 SMF9DTE CHAR(4),       /* DATE RECORD WRITTEN             */ 03300000
         3 SMF9SID CHAR(4),       /* SYSTEM ID FROM INSTALLATION     */ 03310000
         3 SMF9LENN FIXED(15);    /* LGTH OF DEVICE SECTION + THESE 2*/ 03320000
 /*                                                                  */ 03330000
 /*Note: Virtual I/O Devices are identified by the following:   @LYA */ 03340000
 /*      Device Class 0                                         @LYA */ 03350000
 /*      Unit Type 0                                            @LYA */ 03360000
 /*      Device Number x'7FFF'                                  @LYA */ 03370000
 /*                                                                  */ 03380000
    DCL 1 SMF9DVAD BASED BDY(WORD), /*Device Identification      @LYC*/ 03390000
         3 SMF9DUT CHAR(2),       /* DEVICE CLASS AND UNIT TYPE      */ 03400000
         3 SMF9CUA CHAR(2),       /* DEVICE NUMBER           @G860P27*/ 03410000
                                  /*                            1@LYD*/ 03420000
         3 SMF9VPC FIXED(8),      /* VARY PATH CALLER            @TDA*/ 03430000
                                  /* 0 = SYSTEM CONSOLE          @TDA*/ 03440000
                                  /* 2 = ESCON MANAGER           @TDA*/ 03450000
         3 SMF9RSV CHAR(3);       /* RESERVED                    @TDA*/ 03460000
 %IFAL10:;                                                    /* @M1A*/ 03460112
 %IF IBM_CBGENC = 'YES' %THEN %GOTO IFAL10A;                  /* @M1A*/ 03461012
         %IF IFAR10='' %THEN %GOTO IFAL11;                    /* @M1A*/ 03470012
 %IFAL10A:;                                                   /* @M1A*/ 03471012
 /* RECORD TYPE 10 - ALLOCATION RECOVERY                             */ 03490000
 /* WRITTEN AFTER SUCCESSFUL DEVICE ALLOCATION RECOVERY              */ 03500000
    DCL 1 SMFRCD10 BASED(SMF10PTR) BDY(WORD),                           03501006
         3 SMF10LEN FIXED(15),    /* RDW RECORD LENGTH               */ 03510000
         3 SMF10SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */ 03520000
         3 SMF10FLG BIT(8),       /* OPERATING ENVIRONMENT           */ 03530000
         3 SMF10RTY FIXED(8),     /* RECORD TYPE 10                  */ 03540000
         3 SMF10TME FIXED(31) BDY(HWORD), /* TOD RECORD WRITTEN      */ 03550000
         3 SMF10DTE CHAR(4),      /* DATE RECORD WRITTEN             */ 03560000
         3 SMF10SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */ 03570000
         3 SMF10JBN CHAR(8),      /* JOB NAME                        */ 03580000
         3 SMF10RST FIXED(31) BDY(HWORD), /*READER START TIME(.01 SEC)*/03590000
         3 SMF10RSD CHAR(4),      /* READER START DATE               */ 03600000
         3 SMF10UIF CHAR(8),      /* USER IDENTIFICATION FIELD       */ 03610000
         3 SMF10LN FIXED(15);     /* LGTH OF DEVICE SECTION + THESE 2*/ 03620000
    DCL 1 SMF10DEV BASED BDY(WORD), /*Device Identification      @LYC*/ 03630000
 /*                                                                  */ 03640000
 /*Note: Virtual I/O Devices are identified by the following:    @LYA*/ 03650000
 /*      Device Class 0                                          @LYA*/ 03660000
 /*      Unit Type 0                                             @LYA*/ 03670000
 /*      Device Number x'7FFF'                                   @LYA*/ 03680000
 /*                                                                  */ 03690000
         3 SMF10DUT CHAR(2),      /* DEVICE CLASS AND UNIT TYPE      */ 03700000
         3 SMF10CUA CHAR(2);       /* Device Number              @LYC*/ 03710000
                                   /*                           1@LYD*/ 03720000
 %IFAL11:;                                                    /* @M1A*/ 03720112
 %IF IBM_CBGENC = 'YES' %THEN %GOTO IFAL11A;                  /* @M1A*/ 03721012
         %IF IFAR11='' %THEN %GOTO IFAL12;                    /* @M1C*/ 03730012
 %IFAL11A:;                                                   /* @M1A*/ 03731012
 /* RECORD TYPE 11 - VARY OFFLINE                                    */ 03750000
 /* WRITTEN WHEN A DEVICE IS VARIED OFFLINE                          */ 03760000
    DCL 1 SMFRCD11 BASED(SMF11PTR) BDY(WORD),                           03761006
         3 SMF11LEN FIXED(15),    /* RDW RECORD LENGTH               */ 03770000
         3 SMF11SEG FIXED(15),    /* RDW SEGMENT DESCRIPTOR          */ 03780000
         3 SMF11FLG BIT(8),       /* OPERATING ENVIRONMENT           */ 03790000
         3 SMF11RTY FIXED(8),     /* RECORD TYPE 11                  */ 03800000
         3 SMF11TME FIXED(31) BDY(HWORD), /* TOD RECORD WRITTEN         03810000
                                                             @ZMC1210*/ 03820000
         3 SMF11DTE CHAR(4),      /* DATE RECORD WRITTEN             */ 03830000
         3 SMF11SID CHAR(4),      /* SYSTEM ID FROM INSTALLATION     */ 03840000
         3 SMF11LN FIXED(15);     /* LGTH OF DEVICE SECTION + THESE 2*/ 03850000
 /*                                                                  */ 03860000
 /* Note: Virtual I/O Devices are identified by the following:   @LYA*/ 03870000
 /*       Device Class 0                                         @LYA*/ 03880000
 /*       Unit Type 0                                            @LYA*/ 03890000
 /*       Device Number x'7FFF'                                  @LYA*/ 03900000
 /*                                                                  */ 03910000
    DCL 1 SMF11DEV BASED BDY(WORD), /*Device Identification      @LYC*/ 03920000
         3 SMF11DUT CHAR(2),      /* DEVICE CLASS AND UNIT TYPE      */ 03930000
         3 SMF11CUA CHAR(2),      /* DEVICE NUMBER           @G860P27*/ 03940000
                                  /*                            1@LYD*/ 03950000
         3 SMF11VPC FIXED(8),     /* VARY PATH CALLER            @TDA*/ 03960000
                                  /* 0 = SYSTEM CONSOLE          @TDA*/ 03970000
                                  /* 2 = ESCON MANAGER           @TDA*/ 03980000
         3 SMF11RSV CHAR(3);      /* RESERVED                    @TDA*/ 03990000
 %IFAL12: ;                                                             04000000
 %IF IBM_CBGENC = 'YES' %THEN %GOTO IFALR2;                   /* @M1A*/ 04001012
 %IFAL14: IF IFAR14^='' %THEN %GOTO IFAL15;                             04010000
         %IF IFAR15='' %THEN %GOTO IFAL16;                              04020000
 %IFAL15: ;                                                   /* @M1A*/ 04030012
                                    /* Remove Inline Declares  96@01D*/ 04040000
 %INCLUDE SYSLIB(IFGSMF14) NOSEG;   /* Call DFP to Expand Record @01A*/ 04050000
 %IFAL16: ;                                                             04060000
 %IFAL17:;                                                    /* @M1C*/ 04070012
         %IF IFAR17='' %THEN %GOTO IFAL18;                    /* @M1C*/ 04071012
                                    /* Remove Inline Declares  21@01D*/ 04080000
 %INCLUDE SYSLIB(IGGSMF17) NOSEG;   /* Call DFP to Expand Record @01A*/ 04090000
 %IFAL18:;                                                    /* @M1C*/ 04100012
         %IF IFAR18='' %THEN %GOTO IFAL19;                    /* @M1C*/ 04101012
                                    /* Remove Inline Declares  22@01D*/ 04110000
 %INCLUDE SYSLIB(IGGSMF18) NOSEG;   /* Call DFP to Expand Record @01A*/ 04120000
 %IFAL19:;                                                    /* @M1C*/ 04130012
         %IF IFAR19='' %THEN %GOTO IFALR2;                    /* @M1C*/ 04131012
                                    /* Remove Inline Declares  23@01D*/ 04140000
 %INCLUDE SYSLIB(IGGSMF19) NOSEG;   /* Call DFP to Expand Record @01A*/ 04150000
 %IFALR2: ;                                                             04160000
