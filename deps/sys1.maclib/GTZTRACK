&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00474900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00475000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00475100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00475200
                      AIF   (K'&ZLD LE 0).ZL51                          00475300
&ZLC                  SETA  35             COMMENT COLUMN               00475400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00475500
&ZLC                  SETA  33             COMMENT COLUMN               00475600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00475700
&ZLV                  SETC  '&ZLV'.' '                                  00475800
                      AGO   .ZL31                                       00475900
.*                                                                      00476000
.ZL41                 ANOP  ,                                           00476100
&ZLV                  SETC  '&ZLV'.' '                                  00476200
.ZL51                 ANOP  ,                                           00476300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00476400
.*                                                                      00476500
&ZLL     &ZLOP &ZLV                                                     00476600
.*                                                                      00476700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00476800
                      AGO   .ZL21          CONTINUE LOOPING             00476900
.*                                                                      00477000
.ZLX1                 ANOP  ,                                           00477100
.*******************************************************************    00477200
.***AP.X02  END OF GTZTRACK MACRO                                       00477300
&ZGTZTRACK            SETC  'YES'          SET GTZTRACK USED            00477400
         POP   PRINT                                                    00477500
*                                            GTZTRACK-0                 00477600
         SPACE 1                                                        00477700
.ZLX2                 ANOP  ,                                           00477800
         MEND                                                           00477900
**********************************************************************/ 00478000
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00478100
*%GTZTRACK: MACRO KEYS(OWNER,SOURCE,SOURCEPATH,SOURCEPATHLEN,           00478200
*                 EVENTDESC,EVENTDESCLEN,EVENTDATA,EVENTADDR,           00478300
*                 EVENTPSW8,EVENTPSW16,EVENTASID,EVENTASIDVAL,NOABEND,  00478400
