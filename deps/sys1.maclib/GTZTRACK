.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00468700
.*******************************************************************    00468800
                      ACTR  10000                                       00468900
.*                                                                      00469000
                      LCLA  &ZLC           COLUMN POSITION              00469100
                      LCLA  &ZOPX          LIST TABLE INDEX             00469200
                      LCLC  &ZLL           LABEL                        00469300
                      LCLC  &ZLOP          OPERATION                    00469400
                      LCLC  &ZLV           VALUE                        00469500
                      LCLC  &ZLD           DESCRIPTION                  00469600
.*                                                                      00469700
&ZOPX                 SETA  1                                           00469800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00469900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00470000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00470100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00470200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00470300
                      AIF   (K'&ZLD LE 0).ZL5                           00470400
&ZLC                  SETA  35             COMMENT COLUMN               00470500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00470600
&ZLC                  SETA  33             COMMMENT COLUMN              00470700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00470800
&ZLV                  SETC  '&ZLV'.' '                                  00470900
                      AGO   .ZL3                                        00471000
.*                                                                      00471100
.ZL4                  ANOP  ,                                           00471200
&ZLV                  SETC  '&ZLV'.' '                                  00471300
.ZL5                  ANOP  ,                                           00471400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00471500
.*                                                                      00471600
&ZLL     &ZLOP &ZLV                                                     00471700
.*                                                                      00471800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00471900
                      AGO   .ZL2           CONTINUE LOOPING             00472000
.*                                                                      00472100
.ZLX                  ANOP  ,                                           00472200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00472300
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00472400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00472500
         &ZMAC_LLGT 15,16(0,0)      Get CVT address                     00472600
         AIF   ('&SYSASCE' EQ 'P').NOT_AR_1                             00472700
         LAE   15,0(15,0)           Clear AR 15                         00472800
.NOT_AR_1 ANOP                                                          00472900
         &ZMAC_LLGT 15,140(15,0)    Get ECVT address                    00473000
         ICM   15,B'1111',924(15)   Get GTZTABLE address                00473100
         JZ    GTZTRACKNA_&SYSNDX   No GTZTABLE, get out now            00473200
         ICM   0,B'1111',16(15)     Check DoNotTrack                    00473300
         JNZ   GTZTRACKNA_&SYSNDX   Tracking not available              00473400
         LMH   15,15,100(15)        LX SEQ#                             00473500
         ICM   15,B'1111',112(15)   Get GTZTRACK PC NUM                 00473600
         JZ    GTZTRACKNA_&SYSNDX   No PC Num, get out now              00473700
         PC    0(15)                PC to handle GTZTRACK               00473800
         J     GTZTRACKEND_&SYSNDX  Branch around setting bad RC        00473900
GTZTRACKNA_&SYSNDX DS 0H                                                00474000
         LA    15,X'04'             Set RC (Warning)                    00474100
         LA    0,X'401'             Set RSN (Not ready)                 00474200
GTZTRACKEND_&SYSNDX DS 0H                                               00474300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00474400
.ZLINKX               ANOP  ,                                           00474500
.*******************************************************************    00474600
.***AP.X01  FORMAT GENERATED CODE                                       00474700
