&ZOPL(&ZOPN)          SETC  ' '                                         00406700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00406800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00406900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00407000
                      AGO   .ZIMFNX                                     00407100
.*                                                                      00407200
