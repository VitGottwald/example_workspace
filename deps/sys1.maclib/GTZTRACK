.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00140500
                   AIF   (N'&SYSLIST NE 0).ZG73B  POSITIONALS?          00140600
                   AIF   ('&OWNER' NE '').ZG73B  KEYS?                  00140700
                   AIF   ('&SOURCE' NE '').ZG73B  KEYS?                 00140800
                   AIF   ('&SOURCEPATH' NE '').ZG73B  KEYS?             00140900
                   AIF   ('&SOURCEPATHLEN' NE '').ZG73B  KEYS?          00141000
                   AIF   ('&EVENTDESC' NE '').ZG73B  KEYS?              00141100
                   AIF   ('&EVENTDESCLEN' NE '').ZG73B  KEYS?           00141200
                   AIF   ('&EVENTDATA' NE '').ZG73B  KEYS?              00141300
                   AIF   ('&EVENTADDR' NE '').ZG73B  KEYS?              00141400
                   AIF   ('&EVENTPSW8' NE '').ZG73B  KEYS?              00141500
                   AIF   ('&EVENTPSW16' NE '').ZG73B  KEYS?             00141600
                   AIF   ('&EVENTASID' NE '').ZG73B  KEYS?              00141700
                   AIF   ('&EVENTASIDVAL' NE '').ZG73B  KEYS?           00141800
                   AIF   ('&NOABEND' NE '').ZG73B  KEYS?                00141900
                   AIF   ('&NOABENDVAL' NE '').ZG73B  KEYS?             00142000
                   AIF   ('&CNZTRKR' NE '').ZG73B  KEYS?                00142100
                   AIF   ('&GTZATRKP' NE '').ZG73B  KEYS?               00142200
                   AIF   ('&EVENTADDR0' NE '').ZG73B  KEYS?             00142300
                   AIF   ('&RETCODE' NE '').ZG73B  KEYS?                00142400
                   AIF   ('&RSNCODE' NE '').ZG73B  KEYS?                00142500
                   AGO    .ZG73C     INVOCATION OKAY                    00142600
.ZG73B             ANOP  ,                                              00142700
&ZMS               SETC  'THE "L" FORM OF "MF"'                         00142800
&ZMS               SETC  '&ZMS CAN ONLY HAVE'                           00142900
&ZMS               SETC  '&ZMS THE "PLISTVER" KEY'                      00143000
                   MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'         00143100
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00143200
.ZG73C             ANOP  ,                                              00143300
