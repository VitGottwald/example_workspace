.***AC.PLV01  PROCESS PLISTVER                                      *   00131200
.********************************************************************   00131300
&XPLISTVER      SETA  0                                                 00131400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00131500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00131600
&XPLISTVER      SETA  0                                                 00131700
.ZPLV0          ANOP  ,                                                 00131800
.*                                                                      00131900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00132000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00132100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00132200
.*-------------------------------------------------------------------   00132300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00132400
&ZMS            SETC  '"PLISTVER='                                      00132500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00132600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00132700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00132800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00132900
                AGO   .ZPVCK2                                           00133000
.ZPVCK3         ANOP  ,                                                 00133100
.*-------------------------------------------------------------------   00133200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00133300
&ZMS            SETC  '"PLISTVER='                                      00133400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00133500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00133600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00133700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00133800
                AGO   .ZPVCKE                                           00133900
.ZPVCK2         ANOP  ,                                                 00134000
.*-------------------------------------------------------------------   00134100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00134200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00134300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00134400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00134500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00134600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00134700
.ZPVCK6         ANOP  ,                                                 00134800
&ZMS            SETC  '"PLISTVER='                                      00134900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00135000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00135100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00135200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00135300
&XPLISTVER      SETA  0                                                 00135400
                AGO   .ZPVCKE                                           00135500
.*-------------------------------------------------------------------   00135600
.ZPVCK4A        ANOP  ,                                                 00135700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00135800
&ZMS            SETC  'PLISTVER='                                       00135900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00136000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00136100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00136200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00136300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00136400
                AGO   .ZPVCK5                                           00136500
.ZPVCK4         ANOP  ,                                                 00136600
&XPLISTVER      SETA  &PLISTVER(1)                                      00136700
.ZPVCK5         ANOP  ,                                                 00136800
.ZPVCKE         ANOP  ,                                                 00136900
.********************************************************************   00137000
