.*******************************************************************    00950900
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00951000
.*******************************************************************    00951100
.ZMFLX                ANOP  ,              MF=L PROCESSING              00951200
.*******************************************************************    00951300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00951400
.*******************************************************************    00951500
                      ACTR  10000                                       00951600
.*                                                                      00951700
                      LCLA  &ZLC           COLUMN POSITION              00951800
                      LCLA  &ZOPX          LIST TABLE INDEX             00951900
                      LCLC  &ZLL           LABEL                        00952000
                      LCLC  &ZLOP          OPERATION                    00952100
                      LCLC  &ZLV           VALUE                        00952200
                      LCLC  &ZLD           DESCRIPTION                  00952300
.*                                                                      00952400
&ZOPX                 SETA  1                                           00952500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00952600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00952700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00952800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00952900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00953000
                      AIF   (K'&ZLD LE 0).ZL5                           00953100
&ZLC                  SETA  35             COMMENT COLUMN               00953200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00953300
&ZLC                  SETA  33             COMMMENT COLUMN              00953400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00953500
&ZLV                  SETC  '&ZLV'.' '                                  00953600
                      AGO   .ZL3                                        00953700
.*                                                                      00953800
.ZL4                  ANOP  ,                                           00953900
&ZLV                  SETC  '&ZLV'.' '                                  00954000
.ZL5                  ANOP  ,                                           00954100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00954200
.*                                                                      00954300
&ZLL     &ZLOP &ZLV                                                     00954400
.*                                                                      00954500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00954600
                      AGO   .ZL2           CONTINUE LOOPING             00954700
.*                                                                      00954800
.ZLX                  ANOP  ,                                           00954900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00955000
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00955100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00955200
         AIF   ('&XREQINFO' EQ 'CF').IXCLO                        @D1C  00955300
         AIF   ('&XREQINFO' EQ 'STR').IXCLO                       @D1A  00955400
         AIF   ('&XREQINFO' EQ 'CF_ALLDATA').IXCLO                @D1A  00955500
         AIF   ('&XREQINFO' EQ 'STR_ALLDATA').IXCLO               @D1A  00955600
         AGO   .NOTLO                                                   00955700
.IXCLO   ANOP                                                     @D1A  00955800
         MVI   1(1),4                  SET FUNCTION CODE                00955900
         L     14,16(0,0)              GET CVT ADDRESS                  00956000
         L     14,772(14,0)            GET SFT ADDRESS                  00956100
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00956200
         LA    15,37                   GET EX FOR SERVICE ROUTINE       00956300
         OR    14,15                   GET PC NUMBER                    00956400
         PC    0(14)                                                    00956500
         AGO   .LNKDONE                                                 00956600
.NOTLO   ANOP                                                     @D1C  00956700
         AIF   ('&XREQINFO' EQ 'ARMSTATUS').IXCARM                @L2A  00956800
         AIF   ('&XREQINFO' EQ 'ARMS_ALLDATA').IXCARM             @L2A  00956900
         AGO   .NOTARM                                            @L2A  00957000
.IXCARM  ANOP                                                     @L2A  00957100
         LR    14,1                    query parameter address    @L3A  00957200
         LA    1,68(,1)                load parameter address     @L3A  00957300
         MVI   0(1),1                  input version              @L3A  00957400
         MVI   1(1),135                input request              @L3A  00957500
         MVI   2(1),12                 input request type         @L3A  00957600
         ST    14,60(,1)               input parameter address    @L3A  00957700
         AIF   ('&SYSASCE' EQ 'P').ARMNOAR                        @PDA  00957800
         STAM  1,1,64(1)               input parameter list alet  @PDA  00957900
.ARMNOAR ANOP                                                     @PDA  00958000
         L     14,16(0,0)              load CVT address           @L3A  00958100
         L     14,772(14,0)            load SFT address           @L3A  00958200
         L     14,220(14,0)            get LX for service routine @L3A  00958300
         LA    15,48                   get EX for service routine @L3A  00958400
         OR    14,15                   form PC number             @L3A  00958500
         LA    0,1(0,0)                indicate type of call      @L3A  00958600
         PC    0(14)                   issue PC                   @L3A  00958700
         AGO   .LNKDONE                                           @L2A  00958800
.NOTARM  ANOP                                                     @L2A  00958900
         AIF   ('&XREQINFO' EQ 'CDS').IXCCDS                      @LAA  00959000
         AIF   ('&XREQINFO' EQ 'CDS_ALLDATA').IXCCDS              @LAA  00959100
         AGO   .NOTCDS                                            @LAA  00959200
.IXCCDS  ANOP                                                     @LAA  00959300
         MVI   1(1),16                 SET FUNCTION CODE          @LAA  00959400
         L     14,16(0,0)              GET CVT ADDRESS            @LAA  00959500
         L     14,772(14,0)            GET SFT ADDRESS            @LAA  00959600
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE @LAA  00959700
         LA    15,59                   GET EX FOR SERVICE ROUTINE @LAA  00959800
         OR    14,15                   GET PC NUMBER              @LAA  00959900
         PC    0(14)                                              @LAA  00960000
         AGO   .LNKDONE                                           @PIA  00960100
.NOTCDS  ANOP                                                     @LAA  00960200
         AIF   ('&XREQINFO' NE 'GROUP').NOTGRP                          00960300
         AIF   ('&XREQTYPE' EQ 'DEFER').MVIQRD                          00960400
         MVI   1(1),16                 SET FUNCTION CODE                00960500
         AGO   .GRPLNK                                                  00960600
.MVIQRD  ANOP                                                           00960700
         MVI   1(1),5                  SET FUNCTION CODE                00960800
         AGO   .GRPLNK                                                  00960900
.NOTGRP  ANOP                                                           00961000
         MVI   1(1),16                 SET FUNCTION CODE                00961100
.GRPLNK  ANOP                                                           00961200
         L     14,16(0,0)              GET CVT ADDRESS                  00961300
         L     14,772(14,0)            GET SFT ADDRESS                  00961400
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00961500
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00961600
         OR    14,15                   GET PC NUMBER                    00961700
         PC    0(14)                                                    00961800
.LNKDONE ANOP                                                           00961900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00962000
.ZLINKX               ANOP  ,                                           00962100
.*******************************************************************    00962200
.***AP.X01  FORMAT GENERATED CODE                                       00962300
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00962400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00962500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00962600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00962700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00962800
                      AIF   (K'&ZLD LE 0).ZL51                          00962900
&ZLC                  SETA  35             COMMENT COLUMN               00963000
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00963100
&ZLC                  SETA  33             COMMENT COLUMN               00963200
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00963300
&ZLV                  SETC  '&ZLV'.' '                                  00963400
                      AGO   .ZL31                                       00963500
.*                                                                      00963600
.ZL41                 ANOP  ,                                           00963700
&ZLV                  SETC  '&ZLV'.' '                                  00963800
.ZL51                 ANOP  ,                                           00963900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00964000
.*                                                                      00964100
&ZLL     &ZLOP &ZLV                                                     00964200
.*                                                                      00964300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00964400
                      AGO   .ZL21          CONTINUE LOOPING             00964500
.*                                                                      00964600
.ZLX1                 ANOP  ,                                           00964700
.*******************************************************************    00964800
.***AP.X02  END OF IXCQUERY MACRO                                       00964900
&ZIXCQUERY            SETC  'YES'          SET IXCQUERY USED            00965000
         POP   PRINT                                                    00965100
