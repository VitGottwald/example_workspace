.***AC.PLV01  PROCESS PLISTVER                                      *   00180300
.********************************************************************   00180400
&XPLISTVER      SETA  0                                                 00180500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00180600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00180700
&XPLISTVER      SETA  2                                                 00180800
.ZPLV0          ANOP  ,                                                 00180900
.*                                                                      00181000
                AIF   ('&QUAALEVEL' EQ '').ZPLV1E                       00181100
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00181200
&XPLISTVER      SETA  2                                                 00181300
.ZPLV1E         ANOP  ,                                                 00181400
.*                                                                      00181500
                AIF   ('&QUAALEVEL' EQ '').ZPLV2E                       00181600
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00181700
&XPLISTVER      SETA  2                                                 00181800
.ZPLV2E         ANOP  ,                                                 00181900
.*                                                                      00182000
                AIF   ('&QUAALEVEL' EQ '').ZPLV3E                       00182100
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00182200
&XPLISTVER      SETA  2                                                 00182300
.ZPLV3E         ANOP  ,                                                 00182400
.*                                                                      00182500
                AIF   ('&QUAALEVEL' EQ '').ZPLV4E                       00182600
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00182700
&XPLISTVER      SETA  2                                                 00182800
.ZPLV4E         ANOP  ,                                                 00182900
.*                                                                      00183000
                AIF   ('&QUAALEVEL' EQ '').ZPLV5E                       00183100
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00183200
&XPLISTVER      SETA  2                                                 00183300
.ZPLV5E         ANOP  ,                                                 00183400
.*                                                                      00183500
                AIF   ('&QUAALEVEL' EQ '').ZPLV6E                       00183600
                AIF   (&XPLISTVER GE 2).ZPLV6E                          00183700
&XPLISTVER      SETA  2                                                 00183800
.ZPLV6E         ANOP  ,                                                 00183900
.*                                                                      00184000
                AIF   ('&ELEMENT' EQ '').ZPLV7E                         00184100
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00184200
&XPLISTVER      SETA  1                                                 00184300
.ZPLV7E         ANOP  ,                                                 00184400
.*                                                                      00184500
                AIF   ('&RESTARTGRP' EQ '').ZPLV8E                      00184600
                AIF   (&XPLISTVER GE 1).ZPLV8E                          00184700
