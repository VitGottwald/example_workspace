.ZPLV12E        ANOP  ,                                                 00186900
.*                                                                      00187000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00187100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00187200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00187300
.*-------------------------------------------------------------------   00187400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00187500
&ZMS            SETC  '"PLISTVER='                                      00187600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00187700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00187800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00187900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00188000
                AGO   .ZPVCK2                                           00188100
.ZPVCK3         ANOP  ,                                                 00188200
.*-------------------------------------------------------------------   00188300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00188400
&ZMS            SETC  '"PLISTVER='                                      00188500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00188600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00188700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00188800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00188900
                AGO   .ZPVCKE                                           00189000
.ZPVCK2         ANOP  ,                                                 00189100
.*-------------------------------------------------------------------   00189200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00189300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00189400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00189500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00189600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00189700
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00189800
.ZPVCK6         ANOP  ,                                                 00189900
&ZMS            SETC  '"PLISTVER='                                      00190000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00190100
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00190200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00190300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00190400
&XPLISTVER      SETA  2                                                 00190500
                AGO   .ZPVCKE                                           00190600
.*-------------------------------------------------------------------   00190700
.ZPVCK4A        ANOP  ,                                                 00190800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00190900
&ZMS            SETC  'PLISTVER='                                       00191000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00191100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00191200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00191300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00191400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00191500
                AGO   .ZPVCK5                                           00191600
.ZPVCK4         ANOP  ,                                                 00191700
&XPLISTVER      SETA  &PLISTVER(1)                                      00191800
.ZPVCK5         ANOP  ,                                                 00191900
.ZPVCKE         ANOP  ,                                                 00192000
