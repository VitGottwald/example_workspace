.***AO.X02  RETURN CODE PROCESSING                                      00944800
.*******************************************************************    00944900
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00945000
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00945100
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00945200
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00945300
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00945400
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00945500
&ZOPN                 SETA  &ZOPN+1                                     00945600
&ZOPL(&ZOPN)          SETC  ' '                                         00945700
&ZOPO(&ZOPN)          SETC  'ST'                                        00945800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00945900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00946000
                      AGO   .ZMRETX                                     00946100
.*                                                                      00946200
.ZMRET2               ANOP  ,                                           00946300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00946400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00946500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00946600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00946700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00946800
