 %CBRSMFPL:                                                             03747800
    /* STANDARD SMF RECORD HEADER FOR OAM SMF RECORD                 */ 03750000
  IF SMFBASE = '' %THEN                                                 03800008
   %DO;                                                                 03850000
     %IF IBM_CBGENC ^= 'YES' %THEN                                      03860008
     %DO;                                                               03870008
      DECLARE 1 CBRSMF85 BDY(DWORD), /* OAM SMF RECORD               */ 03900000
     %END;                                                              03910008
     %ELSE %DO;                                                         03920008
      DECLARE 1 CBRSMF85 BASED BDY(DWORD), /* OAM SMF RECORD         */ 03921008
     %END;                                                              03930008
   %END;                                                                03950000
 %ELSE                                                                  04000000
   %DO;                                                                 04050000
      DECLARE 1 CBRSMF85 BASED(SMFBASE) BDY(DWORD),/* OAM SMF RECORD */ 04100000
   %END;                                                                04150000
      3 SMF85HDR,                   /* SMF RECORD TYPE 85 HEADER     */ 04200000
        5 SMF85LEN FIXED(16),       /* RECORD LENGTH                 */ 04250000
        5 SMF85SEG FIXED(16),       /* RECORD SEGMENT                */ 04300000
        5 SMF85FLG BIT(8),          /* SYSTEM INDICATOR              */ 04350000
          7 SMF85FL0  BIT(1),       /* RESERVED                      */ 04400000
          7 SMF85FL1  BIT(1),       /* SUBTYPES ARE VALID            */ 04450000
          7 SMF85FL2  BIT(1),       /* RESERVED                      */ 04500000
          7 SMF85FL3  BIT(1),       /* MVS/SP VERSION 4 (MVS/ESA) AND   04525000
                                       ABOVE.  BITS 3, 4, 5 AND 6 ARE   04550000
                                       ON.                           */ 04575000
          7 SMF85FL4  BIT(1),       /* MVS/SP VERSION 3 (MVS/ESA).      04600000
                                       BITS 4, 5 AND 6 ARE ON.       */ 04625000
          7 SMF85FL5  BIT(1),       /* MVS/SP VERSION 2 (MVS/XA)        04650000
                                       BITS 5 AND 6 ARE ON.          */ 04675000
          7 SMF85FL6  BIT(1),       /* VS2                           */ 04700000
          7 SMF85FL7  BIT(1),       /* RESERVED                      */ 04750000
        5 SMF85RTY FIXED(8),        /* RECORD TYPE                   */ 04800000
        5 SMF85TME FIXED(32) BDY(WORD,3), /* TIME SINCE MIDNIGHT, IN    04850000
                                       HUNDREDTHS OF SECONDS, RECORD    04900000
                                       WAS PRESENTED TO SMF */          04950000
        5 SMF85DTE CHAR(4),         /* DATE RECORD WAS PRESENTED TO SMF,05000000
                                       IN THE FORM OCYYDDDF, WHERE F IS 05050000
                                       THE SIGN.                     */ 05100000
        5 SMF85SID CHAR(4),         /* SYSTEM IDENTIFICATION (FROM SID  05150000
                                       PARAMETER)                    */ 05200000
        5 SMF85SSI CHAR(4),         /* SUBSYSTEM IDENTIFICATION,        05250000
                                       CONTAINS 'OAM ' FOR ALL OAM      05300000
                                       SMF RECORDS.                  */ 05350000
        5 SMF85STY FIXED(16),       /* RECORD SUBTYPE                */ 05400000
        5 SMF85TRN FIXED(16),       /* NUMBER OF TRIPLETS IN THIS       05450000
                                       RECORD.  A TRIPLET IS A SET OF   05500000
                                       OFFSET/LENGTH/VALUES THAT DEFINE 05550000
                                       A SECTION OF THE RECORD.      */ 05600000
        5 SMF85PSO FIXED(32) BDY(WORD,3), /* OFFSET TO OAM PROD SEC  */ 05650000
        5 SMF85PSL FIXED(16),       /* LENGTH OF OAM PRODUCT SECTION */ 05700000
        5 SMF85PSN FIXED(16),       /* NUMBER OF OAM PRODUCT SECTIONS*/ 05750000
        5 SMF85OSO FIXED(32) BDY(WORD,3),/* OFFSET TO OAM SUBTYPE SEC */05800000
        5 SMF85OSL FIXED(16),       /* LENGTH OF OAM SUBTYPE SECTION */ 05850000
        5 SMF85OSN FIXED(16),       /* NUMBER OF OAM SUBTYPE SECTIONS*/ 05900000
        5 *        CHAR(6),         /* RESERVED                      */ 05950000
        5 SMF85END CHAR(0),         /* END OF SMF HEADER             */ 05975000
 @EJECT;                                                                06000000
 %CBRSMF_PFX = 'R85';                                         /* @PDA*/ 06010000
 %CBRSMF_PF2 = 'SMF';                                         /* @PDA*/ 06011001
