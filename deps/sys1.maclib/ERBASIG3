                      DS    CL10   Reserved                                     
.L0008                ANOP                                                      
                      AIF   ('&SS_ROW' EQ 'NO').L000B                           
                      AIF   ('&DSECT' EQ 'YES').L000C                           
                      AIF   ('&SS_ROW' NE 'YES').L000B                          
SS_ROW                DS    0F     Maps one service class served table *        
                                   row for a server address space. The *        
                                   # of entries is equal to the actual *        
                                   # of defined service classes                 
                      AGO   .L000D                                              
.L000C                ANOP                                                      
SS_ROW                DSECT        Maps one service class served table *        
                                   row for a server address space. The *        
                                   # of entries is equal to the actual *        
                                   # of defined service classes                 
.L000D                ANOP                                                      
SS_ENTRY              DS    F      Number of times the address space   *        
                                   has served this service class                
.L000B                ANOP                                                      
                      AIF   ('&ZCBPRINT' EQ 'NO').P3                            
                      AIF   ('&LIST' EQ 'YES').P4                               
.P3                   ANOP                                                      
                      POP   PRINT                                               
.P4                   ANOP                                                      
.P_EXIT               ANOP                                                      
                      MEND                                                      
**/ ;                                                                           
* %ERBASIG3_INCLUDED = 'YES';                                                   
* %DCL ZCBPRINT CHAR EXT;                                                       
* %DEACTIVATE ZCBPRINT;                                                         
* %DCL ERBASIG3_LIST CHAR EXT;                                                  
* %DEACTIVATE ERBASIG3_LIST;                                                    
* %IF ERBASIG3_LIST = 'NO' !                                                    
*    ZCBPRINT = 'NO' %THEN                                                      
* %DO;                                                                          
*   @LIST PUSH NOECHO;                                                          
*   @LIST NOASSEMBLE NOECHO;                                                    
*   @LIST OFF C NOECHO;                                                         
* %END;                                                                         
*/* Start of PL/X Source                                             */         
*                                                                               
*%IBM_CBGEN_PADI = '';                                        /*@A04A*/         
*%If IBM_CBGEN = 'YES' ! IBM_PADI = 'YES' %Then %Do;          /*@A04A*/         
*   %IBM_CBGEN_PADI = 'YES';                                  /*@A04A*/         
*%End;                                                        /*@A04A*/         
*                                                                               
*%If IBM_CBGEN <> 'YES' %Then %Do;                            /*@A04A*/         
*/********************************************************************/         
*/* Notes:                                                           */         
*/*                                                                  */         
*/*  1. The constant ASIVERCB and the comment of the field ASIVERG3  */         
*/*     have to be updated whenever this control block becomes       */         
*/*     incompatible to the  previous version.                   @B1A*/         
*/*                                                                  */         
*/*  2. This data aggregate is stored in permanent storage.          */         
*/*     Any changes made must be reflected in the reporter           */         
*/*     conversion process. Especially the control block             */         
*/*     ERBVFMG3 needs to be updated.                            @B1A*/         
*/*                                                                  */         
*/**************************************************************@A04M*/         
*%End;                                                        /*@A04A*/         
*                                                                               
*DCL ASIVERCB BIT(8) CONSTANT('1E'X); /* Control block version number           
*                                        ASIG3                  @A03C*/         
*DCL ALRVERCB BIT(8) CONSTANT('1E'X); /* Control block version number           
*                                        ALRG3                  @A03A*/         
*DCL SSTVERCB BIT(8) CONSTANT('1E'X); /* Control block version number           
*                                        SS_TABLE, so far ASIVERCB was          
*                                        used for SS_Table      @A03A*/         
*DCL SORTVERCB BIT(8) CONSTANT('01'X); /* Control block version number          
*                                        ASIG3_SORT_EXT         @A03A*/         
*DCL ASIV41CB BIT(8) CONSTANT('05'X); /* Release 4.1 version number             
*                                      (for migration purpose)   @Y1A*/         
*DCL ASIMT2CB BIT(8) CONSTANT('18'X); /* Multi-threading (SMT, MT2)             
*                                      introduced with control block            
*                                      version '18'X                            
*                                      (for migration purpose)   @92A*/         
*DCL 1 ASIARRAY BDY(WORD) BASED,    /* ASID table                @Q1M*/         
*      3 ASIHEAD,                   /* ASID table header         @W1A*/         
*        5 ASIASIG3 CHAR(5),        /* Acronym ASIG3             @W1C*/         
*        5 ASIVERG3 CHAR(1),        /* Control block version     @L1C*/         
*        5 ASIHDRLE FIXED(8),       /* Length of ASIG3 header    @W1C*/         
*        5 *        CHAR(1),        /* no longer used           @A03C*/         
*        5 ASIENTMX FIXED(31),      /* Number of entries (Last entry            
*                                      index = ASIENTMX - 1)     @W1C*/         
*        5 ASIENTNR FIXED(31),      /* Index of last filled entry@W1C*/         
*        5 ASIENTLN FIXED(31),      /* Length of one entry       @B3A*/         
*        5 ASISSTVO FIXED(31),      /* Offset to service class served           
*                                      table.                                   
*                                      This field contains the offset           
*                                      from the begin of the ASID               
*                                      table when the data are within           
*                                      the wrap around buffer   @A04C*/         
*          7 ASISSTVP PTR(31),      /* Pointer to service class served          
*                                      table                    @A04C*/         
*        5 ASIFLAG  Bit(8),         /* ASIG3 Flags              @A03A*/         
*          7 ASI_ConvD  Bit(1),     /* ASIG3 converted to lower                 
*                                      service level            @A03A*/         
*          7 ASI_ConvU  Bit(1),     /* ASIG3 converted to higher                
*                                      release or service level @A03A*/         
*          7 *          Bit(6),     /* Reserved                 @A03A*/         
*        5 *        CHAR(7),        /* Reserved                 @A03C*/         
*      3 ASIENTRY(0:*) CHAR(LENGTH(ASIG3));                                     
*                                   /* Array of all ASID table                  
*                                      entries                   @Q1M*/         
*DCL 1 ASIG3 BASED(ASIG3PTR) BDY(DWORD),                                        
