*                                      used by reporter (this is the            
*                                      pointer  into the common                 
*                                      ASIG3 table)              @T2A*/         
*      3 ALRENTLP PTR(31);          /* Last  entry in the ASIG3                 
*                                      table (used by GRB3RTBR)  @T2A*/         
*/*Sort of ASIG3 entry. Any change in this key causes a              */         
*/*a new entry to be built                                           */         
*DCL 1 ASIG3_SORT_EXT BASED BDY(WORD), /* Extended Sort field    @B4A*/         
*      3 ASI_S_NEWJOB CHAR(LENGTH(ASIG3.ASIJOBNA) +            /*@52C*/         
*                          LENGTH(ASIG3.ASIJOBST)),            /*@52C*/         
*        5 ASI_S_ASIJOBNA CHAR(LENGTH(ASIG3.ASIJOBNA)),        /*@52C*/         
*        5 ASI_S_ASIJOBST CHAR(LENGTH(ASIG3.ASIJOBST)),        /*@52C*/         
*      3 ASI_S_ASISORT3 CHAR(LENGTH(ASIG3.ASISORT3)) BDY(WORD),/*@52C*/         
*%IF ASI_PREP_SORT_E = '' %THEN %GOTO ASI_PREP_LAB1;                            
*        5 ASI_S_E BDY(WORD) LIKE ASISORT3,                                     
*%ASI_PREP_LAB1:;                                                               
*      3 * CHAR(0);                     /*                       @B4A*/         
*                                                                               
*/************************************************************39@A04A*/         
*@LOGIC;                                                                        
*#PRAGMA TOOL=CBGEN.                                                            
*#ALIGNOP.                                                                      
*#NOTUPPER.                                                                     
*#NOLEN.                                                                        
*/* internal use */                                                             
