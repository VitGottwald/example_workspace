&GEN     SETB  ('&OPTCD(&I)' EQ 'GEN' OR &GEN)                          01110000
&MVE     SETB  ('&OPTCD(&I)' EQ 'MVE' OR &MVE)                          01120000
&LOC     SETB  ('&OPTCD(&I)' EQ 'LOC' OR &LOC)                          01130000
&FWD     SETB  ('&OPTCD(&I)' EQ 'FWD' OR &FWD)                  X04SVHS 01140000
&BWD     SETB  ('&OPTCD(&I)' EQ 'BWD' OR &BWD)                  X04SVHS 01150000
&ARD     SETB  ('&OPTCD(&I)' EQ 'ARD' OR &ARD)                  X04SVHS 01160000
&LRD     SETB  ('&OPTCD(&I)' EQ 'LRD' OR &LRD)                  X04SVHS 01170000
&FRD     SETB  ('&OPTCD(&I)' EQ 'FRD' OR &FRD)                     @T0A 01180000
&WAITX   SETB  ('&OPTCD(&I)' EQ 'WAITX' OR &WAITX)             @ZA07549 01190000
&NWAITX  SETB  ('&OPTCD(&I)' EQ 'NWAITX' OR &NWAITX)           @ZA07549 01200000
&NRI     SETB  ('&OPTCD(&I)' EQ 'NRI' OR &NRI)                     @L1A 01210000
&CR      SETB  ('&OPTCD(&I)' EQ 'CR' OR &CR)                       @L1A 01220000
&CRE     SETB  ('&OPTCD(&I)' EQ 'CRE' OR &CRE)                     @L1A 01230000
&ARA31   SETB  ('&OPTCD(&I)' EQ 'ARA31' OR &ARA31)                 @T0A 01240000
&KL      SETB  ('&OPTCD(&I)' EQ 'KL' OR &KL)                       @L1A 01250000
&NOKL    SETB  ('&OPTCD(&I)' EQ 'NOKL' OR &NOKL)                   @L1A 01260000
&RBA     SETB  ('&OPTCD(&I)' EQ 'RBA' OR &RBA)                     @EAA 01270000
&XRBA    SETB  ('&OPTCD(&I)' EQ 'XRBA' OR &XRBA)                   @EAA 01280000
&A       SETA  &KEY+&ADR+&CNV+&SEQ+&DIR+&SKP+&SYN+&ASY+&NUP+&UPD+&NSP+&*01290000
               KEQ+&KGE+&FKS+&GEN+&MVE+&LOC+&FWD+&BWD+&ARD+&LRD+&WAITX+*01300000
               &NWAITX+&NRI+&CR+&CRE+&KL+&NOKL+&RBA+&XRBA+&FRD+&ARA64+&*01310000
               ARA31                                               @T0C 01320000
.*             &A CONTAINS THE NUMBER OF VSAM OPTIONS SPECIFIED         01330000
.*             &B, &C, AND &D CONTAIN THE NUMBER OF VTAM OPTIONS SPEC   01340000
.*             &ERR CONTAINS THE NUMBER OF ERRORS                       01350000
         AIF   (&A+&B+&C+&D+&ERR EQ &I).T000100                         01360000
.*             IF &A+&B+&C+&D+&ERR EQUAL &I, THE LAST OPTION CHECKED    01370000
.*             WAS A VALID VSAM OPTION.  ELSE CHECK FOR A VALID VTAM    01380000
.*             OPTION                                                   01390000
&CA      SETB  ('&OPTCD(&I)' EQ 'CA' OR &CA)                     X03004 01400000
&CS      SETB  ('&OPTCD(&I)' EQ 'CS' OR &CS)                     X03004 01410000
&RELEASE SETB  ('&OPTCD(&I)' EQ 'RELEASE' OR &RELEASE)           X03004 01420000
