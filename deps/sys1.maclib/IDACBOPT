.T010050 ANOP                                                      @L1A 05820000
&SVCNV   SETB  (&CNV)                   SAVE ONLY CNV              @T0A 05830001
&LRLS    SETB  (&FRD OR &NRI OR &CR OR &CRE OR &ARA64 OR &ARA31)   @T0A 05840001
         AIF   (NOT(&LRLS)).T010070     BRANCH IF NOT RLS          @T0A 05850001
&CNV     SETB  0                        CLEAR CNV IF RLS           @T0A 05860001
.T010070 ANOP                                                      @T0A 05870001
.*             DOUBLY DEFINE CNV AS CNV AND ARA64 (NO MORE BITS         05880000
.*             AVAILABLE FOR ARA64).  OK TO DOUBLE WITH CNV BECAUSE RLS 05890000
.*             DOES NOT SUPPORT CNV, AND ONLY RLS SUPPORTS ARA64:  @T0A 05900001
&CNV     SETB  (&CNV OR &ARA64)                                    @T0A 05910001
         AIF   ('&CALLER' EQ 'RPL').T010100                     X04SVHS 05920000
.*             IF CALLED BY RPL, THEN GLOBALS MUST BE SET FOR           05930000
.*             RPL TO GENERATE CODE FORM IN THE FORM OF DC'S.           05940001
.*                                                                      05950001
.*             THIS IS WHAT IS PASSED TO IDA019C1.  WHAT IS ADDED OR    05960001
.*             CHANGED HERE MUST ALSO BE ADDED OR CHANGED IN IDAELEM TO 05970001
.*             SET THE CORRECT INDEX VALUES IN CONSTANTS ELEMS.         05980001
.*             FOR EXAMPLE, WHEN ARA31 IS ADDED TO GENCB RPL OPTCD,     05990001
.*             INDEX 27 (ELEMOA31) MUST BE ADDED TO IDAELEM        @T0C 06000001
&IDAVALC(&IDAENO) SETC '&KEY&ADR&CNV&SEQ&DIR&SKP&SYN&ASY&NUP&UPD&NSP&KE*06010000
               Q&KGE&FKS&GEN&MVE&LOC&FWD&BWD&ARD&LRD&FRD&WAITX&NWAITX&R*06020001
               BA&XRBA&ARA31&NRI&CR&CRE&KL&NOKL'                   @T0C 06030001
&IDAOPT2 SETC  '&CA&CS.00&RELEASE&PASS&NERASE&EAU&ERASE&SPEC&ANY&NCONV&*06040000
               CONV&LBT&LBM&BLK&COND&UNCOND&LOCK.00&LGONMSG&DEVCHAR&TER*06050000
               MS&APPSTAT&COUNTS&RNNAME&CIDXLAT&TOPLGON'       @Z40BHUC 06060000
&IDAOPT2 SETC  '&IDAOPT2&BSCID&DISPLAY&RELRQ'                    X03004 06070000
&IDAOPT3 SETC  '&NRELRQ&Q&NQ&PEND&SESSION&ACTIVE&UNCONDL&QUIESCE&START&*06080000
               STOP&ACCEPT&ACQUIRE.0&CONALL&CONANY.00&KEEP&TRUNC&NIBTK.*06090000
               00&SESSKEY&FMHDR&NFMHDR&SESSPRM.000000'         @G40AKSL 06100000
         MEXIT                                                          06110000
.T010100 ANOP                                                   X04SVHS 06120000
         AIF   ('&CALLER' NE 'RPL').T011600                     X04SVHS 06130000
.*             AT THIS POINT EITHER IDACBOPT WAS CALLED BY RPL,         06140000
.*             OPTCD IS UNSPECIFIED, OR BOTH. IF CALLED BY RPL          06150000
.*             SET ANY UNSPECIFIED DEFAULTS.                            06160000
.*             DEFAULTS FOR CBM WILL BE SET AT EXECUTE TIME             06170000
.*                                                                      06180000
&KEY     SETB  (NOT(&ADR OR &SVCNV) OR &KEY)                       @T0C 06190001
.*             IF NOT ADR, OR CNV, SET KEY AS DEFAULT                   06200000
&SEQ     SETB  (NOT(&DIR OR &SKP) OR &SEQ)                      X04SVHS 06210000
.*             IF NOT DIR, OR SKP, SET SEQ AS DEFAULT                   06220000
         AIF   ('&AM' NE 'VTAM').T011500  WAS VTAM SPECIFIED    X04SVHS 06230000
.*             SET VTAM DEFAULTS                                        06240000
&ACCEPT  SETB  (NOT(&ACQUIRE) OR &ACCEPT)                      @Z40BHUC 06250000
.*             IF NOT ACQUIRE, SET ACCEPT DEFAULT                       06260000
&Q       SETB  (NOT(&NQ) OR &Q)                                 X04SVHS 06270000
.*             IF NOT NQ SET Q AS DEFAULT                               06280000
&RELRQ   SETB  (NOT(&NRELRQ) OR &RELRQ)                         X04SVHS 06290000
.*             IF NOT NRELRQ SET RELRQ DEFAULT                          06300000
&NERASE  SETB  (NOT(&EAU OR &ERASE) OR &NERASE)                 X04SVHS 06310000
.*             IF NOT EAU OR ERASE, SET NERASE DEFAULT                  06320000
&LBT     SETB  (NOT(&BLK OR &LBM) OR &LBT)                      X04SVHS 06330000
.*             IF NOT BLK OR LBM, SET LBT DEFAULT                       06340000
&COND    SETB  (NOT(&UNCOND OR &LOCK) OR &COND)                 X04SVHS 06350000
.*             IF NOT UNCOND OR LOCK, SET COND DEFAULT                  06360000
&CONALL  SETB  (NOT(&CONANY) OR &CONALL)                        X04SVHS 06370000
.*             IF NOT CONANY SET CONALL DEFAULT                         06380000
&QUIESCE SETB  (NOT(&START OR &STOP) OR &QUIESCE)               X04SVHS 06390000
.*             IF NOT START OR STOP, SET QUIESCE DEFAULT                06400000
&LGONMSG SETB  (NOT(&DEVCHAR OR &TERMS OR &APPSTAT OR &COUNTS OR &RNNAM*06410000
               E OR &CIDXLAT OR &TOPLGON OR &BSCID OR &DISPLAY OR &SESS*06420000
               PRM OR &SESSKEY) OR &LGONMSG)                   @G40AKCM 06430000
.*             IF NONE OF ABOVE, LOGONMSG IS DEFAULT                    06440000
&PEND    SETB  (NOT(&SESSION OR &ACTIVE OR &UNCONDL) OR &PEND)  X04SVHS 06450000
.*             IF NONE OF ABOVE, SET PEND DEFAULT                       06460000
&NIBTK   SETB  (NOT(&TRUNC OR &KEEP) OR &NIBTK)                 X04SVHS 06470000
.*             IF NOT KEEP OR TRUNC SET NIBTK DEFAULT                   06480000
.T011500 ANOP                                                   X3004BH 06490000
.*             SET GLOBALS TO BE ASSEMBLED BY DECLARATIVE AS CONSTANTS. 06500001
.*             THE FOLLOWING IS MAPPED BY RPLOPT1 TO RPLOPT8 IN         06510001
.*             IFGRPL                                              @T0C 06520001
&IDAOP1  SETC  '&LOC&DIR&SEQ&SKP&ASY&KGE&GEN'   OPTCD BYTE 1    X04SVHS 06530000
&IDAOP2  SETC  '&KEY&ADR&CNV&BWD&LRD&WAITX&UPD&NSP'   BYTE 2       @EAC 06540000
&IDAOP3  SETC  '&KL&NRI&CR&CRE.00&FRD.0'        OPTCD BYTE 3       @T0C 06550001
&IDAOP4  SETC  '&XRBA.0000000'                  OPTCD BYTE 4       @EAA 06560000
         AIF   ('&AM' NE 'VTAM').T011600                        X04SVHS 06570000
.*             IF NOT VTAM, BYPASS SETTING VTAM OPTION BYTES            06580000
&IDAOP5  SETC  '&CS.0&PASS&NERASE&EAU&ERASE&ANY&CONV'          @Z40BHUC 06590000
.*                                      OPTCD BYTE 5            X04SVHS 06600000
&IDAOP6  SETC  '&BLK&LBM&LBT&COND&UNCOND&LOCK.00' OPTCD BYTE 6  X04SVHS 06610000
&IDAOP7  SETC  '&CONALL&CONANY.0&Q.0&RELRQ.00' OPTCD BYTE 7     X04SVHS 06620000
&IDAOP8  SETC  '&ACQUIRE&ACCEPT.0&PEND&SESSION&ACTIVE&UNCONDL.0'        06630000
.*                                      OPTCD BYTE 8           @Z40BHUC 06640000
&IDAOP9  SETC  '&LGONMSG&DEVCHAR&TERMS&COUNTS&APPSTAT&RNNAME&CIDXLAT&TO*06650000
               PLGON'                   OPTCD BYTE 9            X04SVHS 06660000
&IDAOPA  SETC  '&BSCID&DISPLAY&SESSPRM&SESSKEY.0000'                    06670000
