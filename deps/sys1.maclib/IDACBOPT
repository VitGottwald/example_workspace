         MACRO                                                          00010000
         IDACBOPT &OPTCD=,&AM=,&MF=,&CALLER=                            00020000
.*                                                                      00030000
.*  COPYRIGHT =    @01A                                            @01A 00040001
.*                                                                      00050001
.*PROPRIETARY V3 STATEMENT                                           *  00060001
.*LICENSED MATERIALS - PROPERTY OF IBM                               *  00070001
.*"RESTRICTED MATERIALS OF IBM"                                      *  00080001
.*5650-ZOS                                                           *  00090001
.*COPYRIGHT IBM CORP. 1982,2017                                      *  00100001
.*END PROPRIETARY V3 STATEMENT                                       *  00110001
.*                                                                      00120000
.*                                                                      00130000
.*  STATUS = DFSMS/MVS RELEASE 1.3.0                                    00140001
.*                                                                      00150000
.* FUNCTION -  THIS MACRO PROCESSES THE RPL KEYWORD OPTCD. IT IS CALLED 00160001
.*             BY IDACBRPL FOR GENCB, MODCB, AND TESTCB RPL, AND CHECKS 00170001
.*             THE OPTIONS (OPTCD) SPECIFED BY THE USER.                00180001
.*             IT CHECKS FOR MUTUALLY EXCLUSIVE OPTIONS SPECIFIED AND   00190001
.*             ISSUES ERROR MESSAGES VIA IDAERMAC.  IF THE OPTIONS ARE  00200001
.*             VALID, IDACBOPT SETS VALUES IN VARIOUS GLOBAL ARRAYS     00210001
.*             TO BE USED BY IDACB2 TO BUILD A PARAMETER LIST OF        00220001
.*             ARGUMENT CONTROL ENTRIES FOR THE CONTROL BLOCK           00230001
.*             MANIPULATION MODULE (IDA019C1) TO BE USED AT EXECUTE     00240001
.*             TIME.  IT CALLS IDACB3 TO SET THE ELEMENT TYPE CODE      00250001
.*             AND THE KEYWORD TYPE CODE FOR OPTCD.                @01C 00260001
.*                                                                      00270000
.*             IT IS ALSO CALLED BY THE RPL DECLARATIVE MACRO VIA       00280000
.*             IDACBRPL. WHEN CALLED BY THE DECLARATIVE MACRO,          00290000
.*             DIFFERENT GLOBALS ARE SET THAN THASE USED BY THE CBMM.   00300000
.*             THEREFORE, THE BITS SET IN THESE GLOBALS MUST BE         00310000
.*             IDENTICAL TO THE WAY THEY WILL APPEAR WHEN THE           00320000
.*             CONTROL BLOCK IS GENERATED.                              00330000
.*                                                                      00340000
.*                                                                      00350000
.*                                                                      00360000
.*  CHANGE ACTIVITY                                                     00370000
.*   SESSPARM MUTUALLY EXCLUSIVE WITH LOGONMSG DEFAULT         @ZA07555 00380000
.*   UPD AND NSP NO LONGER MUTUALLY EXCLUSIVE                  @YA06306 00390000
.*   BACK OUT CHANGES MADE BY PE06309, UPD & NSP MUTUALLY EXCL @YA07985 00400000
.*   $L1=VSAMRLS,HDZ11C0,930910,SJPLRG: VSAM-RLS CBMM SUPPORT      @L1A 00410000
.*   PREVENT COPYRIGHT FROM SHOWING WHEN MACRO IS INVOKED      @WA04075 00420000
.*   $EA=DFSMS,HDZ11C0,091494,SJPLDKT: EXTENDED ADDRESSABILITY     @EAA 00430000
.*   $T0=127031,HDZ2230,05/21/2015,LLLAW: VSAMDB SUPPORT           @T0A 00440001
.*   $01=OA55466,HDZ2230,05/13/2018,LLLAW: MOVE MVSAMDB FROM            00450001
.*       Z/OS 2.2                                                  @01A 00460001
.******************************************************************@L1C 00470000
.* GLOBALS                                                              00480000
         GBLB  &IDACB01                ERROR INDICATOR                  00490000
         GBLA  &IDAENO                 NUMBER OF ELEMENTS               00500000
         GBLC  &IDAVALC(60)            ELEMENT CHARACTER VALUE ARR @L1C 00510000
         GBLA  &IDAELEN(60)            ELEMENT FIELD LENGTH ARRAY  @L1C 00520000
         GBLC  &IDAOPT2,&IDAOPT3       BIT STRING EXTENSIONS            00530000
.*             THE FOLLOWING GLOBALS ARE USED TO PASS THE VALUES OF     00540000
.*             THE OPTCD BYTES BACK TO THE DECLARATIVE MACRO RPL.       00550000
         GBLC  &IDAOP1,&IDAOP2,&IDAOP5,&IDAOP6,&IDAOP7,&IDAOP8  X04SVHS 00560000
         GBLC  &IDAOP9,&IDAOPA,&IDAOPB,&IDAOPC,&IDAOP3,&IDAOP4     @EAC 00570000
.* LOCALS                                                               00580000
         LCLA  &I,&A,&B,&C,&D,&ERR               WORK VARIABLES X04SVHS 00590000
         LCLB  &LRLS,&SVCNV                      WORK VARIABLES    @T0A 00600001
         LCLB  &KEY,&ADR,&CNV,&SEQ,&DIR,&SKP     OPTCD CODES            00610000
         LCLB  &SYN,&ASY,&NUP,&UPD,&NSP,&KEQ      OPTCD CODES           00620000
         LCLB  &KGE,&FKS,&GEN,&MVE,&LOC          OPTCD CODES            00630000
         LCLB  &CA,&CS,&RELEASE,&PASS            OPTCD CODES   @Z40BHUC 00640000
         LCLB  &WAITX,&NWAITX                    OPTCD CODES   @ZA07549 00650000
         LCLB  &NERASE,&EAU,&ERASE,&SPEC,&ANY    OPTCD CODES     X03004 00660000
         LCLB  &NCONV,&CONV,&LBT,&BLK,&COND      OPTCD CODES     X03004 00670000
         LCLB  &UNCOND,&LOCK,&LGONMSG,&DEVCHAR   OPTCD CODES     X03004 00680000
         LCLB  &TERMS,&APPSTAT,&COUNTS,&RNNAME   OPTCD CODES     X03004 00690000
         LCLB  &CIDXLAT,&TOPLGON,&BSCID,&LBM     OPTCD CODES     X03004 00700000
         LCLB  &DISPLAY,&RELRQ,&NRELRQ,&Q,&NQ    OPTCD CODES     X03004 00710000
         LCLB  &PEND,&SESSION,&ACTIVE,&UNCONDL   OPTCD CODES     X03004 00720000
         LCLB  &QUIESCE,&START,&STOP,&ACCEPT     OPTCD CODES     X03004 00730000
         LCLB  &ACQUIRE,&CONALL,&CONANY          OPTCD CODES   @Z40BHUC 00740000
         LCLB  &KEEP,&TRUNC                      OPTCD CODES   @Z40BHUC 00750000
         LCLB  &NIBTK                            OPTCD CODES   @Z40BHUC 00760000
         LCLB  &FMHDR,&NFMHDR                    OPTCD CODE    X03004HS 00770000
         LCLB  &FWD,&BWD,&ARD,&LRD               OPTCD CODES    X04SVHS 00780000
         LCLB  &SESSPRM                          OPTCD CODE    @Z40BHUC 00790000
         LCLB  &SESSKEY                          OPTCD CODE    @G40AKSL 00800000
         LCLB  &NRI,&CR,&CRE                     RLS OPTCD CODES   @T0C 00810000
         LCLB  &FRD,&ARA64,&ARA31                RLS OPTCD CODES   @T0A 00820001
         LCLB  &KL,&NOKL                         OPTCD CODES       @L1A 00830000
         LCLB  &RBA,&XRBA                        OPTION CODES      @EAA 00840000
.*                                                                      00850000
         AIF   ('&OPTCD' EQ '').T010100 CHECK FOR NULL PARAM    X04SVHS 00860000
         AIF   ('&CALLER' EQ 'RPL').T000100                     X04SVHS 00870000
.*             IF CALLED BY RPL DECLARATIVE, BYPASS CALL OF IDACB3      00880000
         IDACB3 ARG=&OPTCD,ARGNAME=OPTCD,KWTC=X'34',MF=&MF,BIT=YES,    *00890000
               INDRECT=YES                                              00900000
&IDAELEN(&IDAENO) SETA 12                                       X04SVHS 00910000
         AIF   ('&OPTCD(1)' EQ '*').T011600                     X04SVHS 00920000
.T000100 ANOP                                                           00930000
&I       SETA  &I+1                     BUMP INDEX VARIABLE             00940000
         AIF   (&I GT N'&OPTCD).T000300 HAVE ALL OPTIONS BEEN CHECKED   00950000
&KEY     SETB  ('&OPTCD(&I)' EQ 'KEY' OR &KEY)                          00960000
&ADR     SETB  ('&OPTCD(&I)' EQ 'ADR' OR &ADR)                          00970000
&CNV     SETB  ('&OPTCD(&I)' EQ 'CNV' OR &CNV)                          00980000
&ARA64   SETB  ('&OPTCD(&I)' EQ 'ARA64' OR &ARA64)                 @T0A 00990001
&SEQ     SETB  ('&OPTCD(&I)' EQ 'SEQ' OR &SEQ)                          01000000
&DIR     SETB  ('&OPTCD(&I)' EQ 'DIR' OR &DIR)                          01010000
&SKP     SETB  ('&OPTCD(&I)' EQ 'SKP' OR &SKP)                          01020000
