.T002800 AIF   (NOT(&COND AND &LOCK)).T002900                    X03004 02950000
         IDAERMAC 8,OPTCD,COND,LOCK                              X03004 02960000
&IDACB01 SETB  1                                                 X03004 02970000
.T002900 AIF   (NOT(&UNCOND AND &LOCK)).T003000                  X03004 02980000
         IDAERMAC 8,OPTCD,UNCOND,LOCK                            X03004 02990000
&IDACB01 SETB  1                                                 X03004 03000000
.T003000 AIF   (NOT(&LGONMSG AND &DEVCHAR)).T003100              X03004 03010000
         IDAERMAC 8,OPTCD,LOGONMSG,DEVCHAR                       X03004 03020000
&IDACB01 SETB  1                                                 X03004 03030000
.T003100 AIF   (NOT(&LGONMSG AND &TERMS)).T003200                X03004 03040000
         IDAERMAC 8,OPTCD,LOGONMSG,TERMS                         X03004 03050000
&IDACB01 SETB  1                                                 X03004 03060000
.T003200 AIF   (NOT(&LGONMSG AND &APPSTAT)).T003300              X03004 03070000
