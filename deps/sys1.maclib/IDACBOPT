&SESSKEY SETB  ('&OPTCD(&I)' EQ 'SESSKEY' OR &SESSKEY)         @G40AKSL 01880000
&B       SETA  &CA+&CS+&RELEASE+&PASS+&NERASE+&EAU+&ERASE+&SPEC+&ANY+&N*01890000
               CONV+&CONV+&LBT+&LBM+&BLK+&COND+&UNCOND+&LOCK+&LGONMSG+&*01900000
               DEVCHAR+&TERMS+&APPSTAT+&COUNTS+&RNNAME+&CIDXLAT         01910000
&C       SETA  &TOPLGON+&BSCID+&DISPLAY+&RELRQ+&NRELRQ+&Q+&NQ+&PEND+&SE*01920000
               SSION+&ACTIVE+&UNCONDL+&QUIESCE+&START+&STOP+&ACCEPT+&AC*01930000
               QUIRE+&CONALL+&CONANY+&KEEP                              01940000
&D       SETA  &TRUNC+&NIBTK+&FMHDR+&NFMHDR+&SESSPRM+&SESSKEY           01950000
.*                                                             @G40AKSL 01960000
.*             &A CONTAINS THE NUMBER OF VSAM OPTIONS SPECIFIED         01970000
.*             &B, &C, AND &D CONTAIN THE NUMBER OF VTAM OPTIONS SPEC   01980000
.*             &ERR CONTAINS THE NUMBER OF ERRORS                       01990000
         AIF   (&A+&B+&C+&D+&ERR EQ &I).T000200                         02000000
.*             IF &A+&B+&C+&D+&ERR EQUAL &I, THE LAST OPTION CHECKED    02010000
.*             WAS A VALID VTAM OPTION. ELSE IT WAS INVALID OR          02020000
.*             DUPLICATED A PREVIOUSLY SPECIFIED OPTION.                02030000
         IDAERMAC 5,OPTCD,&OPTCD(&I)                                    02040000
&IDACB01 SETB  1                                                        02050000
&ERR     SETA  &ERR+1                   BUMP ERROR COUNT        X04SVHS 02060000
         AGO   .T000100                                                 02070000
.T000200 AIF   ('&AM' EQ 'VTAM').T000100                                02080000
         IDAERMAC 20,&OPTCD(&I),OPTCD                            X03004 02090000
&IDACB01 SETB  1                                                 X03004 02100000
         AGO   .T000100                                                 02110000
.********************************************************************** 02120000
.*                                                                      02130000
.*             THE FOLLOWING CODE CHECKS FOR MUTUALLY EXCLUSIVE OPTIONS 02140000
.*             IF THEY EXIST, AN ERROR MSG IS ISSUED                    02150000
.*                                                                      02160000
.T000300 AIF   (NOT(&KEY AND &ADR)).T000400                             02170000
         IDAERMAC 8,OPTCD,KEY,ADR                                       02180000
&IDACB01 SETB  1                                                        02190000
.T000400 AIF   (NOT(&KEY AND &CNV)).T000500                             02200000
         IDAERMAC 8,OPTCD,KEY,CNV                                       02210000
&IDACB01 SETB  1                                                        02220000
.T000500 AIF   (NOT(&ADR AND &CNV)).T000600                             02230000
         IDAERMAC 8,OPTCD,ADR,CNV                                       02240000
&IDACB01 SETB  1                                                        02250000
.T000600 AIF   (NOT(&SEQ AND &DIR)).T000700                             02260000
         IDAERMAC 8,OPTCD,SEQ,DIR                                       02270000
&IDACB01 SETB  1                                                        02280000
.T000700 AIF   (NOT(&SEQ AND &SKP)).T000800                             02290000
         IDAERMAC 8,OPTCD,SEQ,SKP                                       02300000
&IDACB01 SETB  1                                                        02310000
.T000800 AIF   (NOT(&DIR AND &SKP)).T000900                             02320000
         IDAERMAC 8,OPTCD,DIR,SKP                                       02330000
&IDACB01 SETB  1                                                        02340000
.T000900 AIF   (NOT(&SYN AND &ASY)).T001000                             02350000
         IDAERMAC 8,OPTCD,SYN,ASY                                       02360000
&IDACB01 SETB  1                                                        02370000
.T001000 AIF   (NOT(&NUP AND &UPD)).T001100                             02380000
         IDAERMAC 8,OPTCD,NUP,UPD                                       02390000
&IDACB01 SETB  1                                                        02400000
.T001100 AIF   (NOT(&NUP AND &NSP)).T001150                    @YA07985 02410000
         IDAERMAC 8,OPTCD,NUP,NSP                                       02420000
&IDACB01 SETB  1                                               @YA07985 02430000
.T001150 AIF   (NOT(&UPD AND &NSP)).T001200                    @YA07985 02440000
         IDAERMAC 8,OPTCD,UPD,NSP                              @YA07985 02450000
&IDACB01 SETB  1                                                        02460000
.T001200 AIF   (NOT(&KEQ AND &KGE)).T001350                    @ZA07549 02470000
         IDAERMAC 8,OPTCD,KEQ,KGE                                       02480000
&IDACB01 SETB  1                                                        02490000
.T001350 AIF   (NOT(&WAITX AND &NWAITX)).T001400               @ZA07549 02500000
         IDAERMAC 8,OPTCD,WAITX,NWAITX                         @ZA07549 02510000
&IDACB01 SETB  1                                               @ZA07549 02520000
