         AIF   ('&ZXDIAGDATA' EQ 'OUTPUT').ZO11A  OUTPUT                00242300
         AIF   ('&ZXDIAGDATA' NE 'INOUT').ZO11C  INOUT                  00242400
.ZO11A    AIF   ('&XDIAGDATA'(1,1) NE '(').ZO11B  ADDR IN GPR           00242500
&ZCGPR   SETC  '&XDIAGDATA'(2,K'&XDIAGDATA-2)                           00242600
&ZOPN                 SETA  &ZOPN+1                                     00242700
&ZOPL(&ZOPN)          SETC  ' '                                         00242800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00242900
&ZOPV(&ZOPN)          SETC  '0(16,&ZCGPR),30(1)'                        00243000
&ZOPD(&ZOPN)          SETC  '++ XDIAGDATA CL16'                         00243100
                      AGO   .ZO11C                                      00243200
.*                                                                      00243300
.ZO11B                ANOP  ,                                           00243400
&ZAWRK   SETA  K'&XDIAGDATA                                             00243500
         AIF   ('&XDIAGDATA'(&ZAWRK,1) NE ')').ZO11G                    00243600
&ZN      SETA  2                                                        00243700
.ZO11H    ANOP                                                          00243800
         AIF   (&ZN GE &ZAWRK).ZO11G                                    00243900
         AIF   ('&XDIAGDATA'(&ZN,1) NE '(').ZO11I                       00244000
&ZC      SETC  '&XDIAGDATA'(1,&ZN).'16'.','                             00244100
&ZC      SETC  '&ZC'.'&XDIAGDATA'(&ZN+1,&ZAWRK-&ZN).',30(1)'            00244200
         AGO   .ZO11J                                                   00244300
.ZO11I    ANOP                                                          00244400
&ZN      SETA  &ZN+1                                                    00244500
         AGO   .ZO11H                                                   00244600
.ZO11G    ANOP                                                          00244700
&ZC      SETC  '&XDIAGDATA.(16),30(1)'                                  00244800
.ZO11J    ANOP                                                          00244900
&ZOPN                 SETA  &ZOPN+1                                     00245000
&ZOPL(&ZOPN)          SETC  ' '                                         00245100
&ZOPO(&ZOPN)          SETC  'MVC'                                       00245200
&ZOPV(&ZOPN)          SETC  '&ZC'                                       00245300
&ZOPD(&ZOPN)          SETC  '++ XDIAGDATA CL16'                         00245400
.ZO11C                ANOP  ,                                           00245500
.*******************************************************************    00245600
.***AO.X01  LABEL TO BYPASS INVOCATION                                  00245700
.*******************************************************************    00245800
&ZOPN                 SETA  &ZOPN+1                                     00245900
&ZOPL(&ZOPN)          SETC  'AMGE&SYSNDX'                               00246000
&ZOPO(&ZOPN)          SETC  'DS'                                        00246100
&ZOPV(&ZOPN)          SETC  '0H'                                        00246200
&ZOPD(&ZOPN)          SETC  '++ BYPASS INVOCATION'                      00246300
.*******************************************************************    00246400
.***AO.X02  RETURN CODE PROCESSING                                      00246500
.*******************************************************************    00246600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00246700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00246800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00246900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00247000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00247100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00247200
&ZOPN                 SETA  &ZOPN+1                                     00247300
&ZOPL(&ZOPN)          SETC  ' '                                         00247400
&ZOPO(&ZOPN)          SETC  'ST'                                        00247500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00247600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00247700
                      AGO   .ZMRETX                                     00247800
.*                                                                      00247900
.ZMRET2               ANOP  ,                                           00248000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00248100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00248200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00248300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00248400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00248500
&ZOPN                 SETA  &ZOPN+1                                     00248600
&ZOPL(&ZOPN)          SETC  ' '                                         00248700
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00248800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00248900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00249000
.ZMRETX               ANOP  ,                                           00249100
.*******************************************************************    00249200
.***AO.X03  REASON CODE PROCESSING                                      00249300
.*******************************************************************    00249400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00249500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00249600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00249700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00249800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00249900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00250000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00250100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00250200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00250300
&ZOPN                 SETA  &ZOPN+1                                     00250400
&ZOPL(&ZOPN)          SETC  ' '                                         00250500
&ZOPO(&ZOPN)          SETC  'ST'                                        00250600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00250700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00250800
                      AGO   .ZMRSNX                                     00250900
.*                                                                      00251000
.ZMRSN2               ANOP  ,                                           00251100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00251200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00251300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00251400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00251500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00251600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00251700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00251800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00251900
&ZOPN                 SETA  &ZOPN+1                                     00252000
&ZOPL(&ZOPN)          SETC  ' '                                         00252100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00252200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00252300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00252400
.ZMRSNX               ANOP  ,                                           00252500
.*******************************************************************    00252600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00252700
.*******************************************************************    00252800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00252900
.*******************************************************************    00253000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00253100
.*******************************************************************    00253200
                      ACTR  10000                                       00253300
.*                                                                      00253400
                      LCLA  &ZLC           COLUMN POSITION              00253500
                      LCLA  &ZOPX          LIST TABLE INDEX             00253600
                      LCLC  &ZLL           LABEL                        00253700
                      LCLC  &ZLOP          OPERATION                    00253800
                      LCLC  &ZLV           VALUE                        00253900
                      LCLC  &ZLD           DESCRIPTION                  00254000
.*                                                                      00254100
&ZOPX                 SETA  1                                           00254200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00254300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00254400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00254500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00254600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00254700
                      AIF   (K'&ZLD LE 0).ZL5                           00254800
&ZLC                  SETA  35             COMMENT COLUMN               00254900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00255000
&ZLC                  SETA  33             COMMMENT COLUMN              00255100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00255200
&ZLV                  SETC  '&ZLV'.' '                                  00255300
                      AGO   .ZL3                                        00255400
.*                                                                      00255500
.ZL4                  ANOP  ,                                           00255600
&ZLV                  SETC  '&ZLV'.' '                                  00255700
.ZL5                  ANOP  ,                                           00255800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00255900
.*                                                                      00256000
&ZLL     &ZLOP &ZLV                                                     00256100
.*                                                                      00256200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00256300
                      AGO   .ZL2           CONTINUE LOOPING             00256400
.*                                                                      00256500
.ZLX                  ANOP  ,                                           00256600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00256700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00256800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00256900
         L     15,16(0,0)         ++ Get CVT address                    00257000
         L     15,772(15,0)       ++ Get JESCT address                  00257100
.*********************************************************************  00257200
.*                                                                   *  00257300
.*  Get LX/EX for IEFSJSYM service                                   *  00257400
.*                                                                   *  00257500
.*  Note: LX/EX is calculated by multiplying the PC number (156)     *  00257600
.*        times the length of a PC/AUTH table entry (4) and          *  00257700
.*        then subtracting 4.                                        *  00257800
.*                                                                   *  00257900
.*********************************************************************  00258000
         L     15,620(15,0)       ++ Get PC number                      00258100
         PC    0(15)              ++ PC to IEFSJSRS                     00258200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00258300
.ZLINKX               ANOP  ,                                           00258400
.*******************************************************************    00258500
.***AP.X01  FORMAT GENERATED CODE                                       00258600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00258700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00258800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00258900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00259000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00259100
                      AIF   (K'&ZLD LE 0).ZL51                          00259200
&ZLC                  SETA  35             COMMENT COLUMN               00259300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00259400
&ZLC                  SETA  33             COMMENT COLUMN               00259500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00259600
&ZLV                  SETC  '&ZLV'.' '                                  00259700
                      AGO   .ZL31                                       00259800
.*                                                                      00259900
.ZL41                 ANOP  ,                                           00260000
&ZLV                  SETC  '&ZLV'.' '                                  00260100
.ZL51                 ANOP  ,                                           00260200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00260300
.*                                                                      00260400
&ZLL     &ZLOP &ZLV                                                     00260500
.*                                                                      00260600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00260700
                      AGO   .ZL21          CONTINUE LOOPING             00260800
.*                                                                      00260900
.ZLX1                 ANOP  ,                                           00261000
.*******************************************************************    00261100
.***AP.X02  END OF IEFSJSYM MACRO                                       00261200
