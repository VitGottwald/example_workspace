.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00078200
.********************************************************************   00078300
                  GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL             00078400
                  GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE      00078500
                  GBLA  &SYSALVL    SYSSTATE ARCHLVL                    00078600
                  GBLC  &SYSAM64    SYSSTATE AMODE64                    00078700
.*                                                                      00078800
                  AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED      00078900
                  AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED       00079000
                  SPLEVEL  TEST                                         00079100
.ZSYS2            ANOP  ,                                               00079200
                  AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1           00079300
                  AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1           00079400
                  SYSSTATE TEST                                         00079500
                  AGO   .ZSYS4                                          00079600
.ZSYS3            ANOP  ,                                               00079700
&SYSASCE          SETC  'P'         ASSUME SYSSTATE PRIMARY             00079800
.ZSYS4            ANOP  ,                                               00079900
                  AIF   ('&SYSAM64' NE 'YES').ZSYSN64                   00080000
&ZMAC_LLGT        SETC 'LLGT'                                           00080100
&ZMAC_LGR         SETC 'LGR'                                            00080200
&ZMAC_LG          SETC 'LG'                                             00080300
&ZMAC_STG         SETC 'STG'                                            00080400
.ZSYSN64          ANOP  ,                                               00080500
&ZMAC_G           SETC 'G'                                              00080600
&ZMAC_LR          SETC 'LGR'                                            00080700
&ZMAC_L           SETC 'LG'                                             00080800
&ZMAC_ST          SETC 'STG'                                            00080900
