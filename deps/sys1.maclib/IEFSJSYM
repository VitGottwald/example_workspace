.***AC.PLV01  PROCESS PLISTVER                                      *   00084300
.********************************************************************   00084400
&XPLISTVER      SETA  0                                                 00084500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00084600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00084700
&XPLISTVER      SETA  0                                                 00084800
.ZPLV0          ANOP  ,                                                 00084900
.*                                                                      00085000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00085100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00085200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00085300
.*-------------------------------------------------------------------   00085400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00085500
&ZMS            SETC  '"PLISTVER='                                      00085600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00085700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00085800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00085900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00086000
                AGO   .ZPVCK2                                           00086100
.ZPVCK3         ANOP  ,                                                 00086200
.*-------------------------------------------------------------------   00086300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00086400
&ZMS            SETC  '"PLISTVER='                                      00086500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00086600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00086700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00086800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00086900
                AGO   .ZPVCKE                                           00087000
.ZPVCK2         ANOP  ,                                                 00087100
.*-------------------------------------------------------------------   00087200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00087300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00087400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00087500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00087600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00087700
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00087800
.ZPVCK6         ANOP  ,                                                 00087900
&ZMS            SETC  '"PLISTVER='                                      00088000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00088100
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00088200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00088300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00088400
&XPLISTVER      SETA  0                                                 00088500
                AGO   .ZPVCKE                                           00088600
.*-------------------------------------------------------------------   00088700
.ZPVCK4A        ANOP  ,                                                 00088800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00088900
&ZMS            SETC  'PLISTVER='                                       00089000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00089100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00089200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00089300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00089400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00089500
                AGO   .ZPVCK5                                           00089600
.ZPVCK4         ANOP  ,                                                 00089700
&XPLISTVER      SETA  &PLISTVER(1)                                      00089800
.ZPVCK5         ANOP  ,                                                 00089900
.ZPVCKE         ANOP  ,                                                 00090000
.********************************************************************   00090100
.***AG.MF01 PROCESS MF MACRO FORMS                                      00090200
.********************************************************************   00090300
                  ACTR  10000                                           00090400
.*------------------------------------------------------------------    00090500
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00090600
&XMF              SETC  ''          INITIALIZE XVARIABLE                00090700
.*-------------------------------------------------------------------   00090800
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00090900
.ZG32             ANOP  ,                                               00091000
&ZTLC             SETC 'S'                                              00091100
                  AIF   ('&MF(1)' NE '&ZTLC').ZG33                      00091200
&XMF              SETC  'S'         SET KEYWORD                         00091300
&XMFSCK           SETC  'COMPLETE'  SET XMFSCK                          00091400
                  AIF   (N'&MF LE 1).ZG32A      CK NUM                  00091500
&ZMS              SETC  '"MF='                                          00091600
&ZMS              SETC  '&ZMS.&MF"'                                     00091700
&ZMS              SETC  '&ZMS "MF"'                                     00091800
                  MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'             00091900
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00092000
.ZG32A            ANOP  ,                                               00092100
.*-------------------------------------------------------------------   00092200
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00092300
.ZG33             ANOP  ,                                               00092400
&ZTLC             SETC 'L'                                              00092500
                  AIF   ('&MF(1)' NE '&ZTLC').ZG40                      00092600
&XMF              SETC  'L'         SET KEYWORD                         00092700
                  AIF   (N'&MF LE 3).ZG33A      CK NUM                  00092800
&ZMS              SETC  '"MF='                                          00092900
&ZMS              SETC  '&ZMS.&MF"'                                     00093000
&ZMS              SETC  '&ZMS "MF"'                                     00093100
                  MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'            00093200
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00093300
.ZG33A            ANOP  ,                                               00093400
.*-------------------------------------------------------------------   00093500
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00093600
                  AIF   (N'&SYSLIST NE 0).ZG33B  POSITIONALS?           00093700
                  AIF   ('&REQUEST' NE '').ZG33B  KEYS?                 00093800
                  AIF   ('&SYMLISTARRAY' NE '').ZG33B  KEYS?            00093900
                  AIF   ('&NUMENTRIES' NE '').ZG33B  KEYS?              00094000
                  AIF   ('&SYMBAREA' NE '').ZG33B  KEYS?                00094100
                  AIF   ('&SYMBAREALEN' NE '').ZG33B  KEYS?             00094200
                  AIF   ('&DIAGDATA' NE '').ZG33B  KEYS?                00094300
                  AIF   ('&RETCODE' NE '').ZG33B  KEYS?                 00094400
                  AIF   ('&RSNCODE' NE '').ZG33B  KEYS?                 00094500
