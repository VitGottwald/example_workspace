                  AIF   ('&XMF' EQ 'L').XLAB3                           00199900
&M0M0001 DS    0H                             IEFSJSYM-0                00200000
.XLAB3            ANOP  ,                                               00200100
.*******************************************************************    00200200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00200300
.*******************************************************************    00200400
              PUSH  PRINT                                               00200500
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00200600
                  PRINT OFF         ++ PR01                             00200700
                  AGO   .ZPRNTXX                                        00200800
.ZPRINTX          PRINT ON                                              00200900
.ZPRNTXX          ANOP  ,                                               00201000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00201100
         AIF   ('&XMF' NE 'M').NOTM                                @P2A 00201200
&ZMS     SETC  'MF=M MAY NOT BE SPECIFIED'                         @P2A 00201300
         MNOTE 8,'&ZMS'                                            @P2A 00201400
         POP   PRINT                                               @P2A 00201500
         MEXIT                                                     @P2A 00201600
.NOTM ANOP                                                         @P2A 00201700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00201800
.********************************************************************   00201900
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00202000
.********************************************************************   00202100
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00202200
                      LCLA  &ZOPN1         INVOCATION INDEX             00202300
                      LCLC  &ZOPL(4095)    LABEL                        00202400
                      LCLC  &ZOPO(4095)    OPERATION                    00202500
                      LCLC  &ZOPV(4095)    VALUE                        00202600
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00202700
.*                                                                      00202800
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00202900
.******************************************************************     00203000
.***AL.L10 IEFSJSYM FMT=LIST MF=S!L PARAMETERS                          00203100
.******************************************************************     00203200
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00203300
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00203400
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00203500
&XMFATTR              SETC  '0D'                                        00203600
