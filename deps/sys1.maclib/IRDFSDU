.*******************************************************************    00110500
.***AO.X03  REASON CODE PROCESSING                                      00110600
.*******************************************************************    00110700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00110800
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00110900
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00111000
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00111100
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00111200
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00111300
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00111400
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00111500
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00111600
&ZOPN                 SETA  &ZOPN+1                                     00111700
&ZOPL(&ZOPN)          SETC  ' '                                         00111800
&ZOPO(&ZOPN)          SETC  'ST'                                        00111900
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00112000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00112100
                      AGO   .ZMRSNX                                     00112200
.*                                                                      00112300
.ZMRSN2               ANOP  ,                                           00112400
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00112500
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00112600
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00112700
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00112800
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00112900
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00113000
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00113100
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00113200
