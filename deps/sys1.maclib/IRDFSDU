.***AC.PLV01  PROCESS PLISTVER                                      *   00040400
.********************************************************************   00040500
&XPLISTVER      SETA  1                                                 00040600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00040700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00040800
&XPLISTVER      SETA  1                                                 00040900
.ZPLV0          ANOP  ,                                                 00041000
.*                                                                      00041100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00041200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00041300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00041400
.*-------------------------------------------------------------------   00041500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00041600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00041700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00041800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00041900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00042000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00042100
                AGO   .ZPVCK2                                           00042200
.ZPVCK3         ANOP  ,                                                 00042300
.*-------------------------------------------------------------------   00042400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00042500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00042600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00042700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00042800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00042900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00043000
                AGO   .ZPVCKE                                           00043100
.ZPVCK2         ANOP  ,                                                 00043200
.*-------------------------------------------------------------------   00043300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00043400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00043500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00043600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00043700
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00043800
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00043900
.ZPVCK6         ANOP  ,                                                 00044000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00044100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00044200
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00044300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00044400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00044500
&XPLISTVER      SETA  1                                                 00044600
                AGO   .ZPVCKE                                           00044700
.*-------------------------------------------------------------------   00044800
.ZPVCK4A        ANOP  ,                                                 00044900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00045000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00045100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00045200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00045300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00045400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00045500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00045600
                AGO   .ZPVCK5                                           00045700
.ZPVCK4         ANOP  ,                                                 00045800
&XPLISTVER      SETA  &PLISTVER(1)                                      00045900
.ZPVCK5         ANOP  ,                                                 00046000
.ZPVCKE         ANOP  ,                                                 00046100
.********************************************************************   00046200
.***AG.MF01 PROCESS MF MACRO FORMS                                      00046300
.********************************************************************   00046400
               ACTR  10000                                              00046500
