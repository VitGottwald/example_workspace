.***AO.X04  LABEL FOR MF=L PROCESSING                                   00114000
.*******************************************************************    00114100
.ZMFLX                ANOP  ,              MF=L PROCESSING              00114200
.*******************************************************************    00114300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00114400
.*******************************************************************    00114500
                      ACTR  10000                                       00114600
.*                                                                      00114700
                      LCLA  &ZLC           COLUMN POSITION              00114800
                      LCLA  &ZOPX          LIST TABLE INDEX             00114900
                      LCLC  &ZLL           LABEL                        00115000
                      LCLC  &ZLOP          OPERATION                    00115100
                      LCLC  &ZLV           VALUE                        00115200
                      LCLC  &ZLD           DESCRIPTION                  00115300
.*                                                                      00115400
&ZOPX                 SETA  1                                           00115500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00115600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00115700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00115800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00115900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00116000
                      AIF   (K'&ZLD LE 0).ZL5                           00116100
&ZLC                  SETA  35             COMMENT COLUMN               00116200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00116300
&ZLC                  SETA  33             COMMMENT COLUMN              00116400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00116500
&ZLV                  SETC  '&ZLV'.' '                                  00116600
                      AGO   .ZL3                                        00116700
.*                                                                      00116800
.ZL4                  ANOP  ,                                           00116900
&ZLV                  SETC  '&ZLV'.' '                                  00117000
.ZL5                  ANOP  ,                                           00117100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00117200
.*                                                                      00117300
&ZLL     &ZLOP &ZLV                                                     00117400
.*                                                                      00117500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00117600
                      AGO   .ZL2           CONTINUE LOOPING             00117700
.*                                                                      00117800
.ZLX                  ANOP  ,                                           00117900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00118000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00118100
         L    15,16(0,0)          ++ GET CVT ADDRESS                    00118200
         L    15,124(,15)         ++ GET IOCOM ADDRESS                  00118300
         L    15,8(,15)           ++ GET IOCOMWPT ADDRESS               00118400
         L    15,56(,15)          ++ GET IECA ADDRESS                   00118500
         ICM  15,15,32(15)        ++ GET AND CHECK IECA_IRDVFSD_EP@     00118600
         BNZ   IRDF&SYSNDX        ++ IF VALID, CALL ROUTINE             00118700
         LA   15,4                ++ SET WARNING RETURN CODE            00118800
         LA   0,1                 ++ SET WARNING REASON CODE            00118900
         B     IRDF2&SYSNDX       ++ BRANCH AROUND CALL                 00119000
IRDF&SYSNDX DS     0H                                                   00119100
         BALR 14,15               ++ CALL SERVICE ROUTINE               00119200
IRDF2&SYSNDX DS     0H                                                  00119300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00119400
.ZLINKX               ANOP  ,                                           00119500
.*******************************************************************    00119600
.***AP.X01  FORMAT GENERATED CODE                                       00119700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00119800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00119900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00120000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00120100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00120200
                      AIF   (K'&ZLD LE 0).ZL51                          00120300
&ZLC                  SETA  35             COMMENT COLUMN               00120400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00120500
&ZLC                  SETA  33             COMMENT COLUMN               00120600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00120700
&ZLV                  SETC  '&ZLV'.' '                                  00120800
                      AGO   .ZL31                                       00120900
.*                                                                      00121000
.ZL41                 ANOP  ,                                           00121100
&ZLV                  SETC  '&ZLV'.' '                                  00121200
.ZL51                 ANOP  ,                                           00121300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00121400
.*                                                                      00121500
&ZLL     &ZLOP &ZLV                                                     00121600
.*                                                                      00121700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00121800
                      AGO   .ZL21          CONTINUE LOOPING             00121900
.*                                                                      00122000
.ZLX1                 ANOP  ,                                           00122100
.*******************************************************************    00122200
.***AP.X02  END OF IRDFSDU MACRO                                        00122300
&ZIRDFSDU             SETC  'YES'          SET IRDFSDU USED             00122400
         POP   PRINT                                                    00122500
*                                            IRDFSDU-1                  00122600
         SPACE 1                                                        00122700
.ZLX2                 ANOP  ,                                           00122800
         MEND                                                           00122900
