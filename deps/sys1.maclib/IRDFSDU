                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00098800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00098900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00099000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00099100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00099200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00099300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00099400
