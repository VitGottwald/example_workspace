&ZMS                  SETC  '&ZMS.1'                                    00698700
                      AGO   .ZI11C                                      00698800
.*                                                                      00698900
.ZI11A                ANOP  ,                                           00699000
&ZW                   SETC  '&ZW.0'                                     00699100
         AIF   ('&ZXTARGET_VIEW' EQ '').ZI11B                           00699200
&ZMS                  SETC  '&ZMS.0'                                    00699300
&ZC                   SETC  'YES'          SET BIT OFF FLAG             00699400
                      AGO   .ZI11C                                      00699500
.*                                                                      00699600
.ZI11B                ANOP  ,                                           00699700
&ZMS                  SETC  '&ZMS.1'                                    00699800
