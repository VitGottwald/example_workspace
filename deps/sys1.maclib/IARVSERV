.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00686600
&ZPLLEN               SETA 12                                           00686700
.*                                                                      00686800
.******************************************************************     00686900
.***AI.L01 IARVSERV FMT=LIST FILL IN PARAMETER AREA                     00687000
.******************************************************************     00687100
                      ACTR  10000                                       00687200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00687300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00687400
&ZCVAL                SETC  'LAE'          XAX MODE                     00687500
.ZIMFXA               ANOP ,                                            00687600
.*                                                                      00687700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00687800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00687900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00688000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00688100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00688200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00688300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00688400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00688500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00688600
&ZOPN                 SETA  &ZOPN+1                                     00688700
&ZOPL(&ZOPN)          SETC  ' '                                         00688800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00688900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00689000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00689100
                      AGO   .ZIMFNX                                     00689200
.*                                                                      00689300
.ZIMFN2               ANOP ,                                            00689400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00689500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00689600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00689700
&ZOPN                 SETA  &ZOPN+1                                     00689800
&ZOPL(&ZOPN)          SETC  ' '                                         00689900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00690000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00690100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00690200
                      AGO   .ZIMFNX                                     00690300
.ZIMFNX1              ANOP ,                                            00690400
&ZOPN                 SETA  &ZOPN+1                                     00690500
&ZOPL(&ZOPN)          SETC  ' '                                         00690600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00690700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00690800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00690900
.ZIMFNX               ANOP ,                                            00691000
.********************************************************************   00691100
.***AI.L02 IARVSERV FMT=LIST INITIALIZE PARAMETER AREA                  00691200
.********************************************************************   00691300
.***AI.L03 MF=(SLME)                                                    00691400
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00691500
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            00691600
.*                                                                      00691700
