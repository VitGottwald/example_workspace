.***AC.PLV01  PROCESS PLISTVER                                      *   00249000
.********************************************************************   00249100
&XPLISTVER      SETA  0                                                 00249200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00249300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00249400
&XPLISTVER      SETA  0                                                 00249500
.ZPLV0          ANOP  ,                                                 00249600
.*                                                                      00249700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00249800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00249900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00250000
.*-------------------------------------------------------------------   00250100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00250200
&ZMS            SETC  '"PLISTVER='                                      00250300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00250400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00250500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00250600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00250700
                AGO   .ZPVCK2                                           00250800
.ZPVCK3         ANOP  ,                                                 00250900
.*-------------------------------------------------------------------   00251000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00251100
&ZMS            SETC  '"PLISTVER='                                      00251200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00251300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00251400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00251500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00251600
                AGO   .ZPVCKE                                           00251700
.ZPVCK2         ANOP  ,                                                 00251800
.*-------------------------------------------------------------------   00251900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00252000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00252100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00252200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00252300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00252400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00252500
.ZPVCK6         ANOP  ,                                                 00252600
&ZMS            SETC  '"PLISTVER='                                      00252700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00252800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00252900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00253000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00253100
&XPLISTVER      SETA  0                                                 00253200
                AGO   .ZPVCKE                                           00253300
.*-------------------------------------------------------------------   00253400
.ZPVCK4A        ANOP  ,                                                 00253500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00253600
&ZMS            SETC  'PLISTVER='                                       00253700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00253800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00253900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00254000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00254100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00254200
                AGO   .ZPVCK5                                           00254300
.ZPVCK4         ANOP  ,                                                 00254400
&XPLISTVER      SETA  &PLISTVER(1)                                      00254500
.ZPVCK5         ANOP  ,                                                 00254600
.ZPVCKE         ANOP  ,                                                 00254700
.********************************************************************   00254800
