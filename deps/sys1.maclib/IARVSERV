.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00735600
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00735700
.*******************************************************************    00735800
.***  PROCESS IARVSERV OUTPUT PARAMETERS                                00735900
.*******************************************************************    00736000
                      ACTR  10000                                       00736100
.*******************************************************************    00736200
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00736300
.*******************************************************************    00736400
.ZMFLX                ANOP  ,              MF=L PROCESSING              00736500
.*******************************************************************    00736600
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00736700
.*******************************************************************    00736800
                      ACTR  10000                                       00736900
.*                                                                      00737000
                      LCLA  &ZLC           COLUMN POSITION              00737100
                      LCLA  &ZOPX          LIST TABLE INDEX             00737200
                      LCLC  &ZLL           LABEL                        00737300
                      LCLC  &ZLOP          OPERATION                    00737400
                      LCLC  &ZLV           VALUE                        00737500
                      LCLC  &ZLD           DESCRIPTION                  00737600
.*                                                                      00737700
&ZOPX                 SETA  1                                           00737800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00737900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00738000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00738100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00738200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00738300
                      AIF   (K'&ZLD LE 0).ZL5                           00738400
&ZLC                  SETA  35             COMMENT COLUMN               00738500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00738600
&ZLC                  SETA  33             COMMMENT COLUMN              00738700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00738800
&ZLV                  SETC  '&ZLV'.' '                                  00738900
                      AGO   .ZL3                                        00739000
.*                                                                      00739100
.ZL4                  ANOP  ,                                           00739200
&ZLV                  SETC  '&ZLV'.' '                                  00739300
.ZL5                  ANOP  ,                                           00739400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00739500
.*                                                                      00739600
&ZLL     &ZLOP &ZLV                                                     00739700
.*                                                                      00739800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00739900
                      AGO   .ZL2           CONTINUE LOOPING             00740000
.*                                                                      00740100
.ZLX                  ANOP  ,                                           00740200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00740300
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00740400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00740500
         AIF   ('&XBRANCH' EQ 'YES').PC001                              00740600
         AIF  ('&SYSAM64' EQ 'YES').AM641                               00740700
         L     14,16(0,0)        GET CVT ADDRESS                        00740800
         AGO   .AM31641                                                 00740900
.AM641   ANOP                                                           00741000
         LLGT  14,16(0,0)        GET CVT ADDRESS                        00741100
.AM31641 ANOP                                                           00741200
         L     14,772(14,0)      GET SYSTEM FUNCTION TABLE ADDRESS      00741300
         L     14,208(14,0)      GET LX/EX FOR SERVICE ROUTINE          00741400
         LA    15,12             EX OFFSET FOR IARVSERV ASSIGN          00741500
         OR    14,15             GENERATE LX/EX COMBINATION             00741600
         PC    0(14)             PC TO IARVSERV SERVICE                 00741700
         AGO   .PC002                                                   00741800
.PC001   ANOP  ,                                                        00741900
         AIF  ('&SYSAM64' NE 'YES').AM31642                             00742000
         MNOTE 12,'IARVSERV ---> BRANCH=YES IS NOT VALID IN AMODE 64'   00742100
.AM31642 ANOP                                                           00742200
         L     15,16(0,0)        Get CVT address                        00742300
