.***AM.L05 XSERVICE XL1                                                 00661000
&ZOPN                 SETA &ZOPN+1                                      00661100
&ZOPL(&ZOPN)          SETC '&XMFCTRL._XSERVICE'                         00661200
&ZOPO(&ZOPN)          SETC 'DS'                                         00661300
&ZOPV(&ZOPN)          SETC 'XL1'                                        00661400
&ZOPD(&ZOPN)          SETC '++ &ZXSERVICE XSERVICE'                     00661500
.*                                                                      00661600
&ZOPN                 SETA  &ZOPN+1                                     00661700
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._SHARE'                           00661800
&ZOPO(&ZOPN)          SETC  'EQU'                                       00661900
&ZOPV(&ZOPN)          SETC  '1'                                         00662000
&ZOPD(&ZOPN)          SETC  '++ XSERVICE.'                              00662100
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).SHARE'                        00662200
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00662300
.*                                                                      00662400
&ZOPN                 SETA  &ZOPN+1                                     00662500
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._UNSHARE'                         00662600
&ZOPO(&ZOPN)          SETC  'EQU'                                       00662700
&ZOPV(&ZOPN)          SETC  '2'                                         00662800
&ZOPD(&ZOPN)          SETC  '++ XSERVICE.'                              00662900
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).UNSHARE'                      00663000
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00663100
.*                                                                      00663200
&ZOPN                 SETA  &ZOPN+1                                     00663300
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._CHANGEACCESS'                    00663400
&ZOPO(&ZOPN)          SETC  'EQU'                                       00663500
&ZOPV(&ZOPN)          SETC  '3'                                         00663600
&ZOPD(&ZOPN)          SETC  '++ XSERVICE.'                              00663700
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).CHANGEACCESS'                 00663800
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00663900
.*                                                                      00664000
&ZOPN                 SETA  &ZOPN+1                                     00664100
