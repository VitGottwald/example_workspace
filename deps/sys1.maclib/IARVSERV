.***AE.01  START OF IARVSERV MACRO CODE                                 00652400
.*******************************************************************    00652500
                     LCLC  &M0M0001                 NAME                00652600
&M0M0001             SETC  '&XLABEL'                SET NAME            00652700
                     AIF   (K'&XLABEL NE 0).XLAB2                       00652800
&M0M0001             SETC  'M00M&SYSNDX'            DEFAULT NAME        00652900
.*                                                                      00653000
.XLAB2               ANOP  ,                                            00653100
                     AIF   ('&XMF' EQ 'L').XLAB3                        00653200
&M0M0001 DS    0H                             IARVSERV-0                00653300
.XLAB3               ANOP  ,                                            00653400
.*******************************************************************    00653500
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00653600
.*******************************************************************    00653700
              PUSH  PRINT                                               00653800
                     AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON   00653900
                     PRINT OFF         ++ PR01                          00654000
                     AGO   .ZPRNTXX                                     00654100
.ZPRINTX             PRINT ON                                           00654200
.ZPRNTXX             ANOP  ,                                            00654300
.********************************************************************   00654400
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00654500
.********************************************************************   00654600
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00654700
                      LCLA  &ZOPN1         INVOCATION INDEX             00654800
                      LCLC  &ZOPL(4095)    LABEL                        00654900
                      LCLC  &ZOPO(4095)    OPERATION                    00655000
                      LCLC  &ZOPV(4095)    VALUE                        00655100
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00655200
.*                                                                      00655300
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00655400
.******************************************************************     00655500
.***AL.L10 IARVSERV FMT=LIST MF=S!L PARAMETERS                          00655600
.******************************************************************     00655700
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00655800
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00655900
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00656000
&XMFATTR              SETC  '0D'                                        00656100
.*                                                                      00656200
&ZOPN                 SETA  &ZOPN+1                                     00656300
&ZOPL(&ZOPN)          SETC  ' '                                         00656400
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00656500
&ZOPO(&ZOPN)          SETC  'B'                                         00656600
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00656700
                      AGO   .ZALVL6B                                    00656800
.ZALVL6A              ANOP                                              00656900
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00657000
&ZOPV(&ZOPN)          SETC  '4,8'                                       00657100
&ZOPN                 SETA  &ZOPN+1                                     00657200
&ZOPL(&ZOPN)          SETC  ' '                                         00657300
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00657400
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00657500
.ZALVL6B              ANOP                                              00657600
&ZOPD(&ZOPN)          SETC  '++ IARVSERV ARG PARMS'                     00657700
.*                                                                      00657800
&ZOPN                 SETA  &ZOPN+1                                     00657900
&ZOPL(&ZOPN)          SETC  ' '                                         00658000
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00658100
&ZOPV(&ZOPN)          SETC  '1'                                         00658200
&ZOPD(&ZOPN)          SETC  ' '                                         00658300
.*                                                                      00658400
.ZLMFSL2              ANOP  ,                                           00658500
.*-------------------------------------------------------------------   00658600
.***AM.L00  IARVSERV FMT=LIST name                                      00658700
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00658800
&ZOPN                 SETA &ZOPN+1                                      00658900
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00659000
&ZOPO(&ZOPN)          SETC 'EQU'                                        00659100
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00659200
&ZOPD(&ZOPN)          SETC '++ IARVSERV NAME'                           00659300
.ZLMFSL2M             ANOP  ,                                           00659400
.*-------------------------------------------------------------------   00659500
.***AM.L01  IARVSERV FMT=LIST PARAMETERS                                00659600
