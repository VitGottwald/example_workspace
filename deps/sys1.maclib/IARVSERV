.***AP.X01  FORMAT GENERATED CODE                                       00743200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00743300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00743400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00743500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00743600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00743700
                      AIF   (K'&ZLD LE 0).ZL51                          00743800
&ZLC                  SETA  35             COMMENT COLUMN               00743900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00744000
&ZLC                  SETA  33             COMMENT COLUMN               00744100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00744200
&ZLV                  SETC  '&ZLV'.' '                                  00744300
                      AGO   .ZL31                                       00744400
.*                                                                      00744500
.ZL41                 ANOP  ,                                           00744600
&ZLV                  SETC  '&ZLV'.' '                                  00744700
.ZL51                 ANOP  ,                                           00744800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00744900
.*                                                                      00745000
&ZLL     &ZLOP &ZLV                                                     00745100
.*                                                                      00745200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00745300
                      AGO   .ZL21          CONTINUE LOOPING             00745400
.*                                                                      00745500
.ZLX1                 ANOP  ,                                           00745600
.*******************************************************************    00745700
.***AP.X02  END OF IARVSERV MACRO                                       00745800
