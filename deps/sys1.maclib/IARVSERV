&ZOPO(&ZOPN)          SETC  'XC'                                        00692000
&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00692100
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00692200
.ZIMFLX               ANOP  ,                                           00692300
