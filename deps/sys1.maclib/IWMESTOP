.********************************************************************   00124300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00124400
.********************************************************************   00124500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00124600
                      LCLA  &ZOPN1         INVOCATION INDEX             00124700
                      LCLC  &ZOPL(4095)    LABEL                        00124800
                      LCLC  &ZOPO(4095)    OPERATION                    00124900
                      LCLC  &ZOPV(4095)    VALUE                        00125000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00125100
.*                                                                      00125200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00125300
.******************************************************************     00125400
.***AL.L10 IWMESTOP FMT=LIST MF=S!L PARAMETERS                          00125500
.******************************************************************     00125600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00125700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00125800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00125900
&XMFATTR              SETC  '0D'                                        00126000
.*                                                                      00126100
&ZOPN                 SETA  &ZOPN+1                                     00126200
&ZOPL(&ZOPN)          SETC  ' '                                         00126300
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00126400
&ZOPV(&ZOPN)          SETC  '4,8'                                       00126500
&ZOPN                 SETA  &ZOPN+1                                     00126600
&ZOPL(&ZOPN)          SETC  ' '                                         00126700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00126800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00126900
