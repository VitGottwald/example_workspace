.***AC.PLV01  PROCESS PLISTVER                                      *   00067700
.********************************************************************   00067800
&XPLISTVER      SETA  0                                                 00067900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00068000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00068100
&XPLISTVER      SETA  0                                                 00068200
.ZPLV0          ANOP  ,                                                 00068300
.*                                                                      00068400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00068500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00068600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00068700
.*-------------------------------------------------------------------   00068800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00068900
&ZMS            SETC  '"PLISTVER='                                      00069000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00069100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00069200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00069300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069400
                AGO   .ZPVCK2                                           00069500
.ZPVCK3         ANOP  ,                                                 00069600
.*-------------------------------------------------------------------   00069700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00069800
&ZMS            SETC  '"PLISTVER='                                      00069900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00070000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00070100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00070200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070300
                AGO   .ZPVCKE                                           00070400
.ZPVCK2         ANOP  ,                                                 00070500
.*-------------------------------------------------------------------   00070600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00070700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00070800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00070900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00071000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00071100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00071200
.ZPVCK6         ANOP  ,                                                 00071300
&ZMS            SETC  '"PLISTVER='                                      00071400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00071600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00071700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071800
&XPLISTVER      SETA  0                                                 00071900
                AGO   .ZPVCKE                                           00072000
.*-------------------------------------------------------------------   00072100
.ZPVCK4A        ANOP  ,                                                 00072200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00072300
&ZMS            SETC  'PLISTVER='                                       00072400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00072500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00072600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00072700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00072800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00072900
                AGO   .ZPVCK5                                           00073000
.ZPVCK4         ANOP  ,                                                 00073100
&XPLISTVER      SETA  &PLISTVER(1)                                      00073200
.ZPVCK5         ANOP  ,                                                 00073300
.ZPVCKE         ANOP  ,                                                 00073400
.********************************************************************   00073500
