              MNOTE 8,' &ZMS ARE ALLOWED.'                              00103900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00104000
.ZGLISTX      ANOP  ,                                                   00104100
&ZASYSLNDX    SETA  0                                                   00104200
.*-------------------------------------------------------------------   00104300
.***AG.MF04 MF=(SLE)                                                    00104400
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00104500
.*******************************************************************    00104600
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00104700
.*******************************************************************    00104800
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00104900
.*******************************************************************    00105000
.***AE.01  START OF IWMSSEM MACRO CODE                                  00105100
.*******************************************************************    00105200
              LCLC  &M0M0001                 NAME                       00105300
&M0M0001      SETC  '&XLABEL'                SET NAME                   00105400
              AIF   (K'&XLABEL NE 0).XLAB2                              00105500
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00105600
.*                                                                      00105700
.XLAB2        ANOP  ,                                                   00105800
              AIF   ('&XMF' EQ 'L').XLAB3                               00105900
&M0M0001 DS    0H                             IWMSSEM-0                 00106000
.XLAB3        ANOP  ,                                                   00106100
.*******************************************************************    00106200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00106300
.*******************************************************************    00106400
              PUSH  PRINT                                               00106500
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00106600
              PRINT OFF         ++ PR01                                 00106700
              AGO   .ZPRNTXX                                            00106800
.ZPRINTX      PRINT ON                                                  00106900
.ZPRNTXX      ANOP  ,                                                   00107000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00107100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00107200
.********************************************************************   00107300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00107400
.********************************************************************   00107500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00107600
                      LCLA  &ZOPN1         INVOCATION INDEX             00107700
                      LCLC  &ZOPL(4095)    LABEL                        00107800
                      LCLC  &ZOPO(4095)    OPERATION                    00107900
                      LCLC  &ZOPV(4095)    VALUE                        00108000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00108100
.*                                                                      00108200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00108300
.******************************************************************     00108400
.***AL.L10 IWMSSEM FMT=LIST MF=S!L PARAMETERS                           00108500
.******************************************************************     00108600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00108700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00108800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00108900
&XMFATTR              SETC  '0D'                                        00109000
.*                                                                      00109100
&ZOPN                 SETA  &ZOPN+1                                     00109200
&ZOPL(&ZOPN)          SETC  ' '                                         00109300
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00109400
&ZOPO(&ZOPN)          SETC  'B'                                         00109500
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00109600
                      AGO   .ZALVL6B                                    00109700
.ZALVL6A              ANOP                                              00109800
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00109900
&ZOPV(&ZOPN)          SETC  '4,8'                                       00110000
&ZOPN                 SETA  &ZOPN+1                                     00110100
&ZOPL(&ZOPN)          SETC  ' '                                         00110200
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00110300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00110400
.ZALVL6B              ANOP                                              00110500
&ZOPD(&ZOPN)          SETC  '++ IWMSSEM ARG PARMS'                      00110600
.*                                                                      00110700
&ZOPN                 SETA  &ZOPN+1                                     00110800
&ZOPL(&ZOPN)          SETC  ' '                                         00110900
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00111000
&ZOPV(&ZOPN)          SETC  '1'                                         00111100
&ZOPD(&ZOPN)          SETC  ' '                                         00111200
.*                                                                      00111300
.ZLMFSL2              ANOP  ,                                           00111400
.*-------------------------------------------------------------------   00111500
.***AM.L00  IWMSSEM FMT=LIST name                                       00111600
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00111700
&ZOPN                 SETA &ZOPN+1                                      00111800
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00111900
&ZOPO(&ZOPN)          SETC 'EQU'                                        00112000
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00112100
&ZOPD(&ZOPN)          SETC '++ IWMSSEM NAME'                            00112200
.ZLMFSL2M             ANOP  ,                                           00112300
.*-------------------------------------------------------------------   00112400
.***AM.L01  IWMSSEM FMT=LIST PARAMETERS                                 00112500
