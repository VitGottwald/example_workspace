.*-------------------------------------------------------------------   00124800
.***AI.L04 XPLISTLEN XL2                                                00124900
&ZOPN                 SETA  &ZOPN+1                                     00125000
&ZOPL(&ZOPN)          SETC  ' '                                         00125100
&ZOPO(&ZOPN)          SETC  'MVC'                                       00125200
&ZOPV(&ZOPN)          SETC  '2(2,1),=AL2(&ZPLLEN)'                      00125300
&ZOPD(&ZOPN)          SETC  '++ INPUT XPLISTLEN XL2'                    00125400
.*-------------------------------------------------------------------   00125500
&ZOPN1                SETA &ZOPN                                        00125600
.*******************************************************************    00125700
.***  PROCESS IWMSSEM OUTPUT PARAMETERS                                 00125800
.*******************************************************************    00125900
                      ACTR  10000                                       00126000
.*------------------------------------------------------------------    00126100
.***AO.L02 XUSERDATA CL16                                               00126200
         AIF   ('&ZXUSERDATA' EQ 'OUTPUT').ZO4A  OUTPUT                 00126300
         AIF   ('&ZXUSERDATA' NE 'INOUT').ZO4C  INOUT                   00126400
.ZO4A    AIF   ('&XUSERDATA'(1,1) NE '(').ZO4B  ADDR IN GPR             00126500
&ZCGPR   SETC  '&XUSERDATA'(2,K'&XUSERDATA-2)                           00126600
&ZOPN                 SETA  &ZOPN+1                                     00126700
&ZOPL(&ZOPN)          SETC  ' '                                         00126800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00126900
&ZOPV(&ZOPN)          SETC  '0(16,&ZCGPR),4(1)'                         00127000
&ZOPD(&ZOPN)          SETC  '++ XUSERDATA CL16'                         00127100
                      AGO   .ZO4C                                       00127200
.*                                                                      00127300
.ZO4B                 ANOP  ,                                           00127400
&ZAWRK   SETA  K'&XUSERDATA                                             00127500
         AIF   ('&XUSERDATA'(&ZAWRK,1) NE ')').ZO4G                     00127600
&ZN      SETA  2                                                        00127700
.ZO4H    ANOP                                                           00127800
         AIF   (&ZN GE &ZAWRK).ZO4G                                     00127900
         AIF   ('&XUSERDATA'(&ZN,1) NE '(').ZO4I                        00128000
&ZC      SETC  '&XUSERDATA'(1,&ZN).'16'.','                             00128100
