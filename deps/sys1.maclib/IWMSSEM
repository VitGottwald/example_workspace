.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00136300
.*******************************************************************    00136400
                      ACTR  10000                                       00136500
.*                                                                      00136600
                      LCLA  &ZLC           COLUMN POSITION              00136700
                      LCLA  &ZOPX          LIST TABLE INDEX             00136800
                      LCLC  &ZLL           LABEL                        00136900
                      LCLC  &ZLOP          OPERATION                    00137000
                      LCLC  &ZLV           VALUE                        00137100
                      LCLC  &ZLD           DESCRIPTION                  00137200
.*                                                                      00137300
&ZOPX                 SETA  1                                           00137400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00137500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00137600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00137700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00137800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00137900
                      AIF   (K'&ZLD LE 0).ZL5                           00138000
&ZLC                  SETA  35             COMMENT COLUMN               00138100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00138200
&ZLC                  SETA  33             COMMMENT COLUMN              00138300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00138400
&ZLV                  SETC  '&ZLV'.' '                                  00138500
                      AGO   .ZL3                                        00138600
.*                                                                      00138700
.ZL4                  ANOP  ,                                           00138800
&ZLV                  SETC  '&ZLV'.' '                                  00138900
.ZL5                  ANOP  ,                                           00139000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00139100
.*                                                                      00139200
&ZLL     &ZLOP &ZLV                                                     00139300
.*                                                                      00139400
