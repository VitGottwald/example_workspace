.***AC.PLV01  PROCESS PLISTVER                                      *   00062600
.********************************************************************   00062700
&XPLISTVER      SETA  0                                                 00062800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00062900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00063000
&XPLISTVER      SETA  0                                                 00063100
.ZPLV0          ANOP  ,                                                 00063200
.*                                                                      00063300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00063400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00063500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00063600
.*-------------------------------------------------------------------   00063700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00063800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00063900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00064100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00064200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064300
                AGO   .ZPVCK2                                           00064400
.ZPVCK3         ANOP  ,                                                 00064500
.*-------------------------------------------------------------------   00064600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00064700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00064800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00065000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00065100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065200
                AGO   .ZPVCKE                                           00065300
.ZPVCK2         ANOP  ,                                                 00065400
.*-------------------------------------------------------------------   00065500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00065600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00065700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00065800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00065900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00066000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00066100
.ZPVCK6         ANOP  ,                                                 00066200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00066300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066400
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00066500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00066600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066700
&XPLISTVER      SETA  0                                                 00066800
                AGO   .ZPVCKE                                           00066900
.*-------------------------------------------------------------------   00067000
.ZPVCK4A        ANOP  ,                                                 00067100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00067200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00067300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00067400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00067500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00067600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00067700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067800
                AGO   .ZPVCK5                                           00067900
.ZPVCK4         ANOP  ,                                                 00068000
&XPLISTVER      SETA  &PLISTVER(1)                                      00068100
.ZPVCK5         ANOP  ,                                                 00068200
.ZPVCKE         ANOP  ,                                                 00068300
