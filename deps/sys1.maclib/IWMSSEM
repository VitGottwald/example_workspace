&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00090200
.ZG2A         ANOP  ,                                                   00090300
.*-------------------------------------------------------------------   00090400
.***AG.KXVAR06 NAME, RQD OUTPUT, KEYS -XVARIABLE                        00090500
&ZXUSERDATA   SETC  'OUTPUT'    SET IN/OUTPUT                           00090600
&XUSERDATA    SETC  '&USERDATA(1)'              SET XVARIABLE           00090700
              AIF   (N'&USERDATA GT 1).ZG2B     MULTI PARMS             00090800
&XUSERDATA    SETC  '&USERDATA'                 SET XVARIABLE           00090900
.ZG2B         AIF   (K'&USERDATA(1) GT 0).ZG2C                          00091000
&ZXUSERDATA   SETC  ''          RESET IN/OUTPUT                         00091100
&ZMS          SETC  ' '         *KRQDA*                                 00091200
&ZMS          SETC  '&ZMS "USERDATA" KEY AND ITS'                       00091300
              MNOTE 8,' &ZMS ARGUMENT ARE REQUIRED.'                    00091400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00091500
.ZG2C         ANOP  ,                                                   00091600
.*-------------------------------------------------------------------   00091700
.***AG.KCK02 NAME, RQD OUTPUT, KEYS -GPR OUT OF RANGE                   00091800
              AIF   (K'&ZXUSERDATA LE 0).ZG2R   NOT I/O                 00091900
              AIF   ('&XUSERDATA'(1,1) NE '(').ZG2R                     00092000
&ZCGPR        SETC  '&XUSERDATA'(2,K'&XUSERDATA-2)                      00092100
              AIF   ('&ZCGPR'(1,1) NE '0').ZG2P                         00092200
              AIF   (K'&ZCGPR GT 2).ZG2Q                                00092300
              AIF   ('&ZCGPR' EQ '0').ZG2Q                              00092400
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00092500
              AIF   (T'&ZCGPR NE 'N').ZG2Q      INVALID REG NAME        00092600
.ZG2P         ANOP  ,                                                   00092700
              AIF   (T'&ZCGPR NE 'N').ZG2R      NON NUMERIC             00092800
              AIF   ('&ZCGPR' LT '2').ZG2Q                              00092900
              AIF   ('&ZCGPR' LE '12').ZG2R                             00093000
.ZG2Q         ANOP  ,                                                   00093100
&ZMS          SETC  '"USERDATA='                *KGPRA* MNOTE           00093200
&ZMS          SETC  '&ZMS.&USERDATA"'                                   00093300
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00093400
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00093500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00093600
.ZG2R         ANOP  ,                                                   00093700
.*----------------------------------------------------------------***   00093800
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00093900
