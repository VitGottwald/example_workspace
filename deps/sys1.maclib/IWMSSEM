.******************************************************************     00101400
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00101500
.******************************************************************     00101600
              ACTR  10000                                               00101700
.*----------------------------------------------------------------***   00101800
.***AH.P02 EXTRA POSITIONALS                                            00101900
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00102000
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00102100
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00102200
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00102300
.ZGLISTX1     ANOP  ,                                                   00102400
&ZCVAL        SETC  ''          ERROR FIELD                             00102500
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00102600
.ZGLIST2      ANOP  ,                                                   00102700
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00102800
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00102900
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00103000
              AGO   .ZGLIST2                                            00103100
.*                                                                      00103200
.ZGLIST3      ANOP  ,                                                   00103300
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00103400
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'   *PPOSA* MNOTE           00103500
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00103600
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00103700
