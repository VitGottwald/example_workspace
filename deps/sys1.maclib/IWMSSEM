&ZOPD(&ZOPN)          SETC '++ LENGTH OF PLIST'                         00117200
.*                                                                      00117300
&ZOPN                 SETA &ZOPN+1                                      00117400
&ZOPL(&ZOPN)          SETC ' '                                          00117500
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00117600
&ZOPV(&ZOPN)          SETC '1'                                          00117700
&ZOPD(&ZOPN)          SETC ' '                                          00117800
.*-------------------------------------------------------------------   00117900
.***AL.L12 IWMSSEM FMT=LIST MF=S!L PARAMETERS                           00118000
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00118100
.*                                                                      00118200
&ZOPN                 SETA  &ZOPN+1                                     00118300
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00118400
&ZOPO(&ZOPN)          SETC  'DC'                                        00118500
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00118600
&ZOPD(&ZOPN)          SETC  '++ IWMSSEM ARG PARMS'                      00118700
.*                                                                      00118800
.ZLMFSLX              ANOP  ,                                           00118900
.*-------------------------------------------------------------------   00119000
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00119100
&ZPLLEN               SETA 28                                           00119200
.*                                                                      00119300
.******************************************************************     00119400
.***AI.L01 IWMSSEM FMT=LIST FILL IN PARAMETER AREA                      00119500
.******************************************************************     00119600
                      ACTR  10000                                       00119700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00119800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00119900
&ZCVAL                SETC  'LAE'          XAX MODE                     00120000
.ZIMFXA               ANOP ,                                            00120100
.*                                                                      00120200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00120300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00120400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00120500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00120600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00120700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00120800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00120900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00121000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00121100
&ZOPN                 SETA  &ZOPN+1                                     00121200
&ZOPL(&ZOPN)          SETC  ' '                                         00121300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00121400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00121500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00121600
                      AGO   .ZIMFNX                                     00121700
.*                                                                      00121800
.ZIMFN2               ANOP ,                                            00121900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00122000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00122100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00122200
&ZOPN                 SETA  &ZOPN+1                                     00122300
&ZOPL(&ZOPN)          SETC  ' '                                         00122400
&ZOPO(&ZOPN)          SETC  'LAE'                                       00122500
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00122600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00122700
                      AGO   .ZIMFNX                                     00122800
.ZIMFNX1              ANOP ,                                            00122900
&ZOPN                 SETA  &ZOPN+1                                     00123000
&ZOPL(&ZOPN)          SETC  ' '                                         00123100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00123200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00123300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00123400
.ZIMFNX               ANOP ,                                            00123500
.********************************************************************   00123600
.***AI.L02 IWMSSEM FMT=LIST INITIALIZE PARAMETER AREA                   00123700
.********************************************************************   00123800
.***AI.L03 MF=(SLE)                                                     00123900
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00124000
.*                                                                      00124100
