         MACRO                                                          00008000
         IHASDMSE &DSECT=YES,&LIST=YES,&TITLE=YES,&SDMSE=YES,          *00009000
               &SDMSE_MODEL=COND,&SDMSE_ASIDLST=COND,                  *00010000
               &SDMSE_STORAGE=COND,&SDMSE_STORAGE64=COND,              *00011000
               &SDMSE_JOBLIST=COND,&SDMSE_DSPLIST=COND,                *00012000
               &SDMSE_SUBPLST=COND,&SDMSE_KEYLIST=COND                  00013000
         GBLC  &IHASDMSE_INCLUDED                                       00014000
         GBLC  &ZCBPRINT                                                00015000
&IHASDMSE_INCLUDED SETC 'YES'                                           00016000
         AIF   ('&LIST' EQ 'NONE').P0                                   00017000
         AIF   ('&TITLE' EQ 'NO').P5                                    00018000
