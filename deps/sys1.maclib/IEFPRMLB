&ZOPL(&ZOPN)          SETC  ' '                                         00753500
&ZOPO(&ZOPN)          SETC  'ST'                                        00753600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00753700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00753800
                      AGO   .ZMRSNX                                     00753900
.*                                                                      00754000
.ZMRSN2               ANOP  ,                                           00754100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00754200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00754300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00754400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00754500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00754600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00754700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00754800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00754900
