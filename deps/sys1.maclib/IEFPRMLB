                 LCLC  &ZXALLOCDDNAME           ++ INOUT NAME           00193900
                 LCLC  &ZXREAD     ++ INPUT VALUE                       00194000
                 LCLC  &ZXMEMNAME  ++ INPUT NAME                        00194100
                 LCLC  &ZXREADBUF  ++ INOUT NAME                        00194200
                 LCLC  &ZXBLANK72  ++ INPUT VALUE                       00194300
                 LCLC  &ZXSTARCOMMENT           ++ INPUT VALUE          00194400
                 LCLC  &ZXSYMBOLSUB             ++ INPUT VALUE          00194500
                 LCLC  &ZXREMOVECOM             ++ INPUT VALUE          00194600
                 LCLC  &ZXRDSNINFO              ++ OUTPUT NAME          00194700
                 LCLC  &ZXMEMNOTFOUND           ++ INPUT VALUE          00194800
                 LCLC  &ZXFREECLOSE             ++ INPUT VALUE          00194900
                 LCLC  &ZXDDNAME   ++ INPUT NAME                        00195000
                 LCLC  &ZXMSG      ++ INPUT VALUE                       00195100
                 LCLC  &ZXBUFFER   ++ INPUT NAME                        00195200
                 LCLC  &ZXCALLERNAME            ++ INPUT NAME           00195300
                 LCLC  &ZXRETCODE  ++ OUTPUT NAME                       00195400
                 LCLC  &ZXRSNCODE  ++ OUTPUT NAME                       00195500
                 LCLC  &ZXPLISTVER              ++ INPUT VALUE          00195600
                 LCLC  &ZXMF       ++ INPUT VALUE                       00195700
                 LCLC  &ZXMFCTRL   ++ INPUT NAME                        00195800
                 LCLC  &ZXMFATTR   ++ INPUT VALUE                       00195900
                 LCLC  &ZXMFSCK    ++ INPUT VALUE                       00196000
.********************************************************************   00196100
.***AC.PLV01  PROCESS PLISTVER                                      *   00196200
.********************************************************************   00196300
&XPLISTVER      SETA  0                                                 00196400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00196500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00196600
&XPLISTVER      SETA  1                                                 00196700
.ZPLV0          ANOP  ,                                                 00196800
.*                                                                      00196900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00197000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00197100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00197200
.*-------------------------------------------------------------------   00197300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00197400
&ZMS            SETC  '"PLISTVER='                                      00197500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00197600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00197700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00197800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00197900
                AGO   .ZPVCK2                                           00198000
.ZPVCK3         ANOP  ,                                                 00198100
.*-------------------------------------------------------------------   00198200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00198300
&ZMS            SETC  '"PLISTVER='                                      00198400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00198500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00198600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00198700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00198800
                AGO   .ZPVCKE                                           00198900
.ZPVCK2         ANOP  ,                                                 00199000
.*-------------------------------------------------------------------   00199100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00199200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00199300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00199400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00199500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00199600
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00199700
.ZPVCK6         ANOP  ,                                                 00199800
&ZMS            SETC  '"PLISTVER='                                      00199900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00200000
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00200100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00200200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00200300
&XPLISTVER      SETA  1                                                 00200400
                AGO   .ZPVCKE                                           00200500
.*-------------------------------------------------------------------   00200600
.ZPVCK4A        ANOP  ,                                                 00200700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00200800
&ZMS            SETC  'PLISTVER='                                       00200900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00201000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00201100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00201200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00201300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00201400
                AGO   .ZPVCK5                                           00201500
.ZPVCK4         ANOP  ,                                                 00201600
&XPLISTVER      SETA  &PLISTVER(1)                                      00201700
.ZPVCK5         ANOP  ,                                                 00201800
.ZPVCKE         ANOP  ,                                                 00201900
