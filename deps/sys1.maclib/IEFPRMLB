.*******************************************************************    00755600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00755700
.*******************************************************************    00755800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00755900
.*******************************************************************    00756000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00756100
.*******************************************************************    00756200
                      ACTR  10000                                       00756300
.*                                                                      00756400
                      LCLA  &ZLC           COLUMN POSITION              00756500
                      LCLA  &ZOPX          LIST TABLE INDEX             00756600
                      LCLC  &ZLL           LABEL                        00756700
                      LCLC  &ZLOP          OPERATION                    00756800
                      LCLC  &ZLV           VALUE                        00756900
                      LCLC  &ZLD           DESCRIPTION                  00757000
.*                                                                      00757100
&ZOPX                 SETA  1                                           00757200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00757300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00757400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00757500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00757600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00757700
                      AIF   (K'&ZLD LE 0).ZL5                           00757800
&ZLC                  SETA  35             COMMENT COLUMN               00757900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00758000
&ZLC                  SETA  33             COMMMENT COLUMN              00758100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00758200
&ZLV                  SETC  '&ZLV'.' '                                  00758300
                      AGO   .ZL3                                        00758400
.*                                                                      00758500
.ZL4                  ANOP  ,                                           00758600
&ZLV                  SETC  '&ZLV'.' '                                  00758700
.ZL5                  ANOP  ,                                           00758800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00758900
.*                                                                      00759000
&ZLL     &ZLOP &ZLV                                                     00759100
.*                                                                      00759200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00759300
                      AGO   .ZL2           CONTINUE LOOPING             00759400
.*                                                                      00759500
.ZLX                  ANOP  ,                                           00759600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00759700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00759800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00759900
         L     15,16(0,0)              GET CVT ADDRESS             @L1A 00760000
         L     15,772(15,0)            GET SFT ADDRESS             @L1A 00760100
.*********************************************************************  00760200
.*                                                                   *  00760300
.*  Get LX/EX for Logical Parmlib Service                            *  00760400
.*                                                                   *  00760500
.*  Note: LX/EX is calculated by multiplying the PC number (115)     *  00760600
.*        times the length of a PC/AUTH table entry (4) and          *  00760700
.*        then subtracting 4.                                        *  00760800
.*                                                                   *  00760900
.*        (115*4)-4 = 456                                            *  00761000
.*                                                                   *  00761100
.*********************************************************************  00761200
         L     15,456(15,0)       ++ Get PC number                 @L1A 00761300
         PC    0(15)              ++ PC to Logical Parmlib Service @L1A 00761400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00761500
.ZLINKX               ANOP  ,                                           00761600
.*******************************************************************    00761700
.***AP.X01  FORMAT GENERATED CODE                                       00761800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00761900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00762000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00762100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00762200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00762300
                      AIF   (K'&ZLD LE 0).ZL51                          00762400
&ZLC                  SETA  35             COMMENT COLUMN               00762500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00762600
&ZLC                  SETA  33             COMMENT COLUMN               00762700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00762800
&ZLV                  SETC  '&ZLV'.' '                                  00762900
                      AGO   .ZL31                                       00763000
.*                                                                      00763100
.ZL41                 ANOP  ,                                           00763200
&ZLV                  SETC  '&ZLV'.' '                                  00763300
.ZL51                 ANOP  ,                                           00763400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00763500
.*                                                                      00763600
&ZLL     &ZLOP &ZLV                                                     00763700
.*                                                                      00763800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00763900
                      AGO   .ZL21          CONTINUE LOOPING             00764000
.*                                                                      00764100
.ZLX1                 ANOP  ,                                           00764200
.*******************************************************************    00764300
.***AP.X02  END OF IEFPRMLB MACRO                                       00764400
