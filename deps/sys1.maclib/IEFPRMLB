&ZOPL(&ZOPN)          SETC  ' '                                         00750400
&ZOPO(&ZOPN)          SETC  'ST'                                        00750500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00750600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00750700
                      AGO   .ZMRETX                                     00750800
.*                                                                      00750900
.ZMRET2               ANOP  ,                                           00751000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00751100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00751200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00751300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00751400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00751500
&ZOPN                 SETA  &ZOPN+1                                     00751600
&ZOPL(&ZOPN)          SETC  ' '                                         00751700
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00751800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00751900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00752000
.ZMRETX               ANOP  ,                                           00752100
.*******************************************************************    00752200
.***AO.X03  REASON CODE PROCESSING                                      00752300
.*******************************************************************    00752400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00752500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00752600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00752700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00752800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00752900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00753000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00753100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00753200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00753300
