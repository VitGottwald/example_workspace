                 AIF   (&ZMACRET NE 0).ZLX2     ERROR DETECTED          00637900
.*******************************************************************    00638000
.***AE.01  START OF IEFPRMLB MACRO CODE                                 00638100
.*******************************************************************    00638200
                 LCLC  &M0M0001                 NAME                    00638300
&M0M0001         SETC  '&XLABEL'                SET NAME                00638400
                 AIF   (K'&XLABEL NE 0).XLAB2                           00638500
&M0M0001         SETC  'M00M&SYSNDX'            DEFAULT NAME            00638600
.*                                                                      00638700
.XLAB2           ANOP  ,                                                00638800
                 AIF   ('&XMF' EQ 'L').XLAB3                            00638900
&M0M0001 DS    0H                             IEFPRMLB-1                00639000
.XLAB3           ANOP  ,                                                00639100
.*******************************************************************    00639200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00639300
.*******************************************************************    00639400
              PUSH  PRINT                                               00639500
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00639600
                 PRINT OFF         ++ PR01                              00639700
                 AGO   .ZPRNTXX                                         00639800
.ZPRINTX         PRINT ON                                               00639900
.ZPRNTXX         ANOP  ,                                                00640000
.********************************************************************   00640100
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00640200
.********************************************************************   00640300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00640400
                      LCLA  &ZOPN1         INVOCATION INDEX             00640500
                      LCLC  &ZOPL(4095)    LABEL                        00640600
                      LCLC  &ZOPO(4095)    OPERATION                    00640700
                      LCLC  &ZOPV(4095)    VALUE                        00640800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00640900
.*                                                                      00641000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00641100
.******************************************************************     00641200
.***AL.L10 IEFPRMLB FMT=LIST MF=S!L PARAMETERS                          00641300
.******************************************************************     00641400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00641500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00641600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00641700
&XMFATTR              SETC  '0D'                                        00641800
.*                                                                      00641900
&ZOPN                 SETA  &ZOPN+1                                     00642000
&ZOPL(&ZOPN)          SETC  ' '                                         00642100
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00642200
&ZOPO(&ZOPN)          SETC  'B'                                         00642300
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00642400
                      AGO   .ZALVL6B                                    00642500
.ZALVL6A              ANOP                                              00642600
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00642700
&ZOPV(&ZOPN)          SETC  '4,8'                                       00642800
&ZOPN                 SETA  &ZOPN+1                                     00642900
&ZOPL(&ZOPN)          SETC  ' '                                         00643000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00643100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00643200
.ZALVL6B              ANOP                                              00643300
&ZOPD(&ZOPN)          SETC  '++ IEFPRMLB ARG PARMS'                     00643400
.*                                                                      00643500
&ZOPN                 SETA  &ZOPN+1                                     00643600
&ZOPL(&ZOPN)          SETC  ' '                                         00643700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00643800
&ZOPV(&ZOPN)          SETC  '1'                                         00643900
&ZOPD(&ZOPN)          SETC  ' '                                         00644000
.*                                                                      00644100
.ZLMFSL2              ANOP  ,                                           00644200
.*-------------------------------------------------------------------   00644300
