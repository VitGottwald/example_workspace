.*******************************************************************    00745500
.***  PROCESS IEFPRMLB OUTPUT PARAMETERS                                00745600
.*******************************************************************    00745700
                      ACTR  10000                                       00745800
.*------------------------------------------------------------------    00745900
.***AO.L02 XALLOCDDNAME CL8                                             00746000
         AIF   ('&ZXALLOCDDNAME' EQ 'OUTPUT').ZO36A  OUTPUT             00746100
         AIF   ('&ZXALLOCDDNAME' NE 'INOUT').ZO36C  INOUT               00746200
.ZO36A    AIF   ('&XALLOCDDNAME'(1,1) NE '(').ZO36B  ADDR IN GPR        00746300
&ZCGPR   SETC  '&XALLOCDDNAME'(2,K'&XALLOCDDNAME-2)                     00746400
&ZOPN                 SETA  &ZOPN+1                                     00746500
&ZOPL(&ZOPN)          SETC  ' '                                         00746600
&ZOPO(&ZOPN)          SETC  'MVC'                                       00746700
&ZOPV(&ZOPN)          SETC  '0(8,&ZCGPR),16(1)'                         00746800
&ZOPD(&ZOPN)          SETC  '++ XALLOCDDNAME CL8'                       00746900
                      AGO   .ZO36C                                      00747000
.*                                                                      00747100
.ZO36B                ANOP  ,                                           00747200
&ZAWRK   SETA  K'&XALLOCDDNAME                                          00747300
         AIF   ('&XALLOCDDNAME'(&ZAWRK,1) NE ')').ZO36G                 00747400
&ZN      SETA  2                                                        00747500
.ZO36H    ANOP                                                          00747600
         AIF   (&ZN GE &ZAWRK).ZO36G                                    00747700
         AIF   ('&XALLOCDDNAME'(&ZN,1) NE '(').ZO36I                    00747800
&ZC      SETC  '&XALLOCDDNAME'(1,&ZN).'8'.','                           00747900
&ZC      SETC  '&ZC'.'&XALLOCDDNAME'(&ZN+1,&ZAWRK-&ZN).',16(1)'         00748000
         AGO   .ZO36J                                                   00748100
.ZO36I    ANOP                                                          00748200
&ZN      SETA  &ZN+1                                                    00748300
         AGO   .ZO36H                                                   00748400
.ZO36G    ANOP                                                          00748500
&ZC      SETC  '&XALLOCDDNAME.(8),16(1)'                                00748600
.ZO36J    ANOP                                                          00748700
&ZOPN                 SETA  &ZOPN+1                                     00748800
&ZOPL(&ZOPN)          SETC  ' '                                         00748900
&ZOPO(&ZOPN)          SETC  'MVC'                                       00749000
&ZOPV(&ZOPN)          SETC  '&ZC'                                       00749100
&ZOPD(&ZOPN)          SETC  '++ XALLOCDDNAME CL8'                       00749200
.ZO36C                ANOP  ,                                           00749300
.*******************************************************************    00749400
.***AO.X02  RETURN CODE PROCESSING                                      00749500
.*******************************************************************    00749600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00749700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00749800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00749900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00750000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00750100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00750200
