*          4   DSALLOC  BIT(1),   /* 1 - DATASET ALLOCATED           */ 23150000
*          4   DSOPEN   BIT(1),   /* 1 - DATASET OPENED              */ 23200000
*          4   OUTRECV  BIT(1),   /* 1 - RECFMT IS VARIABLE FOR 'PUT'*/ 23250000
*          4   NEEDFREE BIT(1),   /* FREEMAIN NEEDED FOR 'PUT' BUF   */ 23300000
*          4   NEWDS    BIT(1),   /* NEW DATASET ALLOCATED BY DAIR   */ 23350000
*          4   NOMEMNAM BIT(1),   /* NO MEMBER NAME FOR PO DS        */ 23400000
*          4   OUTRECUN BIT(1),   /* 1 - RECFMT IS UNDEFINED         */ 23450000
