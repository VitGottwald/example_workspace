* /*******************************************************************/ 20200000
* /*THIS IS AN INTERNAL MACRO FOR THE TSO OUTPUT COMMAND PROCESSOR.  */ 20250000
* /*IT IS THE ONLY INTERFACE FROM ONE MODULE (CSECT) TO ANOTHER      */ 20300000
* /* USAGE                                                           */ 20350000
* /*     %NOBASE='1' --------  FOR AN UNBASED EXPANSION              */ 20400000
* /*******************************************************************/ 20450000
*  DCL 1   OUTCOMTB                                                     20500000
*  %IF NOBASE = '1'  %THEN                                              20550000
*  %GOTO SKIPBASE;                                                      20600000
*             BASED(OCMTBPTR)                                           20650000
*  %SKIPBASE: ;                                                         20700000
*            ,                     /* OUTPUT'S COMMUNICATIONS TABLE  */ 20750000
*        2  OUTCPPL  PTR(31),      /* ADDR OF COPY OF CPPL           */ 20800000
*        2  OUTMSGID CHAR(1),      /* ID OF MESSAGE FOR '67 TO WRITE */ 20850000
*        2  OUTFLAGS CHAR(1),      /* FLAGS REQUIRED IN 67           */ 20900000
*         3   KEY1     BIT(1),     /* RUNNING IN KEY 1 SUPR STATE    */ 20950000
*        2  OUTRTCD  FIXED(15),    /* RETN CODE PASSED TO MSG WRITER */ 21000000
*        2  OUTMACN  CHAR(8),      /* NAME OF SVC100'S FAILING MACRO */ 21050000
*        2  OUTCMDNM CHAR(8),      /*COMMAND NAME FROM ECT VIA SVC100*/ 21100000
*        2  OUTATTN  PTR(31),      /* ECB, POSTED BY ATTENTION EXIT  */ 21150000
*         3  *        BIT(1),      /* RESERVED                       */ 21200000
*         3  POSTED   BIT(1),      /* 1 - POSTED BY EXIT             */ 21250000
*        2  OUTEXTRA CHAR(4),      /* FOR FUTURE USE (RESERVED)      */ 21300000
*        2  OUTEMPMN CHAR(8),      /* TEMPNAME FOR PO DS             */ 21350000
*        2  OUTSOBH  PTR(31),      /* ADDR OF SSOB HEADER            */ 21400000
*        2  OUTSOBSO PTR(31),      /* ADDR OF SSSO CTL BLOCK         */ 21450000
*        2  OUTRPL   PTR(31),      /* ADDR OF RPL                    */ 21500000
*        2  OUTRPLL  FIXED(31),    /* RPL LENGTH                     */ 21550000
*        2  OUTACB   PTR(31),      /* ADDR OF ACB,  TO BE PUT IN RPL */ 21600000
*        2  OUTACBL  FIXED(31),    /* ACB LENGTH                     */ 21650000
*        2  OUTEMPSB  PTR(31),     /* SAVE PTR TO SUBCMD FROM ATTN   */ 21700000
*        2  OUTHOLD  CHAR(8),      /* CURRENT RBA OF SYSOUT D.S.     */ 21750000
*     /***************************************************************/ 21800000
*     /* THESE FIELDS ARE USED TO MAINTAIN THE SYSOUT RBA            */ 21850000
*     /* CORRESPONDING TO APPROXIMATELY 10 'PUT' LINES BACK.         */ 21900000
*     /* THIS IS USED FOR RESUMING TERMINAL PRINTING (C HERE)        */ 21950000
*     /* AFTER AN ATTENTION THUS MAKING UP FOR LOST TCAM BUFFERS.    */ 22000000
*     /* IT'S ALSO USED FOR CHKPTING THE CURRENT SYSOUT DS AFTER     */ 22050000
*     /* AN ATTN/END, ATTN/NEXT, OR TERMINATING ERROR.               */ 22100000
*     /***************************************************************/ 22150000
*        2  OUTBKNEW CHAR(8),      /* RBA OF SYSOUT CORRESPONDING TO    22200000
*                                     THE LATEST 10TH RCD PUT. IT'S     22250000
*                                     UPDATED EVERY 10 'PUTS'        */ 22300000
*        2  OUTBKAPX CHAR(8),      /* RBA OF SYSOUT CORRESPONDING TO    22350000
*                                     AT LEAST 10 'PUT' LINES BACK.     22400000
*                                     IT'S SET EQUAL TO OUTBKNEW BEFORE 22450000
*                                     OUTBKNEW IS UPDATED. THIS IS THE  22500000
*                                     OFFICIAL RBA FOR CKPTING AND      22550000
*                                     POINTING IN CERTAIN CASES.     */ 22600000
*        2  OUTBKCNT FIXED(31),    /* COUNT OF LINES 'PUT' SINCE LAST   22650000
*                                     APPROXIMATION OF SYSOUT RBA    */ 22700000
*        2  STRCTNUM FIXED(31),    /* COUNTER FOR ELEMENT IN STRUCT  */ 22750000
*        2  O73PARM  (2),          /*PARAMETERS FOR PRINT/SAVE IN '71*/ 22800000
*         3   OUTDCB   PTR(31),   /* ADDR OF PRINT OR SAVE DCB       */ 22850000
*         3   PRINTDDN CHAR(8),   /* DDNAME OF DATASET ALLOC BY '73  */ 22900000
*         3   OUTBUFA  PTR(31),   /* ADDR OF BUFFER FOR '71'S 'PUT'  */ 22950000
*         3   *        FIXED(31),                                       23000000
