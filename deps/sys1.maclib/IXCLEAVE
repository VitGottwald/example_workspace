.***AC.PLV01  PROCESS PLISTVER                                      *   00053200
.********************************************************************   00053300
&XPLISTVER      SETA  0                                                 00053400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00053500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00053600
&XPLISTVER      SETA  0                                                 00053700
.ZPLV0          ANOP  ,                                                 00053800
.*                                                                      00053900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00054000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00054100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00054200
.*-------------------------------------------------------------------   00054300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00054400
&ZMS            SETC  '"PLISTVER='                                      00054500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00054600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00054700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00054800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054900
                AGO   .ZPVCK2                                           00055000
.ZPVCK3         ANOP  ,                                                 00055100
.*-------------------------------------------------------------------   00055200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00055300
&ZMS            SETC  '"PLISTVER='                                      00055400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00055600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00055700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055800
                AGO   .ZPVCKE                                           00055900
.ZPVCK2         ANOP  ,                                                 00056000
.*-------------------------------------------------------------------   00056100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00056200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00056300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00056400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00056500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00056600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00056700
.ZPVCK6         ANOP  ,                                                 00056800
&ZMS            SETC  '"PLISTVER='                                      00056900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00057100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00057200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057300
&XPLISTVER      SETA  0                                                 00057400
                AGO   .ZPVCKE                                           00057500
.*-------------------------------------------------------------------   00057600
.ZPVCK4A        ANOP  ,                                                 00057700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00057800
&ZMS            SETC  'PLISTVER='                                       00057900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00058000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00058100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00058200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00058300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00058400
                AGO   .ZPVCK5                                           00058500
.ZPVCK4         ANOP  ,                                                 00058600
&XPLISTVER      SETA  &PLISTVER(1)                                      00058700
.ZPVCK5         ANOP  ,                                                 00058800
.ZPVCKE         ANOP  ,                                                 00058900
