.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00146500
.*******************************************************************    00146600
                      ACTR  10000                                       00146700
.*                                                                      00146800
                      LCLA  &ZLC           COLUMN POSITION              00146900
                      LCLA  &ZOPX          LIST TABLE INDEX             00147000
                      LCLC  &ZLL           LABEL                        00147100
                      LCLC  &ZLOP          OPERATION                    00147200
                      LCLC  &ZLV           VALUE                        00147300
                      LCLC  &ZLD           DESCRIPTION                  00147400
.*                                                                      00147500
&ZOPX                 SETA  1                                           00147600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00147700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00147800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00147900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00148000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00148100
                      AIF   (K'&ZLD LE 0).ZL5                           00148200
&ZLC                  SETA  35             COMMENT COLUMN               00148300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00148400
&ZLC                  SETA  33             COMMMENT COLUMN              00148500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00148600
&ZLV                  SETC  '&ZLV'.' '                                  00148700
                      AGO   .ZL3                                        00148800
.*                                                                      00148900
.ZL4                  ANOP  ,                                           00149000
&ZLV                  SETC  '&ZLV'.' '                                  00149100
.ZL5                  ANOP  ,                                           00149200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00149300
.*                                                                      00149400
&ZLL     &ZLOP &ZLV                                                     00149500
.*                                                                      00149600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00149700
                      AGO   .ZL2           CONTINUE LOOPING             00149800
.*                                                                      00149900
.ZLX                  ANOP  ,                                           00150000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00150100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00150200
         L     14,16(0,0)              GET CVT ADDRESS                  00150300
         L     14,772(14,0)            GET SFT ADDRESS                  00150400
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00150500
         LA    15,3                    GET EX FOR SERVICE ROUTINE       00150600
         OR    14,15                   GET PC NUMBER                    00150700
         PC    0(14)                                                    00150800
