.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00141600
.*******************************************************************    00141700
                      ACTR  10000                                       00141800
.*                                                                      00141900
                      LCLA  &ZLC           COLUMN POSITION              00142000
                      LCLA  &ZOPX          LIST TABLE INDEX             00142100
                      LCLC  &ZLL           LABEL                        00142200
                      LCLC  &ZLOP          OPERATION                    00142300
                      LCLC  &ZLV           VALUE                        00142400
                      LCLC  &ZLD           DESCRIPTION                  00142500
.*                                                                      00142600
&ZOPX                 SETA  1                                           00142700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00142800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00142900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00143000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00143100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00143200
                      AIF   (K'&ZLD LE 0).ZL5                           00143300
&ZLC                  SETA  35             COMMENT COLUMN               00143400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00143500
&ZLC                  SETA  33             COMMMENT COLUMN              00143600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00143700
&ZLV                  SETC  '&ZLV'.' '                                  00143800
                      AGO   .ZL3                                        00143900
.*                                                                      00144000
.ZL4                  ANOP  ,                                           00144100
&ZLV                  SETC  '&ZLV'.' '                                  00144200
.ZL5                  ANOP  ,                                           00144300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00144400
.*                                                                      00144500
&ZLL     &ZLOP &ZLV                                                     00144600
.*                                                                      00144700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00144800
                      AGO   .ZL2           CONTINUE LOOPING             00144900
.*                                                                      00145000
.ZLX                  ANOP  ,                                           00145100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00145200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00145300
         L     14,16(0,0)              GET CVT ADDRESS                  00145400
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00145500
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00145600
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00145700
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00145800
         B     END&SYSNDX              BRANCH TO END LABEL              00145900
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00146000
         L     14,772(14,0)            GET SFT ADDRESS                  00146100
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00146200
         LA    15,6                    GET EX FOR SERVICE ROUTINE       00146300
         OR    14,15                   GET PC NUMBER                    00146400
         PC    0(14)                                                    00146500
END&SYSNDX DS    0H                    END LABEL                        00146600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00146700
.ZLINKX               ANOP  ,                                           00146800
