.ZIMFXA               ANOP ,                                            00127000
.*                                                                      00127100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00127200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00127300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00127400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00127500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00127600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00127700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00127800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00127900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00128000
&ZOPN                 SETA  &ZOPN+1                                     00128100
&ZOPL(&ZOPN)          SETC  ' '                                         00128200
