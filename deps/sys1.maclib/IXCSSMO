.***AC.PLV01  PROCESS PLISTVER                                      *   00056800
.********************************************************************   00056900
&XPLISTVER      SETA  0                                                 00057000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00057100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00057200
&XPLISTVER      SETA  0                                                 00057300
.ZPLV0          ANOP  ,                                                 00057400
.*                                                                      00057500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00057600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00057700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00057800
.*-------------------------------------------------------------------   00057900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00058000
&ZMS            SETC  '"PLISTVER='                                      00058100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00058200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00058300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00058400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00058500
                AGO   .ZPVCK2                                           00058600
.ZPVCK3         ANOP  ,                                                 00058700
.*-------------------------------------------------------------------   00058800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00058900
&ZMS            SETC  '"PLISTVER='                                      00059000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00059100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00059200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00059300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059400
                AGO   .ZPVCKE                                           00059500
.ZPVCK2         ANOP  ,                                                 00059600
.*-------------------------------------------------------------------   00059700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00059800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00059900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00060000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00060100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00060200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00060300
.ZPVCK6         ANOP  ,                                                 00060400
&ZMS            SETC  '"PLISTVER='                                      00060500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00060600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00060700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00060800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060900
&XPLISTVER      SETA  0                                                 00061000
                AGO   .ZPVCKE                                           00061100
.*-------------------------------------------------------------------   00061200
.ZPVCK4A        ANOP  ,                                                 00061300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00061400
&ZMS            SETC  'PLISTVER='                                       00061500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00061600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00061700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00061800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00061900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062000
                AGO   .ZPVCK5                                           00062100
.ZPVCK4         ANOP  ,                                                 00062200
&XPLISTVER      SETA  &PLISTVER(1)                                      00062300
.ZPVCK5         ANOP  ,                                                 00062400
.ZPVCKE         ANOP  ,                                                 00062500
