.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00198200
.*******************************************************************    00198300
                      ACTR  10000                                       00198400
.*                                                                      00198500
                      LCLA  &ZLC           COLUMN POSITION              00198600
                      LCLA  &ZOPX          LIST TABLE INDEX             00198700
                      LCLC  &ZLL           LABEL                        00198800
                      LCLC  &ZLOP          OPERATION                    00198900
                      LCLC  &ZLV           VALUE                        00199000
                      LCLC  &ZLD           DESCRIPTION                  00199100
.*                                                                      00199200
&ZOPX                 SETA  1                                           00199300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00199400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00199500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00199600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00199700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00199800
                      AIF   (K'&ZLD LE 0).ZL5                           00199900
&ZLC                  SETA  35             COMMENT COLUMN               00200000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00200100
&ZLC                  SETA  33             COMMMENT COLUMN              00200200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00200300
&ZLV                  SETC  '&ZLV'.' '                                  00200400
                      AGO   .ZL3                                        00200500
.*                                                                      00200600
.ZL4                  ANOP  ,                                           00200700
&ZLV                  SETC  '&ZLV'.' '                                  00200800
.ZL5                  ANOP  ,                                           00200900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00201000
.*                                                                      00201100
