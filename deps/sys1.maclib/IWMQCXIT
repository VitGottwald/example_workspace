&ZOPN1                SETA &ZOPN                                        00176300
.*******************************************************************    00176400
.***  PROCESS IWMQCXIT OUTPUT PARAMETERS                                00176500
.*******************************************************************    00176600
                      ACTR  10000                                       00176700
.*******************************************************************    00176800
.***AO.X02  RETURN CODE PROCESSING                                      00176900
.*******************************************************************    00177000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00177100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00177200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00177300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00177400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00177500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00177600
&ZOPN                 SETA  &ZOPN+1                                     00177700
&ZOPL(&ZOPN)          SETC  ' '                                         00177800
&ZOPO(&ZOPN)          SETC  'ST'                                        00177900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00178000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00178100
                      AGO   .ZMRETX                                     00178200
.*                                                                      00178300
.ZMRET2               ANOP  ,                                           00178400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00178500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00178600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00178700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00178800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00178900
&ZOPN                 SETA  &ZOPN+1                                     00179000
&ZOPL(&ZOPN)          SETC  ' '                                         00179100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00179200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00179300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00179400
.ZMRETX               ANOP  ,                                           00179500
.*******************************************************************    00179600
.***AO.X03  REASON CODE PROCESSING                                      00179700
.*******************************************************************    00179800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00179900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00180000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00180100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00180200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00180300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00180400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00180500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00180600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00180700
&ZOPN                 SETA  &ZOPN+1                                     00180800
&ZOPL(&ZOPN)          SETC  ' '                                         00180900
