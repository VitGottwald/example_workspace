&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00181100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00181200
                      AGO   .ZMRSNX                                     00181300
.*                                                                      00181400
.ZMRSN2               ANOP  ,                                           00181500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00181600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00181700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00181800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00181900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00182000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00182100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00182200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00182300
&ZOPN                 SETA  &ZOPN+1                                     00182400
&ZOPL(&ZOPN)          SETC  ' '                                         00182500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00182600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00182700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00182800
.ZMRSNX               ANOP  ,                                           00182900
.*******************************************************************    00183000
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00183100
.*******************************************************************    00183200
.ZMFLX                ANOP  ,              MF=L PROCESSING              00183300
.*******************************************************************    00183400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00183500
.*******************************************************************    00183600
                      ACTR  10000                                       00183700
.*                                                                      00183800
                      LCLA  &ZLC           COLUMN POSITION              00183900
                      LCLA  &ZOPX          LIST TABLE INDEX             00184000
                      LCLC  &ZLL           LABEL                        00184100
                      LCLC  &ZLOP          OPERATION                    00184200
                      LCLC  &ZLV           VALUE                        00184300
                      LCLC  &ZLD           DESCRIPTION                  00184400
.*                                                                      00184500
&ZOPX                 SETA  1                                           00184600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00184700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00184800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00184900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00185000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00185100
                      AIF   (K'&ZLD LE 0).ZL5                           00185200
&ZLC                  SETA  35             COMMENT COLUMN               00185300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00185400
&ZLC                  SETA  33             COMMMENT COLUMN              00185500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00185600
&ZLV                  SETC  '&ZLV'.' '                                  00185700
                      AGO   .ZL3                                        00185800
.*                                                                      00185900
.ZL4                  ANOP  ,                                           00186000
&ZLV                  SETC  '&ZLV'.' '                                  00186100
.ZL5                  ANOP  ,                                           00186200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00186300
.*                                                                      00186400
&ZLL     &ZLOP &ZLV                                                     00186500
.*                                                                      00186600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00186700
                      AGO   .ZL2           CONTINUE LOOPING             00186800
.*                                                                      00186900
.ZLX                  ANOP  ,                                           00187000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00187100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00187200
         LCLC  &T1                                                      00187300
         AIF   ('&XACTION' EQ 'NULL').FINIS                             00187400
.*                                                                      00187500
&T1      SETC  '&XQMGR_EXIT@'                                           00187600
         AIF   ('&XQMGR_EXIT@'(1,1) NE '(').EXITRS                      00187700
&T1      SETC  '&XQMGR_EXIT@'(2,K'&XQMGR_EXIT@-2)                       00187800
&T1      SETC  '0(&T1.)'                                                00187900
.EXITRS    ANOP                                                         00188000
.* &T1 IS NOW A VALID BASE-DISPLACEMENT EXPRESSION                      00188100
.*                                                                      00188200
         L     15,&T1                  FETCH CONNECT EXIT @             00188300
         BALR  14,15                                                    00188400
.*                                                                      00188500
.FINIS   ANOP                                                           00188600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00188700
