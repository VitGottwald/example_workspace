.***AO.X04  LABEL FOR MF=L PROCESSING                                   00814500
.*******************************************************************    00814600
.ZMFLX                ANOP  ,              MF=L PROCESSING              00814700
.*******************************************************************    00814800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00814900
.*******************************************************************    00815000
                      ACTR  10000                                       00815100
.*                                                                      00815200
                      LCLA  &ZLC           COLUMN POSITION              00815300
                      LCLA  &ZOPX          LIST TABLE INDEX             00815400
                      LCLC  &ZLL           LABEL                        00815500
                      LCLC  &ZLOP          OPERATION                    00815600
                      LCLC  &ZLV           VALUE                        00815700
                      LCLC  &ZLD           DESCRIPTION                  00815800
.*                                                                      00815900
&ZOPX                 SETA  1                                           00816000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00816100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00816200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00816300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00816400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00816500
                      AIF   (K'&ZLD LE 0).ZL5                           00816600
&ZLC                  SETA  35             COMMENT COLUMN               00816700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00816800
&ZLC                  SETA  33             COMMMENT COLUMN              00816900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00817000
&ZLV                  SETC  '&ZLV'.' '                                  00817100
                      AGO   .ZL3                                        00817200
.*                                                                      00817300
.ZL4                  ANOP  ,                                           00817400
&ZLV                  SETC  '&ZLV'.' '                                  00817500
.ZL5                  ANOP  ,                                           00817600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00817700
.*                                                                      00817800
&ZLL     &ZLOP &ZLV                                                     00817900
.*                                                                      00818000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00818100
                      AGO   .ZL2           CONTINUE LOOPING             00818200
.*                                                                      00818300
.ZLX                  ANOP  ,                                           00818400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00818500
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00818600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00818700
         AIF    ('&SYSAM64' EQ 'YES').AM641                        @L4A 00818800
         L    14,16(0,0)           GET CVT ADDRESS                      00818900
         AGO   .AM31641                                            @L4A 00819000
.AM641   ANOP                                                      @L4A 00819100
         LLGT 14,16(0,0)           GET CVT ADDRESS                      00819200
.AM31641 ANOP                                                      @L4A 00819300
         L    14,772(14,0)         GET SFT ADDRESS                      00819400
         L    14,232(14,0)         GET LX,EX (PC NUMBER)                00819500
         PC   0(14)                PC TO CSVQSER                        00819600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00819700
.ZLINKX               ANOP  ,                                           00819800
.*******************************************************************    00819900
.***AP.X01  FORMAT GENERATED CODE                                       00820000
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00820100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00820200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00820300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00820400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00820500
                      AIF   (K'&ZLD LE 0).ZL51                          00820600
&ZLC                  SETA  35             COMMENT COLUMN               00820700
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00820800
&ZLC                  SETA  33             COMMENT COLUMN               00820900
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00821000
&ZLV                  SETC  '&ZLV'.' '                                  00821100
                      AGO   .ZL31                                       00821200
.*                                                                      00821300
.ZL41                 ANOP  ,                                           00821400
&ZLV                  SETC  '&ZLV'.' '                                  00821500
.ZL51                 ANOP  ,                                           00821600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00821700
.*                                                                      00821800
&ZLL     &ZLOP &ZLV                                                     00821900
.*                                                                      00822000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00822100
                      AGO   .ZL21          CONTINUE LOOPING             00822200
.*                                                                      00822300
.ZLX1                 ANOP  ,                                           00822400
.*******************************************************************    00822500
.***AP.X02  END OF CSVQUERY MACRO                                       00822600
&ZCSVQUERY            SETC  'YES'          SET CSVQUERY USED            00822700
         POP   PRINT                                                    00822800
*                                            CSVQUERY-7                 00822900
         SPACE 1                                                        00823000
.ZLX2                 ANOP  ,                                           00823100
         MEND                                                           00823200
