&XPLISTVER      SETA  0                                                 00148900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00149000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00149100
&XPLISTVER      SETA  7                                                 00149200
.ZPLV0          ANOP  ,                                                 00149300
.*                                                                      00149400
                AIF   ('&ANCESTORJPQ' EQ '').ZPLV1E                     00149500
                AIF   (&XPLISTVER GE 4).ZPLV1E                          00149600
&XPLISTVER      SETA  4                                                 00149700
.ZPLV1E         ANOP  ,                                                 00149800
.*                                                                      00149900
                AIF   ('&SEARCHIDPLPA' EQ '').ZPLV2E                    00150000
