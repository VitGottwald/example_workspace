.******************************************************************     00644100
.***AI.L01 CSVQUERY FMT=LIST FILL IN PARAMETER AREA                     00644200
.******************************************************************     00644300
                      ACTR  10000                                       00644400
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00644500
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00644600
&ZCVAL                SETC  'LAE'          XAX MODE                     00644700
.ZIMFXA               ANOP ,                                            00644800
.*                                                                      00644900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00645000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00645100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00645200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00645300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00645400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00645500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00645600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00645700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00645800
&ZOPN                 SETA  &ZOPN+1                                     00645900
&ZOPL(&ZOPN)          SETC  ' '                                         00646000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00646100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00646200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00646300
                      AGO   .ZIMFNX                                     00646400
.*                                                                      00646500
.ZIMFN2               ANOP ,                                            00646600
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00646700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00646800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00646900
&ZOPN                 SETA  &ZOPN+1                                     00647000
&ZOPL(&ZOPN)          SETC  ' '                                         00647100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00647200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00647300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00647400
                      AGO   .ZIMFNX                                     00647500
.ZIMFNX1              ANOP ,                                            00647600
&ZOPN                 SETA  &ZOPN+1                                     00647700
&ZOPL(&ZOPN)          SETC  ' '                                         00647800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00647900
