                AIF   ('&DIRLOAD' EQ '').ZPLV3E                         00150500
                AIF   (&XPLISTVER GE 4).ZPLV3E                          00150600
&XPLISTVER      SETA  4                                                 00150700
.ZPLV3E         ANOP  ,                                                 00150800
.*                                                                      00150900
                AIF   ('&ANCESTORJPQ' EQ '').ZPLV4E                     00151000
                AIF   (&XPLISTVER GE 4).ZPLV4E                          00151100
&XPLISTVER      SETA  4                                                 00151200
.ZPLV4E         ANOP  ,                                                 00151300
.*                                                                      00151400
