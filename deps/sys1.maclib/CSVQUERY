.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00552200
.*******************************************************************    00552300
                  AIF   (&ZMACRET NE 0).ZLX2    ERROR DETECTED          00552400
.*******************************************************************    00552500
.***AE.01  START OF CSVQUERY MACRO CODE                                 00552600
.*******************************************************************    00552700
                  LCLC  &M0M0001                 NAME                   00552800
&M0M0001          SETC  '&XLABEL'                SET NAME               00552900
                  AIF   (K'&XLABEL NE 0).XLAB2                          00553000
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           00553100
.*                                                                      00553200
.XLAB2            ANOP  ,                                               00553300
                  AIF   ('&XMF' EQ 'L').XLAB3                           00553400
&M0M0001 DS    0H                             CSVQUERY-7                00553500
.XLAB3            ANOP  ,                                               00553600
.*******************************************************************    00553700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00553800
.*******************************************************************    00553900
              PUSH  PRINT                                               00554000
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00554100
                  PRINT OFF         ++ PR01                             00554200
                  AGO   .ZPRNTXX                                        00554300
.ZPRINTX          PRINT ON                                              00554400
.ZPRNTXX          ANOP  ,                                               00554500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00554600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00554700
.********************************************************************   00554800
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00554900
.********************************************************************   00555000
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00555100
                      LCLA  &ZOPN1         INVOCATION INDEX             00555200
                      LCLC  &ZOPL(4095)    LABEL                        00555300
                      LCLC  &ZOPO(4095)    OPERATION                    00555400
                      LCLC  &ZOPV(4095)    VALUE                        00555500
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00555600
.*                                                                      00555700
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00555800
.******************************************************************     00555900
.***AL.L10 CSVQUERY FMT=LIST MF=S!L PARAMETERS                          00556000
.******************************************************************     00556100
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00556200
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00556300
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00556400
&XMFATTR              SETC  '0D'                                        00556500
.*                                                                      00556600
&ZOPN                 SETA  &ZOPN+1                                     00556700
&ZOPL(&ZOPN)          SETC  ' '                                         00556800
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00556900
&ZOPO(&ZOPN)          SETC  'B'                                         00557000
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00557100
                      AGO   .ZALVL6B                                    00557200
.ZALVL6A              ANOP                                              00557300
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00557400
&ZOPV(&ZOPN)          SETC  '4,8'                                       00557500
&ZOPN                 SETA  &ZOPN+1                                     00557600
&ZOPL(&ZOPN)          SETC  ' '                                         00557700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00557800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00557900
.ZALVL6B              ANOP                                              00558000
&ZOPD(&ZOPN)          SETC  '++ CSVQUERY ARG PARMS'                     00558100
.*                                                                      00558200
&ZOPN                 SETA  &ZOPN+1                                     00558300
&ZOPL(&ZOPN)          SETC  ' '                                         00558400
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00558500
&ZOPV(&ZOPN)          SETC  '1'                                         00558600
&ZOPD(&ZOPN)          SETC  ' '                                         00558700
.*                                                                      00558800
.ZLMFSL2              ANOP  ,                                           00558900
.*-------------------------------------------------------------------   00559000
