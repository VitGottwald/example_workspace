*  %GOTO SJRCPRO;                         /*                                    
        MACRO                                                                   
        IEFSJRC &SJRCCOM=YES,&SJRCJDT=YES,&SJRCDEF=YES,&SJRCFND=YES,   X        
               &SJRCUPD=YES,&SJRCRET=YES,&SJRCWRT=YES,&SJRCPUT=YES,    X        
               &SJRCGET=YES,&SJRCBLD=YES,&SJRCVER=YES,&SJRCACC=YES,    X        
               &SJRCSWA=YES,&SJRCEQU=NO,&SJRCDEL=YES,&SJRCTOK=YES,     X        
               &SJRCERS=YES,&SJRCSER=YES,&SJRCMRG=YES,&SJRCSCN=YES,    X        
               &SJRCKLS=YES,&SJRCSMS=YES,&SJRCVAL=YES              @LAC         
.*/ %SJRCPRO:;                                                                  
*/***START OF SPECIFICATIONS*************************************@P7C*/         
*/*                                                                  */         
*/* MACRO NAME = IEFSJRC                                         @P7C*/         
*/*                                                                  */         
*/* DESCRIPTIVE NAME = Scheduler JCL Facility Reason Codes           */         
*/*                                                                  */         
*/*01* PROPRIETARY STATEMENT=                                        */         
*/***PROPRIETARY_STATEMENT********************************************/         
*/*                                                                  */         
*/*                                                                  */         
*/* LICENSED MATERIALS - PROPERTY OF IBM                             */         
*/* 5650-ZOS COPYRIGHT IBM CORP. 1982, 2021                          */         
*/*                                                                  */         
*/* STATUS= HBB77D0                                                  */         
*/*                                                                  */         
*/***END_OF_PROPRIETARY_STATEMENT*************************************/         
*/*                                                              @P7C*/         
*/*                                                                  */         
*/* FUNCTION = Maps the reason codes used by all SJFREQ              */         
*/*            Functions                                             */         
*/*                                                                  */         
*/* MODULE TYPE = MACRO                                              */         
*/*                                                                  */         
*/*    DSECT NAME = n/a                                          @P7A*/         
*/*                                                                  */         
*/*    ACRONYM = none                                            @P7A*/         
*/*                                                                  */         
*/*    COMPONENT = Scheduler JCL Facility (BB131)                @P7C*/         
*/*                                                                  */         
*/*01* EXTERNAL CLASSIFICATION:                                  @P7A*/         
*/*02* GUPI: BASE                                                @P7A*/         
*/*02* DMTI: FIELDS                                              @P7A*/         
*/*          SJRCIVID     SJRCDDNM    SJRCIVDT   SJRCPLST        @P7A*/         
*/*          SJRCNSWB     SJRCNBIT    SJRCDUPV   SJRCNOCB        @P7A*/         
*/*          SJRCBKK      SJRCEBIT    SJRCPSWB   SJRCLSTG        @P7A*/         
*/*          SJRCNSLE     SJRCNGRP    SJRCPUGM   SJRCSSTG        @P7A*/         
*/*          SJRCBVC      SJRCNOST    SJRCPLEN   SJRCPRMN        @P7A*/         
*/*          SJRCNOIP     SJRCINAN    SJRCGSWB   SJRCPERR        @P7A*/         
*/*          SJRCVAOC     SJRCINVJ    SJRCMORE   SJRCUSEH        @P7A*/         
*/*          SJRCDLVL     SJRCIVKY    SJRCGLEN   SJRCUSEZ        @P7A*/         
*/*          SJRCNJDT     SJRCDUPK    SJRCGEGM   SJRCLDEL        @P7A*/         
*/*          SJRCDUPJ     SJRCCOPY    SJRCNDTA   SJRCIFUN        @P7A*/         
*/*          SJRCDFTJ     SJRCIVRB    SJRCGETS   SJRCMRDV        @P7A*/         
*/*          SJRCGETJ     SJRCIVLB    SJRCNDAT   SJRCPMOD        @P7A*/         
*/*          SJRCSUHT     SJRCIREF    SJRCNATH   SJRCBUFL        @P7A*/         
*/*          SJRCNSDT     SJRCWSPC    SJRCNSTG   SJRCSWAM        @P7A*/         
*/*          SJRCNSCH     SJRCALDL    SJRCNUPD   SJRCIVCB        @P7A*/         
*/*          SJRCSTEP     SJRCNDYN    SJRCALLW   SJRCIVAC        @P7A*/         
*/*          SJRCITKN     SJRCPTUL    SJRCATBF                   @LCC*/         
*/*          SJRC_NOT_FOUND                                      @P7A*/         
*/*          SJRC_SCAN_VERB_DOESNT_MATCH                         @P7A*/         
*/*          SJRC_SCAN_INVALID_PLIST                             @P7A*/         
*/*          SJRC_SCAN_INVALID_KEY                               @P7A*/         
*/*          SJRC_SCAN_INVALID_PARM                              @P7A*/         
*/*          SJRC_KEYLIST_INVALID_PLIST                          @P7A*/         
*/*          SJRC_KEYLIST_VERB_NOT_DEF                           @P7A*/         
*/*          SJRC_KEYLIST_NO_STORAGE                             @P7A*/         
*/*          SJRC_NO_KEYWORDS_TO_RETURN                          @P7A*/         
*/*          SJRC_SWBTU_WITH_NO_TUS                              @P7A*/         
*/*          SJRC_OUTAREA_OVERFLOW                               @P7A*/         
*/*          SJRC_BASE_SWBTU_CONFLICT                            @P7A*/         
*/*          SJRC_MODIFY_SWBTU_NO_TU                             @P7A*/         
*/*          SJRC_GETMAIN_FAILURE                                @P7A*/         
*/*          SJRC_FREEMAIN_FAILURE                               @P7A*/         
*/*          SJRC_SPLICE_STORAGE_ERROR                           @P7A*/         
*/*          SJRC_MODIFY_SWBTU_ERROR                             @P7A*/         
*/*01* END OF EXTERNAL CLASSIFICATION                            @P7A*/         
*/*                                                                  */         
*/* EYE CATCHER = none                                           @P7A*/         
*/*      OFFSET = n/a                                            @P7A*/         
*/*      LENGTH = n/a                                            @P7A*/         
*/*                                                                  */         
*/* STORAGE ATTRIBUTES =                                         @P7A*/         
*/*       VIRTUAL STORAGE = included in module's dynamic area    @P7A*/         
*/*       SIZE = n/a                                             @P7A*/         
*/*                                                                  */         
*/* CREATED BY = n/a                                             @P7A*/         
*/*                                                                  */         
*/* POINTED TO BY = n/a                                          @P7A*/         
*/*                                                                  */         
*/* SERIALIZATION = None                                         @P7A*/         
*/*                                                                  */         
*/* INVOCATION =                                                     */         
*/*                                                                  */         
*/*   BAL - IEFSJRC                                                  */         
*/*                 The default values are to declare all the    @L4A*/         
*/*                   defined reason codes as constants.         @L4A*/         
*/*                                                                  */         
*/* Note = The reason codes for the SJF Info_Retrieval service are   */         
*/*        in PL/X only.                                         @P7M*/         
*/*                                                                  */         
*/* CONVENTIONS =                                                @L5A*/         
*/*                                                              @L5A*/         
*/*  The reason code values are determined as follows:           @L5A*/         
*/*                                                              @L5A*/         
*/*  The first 2 decimal digits correspond to the function code  @L5A*/         
*/*  for the SJF function. (e.g. SJF Access reason codes begin   @L5A*/         
*/*  with '13'). The remaining digits are sequential starting    @L5A*/         
*/*  with '00'.                                                  @L5A*/         
*/*                                                              @L5A*/         
*/*  For reason codes not belonging to an SJF function (e.g.     @L5A*/         
*/*  IEFSJSWA), the reason codes should be in the range of       @L5A*/         
*/*  decimal '100' more than the previous. For example, IEFSJSWA @L5A*/         
*/*  reason codes are in the 80xx range, followed by IEFSJTOK,   @L5A*/         
*/*  which is in the 81xx range.                                 @L5A*/         
*/*                                                                  */         
*/*                 SJRCEQU=YES OR NO, :Generate EQUates rather  @L4A*/         
*/*                                     than DCs                 @L4A*/         
*/*                                                                  */         
*/*                                                                  */         
*/*              If SJRCEQU is specifed as NO or defaults to NO  @L4A*/         
*/*                 then the following options can control which @L4A*/         
*/*                 subset of reason codes to declare.               */         
*/*                                                                  */         
*/*                 SJRCCOM=YES OR NO, :SJF COMMON REASON CODES      */         
*/*                 SJRCJDT=YES OR NO, :SJF EXTRACT, UPDATE, AND     */         
*/*                                     RETRIEVE REASON CODES        */         
*/*                 SJRCDEF=YES OR NO, :SJF DEFINE JDVT REASON CODES */         
*/*                 SJRCFND=YES OR NO, :SJF FIND SWB REASON CODES    */         
*/*                 SJRCUPD=YES OR NO, :SJF UPDATE REASON CODES      */         
*/*                 SJRCRET=YES OR NO, :SJF RETRIEVE REASON CODES    */         
*/*                 SJRCWRT=YES OR NO, :SJF WRITE REASON CODES       */         
*/*                 SJRCPUT=YES OR NO, :SJF PUT REASON CODES         */         
*/*                 SJRCGET=YES OR NO, :SJF GET REASON CODES         */         
*/*                 SJRCBLD=YES OR NO, :SJF BUILD REASON CODES       */         
*/*                 SJRCSWA=YES OR NO, :IEFSJSWA REASON CODES        */         
*/*                 SJRCACC=YES OR NO, :SJF ACCESS REASON CODES  @L3A*/         
*/*                 SJRCVER=YES OR NO, :SJF VERIFY REASON CODES  @L4A*/         
*/*                 SJRCDEL=YES OR NO  :SJF DELETE REASON CODES  @D1A*/         
*/*                 SJRCERS=YES OR NO  :SJF ERASE REASON CODES   @D3A*/         
*/*                 SJRCSER=YES OR NO  :SJF SERIALIZE REASON CODES   */         
*/*                                                              @L5A*/         
*/*                 SJRCMRG=YES OR NO  :SJF MERGE SWB REASON CODES   */         
*/*                                                              @L7A*/         
*/*                 SJRCSCN=YES OR NO  :SJF SCAN SWB REASON CODES    */         
*/*                                                              @L8A*/         
*/*                 SJRCKLS=YES OR NO  :SJF Keylist reason codes     */         
*/*                                                              @L8A*/         
*/*                 SJRCSMS=YES OR NO  :SJF SWBTU Merge reason codes */         
*/*                                                              @L9A*/         
*/*                 SJRCVAL=YES OR NO  :SJF Validate reason codes    */         
*/*                                                              @LAA*/         
*/*   PL/X - %INCLUDE SYSLIB(IEFSJRC)                            @LCC*/         
*/*                                                                  */         
*/*          Some new SJF services use variable names which are  @P4A*/         
*/*          longer than 8 characters and/or are allowed         @L9C*/         
*/*          in PL/AS only.  To include these variables,         @L9C*/         
*/*          the following must be coded prior to the include    @L9C*/         
*/*          of this macro.                                      @L9A*/         
*/*                                                                  */         
*/*          %DCL SJRCLONG CHAR                                  @P4A*/         
*/*          %SJRCLONG = 'YES'                                   @P4A*/         
*/*                                                                  */         
*/*          If the PL/X specific variable names are not         @LCC*/         
*/*          desired, do not declare SJRCLONG, or declare it     @L9C*/         
*/*          and assign it a value other than 'YES'.  This       @L9C*/         
*/*          allows existing PL/X users of IEFSJRC to not get    @LCC*/         
*/*          the PL/AS specific names as a default.              @L9C*/         
*/*          PL/X does not allow the full range of variable      @LCC*/         
*/*          names that PL/AS allows.                            @L9C*/         
*/*                                                                  */         
*/*          The following services use variable names           @P4A*/         
*/*          which are longer than 8 characters:                 @P4A*/         
*/*            IEFSJSCN - SJF Scan                               @P4A*/         
*/*            IEFSJKLS - SJF Keylist Service                    @P4A*/         
*/*            IEFSJSMS - SJF SWBTU Merge Service                @L9A*/         
*/*            IEFSJIRT - SJF Info_Retrieval Service             @LBA*/         
*/*                                                                  */         
*/*    DISTRIBUTION LIBRARY = AMACLIB                            @L9A*/         
*/*                                                                  */         
*/* CHANGE ACTIVITY = H0,L1,L2,P1,L3,L4,D1,D2,P2,D3,L5,L6,P3,L7, @L8C*/         
*/*                   L8,P4,L9,P5,T1,D4,T2,LA,LB,P6,01,P7        @P7C*/         
*/*                                                                  */         
*/*$H0= EXTJCL JBB2110 820827 PD2Q: SUPPORT FOR EXTENDED JCL         */         
*/*$L1= SJFPERF JBB2220 850102 PDN4: SJF PERFORMANCE             @L1A*/         
*/*$L2= SWABOVE JBB2220 850603 PDT4: SWA ABOVE THE LINE          @L2A*/         
*/*$P1= PA20428 JBB2220 851023 PDT4: CHANGE IEFSJSWA RC'S        @P1A*/         
*/*$L3= SMSSTG2 JBB2223 860324 PDN4: STOR MGMT SUBSYS STGII SUPT @L3A*/         
*/*$L4= SMSSTG2 JBB2223 860623 PDT4: STOR MGMT SUBSYS STGII SUPT @L4A*/         
*/*$D1= DCR0023 JBB2223 861117 PDT4: DYNAMIC OUTPUT SUPPORT      @D1A*/         
*/*$D2= DCR0033 JBB2223 870235 PDN4: DCR 33                      @D2A*/         
*/*$P2= PC20216 JBB2223 870408 PDN4: STOR MGMT SUBSYS STGII SUPT @P2A*/         
*/*$D3= DCR0055 JBB2223 870923 PDN4: DCR 55                      @D3A*/         
*/*$L5= SERVC   HBB4410 871102 PDN4: SERVICEABILITY              @L5A*/         
*/*$L6= EMVS2   HBB4410 880811 PDD1: EMVS2 support               @L6A*/         
*/*$P3= PEO1297 HBB4410 881220 PDM1: ASM comment continuation    @P3A*/         
*/*$L7= APPC1   HBB4420 890405 PDM1: APPC support                @L7A*/         
*/*$L8= EMVS2   HBB4410 890206 PDY1: ENTERPRISE II SUPPORT       @L8A*/         
*/*$P4= PEO1528 HBB4410 890206 PDY1: Long names bad for PLS      @P4A*/         
*/*$L9= EMVS2   HBB4410 890516 PDZ1: SWBTU_MERGE reason codes    @L9A*/         
*/*$P5= PEO2221 HBB4410 890814 PDDZ: New SWBTU_MERGE reason code @P5A*/         
*/*$T1= OY27487 JBB3313 891205 PDCL: IMS SUPPORT                 @T1A*/         
*/*$D4= DCR0283 HBB4410 900328 PDCL: TEXT Data Type              @D4A*/         
*/*$T2= OY30620 JBB3313 901001 PDCL: USERLIB Support             @T2A*/         
*/*$LA= POSIX   HBB4430 911028 PDCL: OPEN/MVS Support            @LAA*/         
*/*$LB= POSIX   HBB4430 920108 PDWL: OPEN/MVS Support            @LBA*/         
*/*$P6= PKB1399 HBB4430 920617 PDCL: 10X Defect Elimination      @P6A*/         
*/*$01= OY54619 JBB2223 920815 PDCL: Expiration Date             @01A*/         
*/*$P7= PIG1424 HBB5510 930715 PDDZ: SHOWHDR compliance          @P7A*/         
*/*$P8= PUX0014 HBB6605 970819 PDDH: FIN APAR OW20253            @P8A*/         
*/*$LC= SWBNATB HBB77D0 210104 PDOO: SWBNext ATB                 @LCA*/         
*/*                                                                  */         
*/***END OF SPECIFICATIONS***************************************@P7C*/         
*/* A 000000-999999                                              @H0A*/         
*/* A HEX VALUE OF REASON CODES                                  @L1A*/         
*/* A REASON CODE FOR SJF HASH TABLE BUILD                       @L1A*/         
*/* A REASON CODES FOR IEFSJSWA INCLUDED PROCEDURE               @L2A*/         
*/* C REASON CODES FOR IEFSJSWA INCLUDED PROCEDURE TO START AT   @P1A*/         
*/*   8000 RATHER THAN 1200                                      @P1A*/         
*/* A REASON CODES FOR SJF ACCESS, SJF FIND, SJF DEFINE AND      @L3A*/         
*/*   COMMON INCLUDE SEGMENTS                                    @L3A*/         
*/* A REASON CODES FOR SJF VERIFY                                @L4A*/         
*/* A LITERAL EQUATE OPTION TO ASSEMBLER VERSION OF MACRO        @L4A*/         
*/* A REASON CODES FOR DYNAMIC OUTPUT SUPPORT                    @D1A*/         
*/* A REASON CODES FOR TOKEN BUILD ROUTINE                       @D2A*/         
*/* C CONTINUATION OF ASM COMMENT FOR SJRCITKN                   @P2A*/         
*/* C REASON CODES FOR SJF ERASE FUNCTION, IEFSJERS              @D3A*/         
*/* A REASON CODES FOR SJF SERIALIZE                             @L5A*/         
*/* A Reason codes for EMVS2 support                             @L6A*/         
*/* C Continuation error for ASM comment                         @P3A*/         
*/* A Reason codes for APPC support (MERGESWB)                   @L7A*/         
*/* A Reason codes for SCAN SWB and Keylist                      @L8A*/         
*/* A Change to get SCAN and KEYLIST PLS reason codes only if    @P4A*/         
*/*   requested. They use variable names greater than 8 chars,   @P4A*/         
*/*   which aren't allowed by PLS.                               @P4A*/         
*/* A Add reason code SJRC_NO_KEYWORDS_TO_RETURN                 @P4A*/         
*/* A Add reason codes for SWBTU_MERGE service (1900 range)      @L9A*/         
*/* A Added new reason code for SWBTU_MERGE.                     @P5A*/         
*/* A Reason code for output text unit buffer different than     @T1A*/         
*/*   the output text unit buffer passed on first call           @T1A*/         
*/* A Reason code for invalid TEXT character found               @D4A*/         
*/* A Reason code for quoted string support in SJF Verify        @T2A*/         
*/* A Reason codes for new special data type                     @LAA*/         
*/* A Reason codes for Info_Retrieval Service                    @LBA*/         
*/* A Reason code for SJF Update to return when a text unit key  @P6A*/         
*/*   value specified as hex zeroes                              @P6A*/         
*/* C Changed prolog to be SHOWHDR compliant                     @P7A*/         
*/* A Reason code for SJF Put to return when the text unit data  @P8A*/         
*/*   area contains a text unit with an invalid count or length  @P8A*/         
*/*   (due to a text unit length overlay).                       @P8A*/         
*/* A Defined SJRCATBF                                           @LCA*/         
*  %GOTO SJRCPLS;                     /*                                        
         AIF   ('&SJRCEQU' EQ 'YES').RCEQATE    Determines if EQUATES  X        
                                       should be generated rather than X        
                                       declares for constants     @L4A          
         AIF   ('&SJRCCOM' EQ 'NO').RCJDT                                       
**********************************************************************          
*                                                                    *          
*   COMMON REASON CODES (decimal)                                    *          
*                                                                    *          
**********************************************************************          
SJRCNOER DC F'0'                       X'000' PROCESSING SUCCESSFUL             
SJRCIVID DC F'1'                       X'001' INVALID SWB ID, OWNER             
*                                      NAME, BLOCK ID, VERB, OR LABEL           
SJRCIVTK DC F'2'                       X'002' INVALID SWB TOKEN                 
SJRCNSWB DC F'3'                       X'003' SWB NOT FOUND ON SWB              
*                                      CHAIN                                    
SJRCNJDV DC F'4'                       X'004' JDVT NOT FOUND ON JDVT            
*                                      CHAIN                                    
SJRCNJCH DC F'5'                       X'005' JDVT CHAIN DOES NOT               
*                                      EXIST                                    
.RCJDT   AIF   ('&SJRCJDT' EQ 'NO').RCDEF                                       
**********************************************************************          
*                                                                    *          
*   JDT REASON CODES - Returned BY SJF Extract and callers of        *          
*                      SJF Extract                               @P6C*          
*                                                                    *          
**********************************************************************          
SJRCNVRB DC F'200'                     X'0C8' VERB NOT DEFINED IN JDT           
SJRCNKWD DC F'201'                     X'0C9' KEYWORD NOT DEFINED IN            
*                                      JDT                                      
SJRCNKEY DC F'202'                     X'0CA' KEY NOT DEFINED IN JDT            
SJRCNPRM DC F'203'                     X'0CB' SUBPARAMETER NOT DEFINED          
*                                      IN JDT                                   
SJRCBKK  DC F'204'                     X'0CC' BOTH A KEYWORD AND KEY            
*                                      ARE SPECIFIED                            
SJRCNSLE DC F'206'                     X'0CE' SUBLIST ELEMENT NOT               
*                                      DEFINED IN THE JDT                       
SJRCNCMD DC F'207'                     X'0CF' COMMAND NOT DEFINED IN   X        
                                       JDT                         @L4A         
SJRCNOPE DC F'208'                     X'0D0' OPERAND NOT DEFINED IN   X        
                                       JDT                         @L4A         
SJRCBVC  DC F'209'                     X'0D1' BOTH VERB AND COMMAND    X        
                                       SPECIFIED                   @L4A         
SJRCNOIP DC F'210'                     X'0D2' NO OPERAND INFORMATION   X        
                                       POINTER                     @L4A         
SJRCVAOC DC F'211'                     X'0D3' VERB AND/OR COMMAND NOT  X        
                                       SPECIFIED                   @L4A         
SJRCDLVL DC F'212'                     X'0D4' Special parameter                 
*                                      information not supported by             
*                                      this parameter list version @LAA         
.RCDEF   AIF   ('&SJRCDEF' EQ 'NO').RCFND                                       
**********************************************************************          
*                                                                    *          
*   DEFINE JDVT REASON CODES                                         *          
*                                                                    *          
**********************************************************************          
SJRCNJDT DC F'300'                     X'12C' JDT NOT FOUND                     
SJRCDUPJ DC F'301'                     X'12D' DUPLICATE JDVT NAME               
SJRCDFTJ DC F'302'                     X'12E' DEFAULT JDVT ALREADY              
*                                      EXISTS                                   
SJRCGETJ DC F'303'                     X'12F' GETMAIN FOR JDVT FAILED           
SJRCSUHT DC F'304'                     X'130' STORAGE UNAVAILABLE FOR  X        
                                       HASH TABLE                 @L1A          
SJRCNSDT DC F'306'                     X'132' SDT NOT FOUND       @L3A          
.RCFND   AIF   ('&SJRCFND' EQ 'NO').RCUPD                                       
**********************************************************************          
*                                                                    *          
*   FIND SWB REASON CODES                                            *          
*                                                                    *          
**********************************************************************          
SJRCNSCH DC F'400'                     X'190' SPECIFIED SWB CHAIN NOT           
*                                      FOUND                                    
SJRCSTEP DC F'401'                     X'191' SPECIFIED STEP OR PROC            
*                                      NAME NOT FOUND                           
SJRCDDNM DC F'402'                     X'192' SPECIFIED DD LABEL NOT            
*                                      FOUND                                    
SJRCNBIT DC F'403'                     X'193' NO SEARCH BITS SPECIFIED          
