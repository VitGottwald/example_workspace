                 AIF   ('&XMF' EQ 'L').XLAB3                            00676400
&M0M0001 DS    0H                             CSVDYNL-0                 00676500
.XLAB3           ANOP  ,                                                00676600
.*******************************************************************    00676700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00676800
.*******************************************************************    00676900
              PUSH  PRINT                                               00677000
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00677100
                 PRINT OFF         ++ PR01                              00677200
                 AGO   .ZPRNTXX                                         00677300
.ZPRINTX         PRINT ON                                               00677400
.ZPRNTXX         ANOP  ,                                                00677500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00677600
         LCLC  &T2,&T1                                             @L1A 00677700
         LCLA  &A1                                                 @L1A 00677800
         AIF   ('&REQUEST' NE 'QUERYDYN').NOQD                     @L1A 00677900
 AIF ('&MF' EQ '' AND '&RETCODE' EQ '' AND '&RSNCODE' EQ '').OKRET @L1A 00678000
&T1      SETC  'MF, RETCODE AND RSNCODE ARE NOT VALID WITH '       @L1A 00678100
&T1      SETC  '&T1.REQUEST=QUERYDYN'                              @L1A 00678200
         MNOTE 8,'&T1'                                             @L1A 00678300
.OKRET   ANOP                                                      @L1A 00678400
         AIF   ('&DYNFUNC'(1,1) NE '(').LREG Not a reg, branch     @L1A 00678500
&T1      SETC  '&DYNFUNC'(2,K'&DYNFUNC-2) Isolate reg number       @L1A 00678600
&T1      SETC  '0(1,&T1.)'             Target expression           @L1A 00678700
&T2      SETC  '0&DYNFUNC'             Target expression           @L1A 00678800
         AGO   .MREG                                               @L1A 00678900
.LREG    ANOP                          Not a reg                   @L1A 00679000
&T2      SETC  '&DYNFUNC(1)'           Target expression           @L1A 00679100
&A1      SETA  K'&DYNFUNC              Length of expression        @L1A 00679200
         AIF   ('&DYNFUNC'(&A1,1) NE ')').NOTRX Branch if not RX   @L1A 00679300
&A2      SETA  2                                                   @L1A 00679400
.OREG    ANOP                          Loop to find open paren     @L1A 00679500
         AIF   (&A2 GE &A1).NOTRX      Reached end, non-RX         @L1A 00679600
         AIF   ('&DYNFUNC'(&A2,1) NE '(').NREG Not "(", branch     @L1A 00679700
&T1      SETC  '&DYNFUNC'(1,&A2).'1'.','   Found paren             @L1A 00679800
&T1      SETC  '&T1'.'&DYNFUNC'(&A2+1,&A1-&A2) Create expression   @L1A 00679900
         AGO   .MREG                                               @L1A 00680000
.NREG    ANOP                          Continue loop               @L1A 00680100
&A2      SETA  &A2+1                   Next character              @L1A 00680200
         AGO   .OREG                   Repeat                      @L1A 00680300
.NOTRX   ANOP                          Not an RX-address           @L1A 00680400
&T1      SETC  '&DYNFUNC(1).(1)'       Get expression              @L1A 00680500
.MREG    ANOP                          Produce resulting statement @L1A 00680600
         L     15,16(0,0)              GET CVT                     @L1A 00680700
         AIF   ('&SYSASCE' EQ 'P').NOAR                            @L1A 00680800
         LAE   15,0(15,0)              CLEAR AR15                  @L1A 00680900
.NOAR    ANOP                                                      @L1A 00681000
         TM    X'4F1'(15),X'04'        Is MVS release correct      @L1A 00681100
         BZ    C&SYSNDX                No, no dynamic LNKLST       @L1A 00681200
         ICM   15,15,X'3E0'(15)        Check security product      @L1A 00681300
         BZ    A&SYSNDX                No security product, OK     @L1A 00681400
         TM    X'19A'(15),X'10'        Is dynamic LNKLST supported @L1A 00681500
         BNZ   A&SYSNDX                Yes, OK                     @L1A 00681600
C&SYSNDX DS    0H                      Function not available      @L1A 00681700
         MVI   &T2,0                   Function not available      @L1A 00681800
         B     B&SYSNDX                Skip other path             @L1A 00681900
A&SYSNDX DS    0H                      Function possibly available @L1A 00682000
         L     15,16(0,0)              GET CVT                     @L1A 00682100
         L     15,140(15,0)            GET ECVT                    @L1A 00682200
         L     15,228(15,0)                                        @L1A 00682300
         MVC   &T1,27(15)              GET AVAILABILITY VALUE      @L1A 00682400
B&SYSNDX DS    0H                      End of QUERYDYN             @L1A 00682500
         POP   PRINT                                               @01A 00682600
         MEXIT                                                     @L1A 00682700
.NOQD    ANOP                                                      @L1A 00682800
         AIF   ('&LINKAGE' EQ '').NOLINKAGE                        @L1A 00682900
&ZMS     SETC  'LINKAGE MAY NOT BE SPECIFIED'                      @L1A 00683000
         MNOTE 8,'&ZMS'                                            @L1A 00683100
         POP   PRINT                                               @01A 00683200
         MEXIT                                                     @L1A 00683300
.NOLINKAGE ANOP                                                    @L1A 00683400
         AIF   ('&REQUEST' NE '').ISREQUEST                        @L1A 00683500
         AIF   ('&XMF' NE 'E').ISREQUEST                           @L1A 00683600
&ZMS     SETC  'REQUEST IS REQUIRED WITH MF=E'                     @L1A 00683700
         MNOTE 8,'&ZMS'                                            @L1A 00683800
         POP   PRINT                                               @01A 00683900
         MEXIT                                                     @L1A 00684000
.ISREQUEST ANOP                                                    @L1A 00684100
         AIF   ('&REQUEST(1)' NE 'INTERNAL').REQUESTOK             @L1A 00684200
&ZMS     SETC  'REQUEST=INTERNAL MAY NOT BE SPECIFIED'             @L1A 00684300
         MNOTE 8,'&ZMS'                                            @L1A 00684400
         POP   PRINT                                               @01A 00684500
         MEXIT                                                     @L1A 00684600
.REQUESTOK ANOP                                                    @L1A 00684700
         AIF   ('&REQUEST(1)' NE 'ACTIVATE').REQUESTOK2            @L1A 00684800
&ZMS     SETC  'REQUEST=ACTIVATE MAY NOT BE SPECIFIED'             @L1A 00684900
         MNOTE 8,'&ZMS'                                            @L1A 00685000
         POP   PRINT                                               @01A 00685100
         MEXIT                                                     @L1A 00685200
.REQUESTOK2 ANOP                                                   @L1A 00685300
