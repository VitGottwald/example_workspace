.***AC.PLV01  PROCESS PLISTVER                                      *   00222100
.********************************************************************   00222200
&XPLISTVER      SETA  0                                                 00222300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00222400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00222500
&XPLISTVER      SETA  0                                                 00222600
.ZPLV0          ANOP  ,                                                 00222700
.*                                                                      00222800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00222900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00223000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00223100
.*-------------------------------------------------------------------   00223200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00223300
&ZMS            SETC  '"PLISTVER='                                      00223400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00223500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00223600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00223700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00223800
                AGO   .ZPVCK2                                           00223900
.ZPVCK3         ANOP  ,                                                 00224000
.*-------------------------------------------------------------------   00224100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00224200
&ZMS            SETC  '"PLISTVER='                                      00224300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00224400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00224500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00224600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00224700
                AGO   .ZPVCKE                                           00224800
.ZPVCK2         ANOP  ,                                                 00224900
.*-------------------------------------------------------------------   00225000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00225100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00225200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00225300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00225400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00225500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00225600
.ZPVCK6         ANOP  ,                                                 00225700
&ZMS            SETC  '"PLISTVER='                                      00225800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00225900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00226000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00226100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00226200
&XPLISTVER      SETA  0                                                 00226300
                AGO   .ZPVCKE                                           00226400
.*-------------------------------------------------------------------   00226500
.ZPVCK4A        ANOP  ,                                                 00226600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00226700
&ZMS            SETC  'PLISTVER='                                       00226800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00226900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00227000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00227100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00227200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00227300
                AGO   .ZPVCK5                                           00227400
.ZPVCK4         ANOP  ,                                                 00227500
&XPLISTVER      SETA  &PLISTVER(1)                                      00227600
.ZPVCK5         ANOP  ,                                                 00227700
.ZPVCKE         ANOP  ,                                                 00227800
.********************************************************************   00227900
.***AG.MF01 PROCESS MF MACRO FORMS                                      00228000
.********************************************************************   00228100
                 ACTR  10000                                            00228200
.*------------------------------------------------------------------    00228300
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00228400
&XMF             SETC  ''          INITIALIZE XVARIABLE                 00228500
.*-------------------------------------------------------------------   00228600
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00228700
.ZG135           ANOP  ,                                                00228800
&ZTLC            SETC 'S'                                               00228900
                 AIF   ('&MF(1)' NE '&ZTLC').ZG136                      00229000
&XMF             SETC  'S'         SET KEYWORD                          00229100
&XMFSCK          SETC  'COMPLETE'  SET XMFSCK                           00229200
                 AIF   (N'&MF LE 1).ZG135A      CK NUM                  00229300
&ZMS             SETC  '"MF='                                           00229400
&ZMS             SETC  '&ZMS.&MF"'                                      00229500
&ZMS             SETC  '&ZMS "MF"'                                      00229600
                 MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'              00229700
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00229800
.ZG135A          ANOP  ,                                                00229900
.*-------------------------------------------------------------------   00230000
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00230100
.ZG136           ANOP  ,                                                00230200
&ZTLC            SETC 'L'                                               00230300
                 AIF   ('&MF(1)' NE '&ZTLC').ZG143                      00230400
&XMF             SETC  'L'         SET KEYWORD                          00230500
                 AIF   (N'&MF LE 3).ZG136A      CK NUM                  00230600
&ZMS             SETC  '"MF='                                           00230700
&ZMS             SETC  '&ZMS.&MF"'                                      00230800
&ZMS             SETC  '&ZMS "MF"'                                      00230900
                 MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'             00231000
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00231100
.ZG136A          ANOP  ,                                                00231200
.*-------------------------------------------------------------------   00231300
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00231400
                 AIF   (N'&SYSLIST NE 0).ZG136B  POSITIONALS?           00231500
                 AIF   ('&REQUEST' NE '').ZG136B  KEYS?                 00231600
                 AIF   ('&LNKLSTNAME' NE '').ZG136B  KEYS?              00231700
                 AIF   ('&COPYFROM' NE '').ZG136B  KEYS?                00231800
                 AIF   ('&CHECKSYS1' NE '').ZG136B  KEYS?               00231900
                 AIF   ('&DSNAME' NE '').ZG136B  KEYS?                  00232000
                 AIF   ('&VOLUME' NE '').ZG136B  KEYS?                  00232100
                 AIF   ('&POS' NE '').ZG136B    KEYS?                   00232200
                 AIF   ('&AFTERDSNAME' NE '').ZG136B  KEYS?             00232300
                 AIF   ('&CHECKCONCAT' NE '').ZG136B  KEYS?             00232400
                 AIF   ('&PROBDSNAME' NE '').ZG136B  KEYS?              00232500
                 AIF   ('&MODNAME' NE '').ZG136B  KEYS?                 00232600
                 AIF   ('&FOUNDDSNAME' NE '').ZG136B  KEYS?             00232700
                 AIF   ('&ANSAREA' NE '').ZG136B  KEYS?                 00232800
                 AIF   ('&ANSLEN' NE '').ZG136B  KEYS?                  00232900
                 AIF   ('&SEARCH' NE '').ZG136B  KEYS?                  00233000
                 AIF   ('&LISTLNKNAME' NE '').ZG136B  KEYS?             00233100
                 AIF   ('&USERINFO' NE '').ZG136B  KEYS?                00233200
                 AIF   ('&JOBNAME' NE '').ZG136B  KEYS?                 00233300
                 AIF   ('&ASID' NE '').ZG136B   KEYS?                   00233400
                 AIF   ('&WHICHAS' NE '').ZG136B  KEYS?                 00233500
                 AIF   ('&DELAY' NE '').ZG136B  KEYS?                   00233600
                 AIF   ('&DYNFUNC' NE '').ZG136B  KEYS?                 00233700
                 AIF   ('&LINKAGE' NE '').ZG136B  KEYS?                 00233800
