.*-------------------------------------------------------------------   00733500
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00733600
&ZPLLEN               SETA 84                                           00733700
.*                                                                      00733800
.******************************************************************     00733900
.***AI.L01 CSVDYNL FMT=LIST FILL IN PARAMETER AREA                      00734000
.******************************************************************     00734100
                      ACTR  10000                                       00734200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00734300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00734400
&ZCVAL                SETC  'LAE'          XAX MODE                     00734500
.ZIMFXA               ANOP ,                                            00734600
.*                                                                      00734700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00734800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00734900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00735000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00735100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00735200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00735300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00735400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00735500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00735600
&ZOPN                 SETA  &ZOPN+1                                     00735700
&ZOPL(&ZOPN)          SETC  ' '                                         00735800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00735900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00736000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00736100
                      AGO   .ZIMFNX                                     00736200
.*                                                                      00736300
.ZIMFN2               ANOP ,                                            00736400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00736500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00736600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00736700
&ZOPN                 SETA  &ZOPN+1                                     00736800
&ZOPL(&ZOPN)          SETC  ' '                                         00736900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00737000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00737100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00737200
                      AGO   .ZIMFNX                                     00737300
.ZIMFNX1              ANOP ,                                            00737400
&ZOPN                 SETA  &ZOPN+1                                     00737500
&ZOPL(&ZOPN)          SETC  ' '                                         00737600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00737700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00737800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00737900
.ZIMFNX               ANOP ,                                            00738000
.********************************************************************   00738100
.***AI.L02 CSVDYNL FMT=LIST INITIALIZE PARAMETER AREA                   00738200
.********************************************************************   00738300
.***AI.L03 MF=(SLME)                                                    00738400
