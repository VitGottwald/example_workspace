.*******************************************************************    00814200
.***  PROCESS CSVDYNL OUTPUT PARAMETERS                                 00814300
.*******************************************************************    00814400
                      ACTR  10000                                       00814500
.*******************************************************************    00814600
.***AO.X02  RETURN CODE PROCESSING                                      00814700
.*******************************************************************    00814800
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00814900
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00815000
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00815100
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00815200
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00815300
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00815400
&ZOPN                 SETA  &ZOPN+1                                     00815500
&ZOPL(&ZOPN)          SETC  ' '                                         00815600
&ZOPO(&ZOPN)          SETC  'ST'                                        00815700
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00815800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00815900
                      AGO   .ZMRETX                                     00816000
.*                                                                      00816100
.ZMRET2               ANOP  ,                                           00816200
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00816300
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00816400
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00816500
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00816600
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00816700
&ZOPN                 SETA  &ZOPN+1                                     00816800
&ZOPL(&ZOPN)          SETC  ' '                                         00816900
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00817000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00817100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00817200
.ZMRETX               ANOP  ,                                           00817300
.*******************************************************************    00817400
.***AO.X03  REASON CODE PROCESSING                                      00817500
.*******************************************************************    00817600
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00817700
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00817800
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00817900
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00818000
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00818100
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00818200
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00818300
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00818400
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00818500
&ZOPN                 SETA  &ZOPN+1                                     00818600
&ZOPL(&ZOPN)          SETC  ' '                                         00818700
&ZOPO(&ZOPN)          SETC  'ST'                                        00818800
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00818900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00819000
                      AGO   .ZMRSNX                                     00819100
.*                                                                      00819200
.ZMRSN2               ANOP  ,                                           00819300
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00819400
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00819500
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00819600
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00819700
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00819800
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00819900
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00820000
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00820100
&ZOPN                 SETA  &ZOPN+1                                     00820200
&ZOPL(&ZOPN)          SETC  ' '                                         00820300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00820400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00820500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00820600
.ZMRSNX               ANOP  ,                                           00820700
.*******************************************************************    00820800
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00820900
.*******************************************************************    00821000
.ZMFLX                ANOP  ,              MF=L PROCESSING              00821100
.*******************************************************************    00821200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00821300
.*******************************************************************    00821400
                      ACTR  10000                                       00821500
.*                                                                      00821600
                      LCLA  &ZLC           COLUMN POSITION              00821700
                      LCLA  &ZOPX          LIST TABLE INDEX             00821800
                      LCLC  &ZLL           LABEL                        00821900
                      LCLC  &ZLOP          OPERATION                    00822000
                      LCLC  &ZLV           VALUE                        00822100
                      LCLC  &ZLD           DESCRIPTION                  00822200
.*                                                                      00822300
&ZOPX                 SETA  1                                           00822400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00822500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00822600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00822700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00822800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00822900
                      AIF   (K'&ZLD LE 0).ZL5                           00823000
&ZLC                  SETA  35             COMMENT COLUMN               00823100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00823200
&ZLC                  SETA  33             COMMMENT COLUMN              00823300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00823400
&ZLV                  SETC  '&ZLV'.' '                                  00823500
                      AGO   .ZL3                                        00823600
.*                                                                      00823700
.ZL4                  ANOP  ,                                           00823800
&ZLV                  SETC  '&ZLV'.' '                                  00823900
.ZL5                  ANOP  ,                                           00824000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00824100
.*                                                                      00824200
&ZLL     &ZLOP &ZLV                                                     00824300
.*                                                                      00824400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00824500
                      AGO   .ZL2           CONTINUE LOOPING             00824600
.*                                                                      00824700
.ZLX                  ANOP  ,                                           00824800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00824900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00825000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00825100
         L     14,16(0,0)              GET CVT ADDRESS             @L1A 00825200
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 00825300
         L     14,448(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 00825400
         PC    0(14)                                               @L1A 00825500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00825600
.ZLINKX               ANOP  ,                                           00825700
.*******************************************************************    00825800
.***AP.X01  FORMAT GENERATED CODE                                       00825900
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00826000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00826100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00826200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00826300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00826400
                      AIF   (K'&ZLD LE 0).ZL51                          00826500
&ZLC                  SETA  35             COMMENT COLUMN               00826600
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00826700
&ZLC                  SETA  33             COMMENT COLUMN               00826800
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00826900
&ZLV                  SETC  '&ZLV'.' '                                  00827000
                      AGO   .ZL31                                       00827100
.*                                                                      00827200
.ZL41                 ANOP  ,                                           00827300
&ZLV                  SETC  '&ZLV'.' '                                  00827400
.ZL51                 ANOP  ,                                           00827500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00827600
.*                                                                      00827700
&ZLL     &ZLOP &ZLV                                                     00827800
.*                                                                      00827900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00828000
                      AGO   .ZL21          CONTINUE LOOPING             00828100
.*                                                                      00828200
.ZLX1                 ANOP  ,                                           00828300
.*******************************************************************    00828400
