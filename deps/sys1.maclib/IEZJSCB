*                                           DATA SET RPL       MDC024*/ 07060000
*   4 JSCBSUB     PTR(31),               /* SAME AS JSCBSUBA            07070000
*                                           BELOW              ICB333*/ 07080000
*    5 JSCRSV31    CHAR(1),              /* RESERVED           ICB333*/ 07090000
*    5 JSCBSUBA    PTR(24),              /* ADDRESS OF JES-SUBTL FOR    07100000
*                                           THIS JOB STEP      ICB333*/ 07110000
*   4 JSCBSONO    FIXED(15),             /* THE NUMBER OF SYSOUT DATA   07120000
*                                           SETS PLUS ONE      ICB335*/ 07130000
*   4 JSCBCRB2    CHAR(2),               /* High order 2 bytes of the   07140000
*                                           SHR RBA used for continue   07150000
*                                           restart. The low order 6    07160000
*                                           bytes of this RBA reside    07170000
*                                           in JSCBCRB6.         @04C*/ 07180000
*   4 JSCBFRBA    CHAR(8) BDY(WORD),     /* RELATIVE BYTE ADDRESS       07190000
*                                           (RBA) OF THE FIRST JOURNAL  07200000
*                                           BLOCK  (MDC032)    YM7086*/ 07210000
*   4 JSCBSSIB    PTR(31),               /* ADDRESS OF THE SUBSYSTEM    07220000
*                                           IDENTIFICATION BLOCK        07230000
*                                                              MDC021*/ 07240000
*   4 JSCDSABQ    PTR(31),               /* ADDRESS OF QDB FOR DSAB     07250000
*                                           CHAIN              MDC007*/ 07260000
*   4 JSCGDDNO    FIXED(32),             /* Counter used by Dynamic     07270000
*                                           Allocation to generate      07280000
*                                           DD names             @P8C*/ 07290000
*   4 JSCSCT      CHAR(4) BDY(WORD),     /* Structure containing SVA of 07300000
*                                           SCT                  @P2C*/ 07310000
*    5 JSCRSV55    CHAR(1),              /* RESERVED                 */ 07320000
*    5 JSCSCTP     CHAR(3) BDY(WORD,2),  /* SVA of SCT, use SWAREQ to   07330000
*                                           convert to a pointer @P2C*/ 07340000
*   4 JSCTMCOR    PTR(31),               /* ADDRESS OF TIOT MAIN        07350000
*                                           STORAGE MANAGEMENT AREA     07360000
*                                                              MDC010*/ 07370000
*   4 JSCBVATA    PTR(31),               /* ADDRESS OF VAT USED         07380000
*                                           DURING SYSTEM RESTART OR    07390000
*                                           AUTOMATIC RESTART  MDC011*/ 07400000
*   4 JSCRSV08    FIXED(15),             /* Reserved, was JSCDDNNO      07410000
*                                                                @P8C*/ 07420000
*   4 JSCBODNO    FIXED(15),             /* COUNTER USED BY DYNAMIC     07430000
*                                           OUTPUT TO GENERATE OUTPUT   07440000
*                                           DESCRIPTOR NAMES            07450000
*                                           THIS NUMBER INCREASES       07460000
*                                           OVER THE LIFE OF THE JOB    07470000
*                                           AND WRAPS.          @DIC */ 07480000
*   4 JSCDDNUM    FIXED(15),             /* NUMBER OF DD ENTRIES        07490000
*                                           CURRENTLY ALLOCATED         07500000
*                                           INCLUDING IN USE AND NOT    07510000
*                                           IN USE ENTRIES     MDC022*/ 07520000
*   4 JSCRSV33    CHAR(1),               /* RESERVED           MDC019*/ 07530000
*   4 JSCBSWSP    PTR(8),                /* SWA SUBPOOL        MDC015*/ 07540000
*   4 JSCBACT     PTR(31),               /* POINTER TO ACTIVE JSCB      07550000
*                                                              MDC014*/ 07560000
*   4 JSCRSV09    PTR(31),        /* Reserved, was JSCBUFPT      @L9C*/ 07570000
*   4 JSCBEACB    PTR(31),               /* Address of                  07580000
*                                           event log ACB        @L8C*/ 07590000
*   4 JSCBPGMN    CHAR(8) BDY(WORD),     /* JOB STEP PROGRAM NAME       07600000
*                                           (MDC304)         @G64RP9A*/ 07610000
*    5 JSCBECB1    FIXED(31),            /* ADDR OF THE CANCEL ECB      07620000
*                                           WHILE WAITING FOR A         07630000
*                                           REGION(IEFSD263) @ZMC1510*/ 07640000
*    5 JSCBECB2    FIXED(31),            /* ADDR OF THE WAIT FOR        07650000
*                                           REGION ECB WHILE WAITING    07660000
*                                           FOR A REGION (IEFSD263)     07670000
*                                                            @ZMC1510*/ 07680000
*   4 JSCDSNQP    PTR(31),               /* Pointer to the first        07690000
*                                           DSENQ Table          @L5C*/ 07700000
*   4 JSCBCSCX    PTR(31),               /* ADDRESS OF CSCX EXTENSION   07710000
*                                           TO CSCB              @L2C*/ 07720000
*                                        /*                     5@L2D*/ 07730000
*   4 JSCAMCPL    PTR(31),               /* ALLOCATION MESSAGE          07740000
*                                           CELLPOOL ID      @YC19251*/ 07750000
*   4 JSCBEND3    CHAR(0) BDY(WORD);     /* END OF SECTION 3   ICB351*/ 07760000
*                                                                       07770000
*        /* MACRO DEFINITIONS OF JSCBDISP, JSCBAOS1, AND JSCBAOS2 */    07780000
*                                                                       07790000
*%IF IBM_CBGENC = 'YES' %THEN            /*                      @L4A*/ 07800000
* %GOTO IEZJSCB_1;                       /*                      @L4A*/ 07810000
*%IF IEZJSCB_GENEQU = 'NO' %THEN         /*                      @L4A*/ 07820000
* %GOTO IEZJSCB_1;                       /*                      @L4A*/ 07830000
*  DCL (JSCBDISP,JSCBAOS1,JSCBAOS2) LABEL; /*                    @L1A*/ 07840000
*                                                                       07850000
*  JSCBDISP:  GEN  (EQU 188);            /* LENGTH OF SECTION 1      */ 07860000
*  JSCBAOS1:  GEN  (EQU  72);            /* LENGTH OF SECTION 2      */ 07870000
*  JSCBAOS2:  GEN  (EQU 192);            /* LENGTH OF SECTION 3      */ 07880000
*%GOTO IEZJSCB_2;                        /*                      @L4A*/ 07890000
*%IEZJSCB_1:;                            /*                      @L4A*/ 07900000
*DCL JSCBDISP CONSTANT(188);             /* LENGTH OF SECTION 1  @L4A*/ 07910000
*DCL JSCBAOS1 CONSTANT(72);              /* LENGTH OF SECTION 2  @L4A*/ 07920000
*DCL JSCBAOS2 CONSTANT(192);             /* LENGTH OF SECTION 3  @L4A*/ 07930000
*%IEZJSCB_2:;                            /*                      @L4A*/ 07940000
*                                                                       07950000
*              /* END OF JSCB */                                        07960000
