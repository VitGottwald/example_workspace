&XPLISTVER      SETA  0                                                 00439800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00439900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00440000
&XPLISTVER      SETA  10                                                00440100
.ZPLV0          ANOP  ,                                                 00440200
.*                                                                      00440300
                AIF   ('&ALLOWDUPREBLD' EQ '').ZPLV1E                   00440400
                AIF   (&XPLISTVER GE 05).ZPLV1E                         00440500
&XPLISTVER      SETA  05                                                00440600
.ZPLV1E         ANOP  ,                                                 00440700
.*                                                                      00440800
                AIF   ('&ALLOWAUTO' EQ '').ZPLV2E                       00440900
                AIF   (&XPLISTVER GE 07).ZPLV2E                         00441000
&XPLISTVER      SETA  07                                                00441100
.ZPLV2E         ANOP  ,                                                 00441200
.*                                                                      00441300
                AIF   ('&SUSPEND' EQ '').ZPLV3E                         00441400
                AIF   (&XPLISTVER GE 07).ZPLV3E                         00441500
&XPLISTVER      SETA  07                                                00441600
.ZPLV3E         ANOP  ,                                                 00441700
.*                                                                      00441800
                AIF   ('&ASYNCDUPLEX' EQ '').ZPLV4E                     00441900
                AIF   (&XPLISTVER GE 09).ZPLV4E                         00442000
&XPLISTVER      SETA  09                                                00442100
.ZPLV4E         ANOP  ,                                                 00442200
.*                                                                      00442300
                AIF   ('&ALLOWALTER' EQ '').ZPLV5E                      00442400
                AIF   (&XPLISTVER GE 01).ZPLV5E                         00442500
&XPLISTVER      SETA  01                                                00442600
.ZPLV5E         ANOP  ,                                                 00442700
.*                                                                      00442800
                AIF   ('&RATIO' EQ '').ZPLV6E                           00442900
