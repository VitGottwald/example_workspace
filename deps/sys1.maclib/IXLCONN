&ZMS            SETC  '"PLISTVER='                                      00452900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00453000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00453100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00453200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00453300
                AGO   .ZPVCK2                                           00453400
.ZPVCK3         ANOP  ,                                                 00453500
.*-------------------------------------------------------------------   00453600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00453700
&ZMS            SETC  '"PLISTVER='                                      00453800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00453900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00454000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00454100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00454200
                AGO   .ZPVCKE                                           00454300
.ZPVCK2         ANOP  ,                                                 00454400
.*-------------------------------------------------------------------   00454500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00454600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00454700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00454800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00454900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00455000
                AIF   (&ZN LE 10).ZPVCK4A  NOT HIGH                     00455100
.ZPVCK6         ANOP  ,                                                 00455200
&ZMS            SETC  '"PLISTVER='                                      00455300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00455400
&ZMS            SETC  '&ZMS THE RANGE IS "0-10"'                        00455500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00455600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00455700
&XPLISTVER      SETA  10                                                00455800
                AGO   .ZPVCKE                                           00455900
.*-------------------------------------------------------------------   00456000
.ZPVCK4A        ANOP  ,                                                 00456100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00456200
&ZMS            SETC  'PLISTVER='                                       00456300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00456400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00456500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00456600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00456700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00456800
                AGO   .ZPVCK5                                           00456900
.ZPVCK4         ANOP  ,                                                 00457000
&XPLISTVER      SETA  &PLISTVER(1)                                      00457100
.ZPVCK5         ANOP  ,                                                 00457200
.ZPVCKE         ANOP  ,                                                 00457300
