.******************************************************************     01040600
.***AI.L01 IXLCONN FMT=LIST FILL IN PARAMETER AREA                      01040700
.******************************************************************     01040800
                      ACTR  10000                                       01040900
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01041000
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01041100
&ZCVAL                SETC  'LAE'          XAX MODE                     01041200
.ZIMFXA               ANOP ,                                            01041300
.*                                                                      01041400
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01041500
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01041600
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01041700
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01041800
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01041900
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01042000
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01042100
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01042200
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01042300
&ZOPN                 SETA  &ZOPN+1                                     01042400
&ZOPL(&ZOPN)          SETC  ' '                                         01042500
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01042600
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01042700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01042800
                      AGO   .ZIMFNX                                     01042900
.*                                                                      01043000
.ZIMFN2               ANOP ,                                            01043100
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01043200
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01043300
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01043400
&ZOPN                 SETA  &ZOPN+1                                     01043500
&ZOPL(&ZOPN)          SETC  ' '                                         01043600
&ZOPO(&ZOPN)          SETC  'LAE'                                       01043700
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01043800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01043900
                      AGO   .ZIMFNX                                     01044000
.ZIMFNX1              ANOP ,                                            01044100
&ZOPN                 SETA  &ZOPN+1                                     01044200
&ZOPL(&ZOPN)          SETC  ' '                                         01044300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01044400
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                01044500
