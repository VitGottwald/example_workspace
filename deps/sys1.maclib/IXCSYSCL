.***AC.PLV01  PROCESS PLISTVER                                      *   00052000
.********************************************************************   00052100
&XPLISTVER      SETA  0                                                 00052200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00052300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00052400
&XPLISTVER      SETA  0                                                 00052500
.ZPLV0          ANOP  ,                                                 00052600
.*                                                                      00052700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00052800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00052900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00053000
.*-------------------------------------------------------------------   00053100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00053200
&ZMS            SETC  '"PLISTVER='                                      00053300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00053500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00053600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00053700
                AGO   .ZPVCK2                                           00053800
.ZPVCK3         ANOP  ,                                                 00053900
.*-------------------------------------------------------------------   00054000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00054100
&ZMS            SETC  '"PLISTVER='                                      00054200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00054300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00054400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00054500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054600
                AGO   .ZPVCKE                                           00054700
.ZPVCK2         ANOP  ,                                                 00054800
.*-------------------------------------------------------------------   00054900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00055000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00055100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00055200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00055300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00055400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00055500
.ZPVCK6         ANOP  ,                                                 00055600
&ZMS            SETC  '"PLISTVER='                                      00055700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00055900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00056000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056100
&XPLISTVER      SETA  0                                                 00056200
                AGO   .ZPVCKE                                           00056300
.*-------------------------------------------------------------------   00056400
.ZPVCK4A        ANOP  ,                                                 00056500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00056600
&ZMS            SETC  'PLISTVER='                                       00056700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00056800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00056900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00057000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00057100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057200
                AGO   .ZPVCK5                                           00057300
.ZPVCK4         ANOP  ,                                                 00057400
&XPLISTVER      SETA  &PLISTVER(1)                                      00057500
.ZPVCK5         ANOP  ,                                                 00057600
.ZPVCKE         ANOP  ,                                                 00057700
