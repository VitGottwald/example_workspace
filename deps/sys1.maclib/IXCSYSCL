.***AO.X04  LABEL FOR MF=L PROCESSING                                   00131600
.*******************************************************************    00131700
.ZMFLX                ANOP  ,              MF=L PROCESSING              00131800
.*******************************************************************    00131900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00132000
.*******************************************************************    00132100
                      ACTR  10000                                       00132200
.*                                                                      00132300
                      LCLA  &ZLC           COLUMN POSITION              00132400
                      LCLA  &ZOPX          LIST TABLE INDEX             00132500
                      LCLC  &ZLL           LABEL                        00132600
                      LCLC  &ZLOP          OPERATION                    00132700
                      LCLC  &ZLV           VALUE                        00132800
                      LCLC  &ZLD           DESCRIPTION                  00132900
.*                                                                      00133000
&ZOPX                 SETA  1                                           00133100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00133200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00133300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00133400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00133500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00133600
                      AIF   (K'&ZLD LE 0).ZL5                           00133700
&ZLC                  SETA  35             COMMENT COLUMN               00133800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00133900
&ZLC                  SETA  33             COMMMENT COLUMN              00134000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00134100
&ZLV                  SETC  '&ZLV'.' '                                  00134200
                      AGO   .ZL3                                        00134300
.*                                                                      00134400
.ZL4                  ANOP  ,                                           00134500
&ZLV                  SETC  '&ZLV'.' '                                  00134600
.ZL5                  ANOP  ,                                           00134700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00134800
.*                                                                      00134900
&ZLL     &ZLOP &ZLV                                                     00135000
.*                                                                      00135100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00135200
                      AGO   .ZL2           CONTINUE LOOPING             00135300
.*                                                                      00135400
.ZLX                  ANOP  ,                                           00135500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00135600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00135700
         L     14,16(0,0)              GET CVT ADDRESS                  00135800
         L     14,772(14,0)            GET SFT ADDRESS                  00135900
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00136000
         LA    15,7                    GET EX FOR SERVICE ROUTINE       00136100
         OR    14,15                   GET PC NUMBER                    00136200
         PC    0(14)                                                    00136300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00136400
.ZLINKX               ANOP  ,                                           00136500
.*******************************************************************    00136600
.***AP.X01  FORMAT GENERATED CODE                                       00136700
