&ZOPD(&ZOPN)          SETC  '++ &ZXFAILEDSYS XFAILEDSYS'                00124400
.*                                                                      00124500
.ZI5E                 ANOP  ,                                           00124600
.*-------------------------------------------------------------------   00124700
&ZOPN1                SETA &ZOPN                                        00124800
.*******************************************************************    00124900
.***  PROCESS IXCSYSCL OUTPUT PARAMETERS                                00125000
.*******************************************************************    00125100
                      ACTR  10000                                       00125200
.*******************************************************************    00125300
.***AO.X02  RETURN CODE PROCESSING                                      00125400
.*******************************************************************    00125500
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00125600
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00125700
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00125800
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00125900
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00126000
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00126100
&ZOPN                 SETA  &ZOPN+1                                     00126200
&ZOPL(&ZOPN)          SETC  ' '                                         00126300
&ZOPO(&ZOPN)          SETC  'ST'                                        00126400
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00126500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00126600
                      AGO   .ZMRETX                                     00126700
.*                                                                      00126800
.ZMRET2               ANOP  ,                                           00126900
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00127000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00127100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00127200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00127300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00127400
&ZOPN                 SETA  &ZOPN+1                                     00127500
&ZOPL(&ZOPN)          SETC  ' '                                         00127600
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00127700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00127800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00127900
.ZMRETX               ANOP  ,                                           00128000
.*******************************************************************    00128100
.***AO.X03  REASON CODE PROCESSING                                      00128200
.*******************************************************************    00128300
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00128400
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00128500
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00128600
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00128700
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00128800
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00128900
