.*-------------------------------------------------------------------   00116900
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00117000
&ZPLLEN               SETA 20                                           00117100
.*                                                                      00117200
.******************************************************************     00117300
.***AI.L01 IWM4MDRG FMT=LIST FILL IN PARAMETER AREA                     00117400
.******************************************************************     00117500
                      ACTR  10000                                       00117600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00117700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00117800
&ZCVAL                SETC  'LAE'          XAX MODE                     00117900
.ZIMFXA               ANOP ,                                            00118000
.*                                                                      00118100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00118200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00118300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00118400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00118500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00118600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00118700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00118800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00118900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00119000
&ZOPN                 SETA  &ZOPN+1                                     00119100
&ZOPL(&ZOPN)          SETC  ' '                                         00119200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00119300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00119400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00119500
                      AGO   .ZIMFNX                                     00119600
.*                                                                      00119700
.ZIMFN2               ANOP ,                                            00119800
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00119900
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00120000
&ZOPN                 SETA  &ZOPN+1                                     00120100
&ZOPL(&ZOPN)          SETC  ' '                                         00120200
&ZOPO(&ZOPN)          SETC  'LAE'                                       00120300
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00120400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00120500
                      AGO   .ZIMFNX                                     00120600
.ZIMFNX1              ANOP ,                                            00120700
&ZOPN                 SETA  &ZOPN+1                                     00120800
&ZOPL(&ZOPN)          SETC  ' '                                         00120900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00121000
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00121100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00121200
.ZIMFNX               ANOP ,                                            00121300
.********************************************************************   00121400
.***AI.L02 IWM4MDRG FMT=LIST INITIALIZE PARAMETER AREA                  00121500
.********************************************************************   00121600
