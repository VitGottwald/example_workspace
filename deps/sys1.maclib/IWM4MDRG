                  MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                00097500
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00097600
.ZG8R             ANOP  ,                                               00097700
.******************************************************************     00097800
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00097900
.******************************************************************     00098000
                  ACTR  10000                                           00098100
.*----------------------------------------------------------------***   00098200
.***AH.P02 EXTRA POSITIONALS                                            00098300
                  AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS            00098400
                  AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL            00098500
                  AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL        00098600
                  AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL         00098700
.ZGLISTX1         ANOP  ,                                               00098800
&ZCVAL            SETC  ''          ERROR FIELD                         00098900
&ZAWRK            SETA  0           POSITIONAL NUMBER                   00099000
.ZGLIST2          ANOP  ,                                               00099100
&ZAWRK            SETA  &ZAWRK+1    NEXT POSITIONAL                     00099200
                  AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE            00099300
&ZCVAL            SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                 00099400
                  AGO   .ZGLIST2                                        00099500
.*                                                                      00099600
.ZGLIST3          ANOP  ,                                               00099700
&ZCVAL            SETC '&ZCVAL'(2,K'&ZCVAL-1)   STRIP COMMA             00099800
&ZMS              SETC  'POSITIONAL ARGUMENT(S),'                       00099900
&ZMS              SETC  '&ZMS "&ZCVAL"'                                 00100000
&ZMS              SETC  '&ZMS CANNOT BE USED.'                          00100100
&ZMS              SETC  '&ZMS NO POSITIONAL ARGUMENTS'                  00100200
                  MNOTE 8,' &ZMS ARE ALLOWED.'                          00100300
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00100400
.ZGLISTX          ANOP  ,                                               00100500
&ZASYSLNDX        SETA  0                                               00100600
