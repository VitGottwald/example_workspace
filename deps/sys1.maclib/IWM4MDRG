&ZOPL(&ZOPN)          SETC  ' '                                         00106700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00106800
&ZOPV(&ZOPN)          SETC  '1'                                         00106900
&ZOPD(&ZOPN)          SETC  ' '                                         00107000
.*                                                                      00107100
.ZLMFSL2              ANOP  ,                                           00107200
.*-------------------------------------------------------------------   00107300
.***AM.L00  IWM4MDRG FMT=LIST name                                      00107400
