.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00132300
.*******************************************************************    00132400
                      ACTR  10000                                       00132500
.*                                                                      00132600
                      LCLA  &ZLC           COLUMN POSITION              00132700
                      LCLA  &ZOPX          LIST TABLE INDEX             00132800
                      LCLC  &ZLL           LABEL                        00132900
                      LCLC  &ZLOP          OPERATION                    00133000
                      LCLC  &ZLV           VALUE                        00133100
                      LCLC  &ZLD           DESCRIPTION                  00133200
.*                                                                      00133300
&ZOPX                 SETA  1                                           00133400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00133500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00133600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00133700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00133800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00133900
                      AIF   (K'&ZLD LE 0).ZL5                           00134000
&ZLC                  SETA  35             COMMENT COLUMN               00134100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00134200
&ZLC                  SETA  33             COMMMENT COLUMN              00134300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00134400
&ZLV                  SETC  '&ZLV'.' '                                  00134500
                      AGO   .ZL3                                        00134600
.*                                                                      00134700
.ZL4                  ANOP  ,                                           00134800
&ZLV                  SETC  '&ZLV'.' '                                  00134900
.ZL5                  ANOP  ,                                           00135000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00135100
.*                                                                      00135200
&ZLL     &ZLOP &ZLV                                                     00135300
.*                                                                      00135400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00135500
                      AGO   .ZL2           CONTINUE LOOPING             00135600
.*                                                                      00135700
.ZLX                  ANOP  ,                                           00135800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00135900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00136000
         L     14,16(0,0)              GET CVT ADDRESS                  00136100
         L     14,772(14,0)            GET SFT ADDRESS                  00136200
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00136300
         LA    15,44                   GET EX FOR SERVICE ROUTINE       00136400
         OR    14,15                   GET PC NUMBER                    00136500
         PC    0(14)                                                    00136600
END&SYSNDX DS    0H                    END LABEL                        00136700
