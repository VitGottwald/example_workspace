.*******************************************************************    00101400
.***AE.01  START OF IWM4MDRG MACRO CODE                                 00101500
.*******************************************************************    00101600
                  LCLC  &M0M0001                 NAME                   00101700
&M0M0001          SETC  '&XLABEL'                SET NAME               00101800
                  AIF   (K'&XLABEL NE 0).XLAB2                          00101900
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           00102000
.*                                                                      00102100
.XLAB2            ANOP  ,                                               00102200
                  AIF   ('&XMF' EQ 'L').XLAB3                           00102300
&M0M0001 DS    0H                             IWM4MDRG-0                00102400
.XLAB3            ANOP  ,                                               00102500
.*******************************************************************    00102600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00102700
.*******************************************************************    00102800
              PUSH  PRINT                                               00102900
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00103000
                  PRINT OFF         ++ PR01                             00103100
                  AGO   .ZPRNTXX                                        00103200
.ZPRINTX          PRINT ON                                              00103300
.ZPRNTXX          ANOP  ,                                               00103400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00103500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00103600
.********************************************************************   00103700
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00103800
.********************************************************************   00103900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00104000
                      LCLA  &ZOPN1         INVOCATION INDEX             00104100
                      LCLC  &ZOPL(4095)    LABEL                        00104200
                      LCLC  &ZOPO(4095)    OPERATION                    00104300
                      LCLC  &ZOPV(4095)    VALUE                        00104400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00104500
.*                                                                      00104600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00104700
.******************************************************************     00104800
.***AL.L10 IWM4MDRG FMT=LIST MF=S!L PARAMETERS                          00104900
.******************************************************************     00105000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00105100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00105200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00105300
&XMFATTR              SETC  '0D'                                        00105400
.*                                                                      00105500
&ZOPN                 SETA  &ZOPN+1                                     00105600
&ZOPL(&ZOPN)          SETC  ' '                                         00105700
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00105800
&ZOPV(&ZOPN)          SETC  '4,8'                                       00105900
&ZOPN                 SETA  &ZOPN+1                                     00106000
&ZOPL(&ZOPN)          SETC  ' '                                         00106100
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00106200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00106300
&ZOPD(&ZOPN)          SETC  '++ IWM4MDRG ARG PARMS'                     00106400
.*                                                                      00106500
