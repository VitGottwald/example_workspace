                  AIF   ('&ZCGPR'(1,1) NE '0').ZG8P                     00096200
                  AIF   (K'&ZCGPR GT 2).ZG8Q                            00096300
                  AIF   ('&ZCGPR' EQ '0').ZG8Q                          00096400
