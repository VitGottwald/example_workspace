.ZG2Q             ANOP  ,                                               00089500
&ZMS              SETC  '"RESOURCE_TKN='                                00089600
&ZMS              SETC  '&ZMS.&RESOURCE_TKN"'                           00089700
&ZMS              SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'       00089800
                  MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                00089900
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00090000
.ZG2R             ANOP  ,                                               00090100
.*----------------------------------------------------------------***   00090200
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00090300
                  AIF   (N'&RETCODE LE 1).ZG5A  CK NUM                  00090400
&ZMS              SETC  '"RETCODE='                                     00090500
&ZMS              SETC  '&ZMS.&RETCODE"'                                00090600
&ZMS              SETC  '&ZMS "RETCODE"'                                00090700
                  MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'             00090800
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00090900
.ZG5A             ANOP  ,                                               00091000
.*-------------------------------------------------------------------   00091100
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00091200
                  AIF   (K'&RETCODE(1) LE 0).ZG5C  NO PARM              00091300
&ZXRETCODE        SETC  'OUTPUT'    SET OUTPUT                          00091400
&XRETCODE         SETC  '&RETCODE(1)'           SET XVARIABLE           00091500
                  AIF   (N'&RETCODE GT 1).ZG5C                          00091600
&XRETCODE         SETC  '&RETCODE'  SET XVARIABLE                       00091700
.ZG5C             ANOP  ,                                               00091800
.*-------------------------------------------------------------------   00091900
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00092000
                  AIF   (K'&ZXRETCODE LE 0).ZG5R  NOT I/O               00092100
                  AIF   ('&XRETCODE'(1,1) NE '(').ZG5R                  00092200
&ZCGPR            SETC  '&XRETCODE'(2,K'&XRETCODE-2)                    00092300
                  AIF   ('&ZCGPR'(1,1) NE '0').ZG5P                     00092400
                  AIF   (K'&ZCGPR GT 2).ZG5Q                            00092500
                  AIF   ('&ZCGPR' EQ '0').ZG5Q                          00092600
&ZCGPR            SETC  '&ZCGPR'(2,1)                                   00092700
                  AIF   (T'&ZCGPR NE 'N').ZG5Q  INVALID REG NAME        00092800
.ZG5P             ANOP  ,                                               00092900
                  AIF   (T'&ZCGPR NE 'N').ZG5R  NON NUMERIC             00093000
                  AIF   ('&ZCGPR' LT '2').ZG5Q                          00093100
                  AIF   ('&ZCGPR' LE '12').ZG5R                         00093200
.ZG5Q             ANOP  ,                                               00093300
&ZMS              SETC  '"RETCODE='                                     00093400
&ZMS              SETC  '&ZMS.&RETCODE"'                                00093500
&ZMS              SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'       00093600
                  MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                00093700
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00093800
.ZG5R             ANOP  ,                                               00093900
.*----------------------------------------------------------------***   00094000
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00094100
                  AIF   (N'&RSNCODE LE 1).ZG8A  CK NUM                  00094200
&ZMS              SETC  '"RSNCODE='                                     00094300
&ZMS              SETC  '&ZMS.&RSNCODE"'                                00094400
&ZMS              SETC  '&ZMS "RSNCODE"'                                00094500
                  MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'             00094600
