.***  PROCESS IWM4MDRG OUTPUT PARAMETERS                                00125300
.*******************************************************************    00125400
                      ACTR  10000                                       00125500
.*******************************************************************    00125600
.***AO.X02  RETURN CODE PROCESSING                                      00125700
.*******************************************************************    00125800
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00125900
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00126000
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00126100
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00126200
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00126300
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00126400
&ZOPN                 SETA  &ZOPN+1                                     00126500
&ZOPL(&ZOPN)          SETC  ' '                                         00126600
&ZOPO(&ZOPN)          SETC  'ST'                                        00126700
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00126800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00126900
                      AGO   .ZMRETX                                     00127000
.*                                                                      00127100
.ZMRET2               ANOP  ,                                           00127200
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00127300
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00127400
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00127500
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00127600
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00127700
&ZOPN                 SETA  &ZOPN+1                                     00127800
&ZOPL(&ZOPN)          SETC  ' '                                         00127900
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00128000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00128100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00128200
.ZMRETX               ANOP  ,                                           00128300
.*******************************************************************    00128400
.***AO.X03  REASON CODE PROCESSING                                      00128500
.*******************************************************************    00128600
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00128700
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00128800
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00128900
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00129000
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00129100
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00129200
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00129300
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00129400
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00129500
&ZOPN                 SETA  &ZOPN+1                                     00129600
&ZOPL(&ZOPN)          SETC  ' '                                         00129700
&ZOPO(&ZOPN)          SETC  'ST'                                        00129800
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00129900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00130000
                      AGO   .ZMRSNX                                     00130100
.*                                                                      00130200
.ZMRSN2               ANOP  ,                                           00130300
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00130400
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00130500
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00130600
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00130700
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00130800
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00130900
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00131000
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00131100
&ZOPN                 SETA  &ZOPN+1                                     00131200
&ZOPL(&ZOPN)          SETC  ' '                                         00131300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00131400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00131500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00131600
.ZMRSNX               ANOP  ,                                           00131700
