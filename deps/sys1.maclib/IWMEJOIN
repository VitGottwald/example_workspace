                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00147800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00147900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00148000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00148100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00148200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00148300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00148400
&ZOPN                 SETA  &ZOPN+1                                     00148500
