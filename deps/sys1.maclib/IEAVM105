* %Dcl Mdb_InclMdbObjType ext char;                                     05990000
* %If translate(Mdb_InclMdbObjType) = 'YES'                             06000000
* %Then                                                                 06010000
*   %Do;                                                                06020000
*/********************************************************************/ 06030000
*/*                                                                  */ 06040000
*/* MDB Generic Object Type                                          */ 06050000
*/*                                                                  */ 06060000
*/*   Each object in the MDB has a 4 byte structure at offset zero   */ 06070000
*/*   that consists of a 2 byte length and a 2 byte type.            */ 06080000
*/*                                                                  */ 06090000
*/********************************************************************/ 06100000
*  Dcl 1 Mdb_tMdbObject type                                            06110000
*       ,3 ObjectLen fixed(15)      /* Length of object          @LAA*/ 06120000
*       ,3 ObjectType isa(Mdb_tObjType);/* Type of MDB Object    @LAA*/ 06130000
*  Dcl Mdb_tObjType fixed(16) type;                                     06140000
*  Dcl Mdb_kGenObj  isa(Mdb_tObjType) constant(MDBGOBJ);                06150000
*  Dcl Mdb_kScpObj  isa(Mdb_tObjType) constant(MDBCOBJ);                06160000
*  Dcl Mdb_kTxtObj  isa(Mdb_tObjType) constant(MDBTOBJ);                06170000
*   %End;                                                               06180000
*/********************************************************************/ 06190000
*/*                                                                  */ 06200000
*/* MDB (Main header for the block)                                  */ 06210000
*/*                                                                  */ 06220000
*/********************************************************************/ 06230000
* %Dcl IEAVM105_TypeDcl ext char;                             /* @LCA*/ 06240000
* Dcl                                                                   06250000
* %If translate(IEAVM105_TypeDcl) = 'YES' /* If Mdb Type requested      06260000
*                                                                @LCA*/ 06270000
* %Then                                                                 06280000
*   %Do;                            /* Do Mdb Type               @LCA*/ 06290000
*    1 tMdbHeader Bdy(hWord) type,  /* Mdb Header Type           @LCA*/ 06300000
*   %End;                           /* EndDo Mdb Type            @LCA*/ 06310000
* %Else                                                                 06320000
*   %Do;                            /* Do Mdb non-type           @LCA*/ 06330000
*    1 MDB     BDY(HWORD) IEAVM105_MDBVIA BASED(MDBPTR),                06340000
*                                   /* Start of MDB header       @LBC*/ 06350000
*   %End;                           /* EndDo Mdb non-type        @LCA*/ 06360000
*      3 MDBLEN     FIXED(16),      /*  MDB length               @PBC*/ 06370000
*      3 MDBTYPE    CHAR(2),        /*  MDB type                     */ 06380000
*      3 MDBMID     CHAR(4),        /*  Acronym 'MDB'                */ 06390000
*      3 MDBVER     BIT(32);        /*  Revision code                */ 06400000
*/********************************************************************/ 06410000
*/*                                                                  */ 06420000
*/* General object for the message data block                        */ 06430000
*/*                                                                  */ 06440000
*/********************************************************************/ 06450000
* Dcl                                                                   06460000
* %If translate(IEAVM105_TypeDcl) = 'YES' /* If Mdb Type requested      06470000
*                                                                @LCA*/ 06480000
* %Then                                                                 06490000
*   %Do;                            /* Do Mdb Type               @LCA*/ 06500000
*    1  tMdbG Bdy(hWord) type,      /* General Object Section Type      06510000
*                                                                @LCA*/ 06520000
*   %End;                           /* EndDo Mdb Type            @LCA*/ 06530000
* %Else                                                                 06540000
*   %Do;                            /* Do Mdb non-type           @LCA*/ 06550000
*    1  MDBG   BDY(HWORD) IEAVM105_MDBVIA BASED,                        06560000
*   %End;                           /* EndDo Mdb non-type        @LCA*/ 06570000
*                                   /*  General object section   @LBC*/ 06580000
*      3 MDBGLEN     FIXED(15),     /*  General object length        */ 06590000
*      3 MDBGTYPE    CHAR(2),       /*  General object type          */ 06600000
*      3 MDBGMID     BIT(32),       /*  Message ID               @P4C*/ 06610000
*        5 MDBGSYID  CHAR(1),       /*  System ID                @L1A*/ 06620000
*        5 MDBGSEQ   BIT(24),       /*  Sequence number          @L1A*/ 06630000
*      3 MDBGTIMH    CHAR(8),       /*  Time stamp HH.MM.SS format   */ 06640000
*      3 MDBGTIMT    CHAR(3),       /*  Time stamp .TH format        */ 06650000
*      3 MDBGRSV1    CHAR(1),       /*  Reserved                 @P4C*/ 06660000
*      3 MDBGDSTP    CHAR(7),       /*  Date stamp                   */ 06670000
*      3 MDBGRSV2    CHAR(1),       /*  Reserved                 @P4C*/ 06680000
*      3 MDBGMFLG    BIT(16),       /*  Message flags                */ 06690000
*        5 MDBGDOM    BIT(1),       /*  DOM bit.  If this bit is on it  06700000
*                                       indicates that this MDB is for  06710000
*                                       a DOM.  The DOM information     06720000
*                                       can be found in the DOM flags   06730000
*                                       in the control program object   06740000
*                                       (MDBDOMFL)               @P7C*/ 06750000
*        5 MDBGALRM   BIT(1),       /*  Sound warning alarm (processor  06760000
