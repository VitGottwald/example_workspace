*/*                                                                  */ 12160000
*/********************************************************************/ 12170000
*DECLARE                                                                12180000
*   1 CLMHDR BASED,                 /* Member Header                 */ 12190000
*     3 CLMID    CHAR(8),           /* Eye catcher                   */ 12200000
*     3 CLMOPRF  FIXED(16),         /* Offset to member prefix          12210000
*                                        section                     */ 12220000
*     3 *        FIXED(16),         /* Reserved for future use       */ 12230000
*     3 CLMNXTM  PTR(31);           /* Next member header address    */ 12240000
*   DCL CLMEYEC  CHAR(8)  CONSTANT('CLASSMBR'); /* Eye catcher value */ 12250000
*/********************************************************************/ 12270000
*/*                                                                  */ 12280000
*/*      Member Section Identifiers                                  */ 12290000
*/*                                                                  */ 12300000
*/********************************************************************/ 12310000
*   DCL CLMIDPRF FIXED(8) CONSTANT('80'X); /* Member Info - Prefix   */ 12320000
*   DCL CLMIDGEN FIXED(8) CONSTANT('81'X); /* Member Info - General  */ 12330000
*   DCL CLMIDJS2 FIXED(8) CONSTANT('82'X); /* Member Info - JES2     */ 12340000
*   DCL CLMIDJS3 FIXED(8) CONSTANT('83'X); /* Member Info - JES3     */ 12350000
*/********************************************************************/ 12360000
*/*                                                                  */ 12370000
*/*      Member Section Modifiers                                    */ 12380000
*/*                                                                  */ 12390000
*/********************************************************************/ 12400000
*   DCL CLMIMGEN FIXED(8) CONSTANT('80'X); /* Modifier - General     */ 12410000
*   DCL CLMIMJ2A FIXED(8) CONSTANT('82'X); /* Modifier - JES2        */ 12420000
