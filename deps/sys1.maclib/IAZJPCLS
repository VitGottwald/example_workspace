* % END_OF_ASM_JPCL: ;                                                  08170000
*                                                                       08180000
* %IF JPCLS_ATRB = ''            /* IF NO ATTRIBUTE SPECIFIED        */ 08190000
*   %THEN                                                               08200000
*     %JPCLS_ATRB = 'BASED';     /* ESTABLISH DEFAULT ATTRIBUTE      */ 08210000
*                                                                       08220000
*/*                                                                  */ 08230000
*/* Return codes from SSI 82 are defined in IAZSSJP macro            */ 08240000
*                                                                       08250000
*DECLARE                                                                08300000
*   1 JPCLS   JPCLS_ATRB,                                               08310000
*     3 JPCLID   CHAR(8),           /* I.Eye catcher                 */ 08320000
*     3 JPCLLEN  FIXED(16),         /* I.Length of JPCLS             */ 08330000
*     3 JPCLVER  FIXED(16),         /* I.Parm list version number    */ 08340000
*       5 JPCLVERL FIXED(8),        /*   I.Parm list version level   */ 08350000
*       5 JPCLVERM FIXED(8),        /*   I.Parm list version         */ 08360000
*     3 JPCLVERO FIXED(16),         /* O.Subsystem version number    */ 08370000
*     3 *        FIXED(16),         /* Reserved                      */ 08380000
*     3 JPCLUSER,                   /* USER AREA                     */ 08390000
*/********************************************************************/ 08400000
*/*      JPCLSTRP is an anchor for use by the subsystem              */ 08410000
*/*      that responds to this request.  It is expected that         */ 08420000
*/*      the caller will set this to zero the FIRST time an          */ 08430000
*/*      SSOB extension is used and from that point on it            */ 08440000
*/*      will be managed by the subsystem.                           */ 08450000
*/********************************************************************/ 08460000
*       5 JPCLSTRP PTR(31),         /* Storage management anchor     */ 08470000
*       5 JPCLFLG1 BIT(8),          /* IS.Flag byte                  */ 08480000
*         7 JPCL1CLS BIT(1),        /*    Return data for class         08490000
*                                           name (see JPCLCNAM)      */ 08500000
*         7 JPCL1GRP BIT(1),        /*    Return data for job class     08502000
*                                           group (JPCLGNAM) @Z21LCLS*/ 08505000
*         7 *        BIT(6),        /*    Reserved                   */ 08506000
*       5 *(3)     FIXED(8),        /* Reserved                      */ 08510000
*/********************************************************************/ 08530000
*/*     NOTE: Each filter below is enabled via one of the            */ 08540000
*/*           bit settings above.                                    */ 08550000
*/********************************************************************/ 08560000
*       5 JPCLCNAM CHAR(8),         /* IS*.Class name for filter        08570000
*                                           See JPCL1CLS bit         */ 08580000
*       5 JPCLGNAM CHAR(8),         /* IS*.Class group name as a        08583000
*                                          filter (JPCL1GRP) @Z21LCLS*/ 08586000
