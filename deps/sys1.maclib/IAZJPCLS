*/********************************************************************/ 12590000
*/*                                                                  */ 12600000
*/*      Member General Information Section                          */ 12610000
*/*                                                                  */ 12620000
*/*      NOTE: To get here, add the CLMOPRF header field             */ 12630000
*/*            and the prefix size (CLMPRSZ) to the header           */ 12640000
*/*            (CLMHDR) address.                                     */ 12650000
*/*                                                                  */ 12660000
*/********************************************************************/ 12670000
*DECLARE                                                                12680000
*   1 CLMGENI  BASED,               /* Member General Information    */ 12690000
*     3 CLMGLN   FIXED(16),         /* Length of this section        */ 12700000
*     3 CLMGTY   FIXED(8),          /* Type     = General  (CLMIDGEN)*/ 12710000
*     3 CLMGMD   FIXED(8),          /* Type Mod = General  (CLMIMGEN)*/ 12720000
*     3 CLMGMNAM CHAR(8),           /* Member name                   */ 12730000
*     3 CLMGSNAM CHAR(8),           /* MVS System name               */ 12740000
*     3 CLMGFLG1 BIT(8),            /* Flag byte                     */ 12750000
*       5 CLMG1ENB BIT(1),          /*   Class is enabled / active      12760000
*                                          on member                 */ 12770000
*       5 CLMG1ACT BIT(1),          /*   Member is active            */ 12780000
*       5 CLMG1PXQ BIT(1),          /*   Class is on halted member,     12790000
*                                          $PXEQ issued                 12800000
*                                          (JES2 only)               */ 12810000
*       5 CLMG1DRN BIT(1),          /*   Class is on draining member    12820000
*                                          (JES2 only)               */ 12830000
*       5 CLMG1DEF BIT(1),          /*   Class is defined on member     12840000
*                                          (JES3 only)               */ 12850000
*       5 *        BIT(3),          /*   Reserved            @Z21LCLS*/ 12855000
*     3 *(3)     FIXED(8),          /* Reserved for future use       */ 12860000
*     3 CLMGJMAX FIXED(32),         /* Maximum job count for            12870000
*                                        this class on member           12880000
*                                        (MDEPTH for JES3 if            12890000
*                                         specified)                 */ 12895000
*     3 CLMGJCUR FIXED(32),         /* Current active job count         12900000
*                                        for this class on member    */ 12910000
*     3 *        CHAR(0);           /* End of structure              */ 12920000
*/********************************************************************/ 12940000
*/*                                                                  */ 12950000
*/*      Class Member Information - JES3 specific                    */ 12960000
*/*                                                                  */ 12970000
*/*      This section includes a variable length array of            */ 12980000
*/*      MLIMIT data for the member being reported.  Each            */ 12990000
*/*      entry in this MLIMIT array defines the number of            */ 13000000
*/*      jobs that can be running on the reported member             */ 13010000
*/*      for a controlling class and still allow jobs in             */ 13020000
*/*      in this class (the "owning" class) to be selected.          */ 13030000
*/*      For example, if we are reporting data for class             */ 13040000
*/*      CLSA (owning class) on member SY1, and the MLIMIT           */ 13045000
*/*      array for SY1 has an entry which says class CLSB            */ 13050000
*/*      has a job count limit of 2 and currently 4 jobs             */ 13060000
*/*      from class CLSB are running on member SY1,                  */ 13070000
*/*      then no jobs from class CLSA will be selected               */ 13080000
*/*      to run on this member.                                      */ 13090000
*/*                                                                  */ 13100000
*/*      Each MLIMIT array entry is mapped by the CLM3MLIM           */ 13110000
*/*      DSECT (see below).                                          */ 13120000
*/*                                                                  */ 13130000
*/*      NOTE: To get here, add the CLMGLN field to the              */ 13140000
*/*            General Info section (CLMGENI) address.               */ 13150000
*/*                                                                  */ 13160000
*/********************************************************************/ 13170000
*DECLARE                                                                13180000
*   1 CLMJES3I BASED,               /* Member JES3 specific info     */ 13190000
*     3 CLM3LN   FIXED(16),         /* Length of this section -         13200000
*                                        includes variable length       13210000
*                                        MLIMIT information          */ 13220000
*     3 CLM3TY   FIXED(8),          /* Type     = JES3     (CLMIDJS3)*/ 13230000
*     3 CLM3MD   FIXED(8),          /* Type Mod = General  (CLMIMGEN)*/ 13240000
*     3 CLM3SELM CHAR(8),           /* Selection mode name           */ 13250000
*     3 CLM3MLOF FIXED(16),         /* Offset to 1st MLIMIT entry    */ 13310000
*     3 CLM3MLCT FIXED(16),         /* MLIMIT entry count            */ 13320000
*     3 CLM3MLSI FIXED(16),         /* Size of a MLIMIT entry        */ 13330000
*     3 *(2)     FIXED(8),          /* Reserved for future use       */ 13340000
*     3 *        CHAR(0);           /* End of structure              */ 13350000
*/********************************************************************/ 13370000
*/*      MLIMIT array entry - JES3 specific                          */ 13380000
*/********************************************************************/ 13390000
*DECLARE                                                                13400000
*   1 CLM3MLIM BASED,               /* MLIMIT entry                  */ 13410000
*     3 CLM3MCLS CHAR(8),           /* Controlling class name        */ 13420000
*     3 CLM3MMAX FIXED(32),         /* Maximum jobs in controlling      13430000
*                                        class                       */ 13440000
*     3 CLM3MCUR FIXED(32),         /* Current jobs in controlling      13450000
*                                        class                       */ 13460000
*     3 *        CHAR(0);           /* End of structure              */ 13470000
* % JPCLSEND: ;                                                         13500000
*/*                          END OF MACRO                            */ 13510000
