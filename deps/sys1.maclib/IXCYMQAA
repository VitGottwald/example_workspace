         MACRO                                                          00008000
         IXCYMQAA &DSECT=YES,&LIST=YES,&TITLE=YES,&MQAHDR=YES,         *00009000
               &MQAENTRY=COND,&MQATARGONLYENTRY=COND,                  *00010000
               &MQATARGRESPENTRY=COND,&MQATARGRESPENTRY1=COND           00011000
