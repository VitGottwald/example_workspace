                   LCLC  &ZXIOCTOKEN            ++ INOUT NAME           00146400
                   LCLC  &ZXLINKAGE  ++ INPUT VALUE                     00146500
                   LCLC  &ZXRETCODE  ++ OUTPUT NAME                     00146600
                   LCLC  &ZXRSNCODE  ++ OUTPUT NAME                     00146700
                   LCLC  &ZXPLISTVER            ++ INPUT VALUE          00146800
                   LCLC  &ZXMF       ++ INPUT VALUE                     00146900
                   LCLC  &ZXMFCTRL   ++ INPUT NAME                      00147000
                   LCLC  &ZXMFATTR   ++ INPUT VALUE                     00147100
                   LCLC  &ZXMFSCK    ++ INPUT VALUE                     00147200
.********************************************************************   00147300
.***AC.PLV01  PROCESS PLISTVER                                      *   00147400
.********************************************************************   00147500
&XPLISTVER      SETA  1                                                 00147600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00147700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00147800
&XPLISTVER      SETA  3                                                 00147900
.ZPLV0          ANOP  ,                                                 00148000
.*                                                                      00148100
                AIF   ('&LDEVNCHAR' EQ '').ZPLV1E                       00148200
                AIF   (&XPLISTVER GE 3).ZPLV1E                          00148300
&XPLISTVER      SETA  3                                                 00148400
.ZPLV1E         ANOP  ,                                                 00148500
.*                                                                      00148600
                AIF   ('&UCBCXPTR' EQ '').ZPLV2E                        00148700
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00148800
&XPLISTVER      SETA  2                                                 00148900
.ZPLV2E         ANOP  ,                                                 00149000
.*                                                                      00149100
                AIF   ('&UCBPXPTR' EQ '').ZPLV3E                        00149200
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00149300
&XPLISTVER      SETA  2                                                 00149400
.ZPLV3E         ANOP  ,                                                 00149500
.*                                                                      00149600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00149700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00149800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00149900
.*-------------------------------------------------------------------   00150000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00150100
&ZMS            SETC  '"PLISTVER='                                      00150200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00150300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00150400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00150500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00150600
                AGO   .ZPVCK2                                           00150700
.ZPVCK3         ANOP  ,                                                 00150800
.*-------------------------------------------------------------------   00150900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00151000
&ZMS            SETC  '"PLISTVER='                                      00151100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00151200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00151300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00151400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00151500
                AGO   .ZPVCKE                                           00151600
.ZPVCK2         ANOP  ,                                                 00151700
.*-------------------------------------------------------------------   00151800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00151900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00152000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00152100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00152200
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00152300
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00152400
.ZPVCK6         ANOP  ,                                                 00152500
&ZMS            SETC  '"PLISTVER='                                      00152600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00152700
&ZMS            SETC  '&ZMS THE RANGE IS "1-3"'                         00152800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00152900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00153000
&XPLISTVER      SETA  3                                                 00153100
                AGO   .ZPVCKE                                           00153200
.*-------------------------------------------------------------------   00153300
.ZPVCK4A        ANOP  ,                                                 00153400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00153500
&ZMS            SETC  'PLISTVER='                                       00153600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00153700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00153800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00153900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00154000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00154100
                AGO   .ZPVCK5                                           00154200
.ZPVCK4         ANOP  ,                                                 00154300
&XPLISTVER      SETA  &PLISTVER(1)                                      00154400
.ZPVCK5         ANOP  ,                                                 00154500
.ZPVCKE         ANOP  ,                                                 00154600
