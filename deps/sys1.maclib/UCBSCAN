.*******************************************************************    00577100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00577200
.*******************************************************************    00577300
                      ACTR  10000                                       00577400
.*                                                                      00577500
                      LCLA  &ZLC           COLUMN POSITION              00577600
                      LCLA  &ZOPX          LIST TABLE INDEX             00577700
                      LCLC  &ZLL           LABEL                        00577800
                      LCLC  &ZLOP          OPERATION                    00577900
                      LCLC  &ZLV           VALUE                        00578000
                      LCLC  &ZLD           DESCRIPTION                  00578100
.*                                                                      00578200
&ZOPX                 SETA  1                                           00578300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00578400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00578500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00578600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00578700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00578800
                      AIF   (K'&ZLD LE 0).ZL5                           00578900
&ZLC                  SETA  35             COMMENT COLUMN               00579000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00579100
&ZLC                  SETA  33             COMMMENT COLUMN              00579200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00579300
&ZLV                  SETC  '&ZLV'.' '                                  00579400
                      AGO   .ZL3                                        00579500
.*                                                                      00579600
.ZL4                  ANOP  ,                                           00579700
&ZLV                  SETC  '&ZLV'.' '                                  00579800
.ZL5                  ANOP  ,                                           00579900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00580000
.*                                                                      00580100
&ZLL     &ZLOP &ZLV                                                     00580200
.*                                                                      00580300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00580400
                      AGO   .ZL2           CONTINUE LOOPING             00580500
.*                                                                      00580600
.ZLX                  ANOP  ,                                           00580700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00580800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00580900
.*********************************************************************  00581000
.*                                                                   *  00581100
.* Keyword UCBPAREA is required if both COPY and SUBCHANNELSET(ALL)  *  00581200
.* are specified.                                                    *  00581300
.*                                                               @L3A*  00581400
.*********************************************************************  00581500
         AIF    ('&XOUTVAR1'       NE 'COPY').ZSSCHK              @L3A  00581600
         AIF    ('&XSUBCHANNELSET' NE 'ALL').ZSSCHK               @L3A  00581700
         AIF    ('&XUCBPAREA'      NE 'NONE').ZSSCHK              @L3A  00581800
&ZXUCBPAREA        SETC  ''          RESET IN/OUTPUT              @L3A  00581900
&ZMS               SETC  ' '         *KRQDA*                      @L3A  00582000
&ZMS               SETC  '&ZMS "UCBPAREA" KEY AND ITS'            @L3A  00582100
                   MNOTE 8,' &ZMS ARGUMENT ARE REQUIRED.'         @L3A  00582200
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE  @L3A  00582300
.ZSSCHK            ANOP  ,                                        @L3A  00582400
.*                                                                      00582500
.*-----------BAL invocation code-------------                           00582600
         LA    0,2                ++ SET UCBSCAN FUNCTION CODE          00582700
         AIF    ('&XLINKAGE' NE 'BRANCH').LNKPC1                        00582800
.*********************************************************************  00582900
.*                                                                   *  00583000
.*  Get addressability to CVT                                        *  00583100
.*                                                                   *  00583200
.*********************************************************************  00583300
         L     15,16(0,0)                                               00583400
.*********************************************************************  00583500
.*                                                                   *  00583600
.*  Get addressability to IOCOM                                      *  00583700
.*                                                                   *  00583800
.*********************************************************************  00583900
         L     15,124(15,0)                                             00584000
.*********************************************************************  00584100
.*                                                                   *  00584200
.*  Get addressability to IOVT                                       *  00584300
.*                                                                   *  00584400
.*********************************************************************  00584500
         L     15,208(15,0)                                             00584600
         L     15,12(15,0)        ++ SET ADDR OF UCB SERVICES           00584700
         BALR  14,15              ++ BRANCH TO UCB SERVICES             00584800
         AGO   .DONE1                                                   00584900
.LNKPC1  ANOP                                                           00585000
         L     14,16(0,0)         ++ GET CVT ADDRESS                    00585100
         L     14,772(14,0)       ++ GET SFT ADDRESS                    00585200
.*********************************************************************  00585300
.*                                                                   *  00585400
.*  Get LX/EX for UCB Services                                       *  00585500
.*                                                                   *  00585600
.*  Note: LX/EX is calculated by multiplying the PC number (78/79)   *  00585700
.*        times the length of a PC/AUTH table entry (4) and          *  00585800
.*        then subtracting 4.                                        *  00585900
.*                                                                   *  00586000
.*        authorized functions (ADDRESS and EXIT) invoke PC 78       *  00586100
.*        unauthorized function (COPY) invokes PC 79                 *  00586200
.*                                                                   *  00586300
.*********************************************************************  00586400
         AIF    ('&XOUTVAR1' EQ 'ADDRESS').LNKPC2                       00586500
         AIF    ('&XOUTVAR1' EQ 'EXIT').LNKPC2                          00586600
         L     14,312(14,0)       ++ GET LX/EX FOR UCB SERVICES         00586700
         AGO   .DONE2                                                   00586800
.LNKPC2  ANOP                                                           00586900
         L     14,308(14,0)       ++ GET LX/EX FOR UCB SERVICES         00587000
.DONE2   ANOP                                                           00587100
         PC    0(14)              ++ PC TO UCB SERVICES                 00587200
.DONE1   ANOP                                                           00587300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00587400
.ZLINKX               ANOP  ,                                           00587500
.*******************************************************************    00587600
.***AP.X01  FORMAT GENERATED CODE                                       00587700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00587800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00587900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00588000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00588100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00588200
                      AIF   (K'&ZLD LE 0).ZL51                          00588300
&ZLC                  SETA  35             COMMENT COLUMN               00588400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00588500
&ZLC                  SETA  33             COMMENT COLUMN               00588600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00588700
&ZLV                  SETC  '&ZLV'.' '                                  00588800
                      AGO   .ZL31                                       00588900
.*                                                                      00589000
.ZL41                 ANOP  ,                                           00589100
&ZLV                  SETC  '&ZLV'.' '                                  00589200
.ZL51                 ANOP  ,                                           00589300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00589400
.*                                                                      00589500
&ZLL     &ZLOP &ZLV                                                     00589600
.*                                                                      00589700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00589800
                      AGO   .ZL21          CONTINUE LOOPING             00589900
.*                                                                      00590000
.ZLX1                 ANOP  ,                                           00590100
.*******************************************************************    00590200
.***AP.X02  END OF UCBSCAN MACRO                                        00590300
&ZUCBSCAN             SETC  'YES'          SET UCBSCAN USED             00590400
         POP   PRINT                                                    00590500
*                                            UCBSCAN-3                  00590600
         SPACE 1                                                        00590700
.ZLX2                 ANOP  ,                                           00590800
         MEND                                                           00590900
**********************************************************************/ 00591000
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00591100
*%UCBSCAN: MACRO KEYS(COPY,WORKAREA,UCBAREA,CMXTAREA,DEVNCHAR,          00591200
*                 LDEVNCHAR,UCBPAREA,DCEAREA,DCELEN,VOLSER,ADDRESS,     00591300
