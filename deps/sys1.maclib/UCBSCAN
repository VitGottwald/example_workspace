                   AIF   ('&XMF' EQ 'L').XLAB3                          00376900
&M0M0001 DS    0H                             UCBSCAN-3                 00377000
.XLAB3             ANOP  ,                                              00377100
.*******************************************************************    00377200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00377300
.*******************************************************************    00377400
              PUSH  PRINT                                               00377500
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     00377600
                   PRINT OFF         ++ PR01                            00377700
                   AGO   .ZPRNTXX                                       00377800
.ZPRINTX           PRINT ON                                             00377900
.ZPRNTXX           ANOP  ,                                              00378000
.********************************************************************   00378100
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00378200
.********************************************************************   00378300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00378400
                      LCLA  &ZOPN1         INVOCATION INDEX             00378500
                      LCLC  &ZOPL(4095)    LABEL                        00378600
                      LCLC  &ZOPO(4095)    OPERATION                    00378700
                      LCLC  &ZOPV(4095)    VALUE                        00378800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00378900
.*                                                                      00379000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00379100
.******************************************************************     00379200
.***AL.L10 UCBSCAN FMT=LIST MF=S!L PARAMETERS                           00379300
.******************************************************************     00379400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00379500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00379600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00379700
&XMFATTR              SETC  '0D'                                        00379800
.*                                                                      00379900
&ZOPN                 SETA  &ZOPN+1                                     00380000
&ZOPL(&ZOPN)          SETC  ' '                                         00380100
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00380200
&ZOPO(&ZOPN)          SETC  'B'                                         00380300
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00380400
                      AGO   .ZALVL6B                                    00380500
.ZALVL6A              ANOP                                              00380600
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00380700
&ZOPV(&ZOPN)          SETC  '4,8'                                       00380800
&ZOPN                 SETA  &ZOPN+1                                     00380900
&ZOPL(&ZOPN)          SETC  ' '                                         00381000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00381100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00381200
.ZALVL6B              ANOP                                              00381300
&ZOPD(&ZOPN)          SETC  '++ UCBSCAN ARG PARMS'                      00381400
.*                                                                      00381500
&ZOPN                 SETA  &ZOPN+1                                     00381600
&ZOPL(&ZOPN)          SETC  ' '                                         00381700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00381800
&ZOPV(&ZOPN)          SETC  '1'                                         00381900
&ZOPD(&ZOPN)          SETC  ' '                                         00382000
.*                                                                      00382100
.ZLMFSL2              ANOP  ,                                           00382200
