.***AO.X02  RETURN CODE PROCESSING                                      00570600
.*******************************************************************    00570700
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00570800
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00570900
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00571000
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00571100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00571200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00571300
&ZOPN                 SETA  &ZOPN+1                                     00571400
&ZOPL(&ZOPN)          SETC  ' '                                         00571500
&ZOPO(&ZOPN)          SETC  'ST'                                        00571600
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00571700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00571800
                      AGO   .ZMRETX                                     00571900
.*                                                                      00572000
.ZMRET2               ANOP  ,                                           00572100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00572200
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00572300
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00572400
