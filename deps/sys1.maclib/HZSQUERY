         AIF   ('&SYSAM64' NE 'YES').NOT_AM64                           01254900
&ZMAC_L  SETC  'LLGT'                                                   01255000
.NOT_AM64 ANOP                                                          01255100
         &ZMAC_L 15,16(0,0)         Get CVT address                     01255200
         AIF   ('&SYSASCE' EQ 'P').NOT_AR_1                             01255300
         LAE   15,0(15,0)           Clear AR 15                         01255400
.NOT_AR_1 ANOP                                                          01255500
         L     15,140(15,0)         Get ECVT address                    01255600
         L     15,204(15,0)         Get CTBL address                    01255700
         ICM   15,B'1111',300(15)   Get HZSTABLE address                01255800
         JZ    HZSQUERYNA_&SYSNDX   No HZSTABLE, get out now            01255900
         TM    1240(15),X'80'       Check if commands accepted          01256000
         JZ    HZSQUERYNA_&SYSNDX   No cmds accepted, get out now       01256100
         AIF   (&SYSALVL GE 2).IS_ZARCH_1                               01256200
         CLI   163(0),00            Z-Architecture?                     01256300
         JE    HZSQUERYESA390_&SYSNDX                                   01256400
.IS_ZARCH_1 ANOP                                                        01256500
         LMH   15,15,1372(15)                                           01256600
         AIF   (&SYSALVL GE 2).IS_ZARCH_2                               01256700
HZSQUERYESA390_&SYSNDX DS 0H                                            01256800
.IS_ZARCH_2 ANOP                                                        01256900
         AIF   ('&INSTANCE' EQ 'LOGSTREAM').LS1A                   @L1A 01257000
         ICM   15,B'1111',1340(15)  Get HZSQUERY PC NUM                 01257100
         AGO   .LS1B                                               @L1A 01257200
.LS1A    ANOP                                                      @L1A 01257300
         ICM   15,B'1111',1344(15)  Get HZSQUERY/LOGSTREAM PC NUM  @L1A 01257400
.LS1B    ANOP                                                      @L1A 01257500
         JZ    HZSQUERYNA_&SYSNDX   No PC Num, get out now              01257600
         PC    0(15)                PC to HZSQUERY Mod                  01257700
         J     HZSQUERYEND_&SYSNDX  Branch around setting bad RC        01257800
HZSQUERYNA_&SYSNDX DS 0H                                                01257900
         LA    15,X'C'              Set RC (HC not active)              01258000
         LA    0,X'C01'             Set RSN not active                  01258100
HZSQUERYEND_&SYSNDX DS 0H                                               01258200
