.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00239100
.********************************************************************   00239200
                   GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL            00239300
                   GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE     00239400
                   GBLA  &SYSALVL    SYSSTATE ARCHLVL                   00239500
                   GBLC  &SYSAM64    SYSSTATE AMODE64                   00239600
.*                                                                      00239700
                   AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED     00239800
                   AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED      00239900
                   SPLEVEL  TEST                                        00240000
.ZSYS2             ANOP  ,                                              00240100
                   AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1          00240200
                   AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1          00240300
                   SYSSTATE TEST                                        00240400
                   AGO   .ZSYS4                                         00240500
.ZSYS3             ANOP  ,                                              00240600
&SYSASCE           SETC  'P'         ASSUME SYSSTATE PRIMARY            00240700
.ZSYS4             ANOP  ,                                              00240800
                   AIF   ('&SYSAM64' NE 'YES').ZSYSN64                  00240900
&ZMAC_LLGT         SETC 'LLGT'                                          00241000
&ZMAC_LGR          SETC 'LGR'                                           00241100
&ZMAC_LG           SETC 'LG'                                            00241200
&ZMAC_STG          SETC 'STG'                                           00241300
.ZSYSN64           ANOP  ,                                              00241400
.*******************************************************************    00241500
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00241600
.*******************************************************************    00241700
                   LCLC  &XREQUEST   ++ VALUE                           00241800
                   LCLC  &XSECCHECKONLY         ++ VALUE                00241900
                   LCLC  &XSECCHECK  ++ VALUE                           00242000
                   LCLC  &XSYSNAME   ++ NAME                            00242100
                   LCLC  &XOWNER1    ++ NAME                            00242200
                   LCLC  &XCHECKOWNER           ++ NAME                 00242300
                   LCLC  &XOWNER     ++ NAME                            00242400
                   LCLC  &XNAME1     ++ NAME                            00242500
                   LCLC  &XCHECKNAME            ++ NAME                 00242600
                   LCLC  &XNAME      ++ NAME                            00242700
                   LCLC  &XEXITRTN   ++ NAME                            00242800
                   LCLC  &XCATEGORY  ++ NAME                            00242900
                   LCLC  &XNUMCAT    ++ NAME                            00243000
                   LCLC  &XCATRULE   ++ VALUE                           00243100
                   LCLC  &XCATRULEVAL           ++ NAME                 00243200
                   LCLC  &XCHECKTYPE            ++ VALUE                00243300
                   LCLC  &XEXCEPTION            ++ VALUE                00243400
                   LCLC  &XRESULT    ++ VALUE                           00243500
                   LCLC  &XOUTPUTSTYLE          ++ VALUE                00243600
                   LCLC  &XOUTPUTSTYLEV         ++ NAME                 00243700
                   LCLC  &XQUAACVER  ++ VALUE                           00243800
                   LCLC  &XLOCALE    ++ VALUE                           00243900
                   LCLC  &XREXX      ++ VALUE                           00244000
                   LCLC  &XGLOBALCHECK          ++ VALUE                00244100
                   LCLC  &XINSTANCE  ++ VALUE                           00244200
                   LCLC  &XMSGTOKEN  ++ NAME                            00244300
                   LCLC  &XQUAAC1HDR            ++ NAME                 00244400
                   LCLC  &XKEEP      ++ VALUE                           00244500
                   LCLC  &XREQUESTTYPE          ++ VALUE                00244600
                   LCLC  &XPOLICYNAME           ++ NAME                 00244700
                   LCLC  &XPOLICYSTMT           ++ NAME                 00244800
                   LCLC  &XDATE      ++ VALUE                           00244900
                   LCLC  &XTEXTSTRING           ++ VALUE                00245000
                   LCLC  &XANSAREA1  ++ NAME                            00245100
                   LCLC  &XANSAREA   ++ NAME                            00245200
                   LCLC  &XANSAREA64            ++ NAME                 00245300
                   LCLC  &XANSLEN1   ++ NAME                            00245400
                   LCLC  &XANSLEN    ++ NAME                            00245500
                   LCLC  &XANSLEN64  ++ NAME                            00245600
                   LCLC  &XRETCODE   ++ NAME                            00245700
                   LCLC  &XRSNCODE   ++ NAME                            00245800
                   LCLA  &XPLISTVER  ++ VALUE                           00245900
                   LCLC  &XMF        ++ VALUE                           00246000
                   LCLC  &XMFCTRL    ++ NAME                            00246100
                   LCLC  &XMFATTR    ++ VALUE                           00246200
                   LCLC  &XMFSCK     ++ VALUE                           00246300
.*******************************************************************    00246400
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00246500
.*******************************************************************    00246600
                   LCLC  &ZXREQUEST  ++ INPUT VALUE                     00246700
