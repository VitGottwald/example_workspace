&ZMS               SETC  '&ZMS "MF"'                                    00263200
                   MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'           00263300
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00263400
.ZG237A            ANOP  ,                                              00263500
.*-------------------------------------------------------------------   00263600
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00263700
                   AIF   (N'&SYSLIST NE 0).ZG237B  POSITIONALS?         00263800
                   AIF   ('&REQUEST' NE '').ZG237B  KEYS?               00263900
                   AIF   ('&SECCHECKONLY' NE '').ZG237B  KEYS?          00264000
                   AIF   ('&SECCHECK' NE '').ZG237B  KEYS?              00264100
                   AIF   ('&SYSNAME' NE '').ZG237B  KEYS?               00264200
                   AIF   ('&CHECKOWNER' NE '').ZG237B  KEYS?            00264300
                   AIF   ('&OWNER' NE '').ZG237B  KEYS?                 00264400
                   AIF   ('&CHECKNAME' NE '').ZG237B  KEYS?             00264500
                   AIF   ('&NAME' NE '').ZG237B  KEYS?                  00264600
                   AIF   ('&EXITRTN' NE '').ZG237B  KEYS?               00264700
                   AIF   ('&CATEGORY' NE '').ZG237B  KEYS?              00264800
                   AIF   ('&NUMCAT' NE '').ZG237B  KEYS?                00264900
                   AIF   ('&CATRULE' NE '').ZG237B  KEYS?               00265000
                   AIF   ('&CATRULEVAL' NE '').ZG237B  KEYS?            00265100
                   AIF   ('&CHECKTYPE' NE '').ZG237B  KEYS?             00265200
                   AIF   ('&EXCEPTION' NE '').ZG237B  KEYS?             00265300
                   AIF   ('&RESULT' NE '').ZG237B  KEYS?                00265400
                   AIF   ('&OUTPUTSTYLE' NE '').ZG237B  KEYS?           00265500
                   AIF   ('&OUTPUTSTYLEV' NE '').ZG237B  KEYS?          00265600
                   AIF   ('&QUAACVER' NE '').ZG237B  KEYS?              00265700
                   AIF   ('&LOCALE' NE '').ZG237B  KEYS?                00265800
                   AIF   ('&REXX' NE '').ZG237B  KEYS?                  00265900
                   AIF   ('&GLOBALCHECK' NE '').ZG237B  KEYS?           00266000
                   AIF   ('&INSTANCE' NE '').ZG237B  KEYS?              00266100
                   AIF   ('&MSGTOKEN' NE '').ZG237B  KEYS?              00266200
                   AIF   ('&QUAAC1HDR' NE '').ZG237B  KEYS?             00266300
                   AIF   ('&KEEP' NE '').ZG237B  KEYS?                  00266400
                   AIF   ('&REQUESTTYPE' NE '').ZG237B  KEYS?           00266500
                   AIF   ('&POLICYNAME' NE '').ZG237B  KEYS?            00266600
                   AIF   ('&POLICYSTMT' NE '').ZG237B  KEYS?            00266700
                   AIF   ('&DATE' NE '').ZG237B  KEYS?                  00266800
                   AIF   ('&TEXTSTRING' NE '').ZG237B  KEYS?            00266900
                   AIF   ('&ANSAREA' NE '').ZG237B  KEYS?               00267000
                   AIF   ('&ANSAREA64' NE '').ZG237B  KEYS?             00267100
                   AIF   ('&ANSLEN' NE '').ZG237B  KEYS?                00267200
                   AIF   ('&ANSLEN64' NE '').ZG237B  KEYS?              00267300
                   AIF   ('&RETCODE' NE '').ZG237B  KEYS?               00267400
                   AIF   ('&RSNCODE' NE '').ZG237B  KEYS?               00267500
                   AGO    .ZG237C    INVOCATION OKAY                    00267600
.ZG237B            ANOP  ,                                              00267700
&ZMS               SETC  'THE "L" FORM OF "MF"'                         00267800
&ZMS               SETC  '&ZMS CAN ONLY HAVE'                           00267900
