               &EXCEPTION=,&RESULT=,&OUTPUTSTYLE=,&OUTPUTSTYLEV=,      *00232600
               &QUAACVER=,&LOCALE=,&REXX=,&GLOBALCHECK=,&INSTANCE=,    *00232700
               &MSGTOKEN=,&QUAAC1HDR=,&KEEP=,&REQUESTTYPE=,            *00232800
               &POLICYNAME=,&POLICYSTMT=,&DATE=,&TEXTSTRING=,&ANSAREA=,*00232900
               &ANSAREA64=,&ANSLEN=,&ANSLEN64=,&RETCODE=,&RSNCODE=,    *00233000
               &PLISTVER=,&MF=                                          00233100
