.***AC.PLV01  PROCESS PLISTVER                                      *   00251400
.********************************************************************   00251500
&XPLISTVER      SETA  0                                                 00251600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00251700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00251800
&XPLISTVER      SETA  2                                                 00251900
.ZPLV0          ANOP  ,                                                 00252000
.*                                                                      00252100
                AIF   ('&QUAAC1HDR' EQ '').ZPLV1E                       00252200
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00252300
&XPLISTVER      SETA  2                                                 00252400
.ZPLV1E         ANOP  ,                                                 00252500
.*                                                                      00252600
                AIF   ('&QUAAC1HDR' EQ '').ZPLV2E                       00252700
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00252800
&XPLISTVER      SETA  2                                                 00252900
.ZPLV2E         ANOP  ,                                                 00253000
.*                                                                      00253100
                AIF   ('&QUAAC1HDR' EQ '').ZPLV3E                       00253200
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00253300
&XPLISTVER      SETA  2                                                 00253400
.ZPLV3E         ANOP  ,                                                 00253500
.*                                                                      00253600
                AIF   ('&POLICYNAME' EQ '').ZPLV4E                      00253700
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00253800
&XPLISTVER      SETA  1                                                 00253900
.ZPLV4E         ANOP  ,                                                 00254000
.*                                                                      00254100
                AIF   ('&ANSAREA64' EQ '').ZPLV5E                       00254200
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00254300
&XPLISTVER      SETA  2                                                 00254400
.ZPLV5E         ANOP  ,                                                 00254500
.*                                                                      00254600
                AIF   ('&ANSLEN64' EQ '').ZPLV6E                        00254700
                AIF   (&XPLISTVER GE 2).ZPLV6E                          00254800
&XPLISTVER      SETA  2                                                 00254900
.ZPLV6E         ANOP  ,                                                 00255000
.*                                                                      00255100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00255200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00255300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00255400
.*-------------------------------------------------------------------   00255500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00255600
&ZMS            SETC  '"PLISTVER='                                      00255700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00255800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00255900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00256000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00256100
                AGO   .ZPVCK2                                           00256200
.ZPVCK3         ANOP  ,                                                 00256300
.*-------------------------------------------------------------------   00256400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00256500
&ZMS            SETC  '"PLISTVER='                                      00256600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00256700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00256800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00256900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00257000
                AGO   .ZPVCKE                                           00257100
.ZPVCK2         ANOP  ,                                                 00257200
.*-------------------------------------------------------------------   00257300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00257400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00257500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00257600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00257700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00257800
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00257900
.ZPVCK6         ANOP  ,                                                 00258000
&ZMS            SETC  '"PLISTVER='                                      00258100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00258200
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00258300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00258400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00258500
&XPLISTVER      SETA  2                                                 00258600
                AGO   .ZPVCKE                                           00258700
.*-------------------------------------------------------------------   00258800
.ZPVCK4A        ANOP  ,                                                 00258900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00259000
&ZMS            SETC  'PLISTVER='                                       00259100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00259200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00259300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00259400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00259500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00259600
                AGO   .ZPVCK5                                           00259700
.ZPVCK4         ANOP  ,                                                 00259800
&XPLISTVER      SETA  &PLISTVER(1)                                      00259900
.ZPVCK5         ANOP  ,                                                 00260000
.ZPVCKE         ANOP  ,                                                 00260100
.********************************************************************   00260200
