                   AIF   ('&ZCGPR' LE '12').ZE229R                      01028800
.ZE229Q            ANOP  ,                                              01028900
&ZMS               SETC  '"RSNCODE='                                    01029000
&ZMS               SETC  '&ZMS.&RSNCODE"'                               01029100
&ZMS               SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'      01029200
                   MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'               01029300
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        01029400
.ZE229R            ANOP  ,                                              01029500
.******************************************************************     01029600
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        01029700
.******************************************************************     01029800
                   ACTR  10000                                          01029900
.*----------------------------------------------------------------***   01030000
.***AF.P02 EXTRA POSITIONALS                                            01030100
                   AIF   (N'&SYSLIST LE 0).ZELISTX  NO EXTRAS           01030200
&ZCVAL             SETC  ''          ERROR FIELD                        01030300
&ZAWRK             SETA  0           POSITIONAL NUMBER                  01030400
.ZELIST2           ANOP  ,                                              01030500
&ZAWRK             SETA  &ZAWRK+1    NEXT POSITIONAL                    01030600
                   AIF   (&ZAWRK GT N'&SYSLIST).ZELIST3  DONE           01030700
&ZCVAL             SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                01030800
                   AGO   .ZELIST2                                       01030900
.*                                                                      01031000
.ZELIST3           ANOP  ,                                              01031100
&ZCVAL             SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA             01031200
&ZMS               SETC  'POSITIONAL ARGUMENT(S),'                      01031300
&ZMS               SETC  '&ZMS "&ZCVAL"'                                01031400
&ZMS               SETC  '&ZMS CANNOT BE USED.'                         01031500
&ZMS               SETC  '&ZMS NO POSITIONAL ARGUMENTS'                 01031600
                   MNOTE 8,' &ZMS ARE ALLOWED.'                         01031700
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        01031800
.ZELISTX           ANOP  ,                                              01031900
&ZASYSLNDX         SETA  0                                              01032000
.*-------------------------------------------------------------------   01032100
.***AE.MF02 MF=(SLME)                                                   01032200
.ZEMFX             ANOP  ,           END OF PARTIAL SYNTAX CHECK        01032300
.*******************************************************************    01032400
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       01032500
.*******************************************************************    01032600
                   AIF   (&ZMACRET NE 0).ZLX2   ERROR DETECTED          01032700
.*******************************************************************    01032800
.***AE.01  START OF HZSQUERY MACRO CODE                                 01032900
.*******************************************************************    01033000
                   LCLC  &M0M0001                 NAME                  01033100
&M0M0001           SETC  '&XLABEL'                SET NAME              01033200
                   AIF   (K'&XLABEL NE 0).XLAB2                         01033300
&M0M0001           SETC  'M00M&SYSNDX'            DEFAULT NAME          01033400
.*                                                                      01033500
.XLAB2             ANOP  ,                                              01033600
                   AIF   ('&XMF' EQ 'L').XLAB3                          01033700
&M0M0001 DS    0H                             HZSQUERY-2                01033800
.XLAB3             ANOP  ,                                              01033900
.*******************************************************************    01034000
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              01034100
.*******************************************************************    01034200
              PUSH  PRINT                                               01034300
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     01034400
                   PRINT OFF         ++ PR01                            01034500
                   AGO   .ZPRNTXX                                       01034600
.ZPRINTX           PRINT ON                                             01034700
.ZPRNTXX           ANOP  ,                                              01034800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01034900
         AIF   ('&XMF' NE 'M').NOTM                                @L1A 01035000
&ZMS     SETC  'MF=M MAY NOT BE SPECIFIED'                         @L1A 01035100
         MNOTE 8,'&ZMS'                                            @L1A 01035200
         POP   PRINT                                               @LAA 01035300
         MEXIT                                                     @L1A 01035400
.NOTM ANOP                                                         @L1A 01035500
         AIF   ('&REQUEST' NE 'POLICY').NOTPOLICY                  @L1A 01035600
&ZMS     SETC  'REQUEST=POLICY MAY NOT BE SPECIFIED'               @L1A 01035700
         MNOTE 8,'&ZMS'                                            @L1A 01035800
         POP   PRINT                                               @LAA 01035900
         MEXIT                                                     @L1A 01036000
.NOTPOLICY ANOP                                                    @L1A 01036100
.NOTLOGSTREAM ANOP                                                 @L2A 01036200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01036300
.********************************************************************   01036400
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     01036500
.********************************************************************   01036600
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       01036700
                      LCLA  &ZOPN1         INVOCATION INDEX             01036800
                      LCLC  &ZOPL(4095)    LABEL                        01036900
                      LCLC  &ZOPO(4095)    OPERATION                    01037000
                      LCLC  &ZOPV(4095)    VALUE                        01037100
                      LCLC  &ZOPD(4095)    DESCRIPTION                  01037200
.*                                                                      01037300
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     01037400
.******************************************************************     01037500
.***AL.L10 HZSQUERY FMT=LIST MF=S!L PARAMETERS                          01037600
.******************************************************************     01037700
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     01037800
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     01037900
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          01038000
&XMFATTR              SETC  '0D'                                        01038100
.*                                                                      01038200
&ZOPN                 SETA  &ZOPN+1                                     01038300
&ZOPL(&ZOPN)          SETC  ' '                                         01038400
                      AIF   (&SYSALVL NE 0).ZALVL6A                     01038500
&ZOPO(&ZOPN)          SETC  'B'                                         01038600
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                01038700
                      AGO   .ZALVL6B                                    01038800
.ZALVL6A              ANOP                                              01038900
&ZOPO(&ZOPN)          SETC  'CNOP'                                      01039000
&ZOPV(&ZOPN)          SETC  '4,8'                                       01039100
&ZOPN                 SETA  &ZOPN+1                                     01039200
&ZOPL(&ZOPN)          SETC  ' '                                         01039300
&ZOPO(&ZOPN)          SETC  'BRAS'                                      01039400
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              01039500
.ZALVL6B              ANOP                                              01039600
&ZOPD(&ZOPN)          SETC  '++ HZSQUERY ARG PARMS'                     01039700
.*                                                                      01039800
&ZOPN                 SETA  &ZOPN+1                                     01039900
&ZOPL(&ZOPN)          SETC  ' '                                         01040000
&ZOPO(&ZOPN)          SETC  'SPACE'                                     01040100
&ZOPV(&ZOPN)          SETC  '1'                                         01040200
&ZOPD(&ZOPN)          SETC  ' '                                         01040300
.*                                                                      01040400
.ZLMFSL2              ANOP  ,                                           01040500
.*-------------------------------------------------------------------   01040600
