.***AG.KCK02 NAME, OPT RQD INPUT, POS(2) -GPR OUT OF RANGE              00276600
                   AIF   (K'&ZXMFCTRL LE 0).ZG246R  NOT I/O             00276700
                   AIF   ('&XMFCTRL'(1,1) NE '(').ZG246R                00276800
&ZCGPR             SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)                     00276900
                   AIF   ('&ZCGPR'(1,1) NE '0').ZG246P                  00277000
                   AIF   (K'&ZCGPR GT 2).ZG246Q                         00277100
                   AIF   ('&ZCGPR' EQ '0').ZG246Q                       00277200
&ZCGPR             SETC  '&ZCGPR'(2,1)                                  00277300
                   AIF   (T'&ZCGPR NE 'N').ZG246Q  INVALID REG NAME     00277400
.ZG246P            ANOP  ,                                              00277500
                   AIF   (T'&ZCGPR NE 'N').ZG246R  NON NUMERIC          00277600
                   AIF   ('&ZCGPR' LT '1').ZG246Q                       00277700
                   AIF   ('&ZCGPR' LE '12').ZG246R                      00277800
.ZG246Q            ANOP  ,                                              00277900
&ZMS               SETC  '"MF='                                         00278000
&ZMS               SETC  '&ZMS.&MF"'                                    00278100
&ZMS               SETC  '&ZMS ARGUMENT 2 MUST SPECIFY A REGISTER'      00278200
                   MNOTE 8,' &ZMS IN THE RANGE (1)-(12).'               00278300
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00278400
.ZG246R            ANOP  ,                                              00278500
