.***AC.PLV01  PROCESS PLISTVER                                      *   00061400
.********************************************************************   00061500
&XPLISTVER      SETA  0                                                 00061600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00061700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00061800
&XPLISTVER      SETA  1                                                 00061900
.ZPLV0          ANOP  ,                                                 00062000
.*                                                                      00062100
                AIF   ('&PREFLIST' EQ '').ZPLV1E                        00062200
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00062300
&XPLISTVER      SETA  1                                                 00062400
.ZPLV1E         ANOP  ,                                                 00062500
.*                                                                      00062600
                AIF   ('&PREFNUM' EQ '').ZPLV2E                         00062700
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00062800
&XPLISTVER      SETA  1                                                 00062900
.ZPLV2E         ANOP  ,                                                 00063000
.*                                                                      00063100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00063200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00063300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00063400
.*-------------------------------------------------------------------   00063500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00063600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00063700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00063900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00064000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064100
                AGO   .ZPVCK2                                           00064200
.ZPVCK3         ANOP  ,                                                 00064300
.*-------------------------------------------------------------------   00064400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00064500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00064600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00064800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00064900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065000
                AGO   .ZPVCKE                                           00065100
.ZPVCK2         ANOP  ,                                                 00065200
.*-------------------------------------------------------------------   00065300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00065400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00065500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00065600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00065700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00065800
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00065900
.ZPVCK6         ANOP  ,                                                 00066000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00066100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066200
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00066300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00066400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066500
&XPLISTVER      SETA  1                                                 00066600
                AGO   .ZPVCKE                                           00066700
.*-------------------------------------------------------------------   00066800
.ZPVCK4A        ANOP  ,                                                 00066900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00067000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00067100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00067200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00067300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00067400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00067500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067600
                AGO   .ZPVCK5                                           00067700
.ZPVCK4         ANOP  ,                                                 00067800
&XPLISTVER      SETA  &PLISTVER(1)                                      00067900
.ZPVCK5         ANOP  ,                                                 00068000
.ZPVCKE         ANOP  ,                                                 00068100
.********************************************************************   00068200
