         AIF   ('&ERROROPT' EQ 'NOABEND').RINT383                  @PAA 20970000
.*       At this point, we must be ERROROPT=ABEND                  @PAA 20980000
&BIX0    SETA  0                    Turn bit off                   @N7C 20990000
         AGO   .RINT384                                            @PAA 21000000
.RINT383 ANOP                                                      @PAA 21010000
&BIT0    SETA  1                    Turn bit on                    @N7C 21020000
.RINT384 ANOP                                                      @PAA 21030000
.*                                                                 @N7A 21040000
.* CHECK FOR NESTED                                                @N7A 21050000
.*                                                                 @N7A 21060000
.*                                                                 @N7A 21070000
         AIF   ('&NESTED' EQ '').RINT389                           @N7A 21080000
         AIF   ('&NESTED' EQ 'YES').RINT387                        @N7A 21090000
         AIF   ('&NESTED' EQ 'COPY').RINT388                       @N7A 21100000
.*       At this point, we must be NESTED=NO                       @N7A 21110000
&BIX1    SETA  0                    Turn off NESTED=YES            @N7A 21120000
&BIX2    SETA  0                    Turn off NESTED=COPY           @N7A 21130000
         AGO   .RINT389                                            @N7A 21140000
.RINT387 ANOP                                                      @N7A 21150000
&BIT1    SETA  1                    Turn on NESTED=YES             @N7A 21160000
&BIX2    SETA  0                    Turn off NESTED=COPY           @N7A 21170000
         AGO   .RINT389                                            @N7A 21180000
.RINT388 ANOP                                                      @N7A 21190000
&BIX1    SETA  0                    Turn off NESTED=YES            @N7A 21200000
&BIT2    SETA  1                    Turn on NESTED=COPY            @N7A 21210000
.RINT389 ANOP                                                      @N7A 21220000
.*                                                                 @NJA 21230000
.* Check for PASSCHK=NOMFA                                         @NJA 21240000
.*                                                                 @NJA 21250000
         AIF   ('&PASSCHK' NE 'NOMFA').RINT38M                     @NJA 21260000
&BIT3    SETA  1                    Turn ON  PASSCHK=NOMFA Bit     @NJA 21270000
.RINT38M ANOP                                                      @NJA 21280000
         AIF   ('&BIX0&BIX1&BIX2&BIX3' EQ '1111').RINT38N          @NJC 21290000
         NI    58(1),B'&BIX0&BIX1&BIX2&BIX3.1111'        INITFLG3  @NJC 21300000
.RINT38N ANOP  ,                                                   @N7A 21310000
         AIF   ('&BIT0&BIT1&BIT2&BIT3' EQ '0000').RINT38O          @NJC 21320000
         OI    58(1),B'&BIT0&BIT1&BIT2&BIT3.0000'        INITFLG3  @NJC 21330000
.RINT38O ANOP  ,                                                   @N7A 21340000
.*                                                                 @MCA 21350000
.* CHECK FOR SERVAUTH                                              @MCA 21360000
.*                                                                 @MCA 21370000
         AIF   ('&SERVAUTH' EQ '').RINT386                         @MCA 21380000
         AIF   ('&RELEASE(1)' EQ '').RINT386                       @MCA 21390000
         AIF   ('&RLVL'(1,2) LT '35').RINT386  -LT RELEASE 7708    @NHC 21400000
         AIF   ('&SERVAUTH'(1,1) NE '(').RINT385                   @MCA 21410000
         ST    &SERVAUTH(1),120(1) - SERVAUTH                      @MCA 21420000
         AGO   .RINT386                                            @MCA 21430000
.RINT385 ANOP                                                      @MCA 21440000
         LA    0,&SERVAUTH    -MOVE SERVAUTH                       @MCA 21450000
         ST    0,120(1)        -INTO PARM LIST                     @MCA 21460000
.RINT386 ANOP                                                      @MCA 21470000
.*                                                                 @MCA 21480000
.* CHECK FOR PHRASE and NEWPHRASE                                  @N8A 21490000
.*                                                                 @N8A 21500000
         AIF   ('&RELEASE(1)' EQ '').RINT399                       @N9C 21510000
         AIF   ('&RLVL'(1,2) LT '38').RINT399   -LT RELEASE 7730   @NHC 21520000
         AIF   ('&PHRASE' EQ '').RINT391                           @N8A 21530000
         AIF   ('&PHRASE'(1,1) NE '(').RINT390                     @N8A 21540000
         ST    &PHRASE(1),124(1) - PHRASE                          @N8A 21550000
         AGO   .RINT391                                            @N8A 21560000
.RINT390 ANOP                                                      @N8A 21570000
         LA    0,&PHRASE      -MOVE PHRASE                         @N8A 21580000
         ST    0,124(1)       -INTO PARM LIST                      @N8A 21590000
.RINT391 ANOP                                                      @N8A 21600000
         AIF   ('&NEWPHRASE' EQ '').RINT393                        @N8A 21610000
         AIF   ('&NEWPHRASE'(1,1) NE '(').RINT392                  @N8A 21620000
         ST    &NEWPHRASE(1),128(1) - NEWPHRASE                    @N8A 21630000
         AGO   .RINT393                                            @N8A 21640000
.RINT392 ANOP                                                      @N8A 21650000
         LA    0,&NEWPHRASE      -MOVE NEWPHRASE                   @N8A 21660000
         ST    0,128(1)       -INTO PARM LIST                      @N8A 21670000
.RINT393 ANOP                                                      @N8A 21680000
         AIF   ('&ICTX' EQ '').RINT395                             @N9A 21690000
         AIF   ('&ICTX'(1,1) NE '(').RINT394                       @N9A 21700000
         ST    &ICTX(1),132(1) - ICTX                              @N9A 21710000
         AGO   .RINT395                                            @N9A 21720000
.RINT394 ANOP                                                      @N9A 21730000
         LA    0,&ICTX           - MOVE ICTX                       @N9A 21740000
         ST    0,132(1)       -INTO PARM LIST                      @N9A 21750000
.RINT395 ANOP                                                      @N9A 21760000
.*                                                                 @NDA 21770000
.* CHECK FOR ICRX and IDID                                         @NDA 21780000
.*                                                                 @NDA 21790000
         AIF   ('&RLVL'(1,2) LT '41').RINT399  -LT RELEASE 7760    @NHC 21800000
         AIF   ('&IDID' EQ '').RINT397                             @NDA 21810000
         AIF   ('&IDID'(1,1) NE '(').RINT396                       @NDA 21820000
         ST    &IDID(1),136(1) - IDID                              @NDA 21830000
         AGO   .RINT397                                            @NDA 21840000
.RINT396 ANOP                                                      @NDA 21850000
         LA    0,&IDID        -MOVE IDID                           @NDA 21860000
         ST    0,136(1)       -INTO PARM LIST                      @NDA 21870000
.RINT397 ANOP                                                      @NDA 21880000
         AIF   ('&ICRX' EQ '').RINEXMPLV0001                       @NKC 21890000
         AIF   ('&ICRX'(1,1) NE '(').RINT398                       @NDA 21900000
         ST    &ICRX(1),140(1) - ICRX                              @NDA 21910000
         AGO   .RINEXMPLV0001                                      @NKC 21920000
.RINT398 ANOP                                                      @NDA 21930000
         LA    0,&ICRX           - MOVE ICRX                       @NDA 21940000
         ST    0,140(1)       -INTO PARM LIST                      @NDA 21950000
.*                                                                 @NKA 21960000
.RINEXMPLV0001 ANOP                                                @NKA 21970000
.*                                                                 @NKA 21980000
.* Check for RELEASE=PLV0001, and later, Parms                     @NKA 21990000
.*                                                                 @NKA 22000000
         AIF   ('&RLVL'(1,2) LT '47').RINT399  -LT RELEASE=PLV0001 @NKA 22010000
.*                                                                 @NKA 22020000
.* Check for the IDTA Parm                                         @NKA 22030000
.*                                                                 @NKA 22040000
         AIF   ('&IDTA' EQ '').RINT399                             @NKA 22050000
         AIF   ('&IDTA'(1,1) NE '(').STRIDTA                       @NKA 22060000
         ST    &IDTA(1),144(1)   - IDTA                            @NKA 22070000
         AGO   .RINT399                                            @NKA 22080000
.STRIDTA ANOP                                                      @NKA 22090000
         LA    0,&IDTA           - MOVE IDTA                       @NKA 22100000
         ST    0,144(1)          - INTO PARM LIST                  @NKA 22110000
.*                                                                 @NKA 22120000
.RINT399 ANOP                                                           22130000
.* PRODUCE SVC ENTRY CODE AND EXIT -LIST MODIFICATION DONE              22140000
         AGO   .RINT221                                                 22150000
.* DIAGNOSTICS   *.                                                     22160000
.ERR002  ANOP                                                           22170000
         MNOTE 8,'*** INVALID ENVIR PARAMETER ***'                      22180000
         AGO   .ERTN002                                                 22190000
