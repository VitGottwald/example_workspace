**/ CSVLPRET_2:;                                                        00785000
* %CSVLPRET_INCLUDED = 'YES';                                           00786000
* %DCL ZCBPRINT CHAR EXT;                                               00787000
* %DEACTIVATE ZCBPRINT;                                                 00788000
* %DCL CSVLPRET_LIST CHAR EXT;                                          00789000
* %DEACTIVATE CSVLPRET_LIST;                                            00790000
* %IF CSVLPRET_LIST ^= 'NO' &                                           00791000
*    ZCBPRINT ^= 'NO' %THEN                                             00792000
*   %GOTO CSVLPRET_3;                                                   00793000
*   @LIST PUSH NOECHO;                                                  00794000
*   @LIST NOASSEMBLE NOECHO;                                            00795000
*   @LIST OFF C NOECHO;                                                 00796000
* %CSVLPRET_3:;                                                         00797000
*/* Start of PL/X Source                                             */ 00798000
*DCL 1 LPMEA Type,                 /* Modinfo entry for ADD             00799000
*                                                                @L1A*/ 00800000
*     2 LpmeaInputInfo,       /* Information provided by caller of      00801000
*                    dynamic LPA services. Provided to exit CSVDYLPA.   00802000
*                    Mapping of module entries within                   00803000
*                    Type 90 Subtype 31 SMF records.             @L1A*/ 00804000
*       3 LpmeaNAME CHAR(8), /* The module name                  @L1A*/ 00805000
*       3 LpmeaInputFLAGS BIT(16), /* Flags set by the caller           00806000
*                    Not applicable to exit CSVDYLPA or SMF record.     00807000
*                                                                @L1A*/ 00808000
*         4 LpmeaInputFLAGS0 BIT(8), /* Byte 0 of Flags.                00809000
*                    Not applicable when BYADDR=YES.                    00810000
*                                                                @L1A*/ 00811000
*           5 LpmeaFixed Bit(1), /* If on, page-fix                     00812000
*                                   this module                  @L1A*/ 00813000
*           5 LpmeaPageProtPage Bit(1), /* If on, page-protect only the 00814000
*                    whole pages of this module. If off, page-protect   00815000
*                    all bytes of this module. Be aware that when       00816000
*                    page-protecting all bytes of the module, storage   00817000
*                    utilization for the module may increase, as the    00818000
*                    system allocates a number of whole pages for the   00819000
*                    module, rather than just the amount of storage     00820000
*                    that is truly necessary to load the module. @L1A*/ 00821000
*           5 LpmeaStorageOwnerSystem Bit(1), /* If on, the storage     00822000
*                    in which the module is placed is to be             00823000
*                    OWNER=SYSTEM. If off, it is OWNER=HOME.            00824000
*                                                                @L1A*/ 00825000
*         4 LpmeaInputFLAGS1 BIT(8), /* Byte 1 of Flags.                00826000
*                    Not applicable when BYADDR=YES.                    00827000
*                                                                @L1A*/ 00828000
*           5 LpmeaSvc Bit(1),     /* This entry is an SVC.             00829000
*                    The SVC number is in LpmeaxSvcnum.          @L6A*/ 00830000
*           5 LpmeaESvc Bit(1),    /* This entry is an extended SVC.    00831000
*                    The SVC number is in LpmeaxSvcnum.                 00832000
*                    The extended svc routing code is in                00833000
*                    LpmeaxESvcrnum.                             @L6A*/ 00834000
*     2 LpmeaOutputInfo Bdy(Word,3), /* Information provided by the     00835000
*                                system (except where noted for         00836000
*                                BYADDR=YES). Provided to exit          00837000
*                                CSVDYLPA which should not modify       00838000
*                                this information                @L1A*/ 00839000
*       3 LpmeaOutputFLAGS BIT(16), /* Output: Flags             @L1A*/ 00840000
*         4 LpmeaOutputFLAGS0 BIT(8), /* Byte 0 of Flags.               00841000
*                  Only bits LpmeaSuccess and LpmeaRmode64              00842000
*                  are applicable to exit                               00843000
*                  CSVDYLPA or SMF record - the module entry is         00844000
*                  only valid when bit LpmeaSuccess is on.       @L1A*/ 00845000
*           5 LpmeaSuccess BIT(1), /* Successfully processed     @L1A*/ 00846000
*           5 LpmeaModprob BIT(1), /* A problem occurred processing     00847000
*                  this entry. Function with problem is indicated by    00848000
*                  LpmeaModprobFunction.                         @L1A*/ 00849000
*           5 LpmeaModprobAbendInfo BIT(1),    /* An unexpected abend   00850000
*                 occurred while processing this entry. Further         00851000
*                 information is in the LpmeaAbendRsnCodes area. @L1A*/ 00852000
*           5 LpmeaModprobReturnCodeInfo BIT(1), /* An unexpected       00853000
*                  return code was received while processing this       00854000
*                  entry. Further information is in the                 00855000
*                  LpmeaRetRsnCodes area.                        @L1A*/ 00856000
*           5 * BIT(3),                                       /* @L7A*/ 00857000
*           5 LpmeaRmode64 BIT(1), /* When LpmeaSuccess is on,          00858000
*                  the entry point address and/or a load point          00859000
*                  address is Rmode 64 (above 2G). This will            00860000
*                  correlate to a value of 7FFFFBAD in                  00861000
*                  LpmeaEntryPointAddr and/or LpmeaLoadPointAddr        00862000
*                  and/or LpmeaLoadPointAddr2, indicating that          00863000
