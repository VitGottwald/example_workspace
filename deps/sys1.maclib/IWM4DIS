.***AC.PLV01  PROCESS PLISTVER                                      *   00067400
.********************************************************************   00067500
&XPLISTVER      SETA  0                                                 00067600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00067700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00067800
&XPLISTVER      SETA  0                                                 00067900
.ZPLV0          ANOP  ,                                                 00068000
.*                                                                      00068100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00068200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00068300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00068400
.*-------------------------------------------------------------------   00068500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00068600
&ZMS            SETC  '"PLISTVER='                                      00068700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00068900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00069000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069100
                AGO   .ZPVCK2                                           00069200
.ZPVCK3         ANOP  ,                                                 00069300
.*-------------------------------------------------------------------   00069400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00069500
&ZMS            SETC  '"PLISTVER='                                      00069600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00069700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00069800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00069900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070000
                AGO   .ZPVCKE                                           00070100
.ZPVCK2         ANOP  ,                                                 00070200
.*-------------------------------------------------------------------   00070300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00070400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00070500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00070600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00070700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00070800
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00070900
.ZPVCK6         ANOP  ,                                                 00071000
&ZMS            SETC  '"PLISTVER='                                      00071100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00071300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00071400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071500
&XPLISTVER      SETA  0                                                 00071600
                AGO   .ZPVCKE                                           00071700
.*-------------------------------------------------------------------   00071800
.ZPVCK4A        ANOP  ,                                                 00071900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00072000
&ZMS            SETC  'PLISTVER='                                       00072100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00072200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00072300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00072400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00072500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00072600
                AGO   .ZPVCK5                                           00072700
.ZPVCK4         ANOP  ,                                                 00072800
&XPLISTVER      SETA  &PLISTVER(1)                                      00072900
.ZPVCK5         ANOP  ,                                                 00073000
.ZPVCKE         ANOP  ,                                                 00073100
.********************************************************************   00073200
