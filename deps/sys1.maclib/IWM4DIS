&ZMS          SETC  '&ZMS "S",'                                         00092600
&ZMS          SETC  '&ZMS "L",'                                         00092700
&ZMS          SETC  '&ZMS OR "E".'                                      00092800
              MNOTE 8,' &ZMS '                                          00092900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00093000
              AGO   .ZG31E                                              00093100
.ZG31D        ANOP  ,                                                   00093200
&XMF          SETC  'S'         SET DEFAULT KEYWORD                     00093300
&XMFSCK       SETC  'COMPLETE'  COMPLETE CHECKING                       00093400
.ZG31E        ANOP  ,                                                   00093500
