.*-------------------------------------------------------------------   00124000
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00124100
&ZPLLEN               SETA 12                                           00124200
.*                                                                      00124300
.******************************************************************     00124400
.***AI.L01 IWM4DIS FMT=LIST FILL IN PARAMETER AREA                      00124500
.******************************************************************     00124600
                      ACTR  10000                                       00124700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00124800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00124900
&ZCVAL                SETC  'LAE'          XAX MODE                     00125000
.ZIMFXA               ANOP ,                                            00125100
.*                                                                      00125200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00125300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00125400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00125500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00125600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00125700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00125800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00125900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00126000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00126100
&ZOPN                 SETA  &ZOPN+1                                     00126200
&ZOPL(&ZOPN)          SETC  ' '                                         00126300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00126400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00126500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00126600
                      AGO   .ZIMFNX                                     00126700
.*                                                                      00126800
.ZIMFN2               ANOP ,                                            00126900
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00127000
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00127100
&ZOPN                 SETA  &ZOPN+1                                     00127200
&ZOPL(&ZOPN)          SETC  ' '                                         00127300
&ZOPO(&ZOPN)          SETC  'LAE'                                       00127400
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00127500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00127600
                      AGO   .ZIMFNX                                     00127700
.ZIMFNX1              ANOP ,                                            00127800
&ZOPN                 SETA  &ZOPN+1                                     00127900
&ZOPL(&ZOPN)          SETC  ' '                                         00128000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00128100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00128200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00128300
.ZIMFNX               ANOP ,                                            00128400
.********************************************************************   00128500
.***AI.L02 IWM4DIS FMT=LIST INITIALIZE PARAMETER AREA                   00128600
.********************************************************************   00128700
.***AI.L03 MF=(SLE)                                                     00128800
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00128900
