.***AC.PLV01  PROCESS PLISTVER                                      *   00126800
.********************************************************************   00126900
&XPLISTVER      SETA  0                                                 00127000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00127100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00127200
&XPLISTVER      SETA  2                                                 00127300
.ZPLV0          ANOP  ,                                                 00127400
.*                                                                      00127500
                AIF   ('&CANREPLY' EQ '').ZPLV1E                        00127600
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00127700
&XPLISTVER      SETA  1                                                 00127800
.ZPLV1E         ANOP  ,                                                 00127900
.*                                                                      00128000
                AIF   ('&GT61KMSG' EQ '').ZPLV2E                        00128100
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00128200
&XPLISTVER      SETA  1                                                 00128300
.ZPLV2E         ANOP  ,                                                 00128400
.*                                                                      00128500
                AIF   ('&NOTIFYEXIT' EQ '').ZPLV3E                      00128600
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00128700
&XPLISTVER      SETA  1                                                 00128800
.ZPLV3E         ANOP  ,                                                 00128900
.*                                                                      00129000
                AIF   ('&MSGISO' EQ '').ZPLV4E                          00129100
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00129200
&XPLISTVER      SETA  2                                                 00129300
.ZPLV4E         ANOP  ,                                                 00129400
.*                                                                      00129500
                AIF   ('&CRITICAL' EQ '').ZPLV5E                        00129600
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00129700
&XPLISTVER      SETA  2                                                 00129800
.ZPLV5E         ANOP  ,                                                 00129900
.*                                                                      00130000
                AIF   ('&FUNCTION' EQ '').ZPLV6E                        00130100
                AIF   (&XPLISTVER GE 2).ZPLV6E                          00130200
&XPLISTVER      SETA  2                                                 00130300
.ZPLV6E         ANOP  ,                                                 00130400
.*                                                                      00130500
                AIF   ('&RECOVERYMGR' EQ '').ZPLV7E                     00130600
                AIF   (&XPLISTVER GE 2).ZPLV7E                          00130700
&XPLISTVER      SETA  2                                                 00130800
.ZPLV7E         ANOP  ,                                                 00130900
.*                                                                      00131000
                AIF   ('&TERMLEVEL' EQ '').ZPLV8E                       00131100
                AIF   (&XPLISTVER GE 2).ZPLV8E                          00131200
&XPLISTVER      SETA  2                                                 00131300
.ZPLV8E         ANOP  ,                                                 00131400
.*                                                                      00131500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00131600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00131700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00131800
.*-------------------------------------------------------------------   00131900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00132000
&ZMS            SETC  '"PLISTVER='                                      00132100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00132200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00132300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00132400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00132500
                AGO   .ZPVCK2                                           00132600
.ZPVCK3         ANOP  ,                                                 00132700
.*-------------------------------------------------------------------   00132800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00132900
&ZMS            SETC  '"PLISTVER='                                      00133000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00133100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00133200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00133300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00133400
                AGO   .ZPVCKE                                           00133500
.ZPVCK2         ANOP  ,                                                 00133600
.*-------------------------------------------------------------------   00133700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00133800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00133900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00134000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00134100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00134200
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00134300
.ZPVCK6         ANOP  ,                                                 00134400
&ZMS            SETC  '"PLISTVER='                                      00134500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00134600
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00134700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00134800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00134900
&XPLISTVER      SETA  2                                                 00135000
                AGO   .ZPVCKE                                           00135100
.*-------------------------------------------------------------------   00135200
.ZPVCK4A        ANOP  ,                                                 00135300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00135400
&ZMS            SETC  'PLISTVER='                                       00135500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00135600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00135700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00135800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00135900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00136000
                AGO   .ZPVCK5                                           00136100
.ZPVCK4         ANOP  ,                                                 00136200
&XPLISTVER      SETA  &PLISTVER(1)                                      00136300
.ZPVCK5         ANOP  ,                                                 00136400
.ZPVCKE         ANOP  ,                                                 00136500
.********************************************************************   00136600
