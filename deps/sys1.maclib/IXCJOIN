*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00464900
*%IXCJOIN: MACRO KEYS(GRPNAME,MSGEXIT,CANREPLY,GT61KMSG,NOTIFYEXIT,     00465000
*                 MEMASSOC,ANSAREA,ANSLEN,LASTING,MEMNAME,GRPEXIT,      00465100
*                 LOCALCLEANUP,STATFLD,STATEXIT,INTERVAL,USTATE,USLEN,  00465200
*                 MEMDATA,SYSCLEANUPMEM,MSGOUTASID,MSGISO,CRITICAL,     00465300
*                 FUNCTION,RECOVERYMGR,TERMLEVEL,RETCODE,RSNCODE,       00465400
*                 PLISTVER,MF);                                         00465500
*  ANS('?'!!MACLABEL!!' '!!'IXCJOINP '!!MACLIST!!                       00465600
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00465700
*%END IXCJOIN;                                                          00465800
