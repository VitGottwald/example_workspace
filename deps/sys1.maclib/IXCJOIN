.CANRP             ANOP   ,                                       @L3A  00310700
                   AIF    ('&XMSGEXIT' NE 'NO_MSGEXIT').EVYOK     @L3A  00310800
                   AIF    ('&CANREPLY(1)' EQ '').EVYOK            @L3A  00310900
&ZMS               SETC  'THE "CANREPLY" KEYWORD'                 @L3A  00311000
&ZMS               SETC  '&ZMS CANNOT BE USED WHEN'               @L3A  00311100
&ZMS               SETC  '&ZMS MSGEXIT=NO_MSGEXIT'                @L3A  00311200
                   MNOTE 8,' &ZMS IS SPECIFIED.'                  @L3A  00311300
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE  @L3A  00311400
.EVYOK             ANOP   ,                                       @L3A  00311500
                   AIF    (&XPLISTVER LT 2).VER01                 @L5A  00311600
                   AIF    ('&XMF' EQ 'L').VER01                   @L5A  00311700
                   AIF    ('&XMF' EQ 'M').VER01                   @L5A  00311800
                   AIF    ('&FUNCTION(1)' NE '').VER01            @L5A  00311900
&ZMS               SETC  'THE "FUNCTION" KEYWORD IS'              @L5A  00312000
&ZMS               SETC  '&ZMS REQUIRED FOR MACRO'                @L5A  00312100
&ZMS               SETC  '&ZMS VERSION 2 AND'                     @L5A  00312200
                   MNOTE 8,' &ZMS ABOVE'                          @L5A  00312300
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE  @L5A  00312400
