.*-------------------------------------------------------------------   00446200
.***AI.LVER02                                                           00446300
.ZI40Z                ANOP  ,                                           00446400
.*-------------------------------------------------------------------   00446500
.***AI.LVER01                                                           00446600
                      AIF  (&XPLISTVER LT 2).ZI41Z                      00446700
.*-------------------------------------------------------------------   00446800
.***AI.L18  XFUNCTION CL24                                              00446900
         AIF   ('&XFUNCTION' NE 'NO_FUNCTION').ZI41                     00447000
&ZXFUNCTION           SETC  ''             RESET INPUT/OUTPUT           00447100
.ZI41                 ANOP  ,                                           00447200
.*-------------------------------------------------------------------   00447300
.***AI.L25  XFUNCTION CL24                                              00447400
                      AIF   ('&ZXFUNCTION' EQ '').ZI41E                 00447500
                      AIF   ('&ZXFUNCTION' EQ 'OUTPUT').ZI41E           00447600
&ZCGPR                SETC  '&XFUNCTION'                                00447700
.*                                                                      00447800
                      AIF   ('&XFUNCTION'(1,1) NE '(').ZI41B            00447900
&ZCGPR   SETC  '0('.'&XFUNCTION'(2,K'&XFUNCTION-2).')'                  00448000
.ZI41B                ANOP  ,                                           00448100
.*                                                                      00448200
&ZOPN                 SETA  &ZOPN+1                                     00448300
&ZOPL(&ZOPN)          SETC  ' '                                         00448400
&ZOPO(&ZOPN)          SETC  'MVC'                                       00448500
&ZOPV(&ZOPN)          SETC  '100(24,1),&ZCGPR'                          00448600
&ZOPD(&ZOPN)          SETC  '++ &ZXFUNCTION XFUNCTION'                  00448700
.*                                                                      00448800
.ZI41E                ANOP  ,                                           00448900
.*-------------------------------------------------------------------   00449000
.***AI.LVER02                                                           00449100
.ZI41Z                ANOP  ,                                           00449200
.*-------------------------------------------------------------------   00449300
&ZOPN1                SETA &ZOPN                                        00449400
.*******************************************************************    00449500
.***  PROCESS IXCJOIN OUTPUT PARAMETERS                                 00449600
.*******************************************************************    00449700
                      ACTR  10000                                       00449800
.*******************************************************************    00449900
.***AO.X02  RETURN CODE PROCESSING                                      00450000
.*******************************************************************    00450100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00450200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00450300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00450400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00450500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00450600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00450700
&ZOPN                 SETA  &ZOPN+1                                     00450800
&ZOPL(&ZOPN)          SETC  ' '                                         00450900
&ZOPO(&ZOPN)          SETC  'ST'                                        00451000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00451100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00451200
                      AGO   .ZMRETX                                     00451300
.*                                                                      00451400
.ZMRET2               ANOP  ,                                           00451500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00451600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00451700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00451800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00451900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00452000
&ZOPN                 SETA  &ZOPN+1                                     00452100
&ZOPL(&ZOPN)          SETC  ' '                                         00452200
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00452300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00452400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00452500
.ZMRETX               ANOP  ,                                           00452600
.*******************************************************************    00452700
.***AO.X03  REASON CODE PROCESSING                                      00452800
.*******************************************************************    00452900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00453000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00453100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00453200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00453300
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00453400
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00453500
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00453600
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00453700
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00453800
&ZOPN                 SETA  &ZOPN+1                                     00453900
&ZOPL(&ZOPN)          SETC  ' '                                         00454000
&ZOPO(&ZOPN)          SETC  'ST'                                        00454100
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00454200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00454300
                      AGO   .ZMRSNX                                     00454400
.*                                                                      00454500
.ZMRSN2               ANOP  ,                                           00454600
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00454700
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00454800
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00454900
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00455000
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00455100
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00455200
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00455300
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00455400
&ZOPN                 SETA  &ZOPN+1                                     00455500
&ZOPL(&ZOPN)          SETC  ' '                                         00455600
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00455700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00455800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00455900
.ZMRSNX               ANOP  ,                                           00456000
.*******************************************************************    00456100
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00456200
.*******************************************************************    00456300
.ZMFLX                ANOP  ,              MF=L PROCESSING              00456400
.*******************************************************************    00456500
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00456600
.*******************************************************************    00456700
                      ACTR  10000                                       00456800
.*                                                                      00456900
                      LCLA  &ZLC           COLUMN POSITION              00457000
                      LCLA  &ZOPX          LIST TABLE INDEX             00457100
                      LCLC  &ZLL           LABEL                        00457200
                      LCLC  &ZLOP          OPERATION                    00457300
                      LCLC  &ZLV           VALUE                        00457400
                      LCLC  &ZLD           DESCRIPTION                  00457500
.*                                                                      00457600
&ZOPX                 SETA  1                                           00457700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00457800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00457900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00458000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00458100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00458200
                      AIF   (K'&ZLD LE 0).ZL5                           00458300
&ZLC                  SETA  35             COMMENT COLUMN               00458400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00458500
&ZLC                  SETA  33             COMMMENT COLUMN              00458600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00458700
&ZLV                  SETC  '&ZLV'.' '                                  00458800
                      AGO   .ZL3                                        00458900
.*                                                                      00459000
.ZL4                  ANOP  ,                                           00459100
&ZLV                  SETC  '&ZLV'.' '                                  00459200
.ZL5                  ANOP  ,                                           00459300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00459400
.*                                                                      00459500
&ZLL     &ZLOP &ZLV                                                     00459600
.*                                                                      00459700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00459800
                      AGO   .ZL2           CONTINUE LOOPING             00459900
.*                                                                      00460000
.ZLX                  ANOP  ,                                           00460100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00460200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00460300
         AIF   (&ZMACRET NE 0).NOLINK                             @L3A  00460400
         L     14,16(0,0)              GET CVT ADDRESS                  00460500
         L     14,772(14,0)            GET SFT ADDRESS                  00460600
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00460700
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00460800
         OR    14,15                   GET PC NUMBER                    00460900
         PC    0(14)                                                    00461000
.NOLINK  ANOP  ,                                                  @L3A  00461100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00461200
.ZLINKX               ANOP  ,                                           00461300
.*******************************************************************    00461400
.***AP.X01  FORMAT GENERATED CODE                                       00461500
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00461600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00461700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00461800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00461900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00462000
                      AIF   (K'&ZLD LE 0).ZL51                          00462100
&ZLC                  SETA  35             COMMENT COLUMN               00462200
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00462300
&ZLC                  SETA  33             COMMENT COLUMN               00462400
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00462500
&ZLV                  SETC  '&ZLV'.' '                                  00462600
                      AGO   .ZL31                                       00462700
.*                                                                      00462800
.ZL41                 ANOP  ,                                           00462900
&ZLV                  SETC  '&ZLV'.' '                                  00463000
.ZL51                 ANOP  ,                                           00463100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00463200
.*                                                                      00463300
&ZLL     &ZLOP &ZLV                                                     00463400
.*                                                                      00463500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00463600
                      AGO   .ZL21          CONTINUE LOOPING             00463700
.*                                                                      00463800
.ZLX1                 ANOP  ,                                           00463900
.*******************************************************************    00464000
.***AP.X02  END OF IXCJOIN MACRO                                        00464100
