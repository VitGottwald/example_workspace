.*******************************************************************    00443700
.***  PROCESS IXGWRITE OUTPUT PARAMETERS                                00443800
.*******************************************************************    00443900
                      ACTR  10000                                       00444000
.*******************************************************************    00444100
.***AO.X02  RETURN CODE PROCESSING                                      00444200
.*******************************************************************    00444300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00444400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00444500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00444600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00444700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00444800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00444900
&ZOPN                 SETA  &ZOPN+1                                     00445000
&ZOPL(&ZOPN)          SETC  ' '                                         00445100
&ZOPO(&ZOPN)          SETC  'ST'                                        00445200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00445300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00445400
                      AGO   .ZMRETX                                     00445500
.*                                                                      00445600
.ZMRET2               ANOP  ,                                           00445700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00445800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00445900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00446000
