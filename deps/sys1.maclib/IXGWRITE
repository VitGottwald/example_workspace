.***AC.PLV01  PROCESS PLISTVER                                      *   00163600
.********************************************************************   00163700
&XPLISTVER      SETA  0                                                 00163800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00163900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00164000
&XPLISTVER      SETA  2                                                 00164100
.ZPLV0          ANOP  ,                                                 00164200
.*                                                                      00164300
                AIF   ('&BUFFER64' EQ '').ZPLV1E                        00164400
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00164500
&XPLISTVER      SETA  2                                                 00164600
.ZPLV1E         ANOP  ,                                                 00164700
.*                                                                      00164800
                AIF   ('&REQDATA' EQ '').ZPLV2E                         00164900
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00165000
&XPLISTVER      SETA  1                                                 00165100
.ZPLV2E         ANOP  ,                                                 00165200
.*                                                                      00165300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00165400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00165500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00165600
.*-------------------------------------------------------------------   00165700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00165800
&ZMS            SETC  '"PLISTVER='                                      00165900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00166000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00166100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00166200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00166300
                AGO   .ZPVCK2                                           00166400
.ZPVCK3         ANOP  ,                                                 00166500
.*-------------------------------------------------------------------   00166600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00166700
&ZMS            SETC  '"PLISTVER='                                      00166800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00166900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00167000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00167100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00167200
                AGO   .ZPVCKE                                           00167300
.ZPVCK2         ANOP  ,                                                 00167400
.*-------------------------------------------------------------------   00167500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00167600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00167700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00167800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00167900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00168000
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00168100
.ZPVCK6         ANOP  ,                                                 00168200
&ZMS            SETC  '"PLISTVER='                                      00168300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00168400
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00168500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00168600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00168700
&XPLISTVER      SETA  2                                                 00168800
                AGO   .ZPVCKE                                           00168900
.*-------------------------------------------------------------------   00169000
.ZPVCK4A        ANOP  ,                                                 00169100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00169200
&ZMS            SETC  'PLISTVER='                                       00169300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00169400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00169500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00169600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00169700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00169800
                AGO   .ZPVCK5                                           00169900
.ZPVCK4         ANOP  ,                                                 00170000
&XPLISTVER      SETA  &PLISTVER(1)                                      00170100
.ZPVCK5         ANOP  ,                                                 00170200
.ZPVCKE         ANOP  ,                                                 00170300
.********************************************************************   00170400
