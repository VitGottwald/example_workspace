**/                                                                     07880000
* % END_OF_ASM_NJND: ;                                                  07890000
*                                                                       07900000
* %IF NJEND_ATRB = ''          /* If no attribute specified          */ 07910000
*   %THEN                                                               07920000
*     %NJEND_ATRB = 'BASED';   /* establish default                  */ 07930000
*                                                                       07940000
*/*                                                                  */ 07950000
*/* Return codes from SSI 82 are defined in IAZSSJP macro            */ 07960000
*                                                                       07970000
*/* Reason codes for non-zero return codes.                          */ 07980000
*/* Reason code is returned in SSJPRETN.                             */ 07990000
* DCL NJNDSUBF FIXED(31) CONSTANT(256); /* Invalid subfunction       */ 08000000
* DCL NJNDSPTE FIXED(31) CONSTANT(260); /* Invalid NJNLSTRP pointer  */ 08010000
* DCL NJNDRNGE FIXED(31) CONSTANT(264); /* NJNLRNGH < NJNLRNGL       */ 08020000
* DCL NJNDRNGZ FIXED(31) CONSTANT(268); /* NJNLRNGL is zero          */ 08030000
* DCL NJNDOWNE FIXED(31) CONSTANT(272); /* Own node filter should       08040000
*                                          not be used with other       08050000
*                                          connection status            08060000
*                                          filters                   */ 08070000
* DCL NJNDSTRE FIXED(31) CONSTANT(276); /* Not enough storage           08080000
*                                          for all data              */ 08090000
* DCL NJNDINTE FIXED(31) CONSTANT(280); /* Internal error building      08092000
*                                          system information data      08094000
*                                          area                      */ 08096000
*                                                                       08100000
*                                                                       08110000
* DCL                                                                   08120000
*   1 NJNL   NJEND_ATRB,                                                08130000
*     3 NJNLEYE  CHAR(8),      /* I.Eyecatcher                       */ 08140000
*     3 NJNLLNG  FIXED(16),    /* I.Length of parameter list         */ 08150000
*     3 NJNLVRM  FIXED(16),    /* I.Parm list ver/mod                */ 08160000
*       5 NJNLVER  FIXED(8),   /* I.Parm list version                */ 08170000
*       5 NJNLMOD  FIXED(8),   /* I.Parm list modification           */ 08180000
*     3 NJNLSVRM FIXED(16),    /* O.Subsystem ver/mod                */ 08190000
*       5 NJNLSVER FIXED(8),   /* O.Subsystem version                */ 08200000
*       5 NJNLSMOD FIXED(8),   /* O.Subsystem modification           */ 08210000
*     3 *        CHAR(1),      /* Reserved                           */ 08220000
*     3 NJNLOPT1 BIT(8),       /* I.Processing options:              */ 08230000
*       5 NJNLODMC BIT(1),     /*   perform security label dominance    08240000
*                                    check (this check is always        08250000
*                                    performed for non-authorized       08260000
*                                    callers)                        */ 08270000
*       5 *        BIT(7),     /*   Reserved                         */ 08275000
*                                                                       08280000
*     3 NJNLFLT1 BIT(8),       /* IS.Filter by node attributes (1):  */ 08290000
*       5 NJNL1NAM BIT(1),     /*   select by node name                 08300000
*                                    (see NJNLNOD1)                  */ 08310000
*       5 NJNL1RNG BIT(1),     /*   select by range of node numbers     08320000
*                                                              (JES2)   08330000
*                                    (see NJNLRNGL and NJNLRNGH)     */ 08340000
*       5 NJNL1SSG BIT(1),     /*   select nodes with secure signon  */ 08350000
*       5 NJNL1CSG BIT(1),     /*   select nodes with compatible        08360000
*                                    signon                          */ 08370000
*       5 NJNL1NET BIT(1),     /*   select by subnet name      (JES2)   08380000
*                                   (see NJNLSUBN)                   */ 08390000
*       5 NJNL1SNA BIT(1),     /*   select nodes using                  08400000
*                                    SNA protocol              (JES3)*/ 08410000
*       5 NJNL1BSC BIT(1),     /*   select nodes using                  08420000
*                                    BSC protocol              (JES3)*/ 08430000
*       5 NJNL1TCP BIT(1),     /*   select nodes using                  08440000
*                                    TCP protocol              (JES3)*/ 08450000
*                                                                       08460000
*     3 NJNLFLT2 BIT(8),       /* IS.Filter by node attributes (2):  */ 08470000
*       5 NJNL2PMY BIT(1),     /*   select nodes managed                08480000
*                                    by path manager           (JES2)*/ 08490000
*       5 NJNL2PMN BIT(1),     /*   select nodes not managed            08500000
*                                    by path manager           (JES2)*/ 08510000
*       5 NJNL2TLS BIT(1),     /*   select nodes using secure           08520000
*                                    sockets (TLS)             (JES3)*/ 08530000
*       5 *        BIT(5),     /*   Reserved                         */ 08535000
*                                                                       08540000
*     3 NJNLFLTC BIT(8),       /* IS.Filter by connection status:    */ 08550000
*       5 NJNLCOWN BIT(1),     /*    select only local (own or home)    08560000
*                                     node - this filter should not     08570000
*                                     be used with any other            08580000
*                                     connection filter              */ 08590000
*       5 NJNLCADJ BIT(1),     /*    select adjacent nodes -            08600000
*                                     adjacent node is one hop away     08610000
*                                     from the local node            */ 08620000
*       5 NJNLCDIR BIT(1),     /*    select directly attached nodes -   08630000
*                                     these are adjacent nodes which    08640000
*                                     use a dedicated line           */ 08650000
*       5 NJNLCCNC BIT(1),     /*    select connected (reachable)       08660000
