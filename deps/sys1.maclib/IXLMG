&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00375500
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00375600
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00375700
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00375800
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00375900
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00376000
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00376100
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00376200
&ZOPN                 SETA  &ZOPN+1                                     00376300
&ZOPL(&ZOPN)          SETC  ' '                                         00376400
