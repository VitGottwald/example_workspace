.*******************************************************************    00382100
.***AP.X01  FORMAT GENERATED CODE                                       00382200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00382300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00382400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00382500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00382600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00382700
                      AIF   (K'&ZLD LE 0).ZL51                          00382800
&ZLC                  SETA  35             COMMENT COLUMN               00382900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00383000
&ZLC                  SETA  33             COMMENT COLUMN               00383100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00383200
&ZLV                  SETC  '&ZLV'.' '                                  00383300
                      AGO   .ZL31                                       00383400
.*                                                                      00383500
.ZL41                 ANOP  ,                                           00383600
&ZLV                  SETC  '&ZLV'.' '                                  00383700
.ZL51                 ANOP  ,                                           00383800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00383900
.*                                                                      00384000
&ZLL     &ZLOP &ZLV                                                     00384100
.*                                                                      00384200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00384300
                      AGO   .ZL21          CONTINUE LOOPING             00384400
.*                                                                      00384500
.ZLX1                 ANOP  ,                                           00384600
