.*******************************************************************    00376900
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00377000
.*******************************************************************    00377100
.ZMFLX                ANOP  ,              MF=L PROCESSING              00377200
.*******************************************************************    00377300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00377400
.*******************************************************************    00377500
                      ACTR  10000                                       00377600
.*                                                                      00377700
                      LCLA  &ZLC           COLUMN POSITION              00377800
                      LCLA  &ZOPX          LIST TABLE INDEX             00377900
                      LCLC  &ZLL           LABEL                        00378000
                      LCLC  &ZLOP          OPERATION                    00378100
                      LCLC  &ZLV           VALUE                        00378200
                      LCLC  &ZLD           DESCRIPTION                  00378300
.*                                                                      00378400
&ZOPX                 SETA  1                                           00378500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00378600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00378700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00378800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00378900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00379000
                      AIF   (K'&ZLD LE 0).ZL5                           00379100
&ZLC                  SETA  35             COMMENT COLUMN               00379200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00379300
&ZLC                  SETA  33             COMMMENT COLUMN              00379400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00379500
&ZLV                  SETC  '&ZLV'.' '                                  00379600
                      AGO   .ZL3                                        00379700
.*                                                                      00379800
.ZL4                  ANOP  ,                                           00379900
&ZLV                  SETC  '&ZLV'.' '                                  00380000
.ZL5                  ANOP  ,                                           00380100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00380200
.*                                                                      00380300
&ZLL     &ZLOP &ZLV                                                     00380400
.*                                                                      00380500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00380600
                      AGO   .ZL2           CONTINUE LOOPING             00380700
.*                                                                      00380800
.ZLX                  ANOP  ,                                           00380900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00381000
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00381100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00381200
         L     14,16(0,0)              GET CVT ADDRESS                  00381300
         L     14,772(14,0)            GET SFT ADDRESS                  00381400
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00381500
         LA    15,18                   GET EX FOR SERVICE ROUTINE       00381600
         OR    14,15                   GET PC NUMBER                    00381700
         PC    0(14)                                                    00381800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00381900
