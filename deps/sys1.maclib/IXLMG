.***AC.PLV01  PROCESS PLISTVER                                      *   00110800
.********************************************************************   00110900
&XPLISTVER      SETA  0                                                 00111000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00111100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00111200
&XPLISTVER      SETA  3                                                 00111300
.ZPLV0          ANOP  ,                                                 00111400
.*                                                                      00111500
                AIF   ('&CHAINTYPE' EQ '').ZPLV1E                       00111600
                AIF   (&XPLISTVER GE 3).ZPLV1E                          00111700
&XPLISTVER      SETA  3                                                 00111800
.ZPLV1E         ANOP  ,                                                 00111900
.*                                                                      00112000
                AIF   ('&STRCOPYCNTLS' EQ '').ZPLV2E                    00112100
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00112200
&XPLISTVER      SETA  2                                                 00112300
.ZPLV2E         ANOP  ,                                                 00112400
.*                                                                      00112500
                AIF   ('&AMDALEVEL' EQ '').ZPLV3E                       00112600
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00112700
&XPLISTVER      SETA  1                                                 00112800
.ZPLV3E         ANOP  ,                                                 00112900
.*                                                                      00113000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00113100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00113200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00113300
.*-------------------------------------------------------------------   00113400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00113500
&ZMS            SETC  '"PLISTVER='                                      00113600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00113700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00113800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00113900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00114000
                AGO   .ZPVCK2                                           00114100
.ZPVCK3         ANOP  ,                                                 00114200
.*-------------------------------------------------------------------   00114300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00114400
&ZMS            SETC  '"PLISTVER='                                      00114500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00114600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00114700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00114800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00114900
                AGO   .ZPVCKE                                           00115000
.ZPVCK2         ANOP  ,                                                 00115100
.*-------------------------------------------------------------------   00115200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00115300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00115400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00115500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00115600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00115700
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00115800
.ZPVCK6         ANOP  ,                                                 00115900
&ZMS            SETC  '"PLISTVER='                                      00116000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00116100
&ZMS            SETC  '&ZMS THE RANGE IS "0-3"'                         00116200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00116300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00116400
&XPLISTVER      SETA  3                                                 00116500
                AGO   .ZPVCKE                                           00116600
.*-------------------------------------------------------------------   00116700
.ZPVCK4A        ANOP  ,                                                 00116800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00116900
&ZMS            SETC  'PLISTVER='                                       00117000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00117100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00117200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00117300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00117400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00117500
                AGO   .ZPVCK5                                           00117600
.ZPVCK4         ANOP  ,                                                 00117700
&XPLISTVER      SETA  &PLISTVER(1)                                      00117800
.ZPVCK5         ANOP  ,                                                 00117900
.ZPVCKE         ANOP  ,                                                 00118000
.********************************************************************   00118100
