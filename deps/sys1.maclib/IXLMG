&ZOPL(&ZOPN)          SETC  ' '                                         00373000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00373100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00373200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00373300
.ZMRETX               ANOP  ,                                           00373400
.*******************************************************************    00373500
.***AO.X03  REASON CODE PROCESSING                                      00373600
.*******************************************************************    00373700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00373800
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00373900
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00374000
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00374100
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00374200
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00374300
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00374400
