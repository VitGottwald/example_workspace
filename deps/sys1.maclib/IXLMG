                  AIF   (K'&ZCGPR GT 2).ZE41Q                           00278300
                  AIF   ('&ZCGPR' EQ '0').ZE41Q                         00278400
&ZCGPR            SETC  '&ZCGPR'(2,1)                                   00278500
                  AIF   (T'&ZCGPR NE 'N').ZE41Q  INVALID REG NAME       00278600
.ZE41P            ANOP  ,                                               00278700
                  AIF   (T'&ZCGPR NE 'N').ZE41R  NON NUMERIC            00278800
                  AIF   ('&ZCGPR' LT '2').ZE41Q                         00278900
                  AIF   ('&ZCGPR' LE '12').ZE41R                        00279000
.ZE41Q            ANOP  ,                                               00279100
&ZMS              SETC  '"RETCODE='                                     00279200
&ZMS              SETC  '&ZMS.&RETCODE"'                                00279300
