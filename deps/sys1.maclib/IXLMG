.*******************************************************************    00370300
.***  PROCESS IXLMG OUTPUT PARAMETERS                                   00370400
.*******************************************************************    00370500
                      ACTR  10000                                       00370600
.*******************************************************************    00370700
.***AO.X02  RETURN CODE PROCESSING                                      00370800
.*******************************************************************    00370900
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00371000
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00371100
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00371200
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00371300
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00371400
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00371500
&ZOPN                 SETA  &ZOPN+1                                     00371600
&ZOPL(&ZOPN)          SETC  ' '                                         00371700
&ZOPO(&ZOPN)          SETC  'ST'                                        00371800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00371900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00372000
                      AGO   .ZMRETX                                     00372100
.*                                                                      00372200
.ZMRET2               ANOP  ,                                           00372300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00372400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00372500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00372600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00372700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00372800
