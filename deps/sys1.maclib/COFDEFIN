                      AGO   .ZIMFNX                                     00135800
.*                                                                      00135900
.ZIMFN2               ANOP ,                                            00136000
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00136100
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00136200
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00136300
