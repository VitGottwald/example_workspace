.*                                                                      00134300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00134400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00134500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00134600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00134700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00134800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00134900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00135000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00135100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00135200
&ZOPN                 SETA  &ZOPN+1                                     00135300
