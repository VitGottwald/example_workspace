.***AC.PLV01  PROCESS PLISTVER                                      *   00052100
.********************************************************************   00052200
&XPLISTVER      SETA  1                                                 00052300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00052400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00052500
&XPLISTVER      SETA  1                                                 00052600
.ZPLV0          ANOP  ,                                                 00052700
.*                                                                      00052800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00052900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00053000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00053100
.*-------------------------------------------------------------------   00053200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00053300
&ZMS            SETC  '"PLISTVER='                                      00053400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00053600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00053700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00053800
                AGO   .ZPVCK2                                           00053900
.ZPVCK3         ANOP  ,                                                 00054000
.*-------------------------------------------------------------------   00054100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00054200
&ZMS            SETC  '"PLISTVER='                                      00054300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00054400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00054500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00054600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054700
                AGO   .ZPVCKE                                           00054800
.ZPVCK2         ANOP  ,                                                 00054900
.*-------------------------------------------------------------------   00055000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00055100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00055200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00055300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00055400
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00055500
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00055600
.ZPVCK6         ANOP  ,                                                 00055700
&ZMS            SETC  '"PLISTVER='                                      00055800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055900
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00056000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00056100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056200
&XPLISTVER      SETA  1                                                 00056300
                AGO   .ZPVCKE                                           00056400
.*-------------------------------------------------------------------   00056500
.ZPVCK4A        ANOP  ,                                                 00056600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00056700
&ZMS            SETC  'PLISTVER='                                       00056800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00056900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00057000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00057100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00057200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057300
                AGO   .ZPVCK5                                           00057400
.ZPVCK4         ANOP  ,                                                 00057500
&XPLISTVER      SETA  &PLISTVER(1)                                      00057600
.ZPVCK5         ANOP  ,                                                 00057700
.ZPVCKE         ANOP  ,                                                 00057800
