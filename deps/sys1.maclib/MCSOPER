.********************************************************************   00092100
.***AC.PLV01  PROCESS PLISTVER                                      *   00092200
.********************************************************************   00092300
&XPLISTVER      SETA  1                                                 00092400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00092500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00092600
&XPLISTVER      SETA  1                                                 00092700
.ZPLV0          ANOP  ,                                                 00092800
.*                                                                      00092900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00093000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00093100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00093200
.*-------------------------------------------------------------------   00093300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00093400
&ZMS            SETC  '"PLISTVER='                                      00093500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00093600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00093700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00093800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00093900
                AGO   .ZPVCK2                                           00094000
.ZPVCK3         ANOP  ,                                                 00094100
.*-------------------------------------------------------------------   00094200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00094300
&ZMS            SETC  '"PLISTVER='                                      00094400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00094500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00094600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00094700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00094800
                AGO   .ZPVCKE                                           00094900
.ZPVCK2         ANOP  ,                                                 00095000
.*-------------------------------------------------------------------   00095100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00095200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00095300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00095400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00095500
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00095600
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00095700
.ZPVCK6         ANOP  ,                                                 00095800
&ZMS            SETC  '"PLISTVER='                                      00095900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00096000
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00096100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00096200
