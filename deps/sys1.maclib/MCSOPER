.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00412700
&ZPLLEN               SETA 76                                           00412800
.*                                                                      00412900
.******************************************************************     00413000
.***AI.L01 MCSOPER FMT=LIST FILL IN PARAMETER AREA                      00413100
.******************************************************************     00413200
                      ACTR  10000                                       00413300
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00413400
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00413500
&ZCVAL                SETC  'LAE'          XAX MODE                     00413600
.ZIMFXA               ANOP ,                                            00413700
.*                                                                      00413800
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00413900
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00414000
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00414100
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00414200
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00414300
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00414400
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00414500
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00414600
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00414700
&ZOPN                 SETA  &ZOPN+1                                     00414800
&ZOPL(&ZOPN)          SETC  ' '                                         00414900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00415000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00415100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00415200
                      AGO   .ZIMFNX                                     00415300
.*                                                                      00415400
.ZIMFN2               ANOP ,                                            00415500
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00415600
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00415700
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00415800
&ZOPN                 SETA  &ZOPN+1                                     00415900
&ZOPL(&ZOPN)          SETC  ' '                                         00416000
&ZOPO(&ZOPN)          SETC  'LAE'                                       00416100
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00416200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00416300
                      AGO   .ZIMFNX                                     00416400
.ZIMFNX1              ANOP ,                                            00416500
&ZOPN                 SETA  &ZOPN+1                                     00416600
