*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00493800
*%MCSOPER: MACRO KEYS(REQUEST,NAME,CONSID,MIGID,MIGIDREL,TERMNAME,      00493900
*                 MCSCSA,MCSCSAA,OPERPARM,MSGDLVRY,MSGECB,QLIMIT,       00494000
*                 ALERTECB,ALERTPCT,QRESUME,ABTERM,RTNCODE,RSNCODE,     00494100
*                 PLISTVER,MF);                                         00494200
*  ANS('?'!!MACLABEL!!' '!!'MCSOPERP '!!MACLIST!!                       00494300
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00494400
*%END MCSOPER;                                                          00494500
