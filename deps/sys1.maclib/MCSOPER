*                   !     ,NAME=xname                                 * 00004700
*                   !     ,CONSID=xconsid                             * 00004800
*                   !    Ý,MIGID=xmigid¨                              * 00004900
*                   !    Ý,MIGIDREL=»NO!AUTOº¨                        * 00005000
*                   !     ,TERMNAME=xtermname                         * 00005100
*                   !     ,MCSCSA=xmcscsa                             * 00005200
*                   !         ,MCSCSAA=xmcscsaa                       * 00005300
