.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00485700
.*******************************************************************    00485800
                      ACTR  10000                                       00485900
.*                                                                      00486000
                      LCLA  &ZLC           COLUMN POSITION              00486100
                      LCLA  &ZOPX          LIST TABLE INDEX             00486200
                      LCLC  &ZLL           LABEL                        00486300
                      LCLC  &ZLOP          OPERATION                    00486400
                      LCLC  &ZLV           VALUE                        00486500
                      LCLC  &ZLD           DESCRIPTION                  00486600
.*                                                                      00486700
&ZOPX                 SETA  1                                           00486800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00486900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00487000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00487100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00487200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00487300
                      AIF   (K'&ZLD LE 0).ZL5                           00487400
&ZLC                  SETA  35             COMMENT COLUMN               00487500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00487600
&ZLC                  SETA  33             COMMMENT COLUMN              00487700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00487800
&ZLV                  SETC  '&ZLV'.' '                                  00487900
                      AGO   .ZL3                                        00488000
.*                                                                      00488100
.ZL4                  ANOP  ,                                           00488200
&ZLV                  SETC  '&ZLV'.' '                                  00488300
.ZL5                  ANOP  ,                                           00488400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00488500
.*                                                                      00488600
&ZLL     &ZLOP &ZLV                                                     00488700
.*                                                                      00488800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00488900
                      AGO   .ZL2           CONTINUE LOOPING             00489000
.*                                                                      00489100
.ZLX                  ANOP  ,                                           00489200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00489300
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00489400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00489500
         L     15,16(0,0)         GET ADDRESS OF CVT FROM PSA           00489600
         L     15,772(15)         GET ADDRESS OF SFT FROM CVT      @PBC 00489700
         LA    14,1(0,0)          GET MCSOPER INDEX INTO LINKAGE TABLE  00489800
         AL    14,100(15)         POINT REG14 TO MCSOPER ETD ENTRY @PBC 00489900
         PC    0(14)              PC TO MCSOPER SERVICE                 00490000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00490100
.ZLINKX               ANOP  ,                                           00490200
.*******************************************************************    00490300
.***AP.X01  FORMAT GENERATED CODE                                       00490400
