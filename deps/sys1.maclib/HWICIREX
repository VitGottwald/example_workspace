/**********************************************************************/        
/*                                                                    */        
/*                       REXX Return Codes                            */        
/*                                                                    */        
/* Values that may be returned in RC, one of the REXX special         */        
/* variables, after a call to a BCPii service.                        */        
/*                                                                    */        
/**********************************************************************/        
HWI_REXXPARMSYNTAXERROR     =    1  /*                   '00000001'x  */        
                                    /* The BCPii REXX host command    */        
                                    /* processor has detected a       */        
                                    /* parameter that is not in the   */        
                                    /* proper format.                 */        
HWI_REXXUNSUPPORTEDSERVICE  =    2  /*                   '00000002'x  */        
                                    /* An unknown BCPii service name  */        
                                    /* was specified on the BCPii     */        
                                    /* REXX host command.             */        
HWI_REXXINVALIDNUMOFPARMS   =    3  /*                   '00000003'x  */        
                                    /* The number of parameters       */        
                                    /* specified on the BCPii REXX    */        
                                    /* host command for the service   */        
                                    /* name specified does not match  */        
                                    /* the number of parameters       */        
                                    /* expected.                      */        
HWI_REXXSTEMVARREQUIRED     =    4  /*                   '00000004'x  */        
                                    /* The BCPii REXX service         */        
                                    /* specified on the BCPii REXX    */        
                                    /* host command is missing one or */        
                                    /* more required stem variables   */        
                                    /* in the positional parameter    */        
                                    /* list.                          */        
HWI_REXXPARMNAMETOOLONG     =    5  /*                   '00000005'x  */        
                                    /* One or more variables specified*/        
                                    /* on the BCPii REXX service call */        
                                    /* is greater than the BCPii      */        
                                    /* maximum REXX variable length   */        
                                    /* (40).                          */        
HWI_REXXINVALIDHOSTENV      =    6  /*                   '00000006'x  */        
                                    /* BCPii detected a specific      */        
                                    /* unexpected system error.       */        
HWI_REXXINVOKERNOTFOUND     =    7  /*                   '00000007'x  */        
                                    /* The address space issuing the  */        
                                    /* AXREXX invocation is no longer */        
                                    /* running. No new BCPii          */        
                                    /* connections are allowed.       */        
HWI_REXXINVALIDEXECUTIONENV =    8  /*                   '00000008'x  */        
                                    /* BCPii does not support the     */        
                                    /* BCPii host command running in  */        
                                    /* the current execution          */        
                                    /* environment.                   */        
HWI_REXXUNSUPPORTEDLISTTYPE =    9  /*                   '00000009'x  */        
                                    /* BCPii does not support the     */        
                                    /* requested ListType on the      */        
                                    /* BCPii HWILIST service for the  */        
                                    /* current execution environment. */        
                                    /*                           @P0A */        
HWI_REXXMISSINGREQUIREDPARM =   10  /*                   '0000000A'x  */        
                                    /* BCPii's REST service requires  */        
                                    /* the URI and HTTPMETHOD parms.  */        
                                    /*                           @L8A */        
HWI_REXXNOTAVALIDPARMVALUE  =   11  /*                   '0000000B'x  */        
                                    /* BCPii's REST service require   */        
                                    /* parms to be of a certain type. */        
                                    /* For example, the URI is to be  */        
                                    /* a string value and not a       */        
                                    /* decimal value.                 */        
                                    /*                           @L8A */        
HWI_REXXPARMVALTOOLONG      =   12  /*                   '0000000C'x  */        
                                    /* BCPii's REST service parameter */        
                                    /* value is longer than supported.*/        
                                    /* For example, the URI does not  */        
                                    /* exceed 2048 bytes.             */        
                                    /*                           @L8A */        
HWI_REXXINTERNALSYSTEMERROR =   32  /*                   '00000020'x  */        
                                    /* BCPii detected an unexpected   */        
                                    /* error while invoking System    */        
                                    /* REXX services.  The system     */        
                                    /* rejects the service call.      */        
HWI_UNEXPECTED_ERROR        = 4095  /*                   '00000FFF'x  */        
                                    /* BCPii detected an unexpected   */        
                                    /* error.  The system rejects the */        
                                    /* service call.                  */        
/**********************************************************************/        
/*                                                                    */        
/*                    HWIHOST Return Codes                            */        
/*                                                                    */        
/* The following return codes only apply to callers running their     */        
/* EXECs in ISV-provided REXX environments.                           */        
/*                                                                    */        
/* Values that may be returned in RC, one of the REXX special         */        
/* variables, after a call to the BCPii service HWIHOST.              */        
/*                                                               @P0A */        
/**********************************************************************/        
HWI_HWIHOST_PARMSYNTAXERROR     = 1 /*                   '00000001'x  */        
                                    /* The argument specified was not */        
                                    /* 'ON' or 'OFF'.            @P2C */        
HWI_HWIHOST_INTERNALSYSTEMERROR = 2 /*                   '00000002'x  */        
                                    /* IRXSUBCM failed to query, add  */        
                                    /* or delete the BCPii host       */        
                                    /* command.                  @P2C */        
                                                                                
/**********************************************************************/        
/*               BCPii's HTTPMETHOD's Constants                       */        
/*****************************************************************@L8A*/        
HWI_REST_POST                   = 1                                             
HWI_REST_GET                    = 2                                             
HWI_REST_PUT                    = 3                                             
HWI_REST_DELETE                 = 4                                             
                                                                                
/**********************************************************************/        
/*               BCPii's ENCODING Constants                           */        
/*****************************************************************@L8A*/        
HWI_ENCODING_UTF8               = 1                                             
HWI_ENCODING_IBM1047            = 2                                             
                                                                                
