*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 04960000
*%GOTO SK17;                       /*                            @M2A*/ 04970000
  1 RPESESSN  BASED(ADDR(RACRPE(RPESESOF+1))),  /* SESSION DATA  @M1A*/ 04980000
*%GOTO SK18;                       /*                            @M2A*/ 04990000
*%SK17:;                           /*                            @M2A*/ 05000000
  1 RPESESSN  BASED(ADDR(RACRPE(RACRPE.RPESESOF+1))),  /* SESSION       05010000
                                                       DATA  @YA23599*/ 05020000
*%SK18:;                           /*                            @M2A*/ 05030000
    3 RPESEFIX,                    /* FIXED LEN SESSION FIELDS   @M1A*/ 05040000
      5 RPEKYDAT CHAR(4),          /* DATE KEY WAS LAST CHANGED  @M1A*/ 05050000
      5 RPEKYINT FIXED(15),        /* # DAYS UNTIL KEY EXPIRES   @M1A*/ 05060000
      5 RPEMFAIL FIXED(15),        /* MAX # OF FAILED ATTEMPTS   @M1A*/ 05070000
      5 RPESLSFG BIT(8),           /* SESSION FLAGS              @M1A*/ 05080000
      5 RPESKYLN FIXED(8),         /* LENGTH OF SESSION KEY      @M1A*/ 05090000
    3 RPESEVAR,                    /* VARIABLE LEN FIELDS        @M1A*/ 05100000
      5 RPESNKEY CHAR(*),          /* SESSION KEY                @M3C*/ 05110000
*%IF PLAS = 'YES' %THEN            /*                            @M3A*/ 05120000
*%GOTO SK19;                       /*                            @M3A*/ 05130000
  1 RPEACL2   BASED(ADDR(RACRPE(RPEAC2OF+1))) BDY(BYTE), /*             05140000
                                   SECOND ACCESS LIST            @M3A*/ 05150000
*%GOTO SK20;                       /*                            @M3A*/ 05160000
*%SK19:;                           /*                            @M3A*/ 05170000
  1 RPEACL2   BASED(ADDR(RACRPE(RACRPE.RPEAC2OF+1))) BDY(BYTE),  /*     05180000
                                            SECOND ACCESS LIST   @M3A*/ 05190000
*%SK20:;                           /*                            @M3A*/ 05200000
    3 RPEA2FIX BDY(BYTE),          /* FIXED LENGTH PORTION OF SECOND    05210000
                                      ACCESS LIST                @M3A*/ 05220000
