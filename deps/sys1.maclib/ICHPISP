  1 RPEIPLOK  BASED(ADDR(RACRPE(RACRPE.RPEIPOFF+1))),  /*        @M8A*/ 05630000
*%SK26:;                           /*                            @M8A*/ 05640000
    3 * CHAR(16),                  /* SERVAUTH IP LOOKUP value   @LAC*/ 05650000
                                                                        05660000
*%IF PLAS = 'YES' %THEN            /*                            @LAA*/ 05670000
*%GOTO SK27;                       /*                            @LAA*/ 05680000
  1 RPEICSF   BASED(ADDR(RACRPE(RPECSFOF+1))),   /*              @LAA*/ 05690000
*%GOTO SK28;                       /*                            @LAA*/ 05700000
*%SK27:;                           /*                            @LAA*/ 05710000
  1 RPEICSF   BASED(ADDR(RACRPE(RACRPE.RPECSFOF+1))),  /*        @LAA*/ 05720000
*%SK28:;                           /*                            @LAA*/ 05730000
    3 RPEICEXP FIXED(8),           /* CSFSEXP-symetric key export@LAA*/ 05740000
    3 RPEICAUS FIXED(32) BDY(BYTE),/* CSFAUSE-asymetric key usage@LAA*/ 05750000
    3 RPEICOFF FIXED(16) BDY(BYTE),/* offset from RPEICSF to            05760000
                                      RPECLABS                   @LAA*/ 05770000
    3 RPEIKLCT FIXED(16) BDY(BYTE),/* CSFSKLCT -PKDS label count @LAA*/ 05780000
    3 RPEIKLBS CHAR(*),            /* CSFSKLBS - len/label pairs @LAA*/ 05790000
    /* The CSFSKLBS repeat group of key labels, is followed by the   */ 05800000
    /* repeat group of certificate labels, CSFSCLBS.  Since we do    */ 05810000
    /* not know how long CSFSKLBS is, we cannot map here where the   */ 05820000
    /* RPEICLCT and RPEICLBS are.  So we have the RPECLABS mapping.  */ 05830000
    /* However to balance this out, the RPEILABS maps the elements   */ 05840000
    /* of both repeat groups.                                    @LAA*/ 05850000
  1 RPECLABS BASED,                /* ICSF certificate labels    @P3C*/ 05860000
    3 RPEICLCT FIXED(16) BDY(BYTE),/*CSFSCKCT-certificate lab cnt@LAA*/ 05870000
    3 RPEICLBS CHAR(*),            /* CSFSCLBS - len/label pairs @LAA*/ 05880000
  1 RPEILABS  BASED,               /* RPEIKLBS or RPEICLBS elems @P3C*/ 05890000
    3 RPEILLN  FIXED(8) BDY(BYTE), /* ICSF label element length  @LAA*/ 05900000
    3 RPEILABE CHAR(*),            /* ICSF label element         @LCC*/ 05910000
*%IF PLAS = 'YES' %THEN            /*                            @LCA*/ 05920000
*%GOTO SK29;                       /*                            @LCA*/ 05930000
  1 RPEDID1   BASED(ADDR(RACRPE(RPEDIDOF+1))),   /*              @LCA*/ 05940000
*%GOTO SK30;                       /*                            @LCA*/ 05950000
*%SK29:;                           /*                            @LCA*/ 05960000
  1 RPEDID1   BASED(ADDR(RACRPE(RACRPE.RPEDIDOF+1))),  /*        @LCA*/ 05970000
*%SK30:;                           /*                            @LCA*/ 05980000
    3 RPEDIDLL FIXED(8),           /* Label length               @LCA*/ 05990000
    3 RPEDIDLB CHAR(*),            /* Label                      @LCA*/ 06000000
  1 RPEDID2 BASED,                 /* Rest of DIDLIST1           @LCA*/ 06010000
    3 RPEDIDUS CHAR(8),            /* UserID                     @LCA*/ 06020000
    3 RPEDIDRL FIXED(8),           /* Registry length            @LCA*/ 06030000
    3 RPEDIDRG CHAR(*);            /* Registry                   @LCA*/ 06040000
                                                                        06050000
  DCL RPEA2DAT FIXED(8) CONSTANT(0);    /* FLAG DATA EQUATE      @M3A*/ 06060000
  DCL RTECURRV FIXED(8) CONSTANT(4); /* Current RACGLIST profile        06070000
                                      version number             @LCC*/ 06080000
*%GOTO ENDPISP;                                                         06090000
.ENDPISP ANOP                                                  @ZA43304 06100000
*%ENDPISP:;                                                             06110000
