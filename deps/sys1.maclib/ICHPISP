      5 RPEA2USR CHAR(8),          /* USERID                     @M3A*/ 05260000
      5 RPEA2ACA BIT(8),           /* ACCESS AUTHORITY           @M3A*/ 05270000
      5 RPEA2CNT FIXED(16)         /* ACCESS COUNT FIELD         @M3A*/ 05280000
          BDY(BYTE),                                                    05290000
      5 RPEA2VRL BIN(8),           /* VARIABLE AREA LENGTH       @M3A*/ 05300000
    3 RPEA2VAR BDY(BYTE),          /* VARIABLE AREA              @M3A*/ 05310000
      5 RPEA2CLI  CHAR(8),         /* CLASS ID.                  @M3A*/ 05320000
      5 RPEA2RSV  CHAR(2),         /* RESERVED.                  @M3A*/ 05330000
      5 RPEA2ELN  BIN(8),          /* ENTITY LENGTH              @M3A*/ 05340000
      5 RPEA2ENT  CHAR(*),         /* ENTITY                     @M4C*/ 05350000
*%IF PLAS = 'YES' %THEN            /*                            @M4A*/ 05360000
*%GOTO SK21;                       /*                            @M4A*/ 05370000
  1 RPEMEM BASED(ADDR(RACRPE(RPEMEMOF+1))) BDY(BYTE), /* MEMBER         05380000
                                                     LIST    @YA23599*/ 05390000
*%GOTO SK22;                       /*                            @M4A*/ 05400000
*%SK21:;                           /*                            @M4A*/ 05410000
  1 RPEMEM BASED(ADDR(RACRPE(RACRPE.RPEMEMOF+1))) BDY(BYTE), /* MEMBER  05420000
