*%PLSPISP:;                        /*                            @06A*/ 03030000
                                                                        03040000
 DECLARE                           /* RACLIST STRUCTURES             */ 03050000
  1 RACRTE BASED(ACEECLCP),        /* RACLIST CLASS TREE                03060000
                                      ANCHOR ELEMENT                 */ 03070000
    3 RTENEXT PTR,                 /* ADDRESS OF NEXT ANCHOR OR 0    */ 03080000
    3 RTECLASS PTR,                /* ADDRESS OF CLASS DESCRIPTOR       03090000
                                      ENTRY FOR THIS CLASS           */ 03100000
    3 RTETREE PTR,                 /* ADDRESS OF TOP NODE IN            03110000
                                      TREE OR 0                      */ 03120000
    3 RTESTORE PTR,                /* ADDRESS OF STORAGE BLOCK          03130000
                                      LIST OR 0                      */ 03140000
    3 RTESPNS,                     /* PROFILE & NODE SUBPOOL NUMBERS */ 03150000
      5 RTEPSPN BIN(8),            /* SUBPOOL NUMBER FOR PROFILES    */ 03160000
      5 RTENSPN BIN(8),            /* SUBPOOL NUMBER FOR TREE NODES  */ 03170000
    3 RTEASPN BIN(8),              /* SUBPOOL NUMBER OF THIS BLOCK   */ 03180000
    3 RTEVERSN FIXED(8),           /* Version number of RACGLIST        03190000
                                      profiles                   @05A*/ 03200000
    3 RTEGENL PTR(31),             /* ADDRESS OF GENERIC PROFILE        03210000
                                      LIST OR 0                  @L5A*/ 03220000
    3 RTESIZE FIXED(31),           /* TOTAL STORAGE USED FOR RACLISTED  03230000
                                    PROFILES AND NODES           @M2A*/ 03240000
    3 RTEGNUM FIXED(31),           /*TOTAL NUMBER OF GROUPING PROFILES  03250000
                                     THAT CONTAIN MEMBERS        @01A*/ 03260000
    3 RTESTOKN CHAR(8),            /* STOKEN FOR GLOBAL=YES             03270000
                                      DATASPACE                  @03C*/ 03280000
    3 RTEALET BIN(32),             /* PASN-AL ALET FOR                  03290000
                                      DATASPACE                  @03C*/ 03300000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 03310000
*%GOTO SK01;                       /*                            @M2A*/ 03320000
  1 RACRSE BASED(RTESTORE) BDY(WORD),/* RACLIST CLASS TREE              03330000
                                      STORAGE BLOCK                  */ 03340000
*%GOTO SK02;                       /*                            @M2A*/ 03350000
*%SK01:;                           /*                            @M2A*/ 03360000
  1 RACRSE BASED(RACRTE.RTESTORE) BDY(WORD),/* RACLIST CLASS TREE       03370000
                                      STORAGE BLOCK              @M2A*/ 03380000
*%SK02:;                           /*                            @M2A*/ 03390000
    3 RSENEXT PTR,                 /* ADDRESS OF NEXT STORAGE           03400000
                                      BLOCK OR 0                     */ 03410000
    3 RSESIZE BIN(15),             /* LENGTH OF STORAGE BLOCK        */ 03420000
    3 RSEPOOL BIN(8),              /* SUBPOOL NUMBER OF STORAGE         03430000
                                      BLOCK                          */ 03440000
    3 * BIN(8),                    /* RESERVED                       */ 03450000
    3 RSESTORE CHAR(*),            /* USEABLE STORAGE (RSESIZE-4        03460000
                                      BYTES)                         */ 03470000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 03480000
*%GOTO SK03;                       /*                            @M2A*/ 03490000
  1 RACRNE BASED(RTETREE) BDY(WORD),/* RACLIST CLASS TREE NODE          03500000
                                      ELEMENT                        */ 03510000
*%GOTO SK04;                       /*                            @M2A*/ 03520000
*%SK03:;                           /*                            @M2A*/ 03530000
  1 RACRNE BASED(RACRTE.RTETREE) BDY(WORD),/* RACLIST CLASS TREE NODE   03540000
                                      ELEMENT                    @M2A*/ 03550000
*%SK04:;                           /*                            @M2A*/ 03560000
   2 RNEHDR,                                                  /* @04A*/ 03570000
    3 RNELEFT PTR,                 /* ADDRESS OF LEFT DAUGHTER          03580000
                                      NODE OR 0                      */ 03590000
    3 RNEPROF PTR,                 /* ADDRESS OF PROFILE FOR THIS       03600000
                                      NODE                           */ 03610000
    3 RNERIGHT PTR,                /* ADDRESS OF RIGHT DAUGHTER         03620000
                                      NODE OR 0                      */ 03630000
    3 RNEBAL BIN(31),              /* TREE BALANCING FACTOR DURING TREE 03640000
                                      CREATION                       */ 03650000
      7 RNEUP PTR(31),             /* POINTER TO MOTHER NODE DURING     03660000
                                      TREE DELETION                  */ 03670000
 2 RNEKEY CHAR(*),                 /* KEY (LENGTH DETERMINED BY MAXIMUM 03680000
                                      NAME LENGTH FOR CLASS IN THE      03690000
                                      CLASS DESCRIPTOR ELEMENT)  @04C*/ 03700000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 03710000
*%GOTO SK05;                       /*                            @M2A*/ 03720000
  RNELINK(3) PTR(31) DEF(RNELEFT), /* ARRAY OF LEFT LINK, PROFILE AND   03730000
                                      RIGHT LINK POINTERS            */ 03740000
*%GOTO SK06;                       /*                            @M2A*/ 03750000
*%SK05:;                           /*                            @M2A*/ 03760000
  RNELINK(3) PTR(31) DEF(RACRNE.RNELEFT), /* ARRAY OF LEFT LINK, PROFILE03770000
                                     AND RIGHT LINK POINTERS     @M2A*/ 03780000
*%SK06:;                           /*                            @M2A*/ 03790000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 03800000
*%GOTO SK07;                       /*                            @M2A*/ 03810000
  1 RACRPE BASED(RNEPROF) BDY(HWORD),/* RESOURCE PROFILE ELEMENT     */ 03820000
*%GOTO SK08;                       /*                            @M2A*/ 03830000
*%SK07:;                           /*                            @M2A*/ 03840000
  1 RACRPE BASED(RACRNE.RNEPROF) BDY(HWORD),/* RESOURCE PROFILE         03850000
                                                        ELEMENT  @M2A*/ 03860000
*%SK08:;                           /*                            @M2A*/ 03870000
    3 RPEPLEN BIN(16),   /* PHYSICAL STORAGE LENGTH OF                  03880000
                                      BLOCK                  @YA28820*/ 03890000
    3 RPELLEN BIN(16),   /* LOGICAL LENGTH OF BLOCK          @YA28820*/ 03900000
    3 RPEUCNT BIN(16),   /* NUMBER OF RESOURCES SHARING                 03910000
                                       THIS PROFILE          @YA28820*/ 03920000
    3 RPEATTR,                     /* ATTRIBUTE FLAGS                */ 03930000
      5 RPEUACC BIT(8),            /* UNIVERSAL ACCESS               */ 03940000
      5 RPEAUDIT BIT(8),           /* AUDIT FLAGS                    */ 03950000
      5 RPEGAUD BIT(8),            /* GLOBAL AUDIT FLAGS             */ 03960000
      5 RPELEVEL BIT(8),           /* RESOURCE LEVEL                 */ 03970000
    3 RPEACCNO BIN(16),  /* NUMBER OF ENTRIES IN ACCESS                 03980000
                                      LIST                   @YA28820*/ 03990000
    3 RPEACCOF BIN(16),  /* OFFSET TO ACCESS LIST            @YA28820*/ 04000000
    3 RPEINSOF BIN(16),  /* OFFSET TO INSTALLATION DATA      @YA28820*/ 04010000
    3 RPEAPPOF BIN(16),  /* OFFSET TO APPLICATION DATA       @YA28820*/ 04020000
    3 RPEOWNER CHAR(8),            /* OWNER OF RESOURCE PROFILE  @L6A*/ 04030000
    3 RPENUMDP BIN(15),            /* NUMBER OF CATEGORIES IN           04040000
                                      LIST                       @L7A*/ 04050000
    3 RPEDPTOF BIN(16),  /* OFFSET TO CATEGORY LIST          @YA28820*/ 04060000
    3 RPELDAYS BIT(8),             /* DAYS TERMINAL MAY NOT BE USED     04070000
                                      (BIT 0 - SUNDAY, BIT 1 -          04080000
                                      MONDAY, ...)               @L7A*/ 04090000
    3 RPESCLVL BIN(8),             /* RESOURCE SECURITY LEVEL    @L7A*/ 04100000
    3 RPELOGNT CHAR(3),            /* EARLIEST TIME TERMINAL MAY BE     04110000
                                      USED (HHMM)                @L7A*/ 04120000
    3 RPELOGFT CHAR(3),            /* LATEST TIME TERMINAL MAY BE       04130000
                                      USED (HHMM)                @L7A*/ 04140000
    3 RPENTFY CHAR(8),             /* USERID TO NOTIFY WHEN THIS        04150000
                                      PROFILE DENIES ACCESS      @L7A*/ 04160000
    3 RPETZONE CHAR(3),            /* TIME OFFSET OF TERMINAL FROM      04170000
                                      CPU. + = EAST  - = WEST.   @L7A*/ 04180000
    3 RPEFLAGS BIT(8),             /* FLAGS FOR IN STORE                04190000
                                      PROFILE                @YA23599*/ 04200000
      5 RPEFWARN BIT(1),           /* WARN OPTION SPECIFIED?     @L8A*/ 04210000
      5 RPEFSCPW BIT(1),           /* ICSF CPACF WRAP ON?        @LBA*/ 04220000
      5 RPEFSCPR BIT(1),           /* ICSF CPACF RET ON?         @LDA*/ 04230000
      5 * BIT(5),                  /* RESERVED                   @LDC*/ 04240000
    3 RPESCLBL CHAR(8),            /* SECLABEL                   @M1A*/ 04250000
    3 RPESESOF BIN(16),            /* SESSION SEG DATA OFF   @YA28820*/ 04260000
    3 RPESESLN BIN(16),            /* SESSION SEG DATA LEN   @YA28820*/ 04270000
    3 RPEAC2NO FIXED(16),          /* NUMBER OF OCCURRENCES  @YA28820*/ 04280000
    3 RPEAC2LN FIXED(16),          /* CONDITIONAL ACCESS LIST           04290000
                                      LENGTH                     @M3A*/ 04300000
    3 RPEAC2OF FIXED(16),          /* SECOND ACCESS LIST OFFSET  @M3A*/ 04310000
    3 RPEMEMCT BIN(16),            /* NUMBER OF MEMBERS      @YA28820*/ 04320000
    3 RPEMEMLN BIN(16),            /* LENGTH OF MEMBER LIST  @YA28820*/ 04330000
    3 RPEMEMOF BIN(16),            /* OFFSET TO MEMBER LIST  @YA28820*/ 04340000
    3 RPESE2LN BIN(15),            /* MORE SESSION DATA LENGTH   @M5A*/ 04350000
    3 RPESE2OF BIN(15),            /* MORE SESSION DATA OFFSET   @M5A*/ 04360000
    3 RPEIPOFF BIN(16),            /* OFFSET TO IPLOOK VALUE     @M8A*/ 04370000
    3 RPECSFLN BIN(16),            /* ICSF SEGMENT INFO LENGTH   @LAA*/ 04380000
    3 RPECSFOF BIN(16),            /* ICSF SEGMENT INFO OFFSET   @LAA*/ 04390000
    3 RPEDIDCT BIN(16),            /* Number of DIDLIST1 entries @LCA*/ 04400000
    3 RPEDIDLN BIN(16),            /* Length of Distributed Identity    04410000
                                      information (DIDLIST1)     @LCA*/ 04420000
    3 RPEDIDOF BIN(16),            /* Offset to Distributed Identity    04430000
                                      information                @LCA*/ 04440000
                                   /* NOTE THAT IF MORE FIELDS ARE      04450000
                                      ADDED HERE, THE RTECURRV VALUE    04460000
                                      WILL PROBABLY NEED TO BE          04470000
                                      UPDATED.                   @LCM*/ 04480000
    3 RPEEND CHAR(0),              /* END OF FIXED PART OF ELEMENT   */ 04490000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 04500000
*%GOTO SK09;                       /*                            @M2A*/ 04510000
  1 RPEINST BASED(ADDR(RACRPE(RPEINSOF+1))),/* INSTALLATION DATA        04520000
                                      VARIABLE LENGTH PORTION        */ 04530000
*%GOTO SK10;                       /*                            @M2A*/ 04540000
*%SK09:;                           /*                            @M2A*/ 04550000
  1 RPEINST BASED(ADDR(RACRPE(RACRPE.RPEINSOF+1))),/* INSTALLATION      04560000
                                      DATA VARIABLE LENGTH              04570000
                                      PORTION                @YA23599*/ 04580000
*%SK10:;                           /*                            @M2A*/ 04590000
    3 RPEINSTL BIN(8),             /* INSTALLATION DATA LENGTH       */ 04600000
    3 RPEINSTE CHAR(0),            /* END OF RPEINST             @M7A*/ 04610000
  RPEINSTD CHAR(*) BASED(ADDR(RPEINSTE)), /* INSTALLATION DATA          04620000
                                             STRING              @M7C*/ 04630000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 04640000
*%GOTO SK11;                       /*                            @M2A*/ 04650000
  1 RPEAPPL BASED(ADDR(RACRPE(RPEAPPOF+1))),/* APPLICATION DATA         04660000
                                      VARIABLE LENGTH PORTION        */ 04670000
*%GOTO SK12;                       /*                            @M2A*/ 04680000
*%SK11:;                           /*                            @M2A*/ 04690000
  1 RPEAPPL BASED(ADDR(RACRPE(RACRPE.RPEAPPOF+1))),/* APPLICATION DATA  04700000
                                      VARIABLE LENGTH PORTION    @M2A*/ 04710000
*%SK12:;                           /*                            @M2A*/ 04720000
    3 RPEAPPLL BIN(8),             /* APPLICATION DATA LENGTH        */ 04730000
    3 RPEAPPLE CHAR(0),            /* END OF RPEAPPL             @M7A*/ 04740000
  RPEAPPLD CHAR(*) BASED(ADDR(RPEAPPLE)), /* APPLICATION DATA           04750000
                                             STRING              @M7C*/ 04760000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 04770000
*%GOTO SK13;                       /*                            @M2A*/ 04780000
  1 RPEACCLE(*) BASED(ADDR(RACRPE(RPEACCOF+1))),/*ACCESS LIST        */ 04790000
*%GOTO SK14;                       /*                            @M2A*/ 04800000
*%SK13:;                           /*                            @M2A*/ 04810000
  1 RPEACCLE(*) BASED(ADDR(RACRPE(RACRPE.RPEACCOF+1))),/*ACCESS         04820000
                                                       LIST  @YA23599*/ 04830000
*%SK14:;                           /*                            @M2A*/ 04840000
    3 RPEAUSR CHAR(8),             /* USER/GROUP ID                  */ 04850000
    3 RPEACS BIT(8),               /* ACCESS AUTHORITY           @L7A*/ 04860000
*%IF PLAS = 'YES' %THEN            /*                            @M2A*/ 04870000
*%GOTO SK15;                       /*                            @M2A*/ 04880000
  1 RPEDPTD(*) BASED(ADDR(RACRPE(RPEDPTOF+1))),/* CATEGORY LIST  @L7A*/ 04890000
