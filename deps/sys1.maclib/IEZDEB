*%DEBL2 : ;                                                             00126300
*%IF IKEPLS=''  %THEN                                                   00126400
*    %GOTO DEBL3;                                                       00126500
*%IHALPUSH='@LISTPUSH';                                                 00126600
*%IHALPOP='@LISTPOP';                                                   00126700
*%IHALOFF='@LISTOFF';                                                   00126800
*IHALPUSH;                                                              00126900
*%IF IHALIST='YES'  %THEN                                               00127000
*    %GOTO DEBL3;                                                       00127100
*IHALOFF;                                                               00127200
*%DEBL3 : ;                                                             00127300
* /* *************************************************************** */ 00127400
* /*                  DEB BASIC SECTION                              */ 00127500
* /* *************************************************************** */ 00127600
*                                                                       00127700
* DECLARE                                                               00127800
* 1 DEBBASIC BASED(DEBPTR) BDY(WORD),                                   00127900
*     3 DEBTCBAD PTR(31),            /* ADDRESS OF TCB FOR THIS DEB  */ 00128000
*      4 DEBNMSUB CHAR(1),           /* NUMBER OF SUBROUTINES LOADED    00128100
