&ZOPL(&ZOPN)          SETC '&XMFCTRL._XCONNTKN'                         00124500
&ZOPO(&ZOPN)          SETC 'DS'                                         00124600
&ZOPV(&ZOPN)          SETC 'BL.32'                                      00124700
&ZOPD(&ZOPN)          SETC '++ &ZXCONNTKN '                             00124800
.*-------------------------------------------------------------------   00124900
.***AM.L09  XRSV000C CL4                                                00125000
&ZOPN                 SETA &ZOPN+1                                      00125100
&ZOPL(&ZOPN)          SETC '&XMFCTRL._XRSV000C'                         00125200
&ZOPO(&ZOPN)          SETC 'DS'                                         00125300
&ZOPV(&ZOPN)          SETC 'CL4'                                        00125400
&ZOPD(&ZOPN)          SETC '++ RESERVED '                               00125500
.*-------------------------------------------------------------------   00125600
.***AM.L10 END OF IWMSCXIT LIST PARAMETERS                              00125700
&ZOPN                 SETA &ZOPN+1                                      00125800
&ZOPL(&ZOPN)          SETC '&XMFCTRL.L'                                 00125900
&ZOPO(&ZOPN)          SETC 'EQU'                                        00126000
&ZOPV(&ZOPN)          SETC '*-&XMFCTRL'                                 00126100
&ZOPD(&ZOPN)          SETC '++ LENGTH OF PLIST'                         00126200
.*                                                                      00126300
&ZOPN                 SETA &ZOPN+1                                      00126400
&ZOPL(&ZOPN)          SETC ' '                                          00126500
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00126600
&ZOPV(&ZOPN)          SETC '1'                                          00126700
&ZOPD(&ZOPN)          SETC ' '                                          00126800
.*-------------------------------------------------------------------   00126900
.***AL.L12 IWMSCXIT FMT=LIST MF=S!L PARAMETERS                          00127000
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00127100
.*                                                                      00127200
&ZOPN                 SETA  &ZOPN+1                                     00127300
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00127400
&ZOPO(&ZOPN)          SETC  'DC'                                        00127500
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00127600
&ZOPD(&ZOPN)          SETC  '++ IWMSCXIT ARG PARMS'                     00127700
.*                                                                      00127800
.ZLMFSLX              ANOP  ,                                           00127900
.*-------------------------------------------------------------------   00128000
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00128100
&ZPLLEN               SETA 16                                           00128200
.*                                                                      00128300
.******************************************************************     00128400
.***AI.L01 IWMSCXIT FMT=LIST FILL IN PARAMETER AREA                     00128500
.******************************************************************     00128600
                      ACTR  10000                                       00128700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00128800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00128900
&ZCVAL                SETC  'LAE'          XAX MODE                     00129000
.ZIMFXA               ANOP ,                                            00129100
.*                                                                      00129200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00129300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00129400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00129500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00129600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00129700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00129800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00129900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00130000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00130100
&ZOPN                 SETA  &ZOPN+1                                     00130200
&ZOPL(&ZOPN)          SETC  ' '                                         00130300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00130400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00130500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00130600
                      AGO   .ZIMFNX                                     00130700
.*                                                                      00130800
.ZIMFN2               ANOP ,                                            00130900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00131000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00131100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00131200
&ZOPN                 SETA  &ZOPN+1                                     00131300
&ZOPL(&ZOPN)          SETC  ' '                                         00131400
&ZOPO(&ZOPN)          SETC  'LAE'                                       00131500
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00131600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00131700
                      AGO   .ZIMFNX                                     00131800
.ZIMFNX1              ANOP ,                                            00131900
&ZOPN                 SETA  &ZOPN+1                                     00132000
&ZOPL(&ZOPN)          SETC  ' '                                         00132100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00132200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00132300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00132400
.ZIMFNX               ANOP ,                                            00132500
.********************************************************************   00132600
.***AI.L02 IWMSCXIT FMT=LIST INITIALIZE PARAMETER AREA                  00132700
.********************************************************************   00132800
.***AI.L03 MF=(SLE)                                                     00132900
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00133000
.*                                                                      00133100
&ZOPN                 SETA  &ZOPN+1                                     00133200
&ZOPL(&ZOPN)          SETC  ' '                                         00133300
&ZOPO(&ZOPN)          SETC  'XC'                                        00133400
&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00133500
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00133600
.ZIMFLX               ANOP  ,                                           00133700
.*-------------------------------------------------------------------   00133800
.***AI.L04 XPLISTLEN XL2                                                00133900
&ZOPN                 SETA  &ZOPN+1                                     00134000
&ZOPL(&ZOPN)          SETC  ' '                                         00134100
&ZOPO(&ZOPN)          SETC  'MVC'                                       00134200
&ZOPV(&ZOPN)          SETC  '2(2,1),=AL2(&ZPLLEN)'                      00134300
&ZOPD(&ZOPN)          SETC  '++ INPUT XPLISTLEN XL2'                    00134400
.*-------------------------------------------------------------------   00134500
.***AI.L05 XACTION BL1                                                  00134600
&ZW                   SETC  ''                                          00134700
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00134800
.*-------------------------------------------------------------------   00134900
.***AI.L08 XACTION "QUIESCE"                                            00135000
.***      BL1'1XXXXXXX'                                                 00135100
         AIF   ('&XACTION' NE 'QUIESCE').ZI5A                           00135200
&ZW                   SETC  '&ZW.1'                                     00135300
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00135400
                      AGO   .ZI5C                                       00135500
.*                                                                      00135600
.ZI5A                 ANOP  ,                                           00135700
&ZW                   SETC  '&ZW.0'                                     00135800
.ZI5C                 ANOP  ,                                           00135900
.*-------------------------------------------------------------------   00136000
.***AI.L08 XACTION "RESUME"                                             00136100
.***      BL1'X1XXXXXX'                                                 00136200
         AIF   ('&XACTION' NE 'RESUME').ZI6A                            00136300
&ZW                   SETC  '&ZW.1000000'                               00136400
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00136500
                      AGO   .ZI6C                                       00136600
.*                                                                      00136700
.ZI6A                 ANOP  ,                                           00136800
&ZW                   SETC  '&ZW.0000000'                               00136900
.ZI6C                 ANOP  ,                                           00137000
.*-------------------------------------------------------------------   00137100
.***AI.L11 XACTION BL1                                                  00137200
                      AIF   ('&ZCLNG' NE 'YES').ZI6E  ALL ZERO BITS     00137300
&ZOPN                 SETA  &ZOPN+1                                     00137400
&ZOPL(&ZOPN)          SETC  ' '                                         00137500
&ZOPO(&ZOPN)          SETC  'OI'                                        00137600
&ZOPV(&ZOPN)          SETC  '4(1),B''&ZW'''                             00137700
&ZOPD(&ZOPN)          SETC  '++ INPUT BL1'                              00137800
.ZI6E                 ANOP  ,                                           00137900
.*-------------------------------------------------------------------   00138000
.***AI.L25  XCONNTKN XL4                                                00138100
                      AIF   ('&ZXCONNTKN' EQ '').ZI8E                   00138200
                      AIF   ('&ZXCONNTKN' EQ 'OUTPUT').ZI8E             00138300
&ZCGPR                SETC  '&XCONNTKN'                                 00138400
.*                                                                      00138500
                      AIF   ('&XCONNTKN'(1,1) NE '(').ZI8B              00138600
&ZCGPR   SETC  '0('.'&XCONNTKN'(2,K'&XCONNTKN-2).')'                    00138700
                      AGO   .ZI8D                                       00138800
.ZI8B                 ANOP  ,                                           00138900
.*                                                                      00139000
&ZC                   SETC  '&XCONNTKN'(1,1)                            00139100
                      AIF   (T'&ZC NE 'N').ZI8D                         00139200
&ZC                   SETC  '&XCONNTKN'                                 00139300
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI8D                00139400
&ZCGPR                SETC  '=BL4''&XCONNTKN'''                         00139500
.*                                                                      00139600
.ZI8D                 ANOP  ,                                           00139700
.*                                                                      00139800
&ZOPN                 SETA  &ZOPN+1                                     00139900
&ZOPL(&ZOPN)          SETC  ' '                                         00140000
&ZOPO(&ZOPN)          SETC  'MVC'                                       00140100
&ZOPV(&ZOPN)          SETC  '8(4,1),&ZCGPR'                             00140200
&ZOPD(&ZOPN)          SETC  '++ &ZXCONNTKN XCONNTKN'                    00140300
.*                                                                      00140400
.ZI8E                 ANOP  ,                                           00140500
.*-------------------------------------------------------------------   00140600
&ZOPN1                SETA &ZOPN                                        00140700
.*******************************************************************    00140800
.***  PROCESS IWMSCXIT OUTPUT PARAMETERS                                00140900
.*******************************************************************    00141000
                      ACTR  10000                                       00141100
.*******************************************************************    00141200
.***AO.X02  RETURN CODE PROCESSING                                      00141300
.*******************************************************************    00141400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00141500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00141600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00141700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00141800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00141900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00142000
&ZOPN                 SETA  &ZOPN+1                                     00142100
&ZOPL(&ZOPN)          SETC  ' '                                         00142200
&ZOPO(&ZOPN)          SETC  'ST'                                        00142300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00142400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00142500
                      AGO   .ZMRETX                                     00142600
.*                                                                      00142700
.ZMRET2               ANOP  ,                                           00142800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00142900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00143000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00143100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00143200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00143300
&ZOPN                 SETA  &ZOPN+1                                     00143400
&ZOPL(&ZOPN)          SETC  ' '                                         00143500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00143600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00143700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00143800
.ZMRETX               ANOP  ,                                           00143900
.*******************************************************************    00144000
.***AO.X03  REASON CODE PROCESSING                                      00144100
.*******************************************************************    00144200
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00144300
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00144400
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00144500
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00144600
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00144700
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00144800
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00144900
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00145000
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00145100
&ZOPN                 SETA  &ZOPN+1                                     00145200
&ZOPL(&ZOPN)          SETC  ' '                                         00145300
&ZOPO(&ZOPN)          SETC  'ST'                                        00145400
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00145500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00145600
                      AGO   .ZMRSNX                                     00145700
.*                                                                      00145800
.ZMRSN2               ANOP  ,                                           00145900
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00146000
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00146100
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00146200
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00146300
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00146400
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00146500
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00146600
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00146700
&ZOPN                 SETA  &ZOPN+1                                     00146800
&ZOPL(&ZOPN)          SETC  ' '                                         00146900
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00147000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00147100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00147200
.ZMRSNX               ANOP  ,                                           00147300
.*******************************************************************    00147400
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00147500
.*******************************************************************    00147600
.ZMFLX                ANOP  ,              MF=L PROCESSING              00147700
.*******************************************************************    00147800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00147900
.*******************************************************************    00148000
                      ACTR  10000                                       00148100
.*                                                                      00148200
                      LCLA  &ZLC           COLUMN POSITION              00148300
                      LCLA  &ZOPX          LIST TABLE INDEX             00148400
                      LCLC  &ZLL           LABEL                        00148500
                      LCLC  &ZLOP          OPERATION                    00148600
                      LCLC  &ZLV           VALUE                        00148700
                      LCLC  &ZLD           DESCRIPTION                  00148800
.*                                                                      00148900
&ZOPX                 SETA  1                                           00149000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00149100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00149200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00149300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00149400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00149500
                      AIF   (K'&ZLD LE 0).ZL5                           00149600
&ZLC                  SETA  35             COMMENT COLUMN               00149700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00149800
&ZLC                  SETA  33             COMMMENT COLUMN              00149900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00150000
&ZLV                  SETC  '&ZLV'.' '                                  00150100
                      AGO   .ZL3                                        00150200
.*                                                                      00150300
.ZL4                  ANOP  ,                                           00150400
&ZLV                  SETC  '&ZLV'.' '                                  00150500
.ZL5                  ANOP  ,                                           00150600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00150700
.*                                                                      00150800
&ZLL     &ZLOP &ZLV                                                     00150900
.*                                                                      00151000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00151100
                      AGO   .ZL2           CONTINUE LOOPING             00151200
.*                                                                      00151300
.ZLX                  ANOP  ,                                           00151400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00151500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00151600
         LCLC  &T1                                                      00151700
         AIF   ('&XACTION' EQ 'NULL').FINIS                             00151800
.*                                                                      00151900
&T1      SETC  '&XSRV_MGR_EXIT@'                                        00152000
         AIF   ('&XSRV_MGR_EXIT@'(1,1) NE '(').EXITRS                   00152100
&T1      SETC  '&XSRV_MGR_EXIT@'(2,K'&XSRV_MGR_EXIT@-2)                 00152200
&T1      SETC  '0(&T1.)'                                                00152300
.EXITRS    ANOP                                                         00152400
.* &T1 IS NOW A VALID BASE-DISPLACEMENT EXPRESSION                      00152500
.*                                                                      00152600
         L     15,&T1                  FETCH CONNECT EXIT @             00152700
         BALR  14,15                                                    00152800
.*                                                                      00152900
.FINIS   ANOP                                                           00153000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00153100
.ZLINKX               ANOP  ,                                           00153200
.*******************************************************************    00153300
.***AP.X01  FORMAT GENERATED CODE                                       00153400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00153500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00153600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00153700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00153800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00153900
                      AIF   (K'&ZLD LE 0).ZL51                          00154000
&ZLC                  SETA  35             COMMENT COLUMN               00154100
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00154200
&ZLC                  SETA  33             COMMENT COLUMN               00154300
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00154400
&ZLV                  SETC  '&ZLV'.' '                                  00154500
                      AGO   .ZL31                                       00154600
.*                                                                      00154700
.ZL41                 ANOP  ,                                           00154800
&ZLV                  SETC  '&ZLV'.' '                                  00154900
.ZL51                 ANOP  ,                                           00155000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00155100
.*                                                                      00155200
&ZLL     &ZLOP &ZLV                                                     00155300
.*                                                                      00155400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00155500
                      AGO   .ZL21          CONTINUE LOOPING             00155600
.*                                                                      00155700
.ZLX1                 ANOP  ,                                           00155800
.*******************************************************************    00155900
.***AP.X02  END OF IWMSCXIT MACRO                                       00156000
