.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00049600
.********************************************************************   00049700
                   GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL            00049800
                   GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE     00049900
                   GBLA  &SYSALVL    SYSSTATE ARCHLVL                   00050000
                   GBLC  &SYSAM64    SYSSTATE AMODE64                   00050100
.*                                                                      00050200
                   AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED     00050300
                   AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED      00050400
                   SPLEVEL  TEST                                        00050500
.ZSYS2             ANOP  ,                                              00050600
                   AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1          00050700
                   AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1          00050800
                   SYSSTATE TEST                                        00050900
                   AGO   .ZSYS4                                         00051000
.ZSYS3             ANOP  ,                                              00051100
&SYSASCE           SETC  'P'         ASSUME SYSSTATE PRIMARY            00051200
.ZSYS4             ANOP  ,                                              00051300
                   AIF   ('&SYSAM64' NE 'YES').ZSYSN64                  00051400
&ZMAC_LLGT         SETC 'LLGT'                                          00051500
&ZMAC_LGR          SETC 'LGR'                                           00051600
&ZMAC_LG           SETC 'LG'                                            00051700
&ZMAC_STG          SETC 'STG'                                           00051800
.ZSYSN64           ANOP  ,                                              00051900
.*******************************************************************    00052000
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00052100
.*******************************************************************    00052200
                   LCLC  &XCONNTKN   ++ NAME                            00052300
                   LCLC  &XSRV_MGR_EXIT@        ++ NAME                 00052400
                   LCLC  &XACTION    ++ VALUE                           00052500
                   LCLC  &XRETCODE   ++ NAME                            00052600
                   LCLC  &XRSNCODE   ++ NAME                            00052700
                   LCLA  &XPLISTVER  ++ VALUE                           00052800
                   LCLC  &XMF        ++ VALUE                           00052900
                   LCLC  &XMFCTRL    ++ NAME                            00053000
                   LCLC  &XMFATTR    ++ VALUE                           00053100
                   LCLC  &XMFSCK     ++ VALUE                           00053200
.*******************************************************************    00053300
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00053400
.*******************************************************************    00053500
                   LCLC  &ZXCONNTKN  ++ INPUT NAME                      00053600
                   LCLC  &ZXSRV_MGR_EXIT@       ++ INPUT NAME           00053700
                   LCLC  &ZXACTION   ++ INPUT VALUE                     00053800
                   LCLC  &ZXRETCODE  ++ OUTPUT NAME                     00053900
                   LCLC  &ZXRSNCODE  ++ OUTPUT NAME                     00054000
                   LCLC  &ZXPLISTVER            ++ INPUT VALUE          00054100
                   LCLC  &ZXMF       ++ INPUT VALUE                     00054200
                   LCLC  &ZXMFCTRL   ++ INPUT NAME                      00054300
                   LCLC  &ZXMFATTR   ++ INPUT VALUE                     00054400
                   LCLC  &ZXMFSCK    ++ INPUT VALUE                     00054500
.********************************************************************   00054600
.***AC.PLV01  PROCESS PLISTVER                                      *   00054700
.********************************************************************   00054800
&XPLISTVER      SETA  0                                                 00054900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00055000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00055100
&XPLISTVER      SETA  0                                                 00055200
.ZPLV0          ANOP  ,                                                 00055300
.*                                                                      00055400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00055500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00055600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00055700
.*-------------------------------------------------------------------   00055800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00055900
&ZMS            SETC  '"PLISTVER='                                      00056000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00056100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00056200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00056300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056400
                AGO   .ZPVCK2                                           00056500
.ZPVCK3         ANOP  ,                                                 00056600
.*-------------------------------------------------------------------   00056700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00056800
&ZMS            SETC  '"PLISTVER='                                      00056900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00057100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00057200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057300
                AGO   .ZPVCKE                                           00057400
.ZPVCK2         ANOP  ,                                                 00057500
.*-------------------------------------------------------------------   00057600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00057700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00057800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00057900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00058000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00058100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00058200
.ZPVCK6         ANOP  ,                                                 00058300
&ZMS            SETC  '"PLISTVER='                                      00058400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00058500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00058600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00058700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00058800
&XPLISTVER      SETA  0                                                 00058900
                AGO   .ZPVCKE                                           00059000
.*-------------------------------------------------------------------   00059100
.ZPVCK4A        ANOP  ,                                                 00059200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00059300
&ZMS            SETC  'PLISTVER='                                       00059400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00059500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00059600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00059700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00059800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059900
                AGO   .ZPVCK5                                           00060000
.ZPVCK4         ANOP  ,                                                 00060100
&XPLISTVER      SETA  &PLISTVER(1)                                      00060200
.ZPVCK5         ANOP  ,                                                 00060300
.ZPVCKE         ANOP  ,                                                 00060400
.********************************************************************   00060500
