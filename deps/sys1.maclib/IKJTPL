*         Z TPLIECB  PTR(31),        /* PTR TO TMP STAI ECB          */ 04750000
*         Z TPLLEND  CHAR(1),        /* HIGH ORDER BIT ON            */ 04800000
*         Z TPLAECB  PTR(24),        /* PTR TO TMP ATTN ECB          */ 04850000
* %       IF TPLOPT1='CONTINUE'                                         04900000
* %         THEN                                                        04950000
