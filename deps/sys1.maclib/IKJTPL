*       Y TPLTPLE  PTR(31);          /* TPL EXTENT ADDRESS   @E2115B7*/ 05350000
*                                                                       05400000
* %TPLBSL6: /* DEACTIVATE THE COMPILE TIME VARIABLES           */ ;     05450000
* %       DEACTIVATE X,Y,Z;                                             05500000
