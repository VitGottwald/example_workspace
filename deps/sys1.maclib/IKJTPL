* DECLARE                                                               03700000
*   1 TPL BASED(TPLPTR),                                                03750000
* %       GOTO TPLBSL3;                                                 03800000
* %TPLBSL2: /* CONTINUE A BSL STRUCTURE USING LEVEL SPECIFIED  */ ;     03850000
* %       Y=X+1                /* SET SECOND LEVEL ONE HIGHER  */ ;     03900000
* %       Z=X+2                /* SET THIRD LEVEL TWO HIGHER   */ ;     03950000
*     X TPL,                                                            04000000
* %TPLBSL3: /* THIS IS THE MAIN TPL STRUCTURE                  */ ;     04050000
*       Y TPLCBUF  PTR(31),          /* PTR TO COMMAND BUFFER        */ 04100000
*       Y TPLUPT   PTR(31),          /* PTR TO UPT                   */ 04150000
*       Y TPLPSCB  PTR(31),          /* PTR TO PSCB                  */ 04200000
