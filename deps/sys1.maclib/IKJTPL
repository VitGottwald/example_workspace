         MEND                                                           01350000
* %TPLBSL:                                                              01400000
* /* THE TPL MAPPING CAN BE PLACED AT THE BEGINNING, MIDDLE,   */       01450000
* /* OR END OF ANY BSL STRUCTURE THROUGH THE USE OF THE        */       01500000
* /* COMPILE TIME OPTIONS 'TPLSLEV' AND/OR 'TPLOPT1'.          */       01550000
* /*   TPLSLEV - THIS IS THE CHARACTER ('2' TO '7') TO BE      */       01600000
* /*             ASSIGNED TO THE HIGHEST LEVEL IF THE TPL IS   */       01650000
* /*             NOT TO BE LOCATED AT THE BEGINNING OF THE     */       01700000
* /*             STRUCTURE.                                    */       01750000
* /*   TPLOPT1 - IF A CHARACTER STRING 'CONTINUE' IS ASSIGNED  */       01800000
* /*             TO THIS OPTION, A SEMI-COLON IS NOT PLACED AT */       01850000
* /*             THE END OF THE STRUCTURE SO THAT ADDITIONAL   */       01900000
* /*             STATEMENTS MAY BE ADDED TO THE STRUCTURE.     */       01950000
* /* IF NEITHER OPTION IS SPECIFIED, THE TPL MAPPING WILL      */       02000000
* /* DEFAULT TO A STRUCTURE OF ITS OWN.                        */ ;     02050000
* %       DECLARE (X,Y,Z) FIXED      /* COMPILE TIME VARIABLES */ ;     02100000
* %       IF TPLSLEV<'2'                                                02107100
* %         THEN                                                        02114200
* %           GOTO TPLBSL1;    /* SET LEVEL NUMBER TO 1        */       02121300
* %       IF TPLSLEV>'7'                                                02128400
* %         THEN                                                        02135500
* %           GOTO TPLBSL1;    /* SET LEVEL NUMBER TO 1        */       02142600
* %       IF TPLSLEV='2'                                                02150000
* %         THEN                                                        02200000
* %           X=2              /* SET FIRST LEVEL AS SPECIFIED */ ;     02250000
* %       IF TPLSLEV='3'                                                02350000
* %         THEN                                                        02400000
* %           X=3              /* SET FIRST LEVEL AS SPECIFIED */ ;     02450000
* %       IF TPLSLEV='4'                                                02550000
* %         THEN                                                        02600000
* %           X=4              /* SET FIRST LEVEL AS SPECIFIED */ ;     02650000
* %       IF TPLSLEV='5'                                                02750000
* %         THEN                                                        02800000
* %           X=5              /* SET FIRST LEVEL AS SPECIFIED */ ;     02850000
* %       IF TPLSLEV='6'                                                02950000
* %         THEN                                                        03000000
* %           X=6              /* SET FIRST LEVEL AS SPECIFIED */ ;     03050000
* %       IF TPLSLEV='7'                                                03150000
* %         THEN                                                        03200000
* %           X=7              /* SET FIRST LEVEL AS SPECIFIED */ ;     03250000
* %           GOTO TPLBSL2;                                             03300000
* %TPLBSL1: /* BEGIN A BSL STRUCTURE - 'TPLSLEV' NOT SPECIFIED */ ;     03350000
* %       Y=2                      /* USE THESE DEFAULT VALUES */ ;     03400000
* %       Z=3                      /* FOR THE LEVEL NUMBERS    */ ;     03450000
* /*******************************************************************/ 03500000
* /* TEST PARAMETER LIST (TPL) IS GENERATED BY THE TMP FOR USE       */ 03550000
* /* WITH TEST PROGRAMS.                                             */ 03600000
* /*                                                                 */ 03603800
