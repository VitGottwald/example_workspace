                      LCLC  &ZLD           DESCRIPTION                  00277800
.*                                                                      00277900
&ZOPX                 SETA  1                                           00278000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00278100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00278200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00278300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00278400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00278500
                      AIF   (K'&ZLD LE 0).ZL5                           00278600
&ZLC                  SETA  35             COMMENT COLUMN               00278700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00278800
&ZLC                  SETA  33             COMMMENT COLUMN              00278900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00279000
&ZLV                  SETC  '&ZLV'.' '                                  00279100
                      AGO   .ZL3                                        00279200
.*                                                                      00279300
.ZL4                  ANOP  ,                                           00279400
&ZLV                  SETC  '&ZLV'.' '                                  00279500
.ZL5                  ANOP  ,                                           00279600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00279700
.*                                                                      00279800
&ZLL     &ZLOP &ZLV                                                     00279900
.*                                                                      00280000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00280100
                      AGO   .ZL2           CONTINUE LOOPING             00280200
.*                                                                      00280300
.ZLX                  ANOP  ,                                           00280400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00280500
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00280600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00280700
         L     14,16(0,0)              GET CVT ADDRESS                  00280800
         L     14,772(14,0)            GET SFT ADDRESS                  00280900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00281000
         LA    15,62                   GET EX FOR SERVICE ROUTINE       00281100
         OR    14,15                   GET PC NUMBER                    00281200
         PC    0(14)                                                    00281300
END&SYSNDX DS    0H                    END LABEL                        00281400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00281500
.ZLINKX               ANOP  ,                                           00281600
.*******************************************************************    00281700
.***AP.X01  FORMAT GENERATED CODE                                       00281800
