.***AO.X02  RETURN CODE PROCESSING                                      00270300
.*******************************************************************    00270400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00270500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00270600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00270700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00270800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00270900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00271000
&ZOPN                 SETA  &ZOPN+1                                     00271100
&ZOPL(&ZOPN)          SETC  ' '                                         00271200
&ZOPO(&ZOPN)          SETC  'ST'                                        00271300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00271400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00271500
                      AGO   .ZMRETX                                     00271600
.*                                                                      00271700
.ZMRET2               ANOP  ,                                           00271800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00271900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00272000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00272100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00272200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00272300
&ZOPN                 SETA  &ZOPN+1                                     00272400
&ZOPL(&ZOPN)          SETC  ' '                                         00272500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00272600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00272700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00272800
.ZMRETX               ANOP  ,                                           00272900
