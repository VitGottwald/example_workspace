* %TSVTPLS  :  ;                                                        03570000
* /* MAPPING OF TSO VECTOR TABLE                                     */ 03580000
* DECLARE                                                               03590000
*   1 TSVT BASED(TSVTPTR) BDY(DWORD),         /*           @E2267F2 */  03600000
*                                                                       03610000
*    2 TSVTTSVT CHAR(4),          /* ACRONYM IN EBCDIC "TSVT"        */ 03620000
*    2 TSVTLEV CHAR(1),           /* TSVT VERSION                    */ 03630000
*    2 TSVTFLG1 CHAR(1),          /* Flag Indicators                    03640000
*                                    Note: Several modules map these    03650000
*                                    bits locally and depend on their   03660000
*                                    current order and location  @L4A*/ 03670000
*      3 TSVTNCTU BIT(1),         /* Instorage copy of system notices   03680000
*                                    needs to be updated     @DG10015*/ 03690000
*      3 TSVTNETL BIT(1),         /* None of the TSO/E Exits            03700000
*                                    were found in LPA/ELPA  @E23D2D5*/ 03710000
*      3 TSVTUPDP BIT(1),         /* IKJBCMSG posted for parmlib update 03720000
