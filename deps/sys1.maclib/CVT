*              END OF CVT                                                       
         POP   PRINT                                                            
         MEND  , */                                                             
* %CVTL2  :  ;                                                                  
*%IF IKEPLS=''  %THEN                                                           
*    %GOTO CVTL3;                                                               
*%IHALPUSH='@LISTPUSH';                                                         
*%IHALPOP='@LISTPOP';                                                           
*%IHALOFF='@LISTOFF';                                                           
*IHALPUSH;                                                                      
*%IF IHALIST='YES'  %THEN                                                       
*    %GOTO CVTL3;                                                               
*IHALOFF;                                                                       
*%CVTL3:;                                                                       
* %DCL ASAXMAC_READONLY CHAR EXT;                          /*  061215*/         
* %ASAXMAC_READONLY = '';                                  /*  061215*/         
* %DCL ASAXMAC_BYPASS CHAR EXT;                            /*  061215*/         
* %IF ASAXMAC_BYPASS ^= '' %THEN %GOTO CVTPLS99;           /*  061215*/         
* %IF SYSVERSION = '' %THEN %GOTO CVTPLS99;                /*  061215*/         
* %IF SYSRULESMOD='PL/S-III ' %THEN %GOTO CVTPLS99;        /*  061215*/         
* ?ASAXMAC ISITPLX(2,4,0);                                 /*  061215*/         
* %DCL ASAXMAC_ISITPLX CHAR EXT;                           /*  061215*/         
* %IF ASAXMAC_ISITPLX ^= 'YES' %THEN %GOTO CVTPLS99;       /*  061215*/         
* ?ASAXMAC SetEsameInfo;                                   /*  031027*/         
*%CVTPLS99:;                                               /*  061215*/         
* /*                   CVT PROPER                                    */         
*                                                                               
* DECLARE                                                                       
*   1 CVTMAP   BASED(CVTPTR) BDY(DWORD),    /*      @(DCR046)    @L6C*/         
*    2 CVT      CHAR(0) BDY(WORD),   /* BEGINNING OF CVT             */         
*    2 CVTTCBP PTR ASAXMAC_READONLY, /* Address of PSATNEW.      @PTC*/         
*    2 CVT0EF00 PTR ASAXMAC_READONLY, /* ADDRESS OF ROUTINE TO SCHEDULE         
