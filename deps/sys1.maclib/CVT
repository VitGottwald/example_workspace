*%IF SYSVERSION = '' %THEN           /*                          @O3A*/         
*    %GOTO CVTPLS23G;                /*                          @O3A*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN   /*                          @O3A*/         
*    %GOTO CVTPLS23G;                /*                          @O3A*/         
*         11 CVTzOS_020400 BIT(1),   /* z/OS V2R4 functions      @O3A*/         
*           13 CVTzOS_V2R4 BIT(1),   /* z/OS V2R4 functions      @O3A*/         
*%CVTPLS23G:;                        /*                          @O3A*/         
*       5 CVTH77D0    BIT(1),        /* HBB77D0   functions      @O6A*/         
*%IF SYSVERSION = '' %THEN           /*                          @O6A*/         
*    %GOTO CVTPLS23H;                /*                          @O6A*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN   /*                          @O6A*/         
*    %GOTO CVTPLS23H;                /*                          @O6A*/         
*         11 CVTzOS_020500 BIT(1),   /* z/OS V2R5 functions      @O6A*/         
*           13 CVTzOS_V2R5 BIT(1),   /* z/OS V2R5 functions      @O6A*/         
*%CVTPLS23H:;                        /*                          @O6A*/         
*                                                                               
*      3  CVTOSLV7  CHAR(1),         /* BYTE 7 OF CVTOSLVL.      @LGA*/         
*      3  CVTOSLV8  CHAR(1),         /* BYTE 8 OF CVTOSLVL.      @LGA*/         
*       5 CVTPAUS3    BIT(1),        /* IEA4xxxx                 @N9A*/         
*       5 CVTPAUS4    BIT(1),        /* Pause with checkpoint-OK @0MA*/         
*       5 CVTECT1     BIT(1),        /* TIMEUSED ECT=YES with                   
*                                       TIME_ON_CP, OFFLOAD_TIME,               
*                                       OFFLOAD_ON_CP            @NSA*/         
*       5 CVTOOCP     BIT(1),        /* TIMEUSED with TIME_ON_CP                
*                                       and OFFLOAD_ON_CP        @NSA*/         
*%IF SYSVERSION = '' %THEN           /*                          @NTA*/         
*    %GOTO CVTPLS22G;                /*                          @NTA*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN   /*                          @NTA*/         
*    %GOTO CVTPLS22G;                /*                          @NTA*/         
*       5 CVTIEFOPZ   BIT(1),        /* IEFOPZ                   @NZA*/         
*       5 CVTBoost    BIT(1),        /* Support for BOOST system                
*                                       parameter is available   @0PA*/         
*%CVTPLS22G:;                        /*                          @NTA*/         
*      3  CVTOSLV9  CHAR(1),         /* BYTE 9 OF CVTOSLVL.      @LGA*/         
*      3  CVTOSLVA  CHAR(1),         /* BYTE 10 OF CVTOSLVL.     @LGA*/         
*      3  CVTOSLVB  CHAR(1),         /* BYTE 11 OF CVTOSLVL.     @LGA*/         
*      3  CVTOSLVC  CHAR(1),         /* BYTE 12 OF CVTOSLVL.     @LGA*/         
*      3  CVTOSLVD  CHAR(1),         /* BYTE 13 OF CVTOSLVL.     @LGA*/         
*      3  CVTOSLVE  CHAR(1),         /* BYTE 14 OF CVTOSLVL.     @LGA*/         
*      3  CVTOSLVF  CHAR(1);         /* BYTE 15 OF CVTOSLVL.     @LGA*/         
* /*                                                                 */         
* /*          OS/VS2 - VIRTUAL STORAGE ADDRESS EXTENSION             */         
* /*                                                                 */         
* /*          ADDRESS OF EXTENSION IS IN CVTSMEXT                    */         
* /*          NOTE- ALL STARTING ADDRESSES POINT TO FIRST BYTE IN    */         
* /*                STORAGE AREA.  ALL ENDING ADDRESSES POINT TO     */         
* /*                LAST BYTE WITHIN STORAGE AREA (NOT BEYOND        */         
* /*                STORAGE AREA).                                   */         
* /*                                                                 */         
* /*                                                                 */         
*                                                                               
* DECLARE                                                                       
*   1  CVTVSTGX  BASED(CVTSMEXT) BDY(DWORD),                  /* @0AC*/         
*      3  CVTBLDLS  PTR(31),         /* RESERVED - WAS STARTING                 
*                                       ADDRESS OF BLDL TABLE.  MUST            
*                                       BE ZERO NOW.             @P1C*/         
*      3  CVTBLDLE  PTR(31),         /* RESERVED - WAS ENDING                   
*                                       ADDRESS OF BLDL TABLE.  MUST            
*                                       BE ZERO NOW.             @P1C*/         
*      3  CVTMLPAS  PTR(31),         /* STARTING VIRTUAL     @G860PVB           
*                                       ADDRESS OF MLPA.     @G860PVB*/         
*      3  CVTMLPAE  PTR(31),         /* ENDING VIRTUAL       @G860PVB           
*                                       ADDRESS OF MLPA.     @G860PVB*/         
*      3  CVTFLPAS  PTR(31),         /* STARTING VIRTUAL     @G860PVB           
*                                       ADDRESS OF FLPA.     @G860PVB*/         
*      3  CVTFLPAE  PTR(31),         /* ENDING VIRTUAL       @G860PVB           
*                                       ADDRESS OF FLPA.     @G860PVB*/         
*      3  CVTPLPAS  PTR(31),         /* STARTING VIRTUAL     @G860PVB           
*                                       ADDRESS OF PLPA.     @G860PVB*/         
*      3  CVTPLPAE  PTR(31),         /* ENDING VIRTUAL       @G860PVB           
*                                       ADDRESS OF PLPA.     @G860PVB*/         
*      3  CVTRWNS   PTR(31),         /* STARTING ADDRESS OF  @G860PVB           
*                                       READ/WRITE NUCLEUS.  @G860PVB*/         
*      3  CVTRWNE   PTR(31),         /* ENDING ADDRESS OF    @G860PVB           
*                                       READ/WRITE NUCLEUS.  @G860PVB*/         
*      3  CVTRONS   PTR(31),         /* STARTING ADDRESS OF  @G860PVB           
*                                       READ-ONLY NUCLEUS.   @G860PVB*/         
*      3  CVTRONE   PTR(31),         /* ENDING ADDRESS OF    @G860PVB           
*                                       READ-ONLY NUCLEUS.   @G860PVB*/         
*      3  CVTERWNS  PTR(31),         /* STARTING EXTENDED    @G860P46           
*                                       ADDRESS READ/WRITE   @G860P46           
*                                       NUCLEUS.  @(DCR658)  @G860P46*/         
*      3  CVTERWNE  PTR(31),         /* ENDING EXTENDED      @G860P46           
*                                       ADDRESS READ/WRITE   @G860P46           
*                                       NUCLEUS.  @(DCR658)  @G860P46*/         
*      3  CVTEPLPS  PTR(31),         /* Starting virtual address                
*                                       of extended PLPA. This is an            
*                                       interface for accessing the             
*                                       LPAT only.           @G860PVB*/         
*      3  CVTEPLPE  PTR(31),         /* ENDING VIRTUAL ADDRESS                  
*                                       OF EXTENDED PLPA.    @G860PVB*/         
*      3  CVTEFLPS  PTR(31),         /* STARTING VIRTUAL ADDRESS                
*                                       OF EXTENDED FLPA.    @G860PVB*/         
*      3  CVTEFLPE  PTR ASAXMAC_READONLY, /* ENDING VIRTUAL ADDRESS             
*                                       OF EXTENDED FLPA.    @G860PVB*/         
*      3  CVTEMLPS  PTR(31),         /* STARTING VIRTUAL ADDRESS                
*                                       OF EXTENDED MLPA.    @G860PVB*/         
*      3  CVTEMLPE  PTR(31);         /* ENDING VIRTUAL ADDRESS                  
*                                       OF EXTENDED MLPA.    @G860PVB*/         
* /*                                                                 */         
* /*           OS - OS/VS COMMON EXTENSION                           */         
* /*                                                                 */         
*                                                                               
* DECLARE                                                                       
*   1 CVTXTNT1 BASED(CVTEXT1) BDY(DWORD), /*START OF OS - OS/VS                 
*                                       COMMON EXTENSION, FORCED TO             
*                                       DOUBLEWORD BOUNDARY      @0AC*/         
*    2 CVTFACHN PTR(31),             /* ADDRESS OF CHAIN OF                     
*                                       DCB FIELD AREAS (ISAM).ICB421*/         
*    2 CVT1R004 CHAR(8);             /* RESERVED                 @LFC*/         
*                                                                               
* /*                                                                 */         
* /*                    OS/VS2 COMMON EXTENSION                      */         
* /*                                                                 */         
* /* THE FOLLOWING LINE ESTABLISHES THE HIGHEST DEGREE OF ALIGNMENT  */         
* /* REQUIRED IN THE EXTENSION, SO THAT THE BOUNDARY ALIGNMENT IS    */         
* /* NOT DETERMINED BY THE ENDING OFFSET OF THE PRECEDING SECTION.   */         
* /*                                                             @0AC*/         
* /*                                                                 */         
*                                                                               
* DECLARE                                                                       
*   1 CVTXTNT2 BASED(CVTEXT2) BDY(DWORD), /* START OF OS/VS2 EXTENSION,         
*                                       FORCED TO DOUBLEWORD BOUNDARY           
*                                                                @0AC*/         
*    2 CVT2R000 CHAR(4),             /* RESERVED                 @LFC*/         
*    2 CVTNUCLS CHAR(1),             /* IDENTIFICATION OF THE                   
*                                       NUCLEUS MEMBER NAME.   ICB322*/         
*    2 CVTFLGBT BIT(8),              /* Flag byte. This byte is an              
*                                       interface only for bits                 
*                                       CVTUNDzVM (CVTUNDVM), CVTzPDT,          
*                                       CVTUNDAltVM & CVTVMENV   @0TC*/         
*     3 CVTNPE   BIT(1),             /* INDICATES NON-PAGING                    
*                                       ENVIRONMENT (VM HANDSHAKING)            
*                                       (OS/VS1).  (MDC302)  @X04EP9A*/         
*     3 CVTVME   BIT(1),             /* INDICATES MACHINE IS                    
*                                       OPERATING IN VM ENVIRONMENT             
*                                       (OS/VS1).  (MDC303)  @X04EP9A*/         
*     3 CVTBAH   BIT(1),             /* INDICATES THAT THE VM/370 -             
*                                       OS/VS1 BTAM AUTOPOLL                    
*                                       HANDSHAKE IS OPERATIONAL                
*                                       (OS/VS1).  (MDC356)  @X60KL9A*/         
*     3 *        BIT(1),             /* CVTUNDZVM                @0TA*/         
*%IF SYSVERSION = '' %THEN %GOTO CVTPLSX05; /*                   @0TA*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN %GOTO CVTPLSX05; /*           @0TA*/         
*       5 CVTUNDzVM BIT(1),          /* Running under z/VM (This is not         
*                                       the same as running under               
*                                       VICOM). Bit is set to 0 when            
*                                       CVTUNDAltVM is 1.        @0TC*/         
*%CVTPLSX05:;                        /*                          @0TA*/         
*         7 CVTUNDVM BIT(1),         /* Same as CVTUNDzVM        @0TA*/         
*     3 CVTVICOM BIT(1),             /* Running under VICOM      @MXA*/         
*     3 CVTZPDT  BIT(1),             /* Running on zPDT (includes               
*                                       running on zD&T)         @O7A*/         
*     3 *        BIT(1),             /* CVTUNDALTVM              @0TA*/         
*%IF SYSVERSION = '' %THEN  %GOTO CVTPLSX06; /*                  @0TA*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN %GOTO CVTPLSX06; /*           @0TA*/         
*       5 CVTUNDAltVM BIT(1),        /* Running under an alternate              
*                                       virtual machine environment. To         
*                                       determine which alternate               
*                                       virtual machine, the STSI               
*                                       output will list the control            
*                                       program identifier in field             
*                                       virtual machine environment.            
*                                       To determine which alternate            
*                                       virtual machine, the STSI               
*                                       output will list the control            
*                                       program identifier in field             
*                                       SI22V3DBCPIdentifier mapped by          
*                                       CSRSIIDF and can be obtained by         
*                                       the CSRSI service.  Bit is set          
*                                       to 0 when CVTUNDzVM is 1 @0TA*/         
*%CVTPLSX06:;                        /*                          @0TA*/         
*     3 CVTVMENV BIT(1),             /* Running in a virtual machine            
*                                       environment.  Bit is set to 1           
*                                       when CVTUNDzVM or CVTUNDAltVM           
*                                       is set to 1.             @0TA*/         
*    2 CVTIOCID CHAR(2),             /* EBCDIC IDENTIFIER OF THE ACTIVE         
*                                       I/O CONFIGURATION SPECIFIED BY          
*                                       THE OPERATOR             @LBA*/         
*    2 CVTDEBVR PTR ASAXMAC_READONLY, /* ADDRESS OF BRANCH                      
*                                       ENTRY POINT OF DEB                      
*                                       VALIDITY CHECK                          
*                                       ROUTINE.     (ICB380)  XM9028*/         
*    2 CVTCVAF  PTR(31),             /* POINTER TO THE CVAF                     
*                                       TABLE WHICH CONTAINS                    
*                                       THE CVAF BRANCH ENTRY                   
*                                       ADDRESS AND NEXT VIB                    
*                                       ADDRESS.   (MDC410)  @ZABDS9A*/         
*    2 CVTMMVT  PTR ASAXMAC_READONLY, /* ADDRESS OF THE MEDIA                   
*                                       MANAGER VECTOR                          
*                                       TABLE.     (MDC410)  @ZABDS9A*/         
*    2 CVTNCVP  PTR(31),             /* ADDRESS OF CSA BUFFER POOL -            
*                                       USED BY NETWORK MANAGEMENT              
*                                       FACILITY (NMF)           @H2A*/         
*    2 CVTQID   PTR(31),             /* SAVE AS CVTQIDA BELOW. ICB381*/         
*     3 *        CHAR(1),            /* RESERVED - FIRST BYTE OF                
*                                       CVTQID                   @LFC*/         
*     3 CVTQIDA  PTR(24),            /* ADDRESS OF QUEUE                        
*                                       IDENTIFICATION (QID)                    
*                                       TABLE PREFIX.          ICB381*/         
*    2 CVTOLTEP PTR(31),             /* POINTER TO CONTROL BLOCK                
*                                       CREATED BY SVC 59 TO POINT TO           
*                                       PSEUDO-DEB'S           ICB423*/         
*    2 CVT2R020 CHAR(4),             /* RESERVED                 @LFC*/         
*    2 CVTAVVT  PTR(31),             /* ADDRESS OF AVM CONTROL BLOCK            
*                                       OWNERSHIP: AVM                          
*                                       SERIALIZATION: CS        @LAA*/         
*     3 CVTAVIN BIT(1),              /* INDICATES AVM INSTALLED  @LAA*/         
*    2 CVTCCVT  PTR(31),             /* ADDRESS OF CRYPTOGRAPHIC                
*                                       FACILITY CVT.                           
*                                                  (MDC370)  @ZA27771*/         
*    2 CVTSKTA  PTR(31),             /* ADDRESS OF STORAGE KEY                  
*                                       TABLE (VM HANDSHAKING)                  
*                                       (OS/VS1).  (MDC304)  @X04EP9A*/         
*    2 CVTICB   PTR(31),             /* ADDRESS OF MASS STORAGE                 
*                                       SYSTEM (MSS) CONTROL                    
*                                       BLOCK.     (MDC305)  @Z30LP9A*/         
*    2 CVTFBYT1 CHAR(1),             /* FLAG BYTE                    */         
*     3 CVTRDE  BIT(1),              /* RELIABILITY DATA EXTRACTOR              
*                                       OWNERSHIP: DFP.                         
*                                       SERIALIZATION: NONE.     @D7A*/         
*     3 *       BIT(7),              /* RESERVED                 @LFC*/         
*    2 CVT2R035 CHAR(3),             /* RESERVED                 @LFC*/         
*      /* CVTLDTO contains the offset value needed to adjust the TOD */         
*      /* value to the local date and time of day.  It is similar to */         
*      /* CVTTZ except that CVTLDTO is a doubleword value. CVTLDTOL  */         
*      /* and CVTTZ contain the same value.                      @Q6C*/         
*    2 CVTLDTO BIT(64) BDY(DWORD),   /* LOCAL TIME/DATE OFFSET   @08C*/         
*      3 CVTLDTOL FIXED(31),         /* HIGH WORD                @08A*/         
*      3 CVTLDTOR FIXED(32),         /* LOW WORD                 @08A*/         
*    2 CVTATCVT PTR(31),             /* POINTER TO VTAM'S CVT  MDC080*/         
*     3 CVTATACT BIT(1),             /* IF ON, VTAM IS ACTIVE  MDC081*/         
*    2 CVT2R044 CHAR(4),             /* RESERVED                 @LFC*/         
*    2 CVTBCLMT FIXED(31),           /* NUMBER OF 130-BYTE RECORDS              
*                                       SET ASIDE FOR BROADCAST                 
*                                       MESSAGES                 @LBA*/         
*    2 CVT2R04C CHAR(4),             /* RESERVED                 @0BA*/         
*    2 CVTLSO   BIT(64) BDY(DWORD),  /* LEAP SECOND VALUE IN TOD                
*                                       FORMAT                   @0BA*/         
*      3 CVTLSOH  BIT(32),           /* HIGH WORD                @0BA*/         
*      3 CVTLSOL  BIT(32),           /* LOW WORD                 @0BA*/         
*    2 CVT2R058 CHAR(44);            /* RESERVED                 @0BC*/         
*                                                                               
* /*                      CVT PREFIX                                 */         
*                                                                               
*    DCL 1 CVTFIX BASED(CVTPTR-256) BDY(DWORD), /* REDEFINED TO  @L6C           
*                                                  MAKE THE CVT  @L6C           
*                                                  PREFIX AND THE@L6C           
*                                                  CVT ALIGN ON A@L6C           
*                                                  DOUBLE WORD   @L6C           
*                                                  BOUNDARY, FOR @L6C           
*                                                  ARCHITECTURE. @L6C           
*                                                     @(DCR046)  @L6C*/         
*    2 *        CHAR(216) BDY(WORD), /* RESERVED                 @D2A*/         
*    2 CVTPROD  CHAR(16) BDY(WORD),  /* SYSTEM CONTROL           @D2A           
*                                       PROGRAM PRODUCT          @D2A           
*                                       LEVEL.                   @D2A*/         
*     3 CVTPRODN CHAR(8) BDY(WORD),  /* PRODUCT NAME OF THE                     
*                                       CONTROL PROGRAM,                        
*                                       EX.(SP3.2).                             
*                                This is maintained for compatibility           
*                                reasons only. The true product name            
*                                version, release, and modification             
*                                level information is in the ECVT               
*                                (ECVTPNAM, PVER, PREL, PMOD).   @M9A*/         
*     3 CVTPRODI CHAR(8) BDY(WORD),  /* PRODUCT FMID IDENT-      @D2A           
*                                       IFIER FOR THE            @D2A           
*                                       CONTROL PROGRAM,         @D2A           
*                                       EX.(JBB1328).            @D2A*/         
*    2 CVTVERID CHAR(16) BDY(WORD),  /* OPTIONAL USER PERSONALIZATION           
*                                       OF SOFTWARE SYSTEM VERSION.             
*                                                  (MDC415)  @G815P9A*/         
*    2 *        CHAR(2) BDY(WORD),   /* RESERVED                     */         
*    2 CVTMDL   CHAR(2) BDY(WORD,3), /* CPU NUMBER IN SIGNLESS PACKED           
*                                       DECIMAL, I.E., A 3090 PROCESSOR         
*                                       WOULD APPEAR AS X'3090'  @LQC*/         
*    2 CVTRELNO CHAR(4) BDY(WORD),   /* RELEASE NUMBER (EBCDIC)      */         
*     3 CVTNUMB  CHAR(2) BDY(WORD),  /* RELEASE NUMBER               */         
*     3 CVTLEVL  CHAR(2) BDY(WORD,3);/* LEVEL NUMBER OF THIS RELEASE */         
*                                                                               
* /*                   END OF CVT MACRO                              */         
*                                                                               
*%IF IKEPLS=''  %THEN                                                           
*    %GOTO CVTL4;                                                               
*IHALPOP;                                                                       
*%IF SYSVERSION = '' %THEN            /* Do GEN if pre-PL/AS     @PHA*/         
*    %GOTO CVTL4A;                    /*                         @PHA*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN    /* Do GEN if pre-PL/AS     @PHA*/         
*    %GOTO CVTL4A;                    /*                         @PHA*/         
*%IF SYSVERSION(1:5) ^= 'PL/AS' %THEN /* Skip GEN if post-PL/AS  @PHA*/         
*    %GOTO CVTL4;                     /*                         @PHA*/         
*%CVTL4A:;                            /*                         @PHA*/         
*  GEN REFS(CVTLSCH);                                                           
*@ENDGEN;                            /* (MDC364)             @G50EP9A*/         
*%CVTL4 : ;                                                                     
