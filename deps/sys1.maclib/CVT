*                                                                               
*        %GOTO CVTL1;       /* BILINGUAL MAPPING                                
         MACRO                                                                  
&CVTNAME CVT   &PREFIX=NO,&DSECT=NO,&LIST=NO,                          *        
               &SYS=,&OPTIONS=,&TSO=,                                  *        
               &FLAG=,&FLAGB=,&PATCH=,&REAL=,                          *        
               &RPS=,&CVTTZ=,&CTLFLGS=,                                *        
               &LOADBAL=,&CBMRTN=,&WARN=,&STORAGE=               @LFC*/         
*%CVTL1 : ;                                                                     
