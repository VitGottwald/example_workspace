*           13 CVTzOS_V1R4 BIT(1),   /* z/OS V1R4 functions      @MUA*/         
*%CVTPLS14E:;                        /*                          @MUP*/         
*       5 CVTH7708    BIT(1),        /* HBB7708   functions      @MVA*/         
*%IF SYSVERSION = '' %THEN           /*                          @MSP*/         
*    %GOTO CVTPLS15E;                /*                          @MSP*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN   /*                          @MSP*/         
*    %GOTO CVTPLS15E;                /*                          @MSP*/         
*         11 CVTzOS_010500 BIT(1),   /* z/OS V1R5 functions      @MSA*/         
*           13 CVTzOS_V1R5 BIT(1),   /* z/OS V1R5 functions      @MSA*/         
*%CVTPLS15E:;                        /*                          @MSP*/         
*       5 CVTH7709    BIT(1),        /* HBB7709   functions      @MWA*/         
*%IF SYSVERSION = '' %THEN           /*                          @MWP*/         
*    %GOTO CVTPLS16E;                /*                          @MWP*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN   /*                          @MWP*/         
*    %GOTO CVTPLS16E;                /*                          @MWP*/         
*         11 CVTzOS_010600 BIT(1),   /* z/OS V1R6 functions      @MWA*/         
*           13 CVTzOS_V1R6 BIT(1),   /* z/OS V1R6 functions      @MWA*/         
*%CVTPLS16E:;                        /*                          @MWP*/         
*      3  CVTOSLV4  CHAR(1),         /* BYTE 4 OF CVTOSLVL.      @LGA*/         
*        5 CVTCSRSI BIT(1),          /* CSRSI service is available              
*                                                                @0EA*/         
*        5 CVTUNICS BIT(1),          /* Unicode callable services               
*                                       are available            @MLA*/         
*        5 CVTCSRUN BIT(1),          /* CSRUNIC callable service                
*                                       is available             @MLA*/         
*       5   CVTILM    BIT(1),        /* IBM License Manager functions           
*                                       are present              @PYM*/         
*       5   CVTALRS   BIT(1),        /* ASN-and-LX-Reuse architecture           
*                                       is supported. It might not be           
*                                       enabled. See CVTALR      @MXA*/         
*       5 CVTTOCP BIT(1),            /* TIMEUSED ON_CP           @0KA*/         
