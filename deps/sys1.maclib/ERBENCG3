**/ ;                                                                           
* %ERBENCG3_INCLUDED = 'YES';                                                   
* %DCL ZCBPRINT CHAR EXT;                                                       
* %DEACTIVATE ZCBPRINT;                                                         
* %DCL ERBENCG3_LIST CHAR EXT;                                                  
* %DEACTIVATE ERBENCG3_LIST;                                                    
* %IF ERBENCG3_LIST = 'NO' !                                                    
*    ZCBPRINT = 'NO' %THEN                                                      
* %DO;                                                                          
*   @LIST PUSH NOECHO;                                                          
*   @LIST NOASSEMBLE NOECHO;                                                    
*   @LIST OFF C NOECHO;                                                         
* %END;                                                                         
*/* Start of PL/X Source                                             */         
*/*                                                                  */         
*/********************************************************************/         
*/*                                                                  */         
*/* RMF Enclave Data Table                                           */         
*/*                                                                  */         
*/********************************************************************/         
*                                                                               
*   Declare                         /* ENCG3 table structure         */         
*                                                                               
*      1 ENCARRAY Based Boundary(Dword),                                        
*                                                                               
*        2 ENCG3HDR,                /* ENCG3 table header            */         
*                                                                               
*          3 ENCG3ACR Char(5),      /* ENCG3 table acronym           */         
*          3 ENCG3VER Fixed(8),     /* ENCG3 table version           */         
*                                                                               
*          3 *        Char(2),      /* Reserved for future use       */         
*                                                                               
*          3 ENCG3TLN Fixed(31),    /* ENCG3 table length            */         
*                                                                               
*          3 ENCG3TET(6),           /* ENCG3 table entry triplets    */         
*                                                                               
*            4 ENCG3TEO Fixed(31),  /* ENCG3 table entry offset      */         
*            4 ENCG3TEL Fixed(31),  /* ENCG3 table entry length      */         
*            4 ENCG3TEN Fixed(31),  /* ENCG3 table entry number      */         
*                                                                               
*          3 ENCG3DEO Fixed(31),    /* ENCG3 descriptor entry offset */         
*          3 ENCG3DEL Fixed(31),    /* ENCG3 descriptor entry length */         
*          3 ENCG3DEN Fixed(31),    /* ENCG3 descriptor entry number */         
*                                                                               
*        2 *          Char(0)       /* ENCG3 doubleword boundary     */         
*                       Boundary(Dword);                                        
*   Declare ENCG3_Acronym           /* ENCG3 table acronym           */         
*              Char(5)   Constant('ENCG3');                                     
*   Declare ENCG3_Acronym_Bytes_0To3                                            
*              Char(4)   Constant('ENCG');                                      
*   Declare ENCG3_Acronym_Byte_4                                                
*              Char(1)   Constant('3');                                         
*                                                                               
*   Declare ENCG3_Version           /* ENCG3 table version       @71C*/         
*              Fixed(8)  Constant(8);                                           
*                                                                               
*   Declare ENCG3_IFAVersion        /* ENCG3IFA support version      */         
*              Fixed(8)  Constant(6);                     /* @OA09061*/         
*                                                                               
