*USF3RURN BIT(16) CONSTANT('0001'X), /* RESOURCE NOT ACTIVE      @D1A*/         
*USF3RUSU BIT(16) CONSTANT('0002'X), /* SERVICE ACCESS POINT IN USE             
*                                                                @A1C*/         
*USF3RUNF BIT(16) CONSTANT('0003'X); /* RESOURCE NOT FOUND       @D1A*/         
*/*                                                              @D1A*/         
*/* THE FOLLOWING ARE SECONDARY RETURN CODES WHEN THE            @D1A*/         
*/* PRIMARY RETURN CODE IS SET TO X'0010' (USF3IREQ).            @D1A*/         
*/*                                                              @D1A*/         
*DECLARE                            /*                           @D1A*/         
*USF3IRCE BIT(16) CONSTANT('0000'X), /* CONNECTION NOT ESTABLISHED              
*                                                                @D1A*/         
*USF3IRCN BIT(16) CONSTANT('0001'X), /* CONNECTION NOT INDICATED                
*                                                                @D1A*/         
*USF3IRRP BIT(16) CONSTANT('0002'X), /* RESET IN PROGRESS        @D1A*/         
*USF3IRRI BIT(16) CONSTANT('0003'X), /* RESET INDICATED          @D1A*/         
*USF3IRRN BIT(16) CONSTANT('0004'X), /* RESET NOT INDICATED      @D1A*/         
*USF3IRRE BIT(16) CONSTANT('0005'X), /* ERROR RESET IN PROGRESS                 
*                                                                @D1A*/         
*USF3IRDP BIT(16) CONSTANT('0006'X), /* DISCONNECT IN PROGRESS                  
*                                                                @D1A*/         
*USF3IRDI BIT(16) CONSTANT('0007'X), /* DISCONNECT INDICATED     @D1A*/         
*USF3IRDN BIT(16) CONSTANT('0008'X), /* DISCONNECT NOT INDICATED                
*                                                                @D1A*/         
*USF3IRDE BIT(16) CONSTANT('0009'X), /* ERROR DISCONNECT IN PROGRESS            
*                                                                @D1A*/         
*USF3IRFM BIT(16) CONSTANT('000A'X), /* REQUEST INCONSISTENT WITH DATA          
*                                       FLOW                     @D1A*/         
*USF3IRAL BIT(16) CONSTANT('000B'X), /* ALREADY LOGGED ONTO THE NETWORK         
*                                       ACCESS POINT             @D1A*/         
*USF3IRLP BIT(16) CONSTANT('000C'X), /* LOGOFF IN PROGRESS       @D1A*/         
*USF3IRSE BIT(16) CONSTANT('000D'X), /* SUBORDINATE CONNECTIONS EXIST           
*                                                                @S2C*/         
*USF3IR0E BIT(16) CONSTANT('000E'X), /* NOT USED - AVAILABLE     @S1C*/         
*USF3IR0F BIT(16) CONSTANT('000F'X), /* NOT USED - AVAILABLE     @S1C*/         
*USF3IR10 BIT(16) CONSTANT('0010'X), /* NOT USED - AVAILABLE     @S1C*/         
*USF3IRRD BIT(16) CONSTANT('0011'X), /* READ REQUEST IN PROGRESS @D1A*/         
*USF3IRSP BIT(16) CONSTANT('0012'X), /* STATUS PHYSICAL REQUEST                 
*                                       IN PROGRESS              @D1A*/         
*USF3IRSV BIT(16) CONSTANT('0013'X), /* STATUS VIRTUAL REQUEST                  
*                                       IN PROGRESS         @D1A @A1C*/         
*USF3IRDR BIT(16) CONSTANT('0014'X); /* DUPLICATE REQUEST        @A1A*/         
*/*                                                              @D1A*/         
*/* THE FOLLOWING ARE SECONDARY RETURN CODES WHEN THE            @D1A*/         
*/* PRIMARY RETURN CODE IS SET TO X'0014' (USF3RCAN).            @D1A*/         
*/*                                                              @D1A*/         
*DECLARE                            /*                           @D1A*/         
*USF3RCRR BIT(16) CONSTANT('0000'X), /* RESET REQUESTED          @D1A*/         
*USF3RCRI BIT(16) CONSTANT('0001'X), /* RESET INDICATED          @D1A*/         
*USF3RCRE BIT(16) CONSTANT('0002'X), /* ERROR RESET              @D1A*/         
*USF3RCDR BIT(16) CONSTANT('0003'X), /* DISCONNECT REQUESTED     @D1A*/         
*USF3RCDI BIT(16) CONSTANT('0004'X), /* DISCONNECT INDICATED     @D1A*/         
*USF3RCDE BIT(16) CONSTANT('0005'X), /* ERROR DISCONNECT         @D1A*/         
