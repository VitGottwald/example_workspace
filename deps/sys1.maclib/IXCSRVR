&ZOPL(&ZOPN)          SETC  ' '                                         00369900
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00370000
&ZOPV(&ZOPN)          SETC  '1'                                         00370100
&ZOPD(&ZOPN)          SETC  ' '                                         00370200
.*                                                                      00370300
.ZLMFSL2              ANOP  ,                                           00370400
