&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00256000
                      AGO   .ZMRETX                                     00256100
.*                                                                      00256200
.ZMRET2               ANOP  ,                                           00256300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00256400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00256500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00256600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00256700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00256800
&ZOPN                 SETA  &ZOPN+1                                     00256900
&ZOPL(&ZOPN)          SETC  ' '                                         00257000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00257100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00257200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00257300
.ZMRETX               ANOP  ,                                           00257400
.*******************************************************************    00257500
.***AO.X03  REASON CODE PROCESSING                                      00257600
.*******************************************************************    00257700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00257800
