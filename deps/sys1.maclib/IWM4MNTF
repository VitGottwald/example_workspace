.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00261400
.*******************************************************************    00261500
                      ACTR  10000                                       00261600
.*                                                                      00261700
                      LCLA  &ZLC           COLUMN POSITION              00261800
                      LCLA  &ZOPX          LIST TABLE INDEX             00261900
                      LCLC  &ZLL           LABEL                        00262000
                      LCLC  &ZLOP          OPERATION                    00262100
                      LCLC  &ZLV           VALUE                        00262200
                      LCLC  &ZLD           DESCRIPTION                  00262300
.*                                                                      00262400
&ZOPX                 SETA  1                                           00262500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00262600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00262700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00262800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00262900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00263000
                      AIF   (K'&ZLD LE 0).ZL5                           00263100
&ZLC                  SETA  35             COMMENT COLUMN               00263200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00263300
&ZLC                  SETA  33             COMMMENT COLUMN              00263400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00263500
&ZLV                  SETC  '&ZLV'.' '                                  00263600
                      AGO   .ZL3                                        00263700
.*                                                                      00263800
.ZL4                  ANOP  ,                                           00263900
&ZLV                  SETC  '&ZLV'.' '                                  00264000
.ZL5                  ANOP  ,                                           00264100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00264200
.*                                                                      00264300
&ZLL     &ZLOP &ZLV                                                     00264400
.*                                                                      00264500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00264600
                      AGO   .ZL2           CONTINUE LOOPING             00264700
.*                                                                      00264800
.ZLX                  ANOP  ,                                           00264900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00265000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00265100
*                                                                       00265200
         LLGT  14,16(0,0)              Get CVT address                  00265300
         TM    CVTOSLV6-CVT(14),CVTH7790   Test for HBB7790             00265400
         JO    Ntfy&SYSNDX._Begin      Branch when Lvl is present       00265500
         LA    0,IWMRSNCODEBADVERSION  HBB7790 is required              00265600
         LA    15,IWMRETCODEINVOCERROR Set Return Code                  00265700
         J     Ntfy&SYSNDX._End        Branch to end label              00265800
Ntfy&SYSNDX._Begin DC 0H                                                00265900
         AIF   ('&XENDTIME' NE 'CURRENT').Ntfy_EndTime                  00266000
         XGR   15,15                   Clear R15 ...                    00266100
         STG   15,16(1)                and save zero in PL              00266200
.Ntfy_EndTime ANOP                                                      00266300
         LLGT  14,X'8C'(14,0)          Get ECVT Address                 00266400
         LLGT  14,X'C4'(14,0)          Get WMVT Address                 00266500
         AIF   ('&SYSAM64' NE 'YES').Ntfy_Amode31                       00266600
         LLGT  15,X'15C'(14,0)         Get Notify Rtn Entry Point Addr  00266700
         BASR  14,15                   Call 64bit Notify Routine        00266800
         AGO   .Ntfy_End                                                00266900
.Ntfy_Amode31 ANOP                                                      00267000
         LLGTR 1,1                     Clear High Half of R1            00267100
         LLGT  15,X'158'(14,0)         Get Notify Rtn Entry Point Addr  00267200
         BASR  14,15                   Call 31bit Notify Routine        00267300
.Ntfy_End ANOP                                                          00267400
Ntfy&SYSNDX._End DC 0H                                                  00267500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00267600
