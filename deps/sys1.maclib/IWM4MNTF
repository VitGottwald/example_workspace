.***AC.PLV01  PROCESS PLISTVER                                      *   00090100
.********************************************************************   00090200
&XPLISTVER      SETA  0                                                 00090300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00090400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00090500
&XPLISTVER      SETA  1                                                 00090600
.ZPLV0          ANOP  ,                                                 00090700
.*                                                                      00090800
                AIF   ('&CPUTIME' EQ '').ZPLV1E                         00090900
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00091000
&XPLISTVER      SETA  1                                                 00091100
.ZPLV1E         ANOP  ,                                                 00091200
.*                                                                      00091300
                AIF   ('&TIMEONCP' EQ '').ZPLV2E                        00091400
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00091500
&XPLISTVER      SETA  1                                                 00091600
.ZPLV2E         ANOP  ,                                                 00091700
.*                                                                      00091800
                AIF   ('&OFFLOADONCP' EQ '').ZPLV3E                     00091900
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00092000
&XPLISTVER      SETA  1                                                 00092100
.ZPLV3E         ANOP  ,                                                 00092200
.*                                                                      00092300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00092400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00092500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00092600
.*-------------------------------------------------------------------   00092700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00092800
&ZMS            SETC  '"PLISTVER='                                      00092900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00093000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00093100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00093200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00093300
                AGO   .ZPVCK2                                           00093400
.ZPVCK3         ANOP  ,                                                 00093500
.*-------------------------------------------------------------------   00093600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00093700
&ZMS            SETC  '"PLISTVER='                                      00093800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00093900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00094000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00094100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00094200
                AGO   .ZPVCKE                                           00094300
.ZPVCK2         ANOP  ,                                                 00094400
.*-------------------------------------------------------------------   00094500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00094600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00094700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00094800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00094900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00095000
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00095100
.ZPVCK6         ANOP  ,                                                 00095200
&ZMS            SETC  '"PLISTVER='                                      00095300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00095400
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00095500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00095600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00095700
&XPLISTVER      SETA  1                                                 00095800
                AGO   .ZPVCKE                                           00095900
.*-------------------------------------------------------------------   00096000
.ZPVCK4A        ANOP  ,                                                 00096100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00096200
&ZMS            SETC  'PLISTVER='                                       00096300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00096400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00096500
