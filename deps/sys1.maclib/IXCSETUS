.***AC.PLV01  PROCESS PLISTVER                                      *   00064900
.********************************************************************   00065000
&XPLISTVER      SETA  0                                                 00065100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00065200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00065300
&XPLISTVER      SETA  0                                                 00065400
.ZPLV0          ANOP  ,                                                 00065500
.*                                                                      00065600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00065700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00065800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00065900
.*-------------------------------------------------------------------   00066000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00066100
&ZMS            SETC  '"PLISTVER='                                      00066200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00066400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00066500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066600
                AGO   .ZPVCK2                                           00066700
.ZPVCK3         ANOP  ,                                                 00066800
.*-------------------------------------------------------------------   00066900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00067000
&ZMS            SETC  '"PLISTVER='                                      00067100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00067200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00067300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00067400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067500
                AGO   .ZPVCKE                                           00067600
.ZPVCK2         ANOP  ,                                                 00067700
.*-------------------------------------------------------------------   00067800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00067900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00068000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00068100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00068200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00068300
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00068400
.ZPVCK6         ANOP  ,                                                 00068500
&ZMS            SETC  '"PLISTVER='                                      00068600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068700
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00068800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00068900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069000
&XPLISTVER      SETA  0                                                 00069100
                AGO   .ZPVCKE                                           00069200
.*-------------------------------------------------------------------   00069300
.ZPVCK4A        ANOP  ,                                                 00069400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00069500
&ZMS            SETC  'PLISTVER='                                       00069600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00069700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00069800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00069900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00070000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070100
                AGO   .ZPVCK5                                           00070200
.ZPVCK4         ANOP  ,                                                 00070300
&XPLISTVER      SETA  &PLISTVER(1)                                      00070400
.ZPVCK5         ANOP  ,                                                 00070500
.ZPVCKE         ANOP  ,                                                 00070600
.********************************************************************   00070700
