.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00193900
.*******************************************************************    00194000
                      ACTR  10000                                       00194100
.*                                                                      00194200
                      LCLA  &ZLC           COLUMN POSITION              00194300
                      LCLA  &ZOPX          LIST TABLE INDEX             00194400
                      LCLC  &ZLL           LABEL                        00194500
                      LCLC  &ZLOP          OPERATION                    00194600
                      LCLC  &ZLV           VALUE                        00194700
                      LCLC  &ZLD           DESCRIPTION                  00194800
.*                                                                      00194900
&ZOPX                 SETA  1                                           00195000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00195100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00195200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00195300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00195400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00195500
                      AIF   (K'&ZLD LE 0).ZL5                           00195600
&ZLC                  SETA  35             COMMENT COLUMN               00195700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00195800
&ZLC                  SETA  33             COMMMENT COLUMN              00195900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00196000
&ZLV                  SETC  '&ZLV'.' '                                  00196100
                      AGO   .ZL3                                        00196200
.*                                                                      00196300
.ZL4                  ANOP  ,                                           00196400
&ZLV                  SETC  '&ZLV'.' '                                  00196500
.ZL5                  ANOP  ,                                           00196600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00196700
.*                                                                      00196800
&ZLL     &ZLOP &ZLV                                                     00196900
.*                                                                      00197000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00197100
                      AGO   .ZL2           CONTINUE LOOPING             00197200
.*                                                                      00197300
.ZLX                  ANOP  ,                                           00197400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00197500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00197600
         L     14,16(0,0)              GET CVT ADDRESS                  00197700
         L     14,772(14,0)            GET SFT ADDRESS                  00197800
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00197900
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00198000
         OR    14,15                   GET PC NUMBER                    00198100
         PC    0(14)                                                    00198200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00198300
.ZLINKX               ANOP  ,                                           00198400
.*******************************************************************    00198500
.***AP.X01  FORMAT GENERATED CODE                                       00198600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00198700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00198800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00198900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00199000
