&ZOPN1                SETA &ZOPN                                        00135200
.*******************************************************************    00135300
.***  PROCESS IXLDISC OUTPUT PARAMETERS                                 00135400
.*******************************************************************    00135500
                      ACTR  10000                                       00135600
.*******************************************************************    00135700
.***AO.X02  RETURN CODE PROCESSING                                      00135800
.*******************************************************************    00135900
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00136000
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00136100
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00136200
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00136300
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00136400
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00136500
&ZOPN                 SETA  &ZOPN+1                                     00136600
&ZOPL(&ZOPN)          SETC  ' '                                         00136700
&ZOPO(&ZOPN)          SETC  'ST'                                        00136800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00136900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00137000
                      AGO   .ZMRETX                                     00137100
.*                                                                      00137200
.ZMRET2               ANOP  ,                                           00137300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00137400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00137500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00137600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00137700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00137800
&ZOPN                 SETA  &ZOPN+1                                     00137900
&ZOPL(&ZOPN)          SETC  ' '                                         00138000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00138100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00138200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00138300
.ZMRETX               ANOP  ,                                           00138400
.*******************************************************************    00138500
.***AO.X03  REASON CODE PROCESSING                                      00138600
.*******************************************************************    00138700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00138800
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00138900
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00139000
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00139100
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00139200
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00139300
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00139400
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00139500
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00139600
