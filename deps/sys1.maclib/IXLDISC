&ZMS            SETC  '"PLISTVER='                                      00053600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00053800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00053900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054000
                AGO   .ZPVCK2                                           00054100
.ZPVCK3         ANOP  ,                                                 00054200
.*-------------------------------------------------------------------   00054300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00054400
&ZMS            SETC  '"PLISTVER='                                      00054500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00054600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00054700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00054800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054900
                AGO   .ZPVCKE                                           00055000
.ZPVCK2         ANOP  ,                                                 00055100
.*-------------------------------------------------------------------   00055200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00055300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00055400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00055500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00055600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00055700
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00055800
.ZPVCK6         ANOP  ,                                                 00055900
&ZMS            SETC  '"PLISTVER='                                      00056000
