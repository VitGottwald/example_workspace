&ZOPO(&ZOPN)          SETC  'ST'                                        00139900
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00140000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00140100
                      AGO   .ZMRSNX                                     00140200
.*                                                                      00140300
.ZMRSN2               ANOP  ,                                           00140400
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00140500
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00140600
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00140700
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00140800
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00140900
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00141000
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00141100
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00141200
&ZOPN                 SETA  &ZOPN+1                                     00141300
&ZOPL(&ZOPN)          SETC  ' '                                         00141400
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00141500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00141600
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00141700
.ZMRSNX               ANOP  ,                                           00141800
.*******************************************************************    00141900
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00142000
.*******************************************************************    00142100
.ZMFLX                ANOP  ,              MF=L PROCESSING              00142200
.*******************************************************************    00142300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00142400
.*******************************************************************    00142500
                      ACTR  10000                                       00142600
.*                                                                      00142700
                      LCLA  &ZLC           COLUMN POSITION              00142800
                      LCLA  &ZOPX          LIST TABLE INDEX             00142900
                      LCLC  &ZLL           LABEL                        00143000
                      LCLC  &ZLOP          OPERATION                    00143100
                      LCLC  &ZLV           VALUE                        00143200
                      LCLC  &ZLD           DESCRIPTION                  00143300
.*                                                                      00143400
&ZOPX                 SETA  1                                           00143500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00143600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00143700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00143800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00143900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00144000
                      AIF   (K'&ZLD LE 0).ZL5                           00144100
&ZLC                  SETA  35             COMMENT COLUMN               00144200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00144300
&ZLC                  SETA  33             COMMMENT COLUMN              00144400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00144500
&ZLV                  SETC  '&ZLV'.' '                                  00144600
                      AGO   .ZL3                                        00144700
.*                                                                      00144800
.ZL4                  ANOP  ,                                           00144900
&ZLV                  SETC  '&ZLV'.' '                                  00145000
.ZL5                  ANOP  ,                                           00145100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00145200
.*                                                                      00145300
&ZLL     &ZLOP &ZLV                                                     00145400
.*                                                                      00145500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00145600
                      AGO   .ZL2           CONTINUE LOOPING             00145700
.*                                                                      00145800
.ZLX                  ANOP  ,                                           00145900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00146000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00146100
         L     14,16(0,0)              GET CVT ADDRESS                  00146200
         L     14,772(14,0)            GET SFT ADDRESS                  00146300
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00146400
         LA    15,20                   GET EX FOR SERVICE ROUTINE       00146500
         OR    14,15                   GET PC NUMBER                    00146600
         PC    0(14)                                                    00146700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00146800
.ZLINKX               ANOP  ,                                           00146900
.*******************************************************************    00147000
.***AP.X01  FORMAT GENERATED CODE                                       00147100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00147200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00147300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00147400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00147500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00147600
                      AIF   (K'&ZLD LE 0).ZL51                          00147700
&ZLC                  SETA  35             COMMENT COLUMN               00147800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00147900
&ZLC                  SETA  33             COMMENT COLUMN               00148000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00148100
&ZLV                  SETC  '&ZLV'.' '                                  00148200
                      AGO   .ZL31                                       00148300
.*                                                                      00148400
.ZL41                 ANOP  ,                                           00148500
&ZLV                  SETC  '&ZLV'.' '                                  00148600
.ZL51                 ANOP  ,                                           00148700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00148800
.*                                                                      00148900
&ZLL     &ZLOP &ZLV                                                     00149000
.*                                                                      00149100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00149200
                      AGO   .ZL21          CONTINUE LOOPING             00149300
.*                                                                      00149400
.ZLX1                 ANOP  ,                                           00149500
.*******************************************************************    00149600
