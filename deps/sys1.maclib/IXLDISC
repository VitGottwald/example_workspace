&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00056200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00056300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056400
&XPLISTVER      SETA  0                                                 00056500
                AGO   .ZPVCKE                                           00056600
.*-------------------------------------------------------------------   00056700
.ZPVCK4A        ANOP  ,                                                 00056800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00056900
&ZMS            SETC  'PLISTVER='                                       00057000
