.***AL.L12 IXGBRWSE FMT=LIST MF=S!L PARAMETERS                          00845600
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00845700
.*                                                                      00845800
&ZOPN                 SETA  &ZOPN+1                                     00845900
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00846000
&ZOPO(&ZOPN)          SETC  'DC'                                        00846100
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00846200
