.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00846700
&ZPLLEN               SETA 92                                           00846800
.*                                                                      00846900
                      AIF  (&XPLISTVER LT 1).ZLLEN2Z                    00847000
&ZPLLEN               SETA 120                                          00847100
.ZLLEN2Z              ANOP  ,                                           00847200
.*                                                                      00847300
.******************************************************************     00847400
.***AI.L01 IXGBRWSE FMT=LIST FILL IN PARAMETER AREA                     00847500
.******************************************************************     00847600
                      ACTR  10000                                       00847700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00847800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00847900
&ZCVAL                SETC  'LAE'          XAX MODE                     00848000
.ZIMFXA               ANOP ,                                            00848100
.*                                                                      00848200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00848300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00848400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00848500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00848600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00848700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00848800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00848900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00849000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00849100
&ZOPN                 SETA  &ZOPN+1                                     00849200
&ZOPL(&ZOPN)          SETC  ' '                                         00849300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00849400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00849500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00849600
                      AGO   .ZIMFNX                                     00849700
.*                                                                      00849800
.ZIMFN2               ANOP ,                                            00849900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00850000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00850100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00850200
&ZOPN                 SETA  &ZOPN+1                                     00850300
&ZOPL(&ZOPN)          SETC  ' '                                         00850400
&ZOPO(&ZOPN)          SETC  'LAE'                                       00850500
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00850600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00850700
                      AGO   .ZIMFNX                                     00850800
.ZIMFNX1              ANOP ,                                            00850900
&ZOPN                 SETA  &ZOPN+1                                     00851000
&ZOPL(&ZOPN)          SETC  ' '                                         00851100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00851200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00851300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00851400
.ZIMFNX               ANOP ,                                            00851500
.********************************************************************   00851600
.***AI.L02 IXGBRWSE FMT=LIST INITIALIZE PARAMETER AREA                  00851700
.********************************************************************   00851800
