                     AIF   ('&BUFFLEN' NE '').ZG162B  KEYS?             00270700
                     AIF   ('&BUFFKEY' NE '').ZG162B  KEYS?             00270800
                     AIF   ('&BUFFALET' NE '').ZG162B  KEYS?            00270900
                     AIF   ('&BLKSIZE' NE '').ZG162B  KEYS?             00271000
                     AIF   ('&MULTIBLOCK' NE '').ZG162B  KEYS?          00271100
                     AIF   ('&RETBLOCKID' NE '').ZG162B  KEYS?          00271200
                     AIF   ('&TIMESTAMP' NE '').ZG162B  KEYS?           00271300
                     AIF   ('&RETBLOCKINFO' NE '').ZG162B  KEYS?        00271400
                     AIF   ('&MAXNUMLOGBLOCKS' NE '').ZG162B  KEYS?     00271500
                     AIF   ('&BLOCKID' NE '').ZG162B  KEYS?             00271600
                     AIF   ('&POSITION' NE '').ZG162B  KEYS?            00271700
                     AIF   ('&MODE' NE '').ZG162B  KEYS?                00271800
