.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00965300
.*******************************************************************    00965400
                      ACTR  10000                                       00965500
.*                                                                      00965600
                      LCLA  &ZLC           COLUMN POSITION              00965700
                      LCLA  &ZOPX          LIST TABLE INDEX             00965800
                      LCLC  &ZLL           LABEL                        00965900
                      LCLC  &ZLOP          OPERATION                    00966000
                      LCLC  &ZLV           VALUE                        00966100
                      LCLC  &ZLD           DESCRIPTION                  00966200
.*                                                                      00966300
&ZOPX                 SETA  1                                           00966400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00966500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00966600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00966700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00966800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00966900
                      AIF   (K'&ZLD LE 0).ZL5                           00967000
&ZLC                  SETA  35             COMMENT COLUMN               00967100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00967200
&ZLC                  SETA  33             COMMMENT COLUMN              00967300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00967400
&ZLV                  SETC  '&ZLV'.' '                                  00967500
                      AGO   .ZL3                                        00967600
.*                                                                      00967700
.ZL4                  ANOP  ,                                           00967800
&ZLV                  SETC  '&ZLV'.' '                                  00967900
.ZL5                  ANOP  ,                                           00968000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00968100
.*                                                                      00968200
&ZLL     &ZLOP &ZLV                                                     00968300
.*                                                                      00968400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00968500
                      AGO   .ZL2           CONTINUE LOOPING             00968600
.*                                                                      00968700
.ZLX                  ANOP  ,                                           00968800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00968900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00969000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00969100
         AIF   ('&SYSASCE' EQ 'P').BRPRIM  SYSSTATE PRIMARY             00969200
         SLR   15,15                SET GPR 15 TO ZERO                  00969300
         SAR   15,15                ENSURE AR 15 IS ZERO FOR PRIMARY    00969400
.BRPRIM  ANOP                                                           00969500
         AIF   ('&SYSAM64' EQ 'YES').BRA64                              00969600
         L     15,16(0,0)           GET CVT ADDRESS                     00969700
         AGO   .BRCONT                                                  00969800
.BRA64   LLGT  15,16(0,0)           GET CVT Address                     00969900
.BRCONT  L     15,140(15,0)         GET ECVT ADDRESS                    00970000
         L     15,288(15,0)         GET LSAB ADDRESS                    00970100
         L     15,124(15,0)         GET ENTRY POINT ADDRESS             00970200
         LA    0,5                  EX OF BROWSE SERVICE                00970300
         BALR  14,15                INVOKE SERVICE ROUTINE              00970400
.*                                                                 @03D 00970500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00970600
