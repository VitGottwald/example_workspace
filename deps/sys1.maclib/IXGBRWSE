.***AC.PLV01  PROCESS PLISTVER                                      *   00256600
.********************************************************************   00256700
&XPLISTVER      SETA  0                                                 00256800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00256900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00257000
&XPLISTVER      SETA  3                                                 00257100
.ZPLV0          ANOP  ,                                                 00257200
.*                                                                      00257300
                AIF   ('&DIAG' EQ '').ZPLV1E                            00257400
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00257500
&XPLISTVER      SETA  1                                                 00257600
.ZPLV1E         ANOP  ,                                                 00257700
.*                                                                      00257800
                AIF   ('&BUFFER64' EQ '').ZPLV2E                        00257900
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00258000
&XPLISTVER      SETA  3                                                 00258100
.ZPLV2E         ANOP  ,                                                 00258200
.*                                                                      00258300
                AIF   ('&MULTIBLOCK' EQ '').ZPLV3E                      00258400
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00258500
&XPLISTVER      SETA  2                                                 00258600
.ZPLV3E         ANOP  ,                                                 00258700
.*                                                                      00258800
                AIF   ('&RETBLOCKINFO' EQ '').ZPLV4E                    00258900
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00259000
&XPLISTVER      SETA  2                                                 00259100
.ZPLV4E         ANOP  ,                                                 00259200
.*                                                                      00259300
                AIF   ('&MAXNUMLOGBLOCKS' EQ '').ZPLV5E                 00259400
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00259500
&XPLISTVER      SETA  2                                                 00259600
.ZPLV5E         ANOP  ,                                                 00259700
.*                                                                      00259800
                AIF   ('&BUFFER64' EQ '').ZPLV6E                        00259900
                AIF   (&XPLISTVER GE 3).ZPLV6E                          00260000
&XPLISTVER      SETA  3                                                 00260100
.ZPLV6E         ANOP  ,                                                 00260200
.*                                                                      00260300
                AIF   ('&REQDATA' EQ '').ZPLV7E                         00260400
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00260500
&XPLISTVER      SETA  1                                                 00260600
.ZPLV7E         ANOP  ,                                                 00260700
.*                                                                      00260800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00260900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00261000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00261100
.*-------------------------------------------------------------------   00261200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00261300
&ZMS            SETC  '"PLISTVER='                                      00261400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00261500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00261600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00261700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00261800
                AGO   .ZPVCK2                                           00261900
.ZPVCK3         ANOP  ,                                                 00262000
.*-------------------------------------------------------------------   00262100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00262200
&ZMS            SETC  '"PLISTVER='                                      00262300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00262400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00262500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00262600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00262700
                AGO   .ZPVCKE                                           00262800
.ZPVCK2         ANOP  ,                                                 00262900
.*-------------------------------------------------------------------   00263000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00263100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00263200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00263300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00263400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00263500
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00263600
.ZPVCK6         ANOP  ,                                                 00263700
&ZMS            SETC  '"PLISTVER='                                      00263800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00263900
&ZMS            SETC  '&ZMS THE RANGE IS "0-3"'                         00264000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00264100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00264200
&XPLISTVER      SETA  3                                                 00264300
                AGO   .ZPVCKE                                           00264400
.*-------------------------------------------------------------------   00264500
.ZPVCK4A        ANOP  ,                                                 00264600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00264700
&ZMS            SETC  'PLISTVER='                                       00264800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00264900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00265000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00265100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00265200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00265300
                AGO   .ZPVCK5                                           00265400
.ZPVCK4         ANOP  ,                                                 00265500
&XPLISTVER      SETA  &PLISTVER(1)                                      00265600
.ZPVCK5         ANOP  ,                                                 00265700
.ZPVCKE         ANOP  ,                                                 00265800
.********************************************************************   00265900
