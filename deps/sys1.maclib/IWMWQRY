.***AO.X04  LABEL FOR MF=L PROCESSING                                   00150700
.*******************************************************************    00150800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00150900
.*******************************************************************    00151000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00151100
.*******************************************************************    00151200
                      ACTR  10000                                       00151300
.*                                                                      00151400
                      LCLA  &ZLC           COLUMN POSITION              00151500
                      LCLA  &ZOPX          LIST TABLE INDEX             00151600
                      LCLC  &ZLL           LABEL                        00151700
                      LCLC  &ZLOP          OPERATION                    00151800
                      LCLC  &ZLV           VALUE                        00151900
                      LCLC  &ZLD           DESCRIPTION                  00152000
.*                                                                      00152100
&ZOPX                 SETA  1                                           00152200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00152300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00152400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00152500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00152600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00152700
                      AIF   (K'&ZLD LE 0).ZL5                           00152800
&ZLC                  SETA  35             COMMENT COLUMN               00152900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00153000
&ZLC                  SETA  33             COMMMENT COLUMN              00153100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00153200
&ZLV                  SETC  '&ZLV'.' '                                  00153300
                      AGO   .ZL3                                        00153400
.*                                                                      00153500
.ZL4                  ANOP  ,                                           00153600
&ZLV                  SETC  '&ZLV'.' '                                  00153700
.ZL5                  ANOP  ,                                           00153800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00153900
.*                                                                      00154000
&ZLL     &ZLOP &ZLV                                                     00154100
.*                                                                      00154200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00154300
                      AGO   .ZL2           CONTINUE LOOPING             00154400
.*                                                                      00154500
.ZLX                  ANOP  ,                                           00154600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00154700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00154800
         L     14,16(0,0)              GET CVT ADDRESS                  00154900
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00155000
         LAE   14,0(14,0)              CLEAR AR 14                      00155100
.NOAR    ANOP                                                           00155200
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00155300
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00155400
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00155500
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00155600
         B     END&SYSNDX              BRANCH TO END LABEL              00155700
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00155800
         L     14,772(14,0)            GET SFT ADDRESS                  00155900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00156000
         LA    15,8                    GET EX FOR SERVICE ROUTINE       00156100
         OR    14,15                   GET PC NUMBER                    00156200
         PC    0(14)                                                    00156300
END&SYSNDX DS    0H                    END LABEL                        00156400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00156500
.ZLINKX               ANOP  ,                                           00156600
.*******************************************************************    00156700
.***AP.X01  FORMAT GENERATED CODE                                       00156800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00156900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00157000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00157100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00157200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00157300
                      AIF   (K'&ZLD LE 0).ZL51                          00157400
&ZLC                  SETA  35             COMMENT COLUMN               00157500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00157600
&ZLC                  SETA  33             COMMENT COLUMN               00157700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00157800
&ZLV                  SETC  '&ZLV'.' '                                  00157900
                      AGO   .ZL31                                       00158000
.*                                                                      00158100
.ZL41                 ANOP  ,                                           00158200
&ZLV                  SETC  '&ZLV'.' '                                  00158300
.ZL51                 ANOP  ,                                           00158400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00158500
.*                                                                      00158600
&ZLL     &ZLOP &ZLV                                                     00158700
.*                                                                      00158800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00158900
                      AGO   .ZL21          CONTINUE LOOPING             00159000
.*                                                                      00159100
.ZLX1                 ANOP  ,                                           00159200
.*******************************************************************    00159300
.***AP.X02  END OF IWMWQRY MACRO                                        00159400
