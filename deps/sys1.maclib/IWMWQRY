.******************************************************************     00104000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00104100
.******************************************************************     00104200
              ACTR  10000                                               00104300
.*----------------------------------------------------------------***   00104400
.***AH.P02 EXTRA POSITIONALS                                            00104500
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00104600
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00104700
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00104800
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00104900
.ZGLISTX1     ANOP  ,                                                   00105000
&ZCVAL        SETC  ''          ERROR FIELD                             00105100
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00105200
.ZGLIST2      ANOP  ,                                                   00105300
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00105400
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00105500
