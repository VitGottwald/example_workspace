.***AC.PLV01  PROCESS PLISTVER                                      *   00051800
.********************************************************************   00051900
&XPLISTVER      SETA  0                                                 00052000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00052100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00052200
&XPLISTVER      SETA  0                                                 00052300
.ZPLV0          ANOP  ,                                                 00052400
.*                                                                      00052500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00052600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00052700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00052800
.*-------------------------------------------------------------------   00052900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00053000
&ZMS            SETC  '"PLISTVER='                                      00053100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00053300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00053400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00053500
                AGO   .ZPVCK2                                           00053600
.ZPVCK3         ANOP  ,                                                 00053700
.*-------------------------------------------------------------------   00053800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00053900
&ZMS            SETC  '"PLISTVER='                                      00054000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00054100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00054200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00054300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054400
                AGO   .ZPVCKE                                           00054500
.ZPVCK2         ANOP  ,                                                 00054600
.*-------------------------------------------------------------------   00054700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00054800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00054900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00055000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00055100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00055200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00055300
.ZPVCK6         ANOP  ,                                                 00055400
&ZMS            SETC  '"PLISTVER='                                      00055500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00055700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00055800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055900
&XPLISTVER      SETA  0                                                 00056000
                AGO   .ZPVCKE                                           00056100
.*-------------------------------------------------------------------   00056200
.ZPVCK4A        ANOP  ,                                                 00056300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00056400
&ZMS            SETC  'PLISTVER='                                       00056500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00056600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00056700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00056800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00056900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057000
                AGO   .ZPVCK5                                           00057100
.ZPVCK4         ANOP  ,                                                 00057200
&XPLISTVER      SETA  &PLISTVER(1)                                      00057300
.ZPVCK5         ANOP  ,                                                 00057400
.ZPVCKE         ANOP  ,                                                 00057500
