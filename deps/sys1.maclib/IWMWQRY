.********************************************************************   00109900
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00110000
.********************************************************************   00110100
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00110200
                      LCLA  &ZOPN1         INVOCATION INDEX             00110300
                      LCLC  &ZOPL(4095)    LABEL                        00110400
                      LCLC  &ZOPO(4095)    OPERATION                    00110500
                      LCLC  &ZOPV(4095)    VALUE                        00110600
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00110700
.*                                                                      00110800
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00110900
.******************************************************************     00111000
.***AL.L10 IWMWQRY FMT=LIST MF=S!L PARAMETERS                           00111100
.******************************************************************     00111200
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00111300
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00111400
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00111500
&XMFATTR              SETC  '0D'                                        00111600
.*                                                                      00111700
&ZOPN                 SETA  &ZOPN+1                                     00111800
&ZOPL(&ZOPN)          SETC  ' '                                         00111900
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00112000
&ZOPO(&ZOPN)          SETC  'B'                                         00112100
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00112200
                      AGO   .ZALVL6B                                    00112300
.ZALVL6A              ANOP                                              00112400
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00112500
&ZOPV(&ZOPN)          SETC  '4,8'                                       00112600
&ZOPN                 SETA  &ZOPN+1                                     00112700
&ZOPL(&ZOPN)          SETC  ' '                                         00112800
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00112900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00113000
.ZALVL6B              ANOP                                              00113100
&ZOPD(&ZOPN)          SETC  '++ IWMWQRY ARG PARMS'                      00113200
.*                                                                      00113300
&ZOPN                 SETA  &ZOPN+1                                     00113400
&ZOPL(&ZOPN)          SETC  ' '                                         00113500
