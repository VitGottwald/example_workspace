.ATTR19  ANOP                                                     @P1A  03070001
         AIF   ('&ATTR(&I)' NE 'MAXRGN').ATTRBAD                  @P1A  03080001
&B(19)   SETB  1                                                  @P1A  03090001
         AGO   .ATTRCON                                           @P1A  03100001
.ATTRBAD ANOP                                                           03110000
         IHBERMAC 1001,ATTR,&ATTR(&I)                                   03120000
&ZMACRET SETA  12                                                       03130000
.ATTRCON ANOP                                                           03140000
&I       SETA  &I+1                                                     03150000
         AGO   .ATTRL                                                   03160000
.ATTREND ANOP                                                           03170000
&B(24)   SETB  ('&ELXLIST' NE '')                                 @L3A  03180000
         AIF   (&B(13) EQ 1 AND &B(14) EQ 1).ATTRBD2                    03190000
         AGO   .ATTROK                                                  03200000
.ATTRBD2 ANOP                                                           03210000
         MNOTE 12,'***  IHB304  THE HIPRI AND NONURG ATTRIBUTES ARE MUT*03220000
               UALLY EXCLUSIVE.'                                        03230000
&ZMACRET SETA  12                                                       03240000
.ATTROK  ANOP                                                           03250000
         AIF   ('&ASNAME' EQ '' OR '&STPARM' EQ '').MUTOK               03260000
         MNOTE 12,'***  IHB304  THE ASNAME AND STPARM PARAMETERS ARE MU*03270000
               TUALLY EXCLUSIVE.'                                       03280000
&ZMACRET SETA  12                                                       03290000
.MUTOK   ANOP                                                           03300000
         AIF   (N'&MF GT 2).MFBAD                                       03310000
         AIF   ('&MF' EQ '').MFBAD                                      03320000
         AIF   (N'&MF LT 2 AND '&MF(1)' EQ 'E').MFBAD                   03330000
         AIF   (N'&MF LT 2 OR '&MF(1)' EQ 'E').MFOK                     03340000
.MFBAD   ANOP                                                           03350000
         IHBERMAC 1012,MF                                               03360000
         MEXIT                                                          03370000
.MFOK    ANOP                                                           03380000
         AIF ('&MF(1)' EQ 'I'                                          *03390000
               OR '&MF(1)' EQ 'E' OR '&MF(1)' EQ 'L').MFOK2             03400000
         MNOTE 12,'***  IHB002  THE FIRST ARGUMENT OF THE MF PARAMETER *03410000
               MUST BE ''I'', ''E'', OR ''L''.'                         03420000
         MEXIT                                                          03430000
.MFOK2   ANOP                                                           03440000
         AIF ('&MF(1)' NE 'I').NOTMFI                                   03450000
         AIF ('&INIT' NE '').OKINIT                                     03460000
         MNOTE 12,'***  IHB001  THE INIT PARAMETER MUST BE SPECIFIED ON*03470000
                THE STANDARD FORM.'                                     03480000
&ZMACRET SETA  12                                                       03490000
.OKINIT  ANOP                                                           03500000
         AIF ('&STPARM' NE '' OR '&ASNAME' NE '').OKSTP                 03510000
         MNOTE 12,'***  IHB001  THE STPARM OR ASNAME PARAMETER MUST BE *03520000
               SPECIFIED ON THE STANDARD FORM.'                         03530000
&ZMACRET SETA  12                                                       03540000
.OKSTP   ANOP                                                           03550000
         AIF ('&ODA' NE '').OKODA                                       03560000
