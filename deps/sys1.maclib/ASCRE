&XINIT   SETA  1                                                        01790000
         AIF   ('&INIT'(1,1) EQ '''').ZM3B                              01800000
&XINIT   SETA  2                                                        01810000
         AIF   ('&INIT'(1,1) NE '(').ZM3B                               01820000
&XINIT   SETA  3                                                        01830000
.ZM3B    ANOP                                                           01840000
.*                                                                      01850000
         AIF   (N'&TRMEXIT LE 1).ZM4A                                   01860000
         MNOTE 12,'***  IHB247  MORE THAN 1 ARGUMENT SPECIFIED FOR THE *01870000
               TRMEXIT PARAMETER.'                                      01880000
&ZMACRET SETA  12                                                       01890000
.ZM4A    ANOP                                                           01900000
.*                                                                      01910000
