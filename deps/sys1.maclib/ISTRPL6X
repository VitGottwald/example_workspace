*/********************************************************************/         
*DCL  RPL6RSET FIXED(8) CONSTANT(0); /* RESET  (INITIAL STATE)   @F3A*/         
*/*    ** HALF-DUPLEX CONVERSATION STATES **                         */         
*DCL  RPL6SND  FIXED(8) CONSTANT(1); /* SEND                     @F3A*/         
*DCL  RPL6RECV FIXED(8) CONSTANT(2); /* RECEIVE                  @F3A*/         
*DCL  RPL6RVCF FIXED(8) CONSTANT(3); /* RECEIVE CONFIRM          @F3A*/         
*DCL  RPL6RVCS FIXED(8) CONSTANT(4); /* RECEIVE CONFIRM SEND     @F3A*/         
*DCL  RPL6RVCD FIXED(8) CONSTANT(5); /* RECEIVE CONFIRM DEALLOCATE              
*                                                                @F3A*/         
*DCL  RPL6PNDD FIXED(8) CONSTANT(6); /* PEND DEALLOCATE          @F3A*/         
*DCL  RPL6PECL FIXED(8) CONSTANT(7); /* PEND END CONVERSATION LOG               
*                                                                @F3A*/         
*DCL  RPL6ENDC FIXED(8) CONSTANT(8); /* END CONVERSATION (FINAL) @F3A*/         
*DCL  RPL6PNDS FIXED(8) CONSTANT(9); /* PENDING SEND             @F3A*/         
*DCL  RPL6PRVL FIXED(8) CONSTANT(10);/* PENDING RCV LOG          @F3A*/         
*/*    ** FULL-DUPLEX CONVERSATION STATES **                         */         
*DCL  RPL6FDRS FIXED(8) CONSTANT(128); /* FDX RESET (FINAL)'80'X @I3A*/         
*DCL  RPL6FDSR FIXED(8) CONSTANT(129); /* SEND/RECEIVE     '81'X @I3A*/         
*DCL  RPL6FDSO FIXED(8) CONSTANT(130); /* SEND-ONLY        '82'X @I3A*/         
*DCL  RPL6FDRO FIXED(8) CONSTANT(131); /* RECEIVE-ONLY     '83'X @I3A*/         
*DCL  RPL6FSRL FIXED(8) CONSTANT(132); /* PEND SEND/RCV LOG '84'X               
*                                                                @I3A*/         
*DCL  RPL6FROL FIXED(8) CONSTANT(133); /* PEND RCV-ONLY LOG '85'X               
*                                                                @I3A*/         
*DCL  RPL6FRSL FIXED(8) CONSTANT(134); /* PEND RESET LOG   '86'X @I3A*/         
*                                                                               
*/*    ** PENDING CONVERSATION ALLOCATION **                         */         
*DCL  RPL6PALC FIXED(8) CONSTANT(255); /* Pending Allocate 'FF'X @L1A*/         
