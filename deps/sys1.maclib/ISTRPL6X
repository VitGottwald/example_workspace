*DCL  RPL6RCV  BIT(8) CONSTANT ('70'X); /* RECEIVE                   */         
*DCL  RPL6RCVX BIT(8) CONSTANT ('71'X); /* RCVEXPD               @I3A*/         
*DCL  RPL6RJCT BIT(8) CONSTANT ('80'X); /* REJECT                    */         
*DCL  RPL6SEND BIT(8) CONSTANT ('90'X); /* SEND                      */         
*DCL  RPL6SNDX BIT(8) CONSTANT ('91'X); /* SENDEXPD              @I3A*/         
*DCL  RPL6SNRC BIT(8) CONSTANT ('92'X); /* SENDRCV               @D1A*/         
*DCL  RPL6SETS BIT(8) CONSTANT ('A0'X); /* SETSESS               @F4A*/         
*DCL  RPL6TSTS BIT(8) CONSTANT ('B0'X); /* TESTSTAT              @I4A*/         
