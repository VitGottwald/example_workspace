*%GOTO   RPL6DCL;           /*                                                  
         AGO   .RPL6BAL         */                                              
*%RPL6DCL: ;                                                                    
*DCL  1 ISTRPL6X BASED,                  /* START OF APPC                       
*                                           EXTENSION                */         
*         2 RPL6AREA,                                                           
*                                                                               
*             3 RPL6CBID    CHAR(4),     /* CONTROL BLOCK IDENTIFIER */         
*             3 RPL6REQ     BIT(8),      /* CONTROL VALUE OF APPCCMD */         
*             3 RPL6QUAL    BIT(8),      /* QUALIFY VALUE OF APPCCMD */         
*             3 *           CHAR(2),     /* NOT USED                 */         
*             3 RPL6CNVD    FIXED(32),   /* CONVERSATION ID          */         
*             3 RPL6USR     BIT(32),     /* USER FIELD               */         
*             3 RPL6SNSO    BIT(32),     /* SENSE DATA SPECIFIED                
*                                           ON APPCCMD               */         
*             3 RPL6SNSI    BIT(32),     /* SENSE DATA RETURNED                 
*                                           BY APPCCMD               */         
*             3 RPL6SGNL    BIT(32),     /* SIGNAL DATA RETURNED ON ALL         
*                                           APPCCMDS IF SIGRCV=YES,             
*                                           RESERVED IF SIGRCV = NO  */         
*             3 RPL6SIDL    FIXED(8),    /* LENGTH OF SESSION ID     */         
*             3 *           BIT(24),     /* NOT USED                 */         
*             3 RPL6SSID    BIT(64),     /* SESSION IDENTIFICATION   */         
*             3 RPL6RC,                  /* RPL6 RETURN CODES        */         
*               4 RPL6RCPR  BIT(16),     /* PRIMARY RETURN CODE      */         
*               4 RPL6RCSC  BIT(16),     /* SECONDARY RETURN CODE    */         
*             3 RPL6FLGS      BIT(32),   /* ALL INDICATORS SPECIFIC             
*                                           TO VTAM'S APPCCMD MACRO  */         
*               4 RPL6FLG1    BIT(8),    /* FIRST INDICATORS BYTE    */         
*                 5 RPL6FILL  BIT(1),    /* FILL INDICATOR                      
*                                           '0'B = "FILL=BUFF"                  
*                                           '1'B = "FILL=LL"         */         
*                 5 RPL6CD    BIT(1),    /* CD INDICATOR                        
*                                           '0'B ="CD=IMMED"                    
*                                           '1'B ="CD=DEFER" @P058398*/         
*                 5 *         BIT(1),    /* NOT USED             @Q2C*/         
*                 5 RPL6SLS   BIT(1),    /* PARTNER LU VERIFIED                 
*                                           '1'B = "VERIFIED"                   
*                                           '0'B = "NOT VERIFIED"               
*                                                                @F2A*/         
*                 5 RPL6CFTX  BIT(1),    /* CONFTXT INDICATOR                   
*                                           '1'B = "CONFTXT=YES"                
*                                           '0'B = "CONFTXT=NO"      */         
*                 5 RPL6LIST  BIT(2),    /* SCOPE OF INFORMATION TO BE          
*                                           RETURNED IN THE RESTORE             
*                                           COMMAND              @B2A*/         
*                 5 *         BIT(1),    /* NOT USED             @B2C*/         
*               4 RPL6FLG2    BIT(8),    /* SECOND INDICATORS BYTE   */         
*                 5 RPL6BISC  BIT(1),    /* Bis Exchange completed              
*                                                                @21A*/         
*                 5 RPL6RTSX  BIT(1),    /* RTS_RCVD RETURN INDICATOR           
*                                           '0'B = RTSRTRN=BOTH                 
*                                           '1'B = RTSRTRN=EXPD  @I3A*/         
*                 5 RPL6CXMD  BIT(2),    /* CONVERSATION EXPEDITED              
*                                           DATA MODE (CONXMOD)  @I3A*/         
*                 5 RPL6TYPE  BIT(2),    /* TYPE INDICATOR           */         
*                 5 RPL6NAMU  BIT(2),    /* NAME USE REQUESTED WHEN             
*                                           APPLICATION IS ACTING AS            
*                                           A GENERIC RESOURCE   @U2C*/         
*               4 RPL6FLG3    BIT(8),    /* THIRD INDICATORS BYTE    */         
*                 5 RPL6LOCK  BIT(1),    /* LOCKS INDICATOR                     
*                                           '0'B = "LOCKS=LONG"                 
*                                           '1'B = "LOCKS=SHORT"     */         
*                 5 RPL6DERC  BIT(2),    /* SESSION DEACTIVATION                
*                                           REASON CODE          @Q1A*/         
*                 5 RPL6EXDR  BIT(1),    /* EXPEDITED DATA RECEIVED             
*                                                                @I3C*/         
*                 5 RPL6CMOD  BIT(2),    /* CONMODE INDICATOR        */         
*                 5 RPL6LAST  BIT(2),    /* LAST INDICATOR                      
*                                           FOR ATTN LOSS EXIT   @F4A*/         
*               4 RPL6FLG4    BIT(8),    /* FOURTH INDICATORS BYTE   */         
                  5 RPL6AFFN  BIT(2),    /* LU AFFINITY          @L4A*/         
*                 5 *         BIT(6),    /* NOT USED             @L4C*/         
*             3 RPL6LU      CHAR(8),     /* NAME OF LU               */         
*             3 RPL6MODE    CHAR(8),     /* MODE NAME TO BE USED     */         
*             3 RPL6WHAT    BIT(16),     /* "WHAT RECEIVED" INDICATOR           
*                                           RETURNED AT COMPLETION OF           
*                                           APPCCMD RECEIVE COMMAND  */         
*               4 RPL6RCV1    BIT(8),                                           
*                 5 RPL6WDAT  BIT(1),    /* WHATRCV=DATA             */         
*                 5 RPL6WDAC  BIT(1),    /* WHATRCV=DATA_COMPLETE    */         
*                 5 RPL6WDAI  BIT(1),    /* WHATRCV=DATA_INCOMPLETE  */         
*                 5 RPL6WSND  BIT(1),    /* WHATRCV=SEND             */         
*                 5 RPL6WCFM  BIT(1),    /* WHATRCV=CONFIRM          */         
*                 5 RPL6WDAL  BIT(1),    /* WHATRCV=DEALLOCATE       */         
*                 5 RPL6WLOG  BIT(1),    /* WHATRCV=LOG_DATA @P051251*/         
*                 5 RPL6WPSH  BIT(1),    /* WHATRCV=PS_HEADER    @F3A*/         
*               4 RPL6RCV2   BIT(8),                                            
*                 5 RPL6WPSI  BIT(1),    /* WHATRCV=PARTIAL_PS_HEADER           
*                                                                @F3A*/         
*                 5 *         BIT(7),    /* NOT USED             @F3C*/         
*             3 RPL6RTUN    BIT(8),      /* RETURNED INDICATORS AS              
*                                           A RESULT OF APPCCMD      */         
*                 5 RPL6RMH5  BIT(1),    /* FMH5RCV INDICATOR        */         
*                 5 RPL6RLOG  BIT(1),    /* LOGRCV INDICATOR         */         
*                 5 RPL6RSIG  BIT(1),    /* SIGRCV INDICATOR         */         
*                 5 RPL6SECL  BIT(3),    /* LU SECURITY      @R507300*/         
*                   6 RPL6CLSA BIT(1),   /* PARTNER LU ACCEPTS SECURITY         
*                                           SUBFIELDS ON FMH5                   
*                                                            @P052802*/         
*                   6 RPL6AVFA BIT(1),   /* PARTNER LU ACCEPTS REQUESTS         
*                                           FOR ALREADY VERIFIED                
*                                                            @P052442*/         
*                   6 RPL6PV   BIT(1),   /* PARTNER LU ACCEPTS REQUESTS         
*                                           FOR PERSISTENT VERIFICATION         
*                                                                @B3C*/         
*                 5 RPL6CRYP  BIT(2),    /* ENCRYPTION LEVEL     @B1C*/         
*             3 RPL6MH5L      FIXED(8),  /* LENGTH OF THE FMH5                  
*                                           RECEIVED                 */         
*             3 RPL6CCST      FIXED(8),  /* CURRENT CONVERSATION                
*                                           STATE                @F3A*/         
*             3 RPL6ACTV      BIT(8),    /* RPL6 ACTIVE INDICATOR               
*                                           X'FF'=ACTIVE                        
*                                           X'00'=INACTIVE           */         
*             3 RPL6DETP      BIT(8),    /* SESSION DEACTIVATION                
*                                           TYPE CODE            @Q1A*/         
*             3 RPL6EXDL      FIXED(8),  /* LENGTH OF EXPEDITED DATA            
*                                           RECEIVED             @I3A*/         
*             3 RPL6TID       PTR,       /* TASK ID                             
*                                           (THE SUBLEVEL NAMES ARE             
*                                           REFERENCED BY VM SYSTEM) */         
*                 5 RPL6MID   FIXED(15), /* MACHINE ID               */         
*                 5 RPL6TIX   FIXED(15), /* TASK INDEX OF CURRENTLY             
*                                           EXECUTING TASK           */         
*             3 RPL6RPL     PTR(31),     /* POINTER BACK TO THE RPL  */         
*             3 RPL6STBF    PTR(31),     /* POINTER TO CURRENT BUFFER           
*                                           AT STORAGE SHORTAGE                 
*                                                            @P043961*/         
*             3 RPL6STDS    PTR(31),     /* CURRENT DISPLACEMENT IN             
*                                           BUFFER AT STORAGE SHORTAGE          
*                                                            @P043961*/         
*             3 RPL6VAOL    FIXED(15),   /* APPL-to-VTAM vectorlist             
*                                           area length          @D2C*/         
*             3 RPL6VAIL    FIXED(15),   /* VTAM-to-APPL vectorlist             
*                                           area length          @L2C*/         
*             3 RPL6NET     CHAR(8),     /* NQN Netid            @KGA*/         
*             3 RPL6CGID    FIXED(32),   /* Conversation Group ID               
*                                                                @KFA*/         
*             3 RPL6VAOA    PTR(31),     /* APPL-to-VTAM vectorlist             
*                                           area                 @D2C*/         
*             3 RPL6VAIA    PTR(31),     /* VTAM-to-APPL vectorlist             
*                                           area                 @L2C*/         
*             3 RPL6END     CHAR(0);     /* END OF RPL6              */         
*/********************************************************************/         
