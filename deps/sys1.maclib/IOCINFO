                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00241200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00241300
&ZOPN                 SETA  &ZOPN+1                                     00241400
&ZOPL(&ZOPN)          SETC  ' '                                         00241500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00241600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00241700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00241800
.ZMRETX               ANOP  ,                                           00241900
.*******************************************************************    00242000
.***AO.X03  REASON CODE PROCESSING                                      00242100
.*******************************************************************    00242200
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00242300
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00242400
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00242500
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00242600
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00242700
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00242800
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00242900
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00243000
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00243100
&ZOPN                 SETA  &ZOPN+1                                     00243200
&ZOPL(&ZOPN)          SETC  ' '                                         00243300
&ZOPO(&ZOPN)          SETC  'ST'                                        00243400
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00243500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00243600
                      AGO   .ZMRSNX                                     00243700
.*                                                                      00243800
.ZMRSN2               ANOP  ,                                           00243900
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00244000
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00244100
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00244200
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00244300
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00244400
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00244500
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00244600
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00244700
&ZOPN                 SETA  &ZOPN+1                                     00244800
&ZOPL(&ZOPN)          SETC  ' '                                         00244900
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00245000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00245100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00245200
.ZMRSNX               ANOP  ,                                           00245300
.*******************************************************************    00245400
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00245500
.*******************************************************************    00245600
.ZMFLX                ANOP  ,              MF=L PROCESSING              00245700
.*******************************************************************    00245800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00245900
.*******************************************************************    00246000
                      ACTR  10000                                       00246100
.*                                                                      00246200
                      LCLA  &ZLC           COLUMN POSITION              00246300
                      LCLA  &ZOPX          LIST TABLE INDEX             00246400
                      LCLC  &ZLL           LABEL                        00246500
                      LCLC  &ZLOP          OPERATION                    00246600
                      LCLC  &ZLV           VALUE                        00246700
                      LCLC  &ZLD           DESCRIPTION                  00246800
.*                                                                      00246900
&ZOPX                 SETA  1                                           00247000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00247100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00247200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00247300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00247400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00247500
                      AIF   (K'&ZLD LE 0).ZL5                           00247600
&ZLC                  SETA  35             COMMENT COLUMN               00247700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00247800
&ZLC                  SETA  33             COMMMENT COLUMN              00247900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00248000
&ZLV                  SETC  '&ZLV'.' '                                  00248100
                      AGO   .ZL3                                        00248200
.*                                                                      00248300
.ZL4                  ANOP  ,                                           00248400
&ZLV                  SETC  '&ZLV'.' '                                  00248500
.ZL5                  ANOP  ,                                           00248600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00248700
.*                                                                      00248800
&ZLL     &ZLOP &ZLV                                                     00248900
.*                                                                      00249000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00249100
                      AGO   .ZL2           CONTINUE LOOPING             00249200
.*                                                                      00249300
.ZLX                  ANOP  ,                                           00249400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00249500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00249600
.*-----------BAL invocation code-------------                           00249700
         LA    0,7                ++ SET IOCINFO FUNCTION CODE          00249800
         AIF    ('&XLINKAGE' NE 'BRANCH').LNKPC1                        00249900
.*********************************************************************  00250000
.*                                                                   *  00250100
.*  Get addressability to CVT                                        *  00250200
.*                                                                   *  00250300
.*********************************************************************  00250400
         L     15,16(0,0)                                               00250500
.*********************************************************************  00250600
.*                                                                   *  00250700
.*  Get addressability to IOCOM                                      *  00250800
.*                                                                   *  00250900
.*********************************************************************  00251000
         L     15,124(15,0)                                             00251100
.*********************************************************************  00251200
.*                                                                   *  00251300
.*  Get addressability to IOVT                                       *  00251400
.*                                                                   *  00251500
.*********************************************************************  00251600
         L     15,208(15,0)                                             00251700
         L     15,12(15,0)        ++ SET ADDR OF UCB SERVICES           00251800
         BALR  14,15              ++ BRANCH TO UCB SERVICES             00251900
         AGO   .DONE1                                                   00252000
.LNKPC1  ANOP                                                           00252100
         L     14,16(0,0)         ++ GET CVT ADDRESS                    00252200
         L     14,772(14,0)       ++ GET SFT ADDRESS                    00252300
.*********************************************************************  00252400
.*                                                                   *  00252500
.*  Get LX/EX for UCB Services                                       *  00252600
.*                                                                   *  00252700
.*  Note: LX/EX is calculated by multiplying the PC number (79)      *  00252800
.*        times the length of a PC/AUTH table entry (4) and          *  00252900
.*        then subtracting 4.                                        *  00253000
.*                                                                   *  00253100
.*********************************************************************  00253200
         L     14,312(14,0)       ++ GET LX/EX FOR UCB SERVICES         00253300
         PC    0(14)              ++ PC TO UCB SERVICES                 00253400
.DONE1   ANOP                                                           00253500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00253600
.ZLINKX               ANOP  ,                                           00253700
.*******************************************************************    00253800
.***AP.X01  FORMAT GENERATED CODE                                       00253900
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00254000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00254100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00254200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00254300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00254400
                      AIF   (K'&ZLD LE 0).ZL51                          00254500
&ZLC                  SETA  35             COMMENT COLUMN               00254600
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00254700
&ZLC                  SETA  33             COMMENT COLUMN               00254800
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00254900
&ZLV                  SETC  '&ZLV'.' '                                  00255000
                      AGO   .ZL31                                       00255100
.*                                                                      00255200
.ZL41                 ANOP  ,                                           00255300
&ZLV                  SETC  '&ZLV'.' '                                  00255400
.ZL51                 ANOP  ,                                           00255500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00255600
.*                                                                      00255700
&ZLL     &ZLOP &ZLV                                                     00255800
.*                                                                      00255900
