                  AIF   ('&IOCAREA' NE '').ZG47B  KEYS?                 00081300
                  AIF   ('&IOCAREASIZE' NE '').ZG47B  KEYS?             00081400
                  AIF   ('&UCBSTOR' NE '').ZG47B  KEYS?                 00081500
                  AIF   ('&DCMINFO' NE '').ZG47B  KEYS?                 00081600
                  AIF   ('&CSSID' NE '').ZG47B  KEYS?                   00081700
                  AIF   ('&IOFACILITIES' NE '').ZG47B  KEYS?            00081800
                  AIF   ('&IODFINFO' NE '').ZG47B  KEYS?                00081900
                  AIF   ('&LINKAGE' NE '').ZG47B  KEYS?                 00082000
                  AIF   ('&RETCODE' NE '').ZG47B  KEYS?                 00082100
                  AIF   ('&RSNCODE' NE '').ZG47B  KEYS?                 00082200
                  AGO    .ZG47C     INVOCATION OKAY                     00082300
.ZG47B            ANOP  ,                                               00082400
&ZMS              SETC  'THE "L" FORM OF "MF"'                          00082500
&ZMS              SETC  '&ZMS CAN ONLY HAVE'                            00082600
&ZMS              SETC  '&ZMS THE "PLISTVER" KEY'                       00082700
                  MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'          00082800
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00082900
.ZG47C            ANOP  ,                                               00083000
.*-------------------------------------------------------------------   00083100
