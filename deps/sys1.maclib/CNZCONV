.***AC.PLV01  PROCESS PLISTVER                                      *   00071800
.********************************************************************   00071900
&XPLISTVER      SETA  1                                                 00072000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00072100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00072200
&XPLISTVER      SETA  1                                                 00072300
.ZPLV0          ANOP  ,                                                 00072400
.*                                                                      00072500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00072600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00072700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00072800
.*-------------------------------------------------------------------   00072900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00073000
&ZMS            SETC  '"PLISTVER='                                      00073100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00073200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00073300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00073400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00073500
                AGO   .ZPVCK2                                           00073600
.ZPVCK3         ANOP  ,                                                 00073700
.*-------------------------------------------------------------------   00073800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00073900
&ZMS            SETC  '"PLISTVER='                                      00074000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00074100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00074200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00074300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00074400
                AGO   .ZPVCKE                                           00074500
.ZPVCK2         ANOP  ,                                                 00074600
.*-------------------------------------------------------------------   00074700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00074800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00074900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00075000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00075100
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00075200
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00075300
.ZPVCK6         ANOP  ,                                                 00075400
&ZMS            SETC  '"PLISTVER='                                      00075500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00075600
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00075700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00075800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00075900
&XPLISTVER      SETA  1                                                 00076000
                AGO   .ZPVCKE                                           00076100
.*-------------------------------------------------------------------   00076200
.ZPVCK4A        ANOP  ,                                                 00076300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00076400
&ZMS            SETC  'PLISTVER='                                       00076500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00076600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00076700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00076800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00076900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00077000
                AGO   .ZPVCK5                                           00077100
.ZPVCK4         ANOP  ,                                                 00077200
&XPLISTVER      SETA  &PLISTVER(1)                                      00077300
.ZPVCK5         ANOP  ,                                                 00077400
.ZPVCKE         ANOP  ,                                                 00077500
.********************************************************************   00077600
