.******************************************************************     00189300
.***AI.L01 CNZCONV FMT=LIST FILL IN PARAMETER AREA                      00189400
.******************************************************************     00189500
                      ACTR  10000                                       00189600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00189700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00189800
&ZCVAL                SETC  'LAE'          XAX MODE                     00189900
.ZIMFXA               ANOP ,                                            00190000
.*                                                                      00190100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00190200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00190300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00190400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00190500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00190600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00190700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00190800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00190900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00191000
&ZOPN                 SETA  &ZOPN+1                                     00191100
&ZOPL(&ZOPN)          SETC  ' '                                         00191200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00191300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00191400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00191500
                      AGO   .ZIMFNX                                     00191600
.*                                                                      00191700
.ZIMFN2               ANOP ,                                            00191800
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00191900
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00192000
&ZOPN                 SETA  &ZOPN+1                                     00192100
&ZOPL(&ZOPN)          SETC  ' '                                         00192200
&ZOPO(&ZOPN)          SETC  'LAE'                                       00192300
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00192400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00192500
                      AGO   .ZIMFNX                                     00192600
.ZIMFNX1              ANOP ,                                            00192700
&ZOPN                 SETA  &ZOPN+1                                     00192800
&ZOPL(&ZOPN)          SETC  ' '                                         00192900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00193000
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00193100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00193200
.ZIMFNX               ANOP ,                                            00193300
.********************************************************************   00193400
