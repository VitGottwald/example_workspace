.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00154100
.******************************************************************     00154200
                     ACTR  10000                                        00154300
.*----------------------------------------------------------------***   00154400
.***AH.P02 EXTRA POSITIONALS                                            00154500
                     AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS         00154600
                     AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL         00154700
                     AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL     00154800
                     AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL      00154900
.ZGLISTX1            ANOP  ,                                            00155000
&ZCVAL               SETC  ''          ERROR FIELD                      00155100
&ZAWRK               SETA  0           POSITIONAL NUMBER                00155200
.ZGLIST2             ANOP  ,                                            00155300
&ZAWRK               SETA  &ZAWRK+1    NEXT POSITIONAL                  00155400
                     AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE         00155500
&ZCVAL               SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA              00155600
                     AGO   .ZGLIST2                                     00155700
.*                                                                      00155800
.ZGLIST3             ANOP  ,                                            00155900
&ZCVAL               SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA           00156000
&ZMS                 SETC  'POSITIONAL ARGUMENT(S),'                    00156100
&ZMS                 SETC  '&ZMS "&ZCVAL"'                              00156200
&ZMS                 SETC  '&ZMS CANNOT BE USED.'                       00156300
&ZMS                 SETC  '&ZMS NO POSITIONAL ARGUMENTS'               00156400
                     MNOTE 8,' &ZMS ARE ALLOWED.'                       00156500
&ZMACRET             SETA  8           SET MACRO ERROR RETURN CODE      00156600
.ZGLISTX             ANOP  ,                                            00156700
&ZASYSLNDX           SETA  0                                            00156800
