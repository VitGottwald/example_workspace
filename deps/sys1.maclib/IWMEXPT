&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00125100
.ZALVL6B              ANOP                                              00125200
&ZOPD(&ZOPN)          SETC  '++ IWMEXPT ARG PARMS'                      00125300
.*                                                                      00125400
&ZOPN                 SETA  &ZOPN+1                                     00125500
&ZOPL(&ZOPN)          SETC  ' '                                         00125600
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00125700
