.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00156700
.*******************************************************************    00156800
                      ACTR  10000                                       00156900
.*                                                                      00157000
                      LCLA  &ZLC           COLUMN POSITION              00157100
                      LCLA  &ZOPX          LIST TABLE INDEX             00157200
                      LCLC  &ZLL           LABEL                        00157300
                      LCLC  &ZLOP          OPERATION                    00157400
                      LCLC  &ZLV           VALUE                        00157500
                      LCLC  &ZLD           DESCRIPTION                  00157600
.*                                                                      00157700
&ZOPX                 SETA  1                                           00157800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00157900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00158000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00158100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00158200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00158300
                      AIF   (K'&ZLD LE 0).ZL5                           00158400
&ZLC                  SETA  35             COMMENT COLUMN               00158500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00158600
&ZLC                  SETA  33             COMMMENT COLUMN              00158700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00158800
&ZLV                  SETC  '&ZLV'.' '                                  00158900
                      AGO   .ZL3                                        00159000
.*                                                                      00159100
.ZL4                  ANOP  ,                                           00159200
&ZLV                  SETC  '&ZLV'.' '                                  00159300
.ZL5                  ANOP  ,                                           00159400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00159500
.*                                                                      00159600
&ZLL     &ZLOP &ZLV                                                     00159700
.*                                                                      00159800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00159900
                      AGO   .ZL2           CONTINUE LOOPING             00160000
.*                                                                      00160100
.ZLX                  ANOP  ,                                           00160200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00160300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00160400
         L     14,16(0,0)              GET CVT ADDRESS                  00160500
         L     14,772(14,0)            GET SFT ADDRESS                  00160600
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00160700
         LA    15,86                   GET EX FOR SERVICE ROUTINE       00160800
         OR    14,15                   GET PC NUMBER                    00160900
         PC    0(14)                                                    00161000
END&SYSNDX DS    0H                    END LABEL                        00161100
