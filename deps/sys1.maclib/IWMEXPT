.********************************************************************   00122000
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00122100
.********************************************************************   00122200
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00122300
                      LCLA  &ZOPN1         INVOCATION INDEX             00122400
                      LCLC  &ZOPL(4095)    LABEL                        00122500
                      LCLC  &ZOPO(4095)    OPERATION                    00122600
                      LCLC  &ZOPV(4095)    VALUE                        00122700
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00122800
.*                                                                      00122900
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00123000
.******************************************************************     00123100
.***AL.L10 IWMEXPT FMT=LIST MF=S!L PARAMETERS                           00123200
.******************************************************************     00123300
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00123400
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00123500
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00123600
&XMFATTR              SETC  '0D'                                        00123700
.*                                                                      00123800
&ZOPN                 SETA  &ZOPN+1                                     00123900
&ZOPL(&ZOPN)          SETC  ' '                                         00124000
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00124100
&ZOPO(&ZOPN)          SETC  'B'                                         00124200
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00124300
                      AGO   .ZALVL6B                                    00124400
.ZALVL6A              ANOP                                              00124500
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00124600
