**/ IDCDF70_2:;                                                         24310001
* %IDCDF70_INCLUDED = 'YES';                                            24320001
* %DCL ZCBPRINT CHAR EXT;                                               24330001
* %DEACTIVATE ZCBPRINT;                                                 24340001
* %DCL IDCDF70_LIST CHAR EXT;                                           24350001
* %DEACTIVATE IDCDF70_LIST;                                             24360001
* %IF IDCDF70_LIST ^= 'NO' &                                            24370001
*    ZCBPRINT ^= 'NO' %THEN                                             24380001
*   %GOTO IDCDF70_3;                                                    24390001
*   @LIST PUSH NOECHO;                                                  24400001
*   @LIST NOASSEMBLE NOECHO;                                            24410001
*   @LIST OFF C NOECHO;                                                 24420001
* %IDCDF70_3:;                                                          24430001
*/* Start of PL/X Source                                             */ 24440001
*/********************************************************************/ 24450001
*/*VALID VALUES FOR THE SSGAVOL FIELD WHEN A SUBSYSTEM IS DESIRED ARE*/ 24460001
*/*'FFFFFFFFFFXX'X WHERE XX IS X'02' FOR AN EMULATED 3990 SUBSYSTEM  */ 24470001
*/*'FFFFFFFFFFXX'X WHERE XX IS X'0B' FOR AN IBM 3880 MODEL 23        */ 24480001
*/*'FFFFFFFFFFXX'X WHERE XX IS X'0C' FOR AN IBM 3990 MODEL 03        */ 24490001
*/*'FFFFFFFFFFXX'X WHERE XX IS X'E9' FOR AN IBM 3990 MODEL 09        */ 24500001
*/*'FFFFFFFFFFXX'X WHERE XX IS X'E8' FOR AN IBM 2105,2107,1750       */ 24510001
*/********************************************************************/ 24520001
*DCL 1 SSGARGL BDY(DWORD) BASED,                                        24530001
*      2 SSGHEAD CHAR(8),       /* SSGARGL IDENTIFIER               */  24540001
*      2 SSGADDN PTR(31),       /* DDNAME ADDRESS                   */  24550001
*      2 SSGAVOL PTR(31),       /* VOLUME ADDRESS                   */  24560001
*      2 SSGUNIT BIT(32),       /* DEVICE TYPE                      */  24570001
*        3 SSGUNIT1 BIT(8),     /* GENERAL FLAGS                    */  24580001
*        3 SSGUNIT2 BIT(8),     /* GENERAL FLAGS                    */  24590001
*        3 SSGUNIT3 BIT(8),     /* DEVICE CLASS                     */  24600001
*        3 SSGUNIT4 BIT(8),     /* DEVICE TYPE WITHIN THE CLASS     */  24610001
*      2 SSGOLN FIXED(31),      /* LENGTH OF OUTPUT BUFFER          */  24620001
*      2 SSGOADR PTR(31),       /* ADDRESS OF OUTPUT BUFFER         */  24630001
*      2 SSGOPT BIT(16),        /* OPTIONS BYTE                @H17C*/  24640001
