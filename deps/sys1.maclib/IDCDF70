*DCL SS01CC00 FIXED(15) CONSTANT(0);  /* SUCCESSFUL COMPLETION      */  49010001
*DCL SS01CC04 FIXED(15) CONSTANT(4);  /* GETMAIN FAILED             */  49020001
*DCL SS01CC08 FIXED(15) CONSTANT(8);  /* I/O ERROR OCCURRED         */  49030001
*DCL SS01CC12 FIXED(15) CONSTANT(12); /* VOLUME NOT FOUND           */  49040001
*DCL SS01CC16 FIXED(15) CONSTANT(16); /* ESTAE FAILED               */  49050001
*DCL SS01CC20 FIXED(15) CONSTANT(20); /* LRA INSTRUCTION FAILED     */  49060001
*DCL SS01CC24 FIXED(15) CONSTANT(24); /* BUFFER NOT LARGE ENOUGH    */  49070001
*DCL SS01CC28 FIXED(15) CONSTANT(28); /* FILE NOT IN TIOT      @H19A*/  49080001
*DCL SS01CC32 FIXED(15) CONSTANT(32); /* VOLUME NOT FOUND ALL  @H19A*/  49090001
*DCL SS01CC36 FIXED(15) CONSTANT(36); /* NO PATH(S) TO SD(S)    @H7A*/  49100001
*DCL SS01CC40 FIXED(15) CONSTANT(40); /* NO ONLINE PATH FROM IOS@H7A*/  49110001
*DCL SS01CC44 FIXED(15) CONSTANT(44); /* REQUEST NOT SUPPORTED @H18A*/  49120001
*DCL SS01CC48 FIXED(15) CONSTANT(48); /* WARNING MSG ISSUED    @H19A*/  49130001
*DCL SS01CC52 FIXED(15) CONSTANT(52); /* ERROR MSG ISSUED      @H19A*/  49140001
