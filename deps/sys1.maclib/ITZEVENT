.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00343500
&ZPLLEN               SETA 222                                          00343600
.*                                                                      00343700
.******************************************************************     00343800
.***AI.L01 ITZEVENT FMT=LIST FILL IN PARAMETER AREA                     00343900
.******************************************************************     00344000
                      ACTR  10000                                       00344100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00344200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00344300
&ZCVAL                SETC  'LAE'          XAX MODE                     00344400
.ZIMFXA               ANOP ,                                            00344500
.*                                                                      00344600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00344700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00344800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00344900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00345000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00345100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00345200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00345300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00345400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00345500
&ZOPN                 SETA  &ZOPN+1                                     00345600
&ZOPL(&ZOPN)          SETC  ' '                                         00345700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00345800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00345900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00346000
                      AGO   .ZIMFNX                                     00346100
.*                                                                      00346200
.ZIMFN2               ANOP ,                                            00346300
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00346400
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00346500
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00346600
&ZOPN                 SETA  &ZOPN+1                                     00346700
&ZOPL(&ZOPN)          SETC  ' '                                         00346800
&ZOPO(&ZOPN)          SETC  'LAE'                                       00346900
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00347000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00347100
                      AGO   .ZIMFNX                                     00347200
.ZIMFNX1              ANOP ,                                            00347300
&ZOPN                 SETA  &ZOPN+1                                     00347400
&ZOPL(&ZOPN)          SETC  ' '                                         00347500
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00347600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00347700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00347800
.ZIMFNX               ANOP ,                                            00347900
.********************************************************************   00348000
.***AI.L02 ITZEVENT FMT=LIST INITIALIZE PARAMETER AREA                  00348100
.********************************************************************   00348200
.***AI.L03 MF=(SLME)                                                    00348300
