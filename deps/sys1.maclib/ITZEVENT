.ZI33A                ANOP  ,                                           00391500
.*                                                                      00391600
                      AIF   ('&XMONTKN64'(1,1) NE '(').ZI33B            00391700
&ZCGPR   SETC  '0('.'&XMONTKN64'(2,K'&XMONTKN64-2).')'                  00391800
                      AGO   .ZI33D                                      00391900
.ZI33B                ANOP  ,                                           00392000
.*                                                                      00392100
&ZC                   SETC  '&XMONTKN64'(1,1)                           00392200
                      AIF   (T'&ZC NE 'N').ZI33D                        00392300
