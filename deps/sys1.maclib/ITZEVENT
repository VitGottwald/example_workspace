.***AC.PLV01  PROCESS PLISTVER                                      *   00083400
.********************************************************************   00083500
&XPLISTVER      SETA  0                                                 00083600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00083700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00083800
&XPLISTVER      SETA  1                                                 00083900
.ZPLV0          ANOP  ,                                                 00084000
.*                                                                      00084100
                AIF   ('&MONTKN' EQ '').ZPLV1E                          00084200
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00084300
&XPLISTVER      SETA  1                                                 00084400
.ZPLV1E         ANOP  ,                                                 00084500
.*                                                                      00084600
                AIF   ('&MONTKN64' EQ '').ZPLV2E                        00084700
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00084800
&XPLISTVER      SETA  1                                                 00084900
.ZPLV2E         ANOP  ,                                                 00085000
.*                                                                      00085100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00085200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00085300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00085400
.*-------------------------------------------------------------------   00085500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00085600
&ZMS            SETC  '"PLISTVER='                                      00085700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00085800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00085900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00086000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00086100
                AGO   .ZPVCK2                                           00086200
.ZPVCK3         ANOP  ,                                                 00086300
.*-------------------------------------------------------------------   00086400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00086500
&ZMS            SETC  '"PLISTVER='                                      00086600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00086700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00086800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00086900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00087000
                AGO   .ZPVCKE                                           00087100
.ZPVCK2         ANOP  ,                                                 00087200
.*-------------------------------------------------------------------   00087300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00087400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00087500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00087600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00087700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00087800
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00087900
.ZPVCK6         ANOP  ,                                                 00088000
&ZMS            SETC  '"PLISTVER='                                      00088100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00088200
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00088300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00088400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00088500
&XPLISTVER      SETA  1                                                 00088600
                AGO   .ZPVCKE                                           00088700
.*-------------------------------------------------------------------   00088800
.ZPVCK4A        ANOP  ,                                                 00088900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00089000
&ZMS            SETC  'PLISTVER='                                       00089100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00089200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00089300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00089400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00089500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00089600
                AGO   .ZPVCK5                                           00089700
.ZPVCK4         ANOP  ,                                                 00089800
&XPLISTVER      SETA  &PLISTVER(1)                                      00089900
.ZPVCK5         ANOP  ,                                                 00090000
.ZPVCKE         ANOP  ,                                                 00090100
.********************************************************************   00090200
