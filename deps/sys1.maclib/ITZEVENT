                      AIF   (K'&ZLD LE 0).ZL5                           00396800
&ZLC                  SETA  35             COMMENT COLUMN               00396900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00397000
&ZLC                  SETA  33             COMMMENT COLUMN              00397100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00397200
&ZLV                  SETC  '&ZLV'.' '                                  00397300
                      AGO   .ZL3                                        00397400
.*                                                                      00397500
.ZL4                  ANOP  ,                                           00397600
&ZLV                  SETC  '&ZLV'.' '                                  00397700
.ZL5                  ANOP  ,                                           00397800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00397900
.*                                                                      00398000
&ZLL     &ZLOP &ZLV                                                     00398100
.*                                                                      00398200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00398300
                      AGO   .ZL2           CONTINUE LOOPING             00398400
.*                                                                      00398500
.ZLX                  ANOP  ,                                           00398600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00398700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00398800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00398900
        L     15,616(15,0)                                         @L1A 00399000
        L     15,12(15,0)          Get event rtn address           @L1A 00399100
        BALR  14,15                Invoke TTrace Event Rtn         @L1A 00399200
        B     &EDONE               Done                            @L1A 00399300
&ENOTR   DS    0H                                                  @L1A 00399400
        LA    15,4                                                 @L1A 00399500
        LA    0,1026                                               @L1A 00399600
&EDONE   DS    0H                                                  @L1A 00399700
