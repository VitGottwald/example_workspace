.***AC.PLV01  PROCESS PLISTVER                                      *   00059700
.********************************************************************   00059800
&XPLISTVER      SETA  0                                                 00059900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00060000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00060100
&XPLISTVER      SETA  0                                                 00060200
.ZPLV0          ANOP  ,                                                 00060300
.*                                                                      00060400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00060500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00060600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00060700
.*-------------------------------------------------------------------   00060800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00060900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00061000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00061100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00061200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00061300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00061400
                AGO   .ZPVCK2                                           00061500
.ZPVCK3         ANOP  ,                                                 00061600
.*-------------------------------------------------------------------   00061700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00061800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00061900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00062100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00062200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062300
                AGO   .ZPVCKE                                           00062400
.ZPVCK2         ANOP  ,                                                 00062500
.*-------------------------------------------------------------------   00062600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00062700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00062800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00062900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00063000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00063100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00063200
.ZPVCK6         ANOP  ,                                                 00063300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00063400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00063600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00063700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063800
&XPLISTVER      SETA  0                                                 00063900
                AGO   .ZPVCKE                                           00064000
.*-------------------------------------------------------------------   00064100
.ZPVCK4A        ANOP  ,                                                 00064200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00064300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00064400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00064500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00064600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00064700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00064800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064900
                AGO   .ZPVCK5                                           00065000
.ZPVCK4         ANOP  ,                                                 00065100
&XPLISTVER      SETA  &PLISTVER(1)                                      00065200
.ZPVCK5         ANOP  ,                                                 00065300
.ZPVCKE         ANOP  ,                                                 00065400
.********************************************************************   00065500
