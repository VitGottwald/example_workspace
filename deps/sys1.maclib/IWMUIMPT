.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00139300
.*******************************************************************    00139400
                      ACTR  10000                                       00139500
.*                                                                      00139600
                      LCLA  &ZLC           COLUMN POSITION              00139700
                      LCLA  &ZOPX          LIST TABLE INDEX             00139800
                      LCLC  &ZLL           LABEL                        00139900
                      LCLC  &ZLOP          OPERATION                    00140000
                      LCLC  &ZLV           VALUE                        00140100
                      LCLC  &ZLD           DESCRIPTION                  00140200
.*                                                                      00140300
&ZOPX                 SETA  1                                           00140400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00140500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00140600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00140700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00140800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00140900
                      AIF   (K'&ZLD LE 0).ZL5                           00141000
&ZLC                  SETA  35             COMMENT COLUMN               00141100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00141200
&ZLC                  SETA  33             COMMMENT COLUMN              00141300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00141400
&ZLV                  SETC  '&ZLV'.' '                                  00141500
                      AGO   .ZL3                                        00141600
.*                                                                      00141700
.ZL4                  ANOP  ,                                           00141800
&ZLV                  SETC  '&ZLV'.' '                                  00141900
.ZL5                  ANOP  ,                                           00142000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00142100
.*                                                                      00142200
&ZLL     &ZLOP &ZLV                                                     00142300
.*                                                                      00142400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00142500
                      AGO   .ZL2           CONTINUE LOOPING             00142600
.*                                                                      00142700
.ZLX                  ANOP  ,                                           00142800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00142900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00143000
