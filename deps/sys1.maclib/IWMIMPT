.***AC.PLV01  PROCESS PLISTVER                                      *   00064700
.********************************************************************   00064800
&XPLISTVER      SETA  0                                                 00064900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00065000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00065100
&XPLISTVER      SETA  0                                                 00065200
.ZPLV0          ANOP  ,                                                 00065300
.*                                                                      00065400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00065500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00065600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00065700
.*-------------------------------------------------------------------   00065800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00065900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00066000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00066200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00066300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066400
                AGO   .ZPVCK2                                           00066500
.ZPVCK3         ANOP  ,                                                 00066600
.*-------------------------------------------------------------------   00066700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00066800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00066900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00067000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00067100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00067200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067300
                AGO   .ZPVCKE                                           00067400
.ZPVCK2         ANOP  ,                                                 00067500
.*-------------------------------------------------------------------   00067600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00067700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00067800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00067900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00068000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00068100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00068200
.ZPVCK6         ANOP  ,                                                 00068300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00068400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00068600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00068700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068800
&XPLISTVER      SETA  0                                                 00068900
                AGO   .ZPVCKE                                           00069000
.*-------------------------------------------------------------------   00069100
.ZPVCK4A        ANOP  ,                                                 00069200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00069300
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00069400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00069500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00069600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00069700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00069800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069900
                AGO   .ZPVCK5                                           00070000
.ZPVCK4         ANOP  ,                                                 00070100
&XPLISTVER      SETA  &PLISTVER(1)                                      00070200
.ZPVCK5         ANOP  ,                                                 00070300
.ZPVCKE         ANOP  ,                                                 00070400
.********************************************************************   00070500
