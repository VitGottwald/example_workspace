.********************************************************************   00118200
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00118300
.********************************************************************   00118400
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00118500
                      LCLA  &ZOPN1         INVOCATION INDEX             00118600
                      LCLC  &ZOPL(4095)    LABEL                        00118700
                      LCLC  &ZOPO(4095)    OPERATION                    00118800
                      LCLC  &ZOPV(4095)    VALUE                        00118900
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00119000
.*                                                                      00119100
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00119200
.******************************************************************     00119300
.***AL.L10 IWMIMPT FMT=LIST MF=S!L PARAMETERS                           00119400
.******************************************************************     00119500
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00119600
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00119700
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00119800
&XMFATTR              SETC  '0D'                                        00119900
.*                                                                      00120000
&ZOPN                 SETA  &ZOPN+1                                     00120100
&ZOPL(&ZOPN)          SETC  ' '                                         00120200
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00120300
&ZOPO(&ZOPN)          SETC  'B'                                         00120400
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00120500
                      AGO   .ZALVL6B                                    00120600
.ZALVL6A              ANOP                                              00120700
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00120800
&ZOPV(&ZOPN)          SETC  '4,8'                                       00120900
&ZOPN                 SETA  &ZOPN+1                                     00121000
&ZOPL(&ZOPN)          SETC  ' '                                         00121100
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00121200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00121300
.ZALVL6B              ANOP                                              00121400
&ZOPD(&ZOPN)          SETC  '++ IWMIMPT ARG PARMS'                      00121500
.*                                                                      00121600
&ZOPN                 SETA  &ZOPN+1                                     00121700
&ZOPL(&ZOPN)          SETC  ' '                                         00121800
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00121900
&ZOPV(&ZOPN)          SETC  '1'                                         00122000
&ZOPD(&ZOPN)          SETC  ' '                                         00122100
.*                                                                      00122200
.ZLMFSL2              ANOP  ,                                           00122300
