.*******************************************************************    00282400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00282500
.*******************************************************************    00282600
                      ACTR  10000                                       00282700
.*                                                                      00282800
                      LCLA  &ZLC           COLUMN POSITION              00282900
                      LCLA  &ZOPX          LIST TABLE INDEX             00283000
                      LCLC  &ZLL           LABEL                        00283100
                      LCLC  &ZLOP          OPERATION                    00283200
                      LCLC  &ZLV           VALUE                        00283300
                      LCLC  &ZLD           DESCRIPTION                  00283400
.*                                                                      00283500
&ZOPX                 SETA  1                                           00283600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00283700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00283800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00283900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00284000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00284100
                      AIF   (K'&ZLD LE 0).ZL5                           00284200
&ZLC                  SETA  35             COMMENT COLUMN               00284300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00284400
&ZLC                  SETA  33             COMMMENT COLUMN              00284500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00284600
&ZLV                  SETC  '&ZLV'.' '                                  00284700
                      AGO   .ZL3                                        00284800
.*                                                                      00284900
.ZL4                  ANOP  ,                                           00285000
&ZLV                  SETC  '&ZLV'.' '                                  00285100
.ZL5                  ANOP  ,                                           00285200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00285300
.*                                                                      00285400
&ZLL     &ZLOP &ZLV                                                     00285500
.*                                                                      00285600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00285700
                      AGO   .ZL2           CONTINUE LOOPING             00285800
.*                                                                      00285900
.ZLX                  ANOP  ,                                           00286000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00286100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00286200
         L     14,16(0,0)              GET CVT ADDRESS                  00286300
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00286400
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00286500
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00286600
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00286700
         B     END&SYSNDX              BRANCH TO END LABEL              00286800
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00286900
         L     14,772(14,0)            GET SFT ADDRESS                  00287000
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00287100
         LA    15,1                    GET EX FOR SERVICE ROUTINE       00287200
         OR    14,15                   GET PC NUMBER                    00287300
         PC    0(14)                                                    00287400
END&SYSNDX DS    0H                    END LABEL                        00287500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00287600
.ZLINKX               ANOP  ,                                           00287700
.*******************************************************************    00287800
.***AP.X01  FORMAT GENERATED CODE                                       00287900
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00288000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00288100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00288200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00288300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00288400
                      AIF   (K'&ZLD LE 0).ZL51                          00288500
&ZLC                  SETA  35             COMMENT COLUMN               00288600
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00288700
&ZLC                  SETA  33             COMMENT COLUMN               00288800
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00288900
&ZLV                  SETC  '&ZLV'.' '                                  00289000
                      AGO   .ZL31                                       00289100
.*                                                                      00289200
.ZL41                 ANOP  ,                                           00289300
&ZLV                  SETC  '&ZLV'.' '                                  00289400
.ZL51                 ANOP  ,                                           00289500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00289600
.*                                                                      00289700
&ZLL     &ZLOP &ZLV                                                     00289800
.*                                                                      00289900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00290000
                      AGO   .ZL21          CONTINUE LOOPING             00290100
.*                                                                      00290200
.ZLX1                 ANOP  ,                                           00290300
.*******************************************************************    00290400
.***AP.X02  END OF IWMMCREA MACRO                                       00290500
&ZIWMMCREA            SETC  'YES'          SET IWMMCREA USED            00290600
         POP   PRINT                                                    00290700
*                                            IWMMCREA-2                 00290800
         SPACE 1                                                        00290900
.ZLX2                 ANOP  ,                                           00291000
         MEND                                                           00291100
**********************************************************************/ 00291200
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00291300
*%IWMMCREA: MACRO KEYS(REQTYPE,MONTKN,AMOUNT,MONTKN_LIST,               00291400
*                 MONTKN_LISTLEN,REPORTONLY,SUBSYSP,CONNTKN,SUBSYS,     00291500
*                 SUBSYSNM,MONTKNKEYP,MONTKNKEY,RETCODE,RSNCODE,        00291600
*                 PLISTVER,MF);                                         00291700
*  ANS('?'!!MACLABEL!!' '!!'IWMMCREP '!!MACLIST!!                       00291800
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00291900
*%END IWMMCREA;                                                         00292000
