.***AC.PLV01  PROCESS PLISTVER                                      *   00092200
.********************************************************************   00092300
&XPLISTVER      SETA  0                                                 00092400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00092500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00092600
&XPLISTVER      SETA  2                                                 00092700
.ZPLV0          ANOP  ,                                                 00092800
.*                                                                      00092900
                AIF   ('&REQTYPE' EQ '').ZPLV1E                         00093000
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00093100
&XPLISTVER      SETA  1                                                 00093200
.ZPLV1E         ANOP  ,                                                 00093300
.*                                                                      00093400
                AIF   ('&AMOUNT' EQ '').ZPLV2E                          00093500
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00093600
&XPLISTVER      SETA  1                                                 00093700
.ZPLV2E         ANOP  ,                                                 00093800
.*                                                                      00093900
                AIF   ('&MONTKN_LIST' EQ '').ZPLV3E                     00094000
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00094100
&XPLISTVER      SETA  1                                                 00094200
.ZPLV3E         ANOP  ,                                                 00094300
.*                                                                      00094400
                AIF   ('&MONTKN_LISTLEN' EQ '').ZPLV4E                  00094500
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00094600
&XPLISTVER      SETA  1                                                 00094700
.ZPLV4E         ANOP  ,                                                 00094800
.*                                                                      00094900
                AIF   ('&REPORTONLY' EQ '').ZPLV5E                      00095000
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00095100
&XPLISTVER      SETA  2                                                 00095200
.ZPLV5E         ANOP  ,                                                 00095300
.*                                                                      00095400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00095500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00095600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00095700
.*-------------------------------------------------------------------   00095800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00095900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00096000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00096100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00096200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00096300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00096400
                AGO   .ZPVCK2                                           00096500
.ZPVCK3         ANOP  ,                                                 00096600
.*-------------------------------------------------------------------   00096700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00096800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00096900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00097000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00097100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00097200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00097300
                AGO   .ZPVCKE                                           00097400
.ZPVCK2         ANOP  ,                                                 00097500
.*-------------------------------------------------------------------   00097600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00097700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00097800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00097900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00098000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00098100
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00098200
.ZPVCK6         ANOP  ,                                                 00098300
