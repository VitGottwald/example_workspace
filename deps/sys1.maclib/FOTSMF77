* SMF_119SSH_FSOperGET     Fixed Constant(04x), /* Get               */         
* SMF_119SSH_FSOperPUT     Fixed Constant(05x), /* Put               */         
* SMF_119SSH_FSOperCHMD    Fixed Constant(06x), /* Chmod             */         
