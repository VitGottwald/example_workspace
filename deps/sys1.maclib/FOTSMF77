*%dcl fotsmf77_inc Fixed external;                                              
*%if fotsmf77_inc=0 %then %do;                                                  
*%fotsmf77_inc=1;                                                               
*%GOTO FOT771; /*                                                               
         MACRO                                                                  
         FOTSMF77 &SSH=YES,       YES = GEN SSH SMF DSECTS             X        
               &HEADER=YES        YES = SMF HEADER DSECTS FOR ALL RECS          
         AGO   .M010000      */                                                 
*%FOT771: ;                                                                     
*/********************* START OF SPECIFICATIONS **********************/         
*/*                                                                  */         
*/* MACRO NAME:       FOTSMF77                                       */         
*/*                                                                  */         
*/* DESCRIPTIVE NAME: SMF Record Type 119 Mapping for OpenSSH        */         
*/*                                                                  */         
*/* COMPONENT NAME:   OpenSSH for z/OS                               */         
*/*                                                                  */         
*/* LICENSED MATERIALS - PROPERTY OF IBM                             */         
*/*                                                                  */         
*/* 5655-M23                                                         */         
*/*                                                                  */         
*/*  COPYRIGHT IBM CORP. 2010, 2018                                  */         
*/*                                                                  */         
*/* US GOVERNMENT USERS RESTRICTED RIGHTS - USE,                     */         
*/* DUPLICATION OR DISCLOSURE RESTRICTED BY GSA ADP                  */         
*/* SCHEDULE CONTRACT WITH IBM CORP.                                 */         
*/*                                                                  */         
*/* Status:    HOS2240                                               */         
*/*                                                                  */         
*/* Function:           This macro is used to map SMF type 119       */         
*/*                     records for OpenSSH.                         */         
*/*                                                                  */         
*/* Part Type:          MACRO                                        */         
*/*                                                                  */         
*/* Method of Access:   ASSEMBLER                                    */         
*/*                                                                  */         
*/* Notes for Use:      None.                                        */         
*/*                                                                  */         
*/* Serialization:      N/A                                          */         
*/*                                                                  */         
*/* Change Activity:                                                 */         
*/*                                                                  */         
*/* Flag Reason   Release  Date   Origin    Description              */         
*/* ---- -------- -------- ------ --------  ------------------------ */         
*/* $A0= B615.00  HOS1120  080908 z/OS    : Create mapping           */         
*/* $A1= C356.00  HOS1120  110606 z/OS    : Add values for ICSF      */         
*/*                                         ciphers and MACs.        */         
*/* $A2= C761.00  HOS1130  141119 z/OS    : New Ciphers/MACs,        */         
*/*                                         New subtypes (94,95),    */         
*/*                                         SFTP target path,        */         
*/*                                         Login failure reasons,   */         
*/*                                         get -a (reget) flag.     */         
*/* $A3= C797.00  HOS2220  150213 z/OS    : GSS/Kerberos Auth types  */         
*/* $A4= C399.00  HOS2220  150602 z/OS    : Add values for FIPS      */         
*/*                                         mode and key exchange    */         
*/*                                         method                   */         
*/* $A5= FP1986   HOS2230  161116 z/OS    : Correct AES CTR ICSF     */         
*/*                                         constants. Old symbols   */         
*/*                                         deprecated               */         
*/* $A6= D070.00  HOS2230  170505 z/OS    : Add values for CPACF     */         
*/*                                         ciphers and MACs.        */         
*/* $A7= FP2184   HOS2240  180626 z/OS    : Add Algs for OpenSSH 7.6 */         
*/* $A8= FP2184   HOS2240  180626 z/OS    : Includes SMF_119SSH_LF   */         
*/*                                         section with subtype 94  */         
*/*                                         and 95 records           */         
*/********************************************************************/         
*/* NOTE: Changes to this part likely need to be mirrored in         */         
*/*       /samples/ssh_smf.h as well.                                */         
*/********************************************************************/         
*%GOTO FOT772; /*                                                               
.M010000 ANOP                                                                   
         AIF   ('&HEADER' NE 'YES').M020000                                     
*---------------------------------------------------------------------*         
*                                                                               
* SMF Type 119 Record for OpenSSH                                               
*                                                                               
SMF_119SSH_Header   DSECT            SMF Record Header                          
SMF_119SSH_HDLength  DS  BL2         Record length                              
SMF_119SSH_HDSegDesc DS  BL2         Segment descriptor                         
SMF_119SSH_HDFlags   DS  BL1         Header flags                               
SMF_119SSH_HDType    DS  BL1         Record type (119, x'77')                   
SMF_119SSH_HDTime    DS  BL4         Time record moved to SMF buffer            
SMF_119SSH_HDDate    DS  PL4         Date record moved to SMF buffer            
SMF_119SSH_HDSID     DS  CL4         System ID                                  
SMF_119SSH_HDSSI     DS  CL4         Subsystem ID                               
SMF_119SSH_HDSubType DS  BL2         Record subtype                             
SMF_119SSH_HDST_CliConnStart EQU X'005E'                          @A2A          
SMF_119SSH_HDST_SvrConnStart EQU X'005F'                          @A2A          
SMF_119SSH_HDST_SSHServer  EQU X'0060'                                          
SMF_119SSH_HDST_SSHClient  EQU X'0061'                                          
SMF_119SSH_HDST_SrvLoginF  EQU X'0062'                                          
SMF_119SSH_HDLN      EQU *-SMF_119SSH_Header                                    
*                                                                               
* Self Defining section, basic level. All records will have at least            
* two triplets, one for the TCP/IP Identification section and one for           
* a record specific section                                                     
*                                                                               
SMF_119SSH_SDefSect DSECT            SMF Self-defining section                  
SMF_119SSH_SD_TRN    DS  BL2         Number of triplets                         
SMF_119SSH_SDrsvd    DS  BL2         reserved                                   
SMF_119SSH_IDOff     DS  BL4         Offset to TCP/IP Ident section             
SMF_119SSH_IDLen     DS  BL2         Length of TCP/IP Ident section             
SMF_119SSH_IDNum     DS  BL2         Number of TCP/IP Ident section             
SMF_119SSH_S1Off     DS  BL4         Offset to first record section             
SMF_119SSH_S1Len     DS  BL2         Length of first record section             
SMF_119SSH_S1Num     DS  BL2         Number of section instances                
SMF_119SSH_SDLN     EQU *-SMF_119SSH_SDefSect                                   
*                                                                               
* Additional mapping for records with three sections.                           
SMF_119SSH_SD2Triples  DSECT         SMF Self-defining sect                     
SMF_119SSH_S2Off     DS  BL4         Offset to 2nd record section               
SMF_119SSH_S2Len     DS  BL2         Length of 2nd record section               
SMF_119SSH_S2Num     DS  BL2         Number of section instances                
SMF_119SSH_S2LN     EQU *-SMF_119SSH_SD2Triples                                 
*                                                                               
* Additional mapping for records with four sections.                            
SMF_119SSH_SD3Triples  DSECT         SMF Self-defining sect                     
SMF_119SSH_S3Off     DS  BL4         Offset to 3rd record section               
SMF_119SSH_S3Len     DS  BL2         Length of 3rd record section               
SMF_119SSH_S3Num     DS  BL2         Number of section instances                
SMF_119SSH_S3LN     EQU *-SMF_119SSH_SD3Triples                                 
*                                                                               
* Additional mapping for records with five sections.                            
SMF_119SSH_SD4Triples  DSECT         SMF Self-defining sect                     
SMF_119SSH_S4Off     DS  BL4         Offset to 4th record section               
SMF_119SSH_S4Len     DS  BL2         Length of 4th record section               
SMF_119SSH_S4Num     DS  BL2         Number of section instances                
SMF_119SSH_S4LN     EQU *-SMF_119SSH_SD4Triples                                 
*                                                                               
* Additional mapping for records with six sections.                             
SMF_119SSH_SD5Triples  DSECT         SMF Self-def section                       
SMF_119SSH_S5Off     DS  BL4         Offset to 5th record section               
SMF_119SSH_S5Len     DS  BL2         Length of 5th record section               
SMF_119SSH_S5Num     DS  BL2         Number of section instances                
SMF_119SSH_S5LN     EQU *-SMF_119SSH_SD5Triples                                 
*                                                                               
* Additional mapping for records with seven sections.             @A2A          
SMF_119SSH_SD6Triples  DSECT         SMF Self-def section         @A2A          
SMF_119SSH_S6Off     DS  BL4         Offset to 6th record section @A2A          
SMF_119SSH_S6Len     DS  BL2         Length of 6th record section @A2A          
SMF_119SSH_S6Num     DS  BL2         Number of section instances  @A2A          
SMF_119SSH_S6LN     EQU *-SMF_119SSH_SD6Triples                   @A2A          
*                                                                               
*---------------------------------------------------------------------*         
*                                                                               
* TCP/IP Identification section, common to all records                          
*                                                                               
SMF_119SSH_Ident DSECT                                                          
SMF_119SSH_TI_SYSName      DS  CL8     System name                              
SMF_119SSH_TI_SysplexName  DS  CL8     Sysplex name                             
SMF_119SSH_TI_Stack        DS  CL8     Stack name                               
SMF_119SSH_TI_ReleaseID    DS  CL8     z/OS release identifier                  
SMF_119SSH_TI_Comp         DS  CL8     OpenSSH subcomponent                     
SMF_119SSH_TI_ASName       DS  CL8     Address space name of writer             
SMF_119SSH_TI_UserID       DS  CL8     User ID of security context              
SMF_119SSH_TI_Reserved     DS  BL2     Reserved                                 
SMF_119SSH_TI_ASID         DS  BL2     ASID of writer                           
SMF_119SSH_TI_Reason       DS  BL1     Reason for writing record                
SMF_119SSH_TI_ReasonEvt      EQU X'08'  - Event record                          
SMF_119SSH_TI_RecordID     DS  FL1     Record ID                                
SMF_119SSH_TI_rsvd         DS  BL2'0'  Reserved                                 
SMF_119SSHTILN    EQU *-SMF_119SSH_Ident                                        
*                                                                               
SMF_119SSH_TI_CompDefs   DSECT                                                  
SMF_119SSH_TI_CompSFTPS    DC  C'SFTPS   '                                      
SMF_119SSH_TI_CompSFTPC    DC  C'SFTPC   '                                      
SMF_119SSH_TI_CompSCPS     DC  C'SCPS    '                                      
SMF_119SSH_TI_CompSCPC     DC  C'SCPC    '                                      
SMF_119SSH_TI_CompSSHD     DC  C'SSHD    '                                      
SMF_119SSH_TI_CompSSH      DC  C'SSH     '                        @A2A          
*                                                                               
.M020000 ANOP                                                                   
         AIF   ('&SSH' NE 'YES').M030000                                        
*---------------------------------------------------------------------*         
*                                                                               
* Security Information section, common to all records                           
*                                                                               
SMF_119SSH_SEC DSECT                                                            
SMF_119SSH_SSHV          DS CL16    OpenSSH version                             
SMF_119SSH_SSLV          DS CL32    OpenSSL version                             
SMF_119SSH_ZlibV         DS CL16    zlib version                                
SMF_119SSH_ProtoV        DS CL8     Protocol version                            
SMF_119SSH_AuthMethod    DS BL2     Authentication method being used            
SMF_119SSH_AuthMethUnknown EQU X'0000' - Unknown                                
SMF_119SSH_AuthMethNone    EQU X'0001' - None                                   
SMF_119SSH_AuthMethPwd     EQU X'0002' - Password                               
SMF_119SSH_AuthMethPubkey  EQU X'0003' - Publickey                              
SMF_119SSH_AuthMethHost    EQU X'0004' - Hostbased                              
SMF_119SSH_AuthMethRhosts  EQU X'0005' - Rhosts                                 
SMF_119SSH_AuthMethRhstRSA EQU X'0006' - RhostsRSA                              
SMF_119SSH_AuthMethRSA     EQU X'0007' - RSA                                    
SMF_119SSH_AuthMethKeybInt EQU X'0008' - Keyboard-interactive                   
SMF_119SSH_AuthMethChalRsp EQU X'0009' - Challenge response                     
SMF_119SSH_AuthMethCtlSock EQU X'000A' - Control socket                         
SMF_119SSH_AuthMethGssMic  EQU X'000B' - GSSAPI w/ MIC   @A3A                   
SMF_119SSH_AuthMethGssKeyex EQU X'000C' - GSSAPI w/ Keyex @A3A                  
SMF_119SSH_Cipher        DS BL2     Cipher type being used                      
SMF_119SSH_CipherUNKNOWN   EQU X'0000' - Unknown                                
SMF_119SSH_CipherNONE      EQU X'0001' - None                                   
SMF_119SSH_Cipher3DES      EQU X'0002' - 3DES                                   
SMF_119SSH_CipherBLOWFISH  EQU X'0003' - BLOWFISH                               
SMF_119SSH_CipherDES       EQU X'0004' - DES                                    
SMF_119SSH_Cipher3DES_CBC  EQU X'0005' - 3DES-CBC                               
SMF_119SSH_CipherBLFISHCBC EQU X'0006' - BLOWFISH-CBC                           
SMF_119SSH_CipherCAST128CB EQU X'0007' - CAST128-CBC                            
SMF_119SSH_CipherARCF128   EQU X'0008' - ARCFOUR128                             
SMF_119SSH_CipherARCF256   EQU X'0009' - ARCFOUR256                             
SMF_119SSH_CipherARCFOUR   EQU X'000A' - ARCFOUR                                
SMF_119SSH_CipherAES128CBC EQU X'000B' - AES128-CBC                             
SMF_119SSH_CipherAES192CBC EQU X'000C' - AES192-CBC                             
SMF_119SSH_CipherAES256CBC EQU X'000D' - AES256-CBC                             
SMF_119SSH_CipherAES128CTR EQU X'000E' - AES128-CTR                             
SMF_119SSH_CipherAES192CTR EQU X'000F' - AES192-CTR                             
SMF_119SSH_CipherAES256CTR EQU X'0010' - AES256-CTR                             
SMF_119SSH_CipherRIJNDAEL  EQU X'0011' - RIJNDAEL-CBC@LYSATOR.LIU.SE            
SMF_119SSH_CipherACSS      EQU X'0012' - ACSS@OPENSSH.ORG                       
SMF_119SSH_CipherAES128GCM EQU X'0013' - AES128-GCM@OPENSSH.COM @A2A            
SMF_119SSH_CipherAES256GCM EQU X'0014' - AES256-GCM@OPENSSH.COM @A2A            
SMF_119SSH_CipherCHACHAPOLY EQU X'0015' - chacha20-poly1305     @A7A            
*                                         @openssh.com          @A7A            
* ICSF enabled Ciphers have byte 0 = x10                                        
SMF_119SSH_Cipher3DES_CB_I EQU X'1005' - 3DES-CBC (ICSF)                        
SMF_119SSH_CipherBLFISHC_I EQU X'1006' - BLOWFISH-CBC (ICSF)                    
SMF_119SSH_CipherARCF128_I EQU X'1008' - ARCFOUR128 (ICSF)                      
SMF_119SSH_CipherARCF256_I EQU X'1009' - ARCFOUR256 (ICSF)                      
SMF_119SSH_CipherARCFOUR_I EQU X'100A' - ARCFOUR (ICSF)                         
SMF_119SSH_CipherAES128C_I EQU X'100B' - AES128-CBC (ICSF)                      
SMF_119SSH_CipherAES192C_I EQU X'100C' - AES192-CBC (ICSF)                      
SMF_119SSH_CipherAES256C_I EQU X'100D' - AES256-CBC (ICSF)                      
SMF_119SSH_CipherAES128T_I2 EQU X'100E' - AES128-CTR (ICSF)     @A5C            
SMF_119SSH_CipherAES192T_I2 EQU X'100F' - AES192-CTR (ICSF)     @A5C            
SMF_119SSH_CipherAES256T_I2 EQU X'1010' - AES256-CTR (ICSF)     @A5C            
SMF_119SSH_CipherRIJNDAE_I EQU X'1011' - RIJNDAEL-CBC@LYSATOR.LIU.SE(I)         
SMF_119SSH_CipherAES128T_I EQU X'1012' - AES128-CTR (ICSF) DEPR  @A2A           
SMF_119SSH_CipherAES192T_I EQU X'1013' - AES192-CTR (ICSF) DEPR  @A2A           
SMF_119SSH_CipherAES256T_I EQU X'1014' - AES256-CTR (ICSF) DEPR  @A2A           
* CPACF enabled Ciphers have byte 0 = x40                                       
SMF_119SSH_Cipher3DES_CB_C EQU X'4005' - 3DES-CBC (CPACF)                       
SMF_119SSH_CipherAES128C_C EQU X'400B' - AES128-CBC (CPACF) @A6A                
SMF_119SSH_CipherAES192C_C EQU X'400C' - AES192-CBC (CPACF) @A6A                
SMF_119SSH_CipherAES256C_C EQU X'400D' - AES256-CBC (CPACF) @A6A                
SMF_119SSH_CipherAES128T_C EQU X'400E' - AES128-CTR (CPACF) @A6A                
SMF_119SSH_CipherAES192T_C EQU X'400F' - AES192-CTR (CPACF) @A6A                
SMF_119SSH_CipherAES256T_C EQU X'4010' - AES256-CTR (CPACF) @A6A                
SMF_119SSH_CipherRIJNDAE_C EQU X'4011' - RIJNDAEL-CBC@LYSATOR.LIU.SE(I)         
                                                                                
SMF_119SSH_MAC           DS BL2     MAC algorithm being used                    
SMF_119SSH_MAC_Unknown     EQU X'0000' - Unknown                                
SMF_119SSH_MAC_None        EQU X'0001' - None (SSHV1)                           
*                                        or SSHV2 w/Auth Cipher @A2A            
*                                        (e.g. AES-GCM)         @A2A            
SMF_119SSH_MAC_MD5         EQU X'0002' - HMAC-MD5                               
SMF_119SSH_MAC_SHA1        EQU X'0003' - HMAC-SHA1                              
SMF_119SSH_MAC_64OPENSSH   EQU X'0004' - UMAC-64@OPENSSH.COM                    
SMF_119SSH_MAC_RIPEMD160   EQU X'0005' - HMAC-RIPEMD160                         
SMF_119SSH_MAC_SHA1_96     EQU X'0006' - HMAC-SHA1-96                           
SMF_119SSH_MAC_MD5_96      EQU X'0007' - HMAC-MD5-96                            
SMF_119SSH_MAC_RIPEMD160CM EQU X'0008' - HMAC-RIPEMD160@OPENSSH.COM             
SMF_119SSH_MAC_SHA2_256    EQU X'0009' - HMAC-SHA2-256           @A2A           
SMF_119SSH_MAC_SHA2_512    EQU X'000A' - HMAC-SHA2-512           @A2A           
SMF_119SSH_MAC_128OPENSSH  EQU X'000B' - UMAC-128@OPENSSH.COM    @A2A           
*  ICSF enabled HMAC algorithms (byte0=x10)                                     
SMF_119SSH_MAC_MD5_I       EQU X'1002' - HMAC-MD5 (ICSF)                        
SMF_119SSH_MAC_SHA1_I      EQU X'1003' - HMAC-SHA1 (ICSF)                       
SMF_119SSH_MAC_RPMD160_I   EQU X'1005' - HMAC-RIPEMD160 (ICSF)                  
SMF_119SSH_MAC_SHA1_96_I   EQU X'1006' - HMAC-SHA1-96 (ICSF)                    
SMF_119SSH_MAC_MD5_96_I    EQU X'1007' - HMAC-MD5-96 (ICSF)                     
SMF_119SSH_MAC_RPMD160CM_I EQU X'1008' - HMAC-RIPEMD160@OPENSSH.COM (I)         
SMF_119SSH_MAC_SHA2_256_I  EQU X'1009' - HMAC-SHA2-256 (ICSF)    @A2A           
SMF_119SSH_MAC_SHA2_512_I  EQU X'100A' - HMAC-SHA2-512 (ICSF)    @A2A           
*  CPACF enabled HMAC algorithms (byte0=x40)                                    
SMF_119SSH_MAC_SHA1_C      EQU X'4003' - HMAC-SHA1 (CPACF)       @A6A           
SMF_119SSH_MAC_SHA1_96_C   EQU X'4006' - HMAC-SHA1-96 (CPACF)    @A6A           
SMF_119SSH_MAC_SHA2_256_C  EQU X'4009' - HMAC-SHA2-256 (CPACF)   @A6A           
SMF_119SSH_MAC_SHA2_512_C  EQU X'400A' - HMAC-SHA2-512 (CPACF)   @A6A           
*  -etm@openssh.com variants of HMAC algorithms (byte0=x20)      @A2A           
SMF_119SSH_MAC_MD5_E       EQU X'2002' - HMAC-MD5-ETM@O.C        @A2A           
SMF_119SSH_MAC_SHA1_E      EQU X'2003' - HMAC-SHA1-ETM@O.C       @A2A           
SMF_119SSH_MAC_64OPENSSH_E EQU X'2004' - UMAC-64-ETM@O.C         @A2A           
SMF_119SSH_MAC_RIPEMD160_E EQU X'2005' - HMAC-RIPEMD160ETM@O.C   @A2A           
SMF_119SSH_MAC_SHA1_96_E   EQU X'2006' - HMAC-SHA1-96-ETM@O.C    @A2A           
SMF_119SSH_MAC_MD5_96_E    EQU X'2007' - HMAC-MD5-96-ETM@O.C     @A2A           
SMF_119SSH_MAC_SHA2_256_E  EQU X'2009' - HMAC-SHA2-256-ETM@O.C   @A2A           
SMF_119SSH_MAC_SHA2_512_E  EQU X'200A' - HMAC-SHA2-512-ETM@O.C   @A2A           
SMF_119SSH_MAC_128OPENSSH_E EQU X'200B' - UMAC-128-ETM@O.C       @A2A           
*  -etm@openssh.com variants of HMAC algorithms (with ICSF):     @A2A           
SMF_119SSH_MAC_MD5_EI      EQU X'3002' - HMAC-MD5-ETM@O.C (I)    @A2A           
SMF_119SSH_MAC_SHA1_EI     EQU X'3003' - HMAC-SHA1-ETM@O.C (I)   @A2A           
SMF_119SSH_MAC_RPMD160_EI  EQU X'3005' - HMAC-RIPEMD160ETM@O.C(I)@A2A           
SMF_119SSH_MAC_SHA1_96_EI  EQU X'3006' - HMAC-SHA1-96-ETM@O.C(I) @A2A           
SMF_119SSH_MAC_MD5_96_EI   EQU X'3007' - HMAC-MD5-96-ETM@O.C (I) @A2A           
SMF_119SSH_MAC_SHA2_256_EI EQU X'3009' - HMAC-SHA2-256-ETM@O.C(I)@A2A           
SMF_119SSH_MAC_SHA2_512_EI EQU X'300A' - HMAC-SHA2-512-ETM@O.C(I)@A2A           
*  CPACF enabled and -ETM HMAC algorithms (byte0=x60)            @A6A           
SMF_119SSH_MAC_SHA1_EC     EQU X'6003' - HMAC-SHA1-ETM@O.C (C)   @A6A           
SMF_119SSH_MAC_SHA1_96_EC  EQU X'6006' - HMAC-SHA1-96-ETM@O.C(C) @A6A           
SMF_119SSH_MAC_SHA2_256_EC EQU X'6009' - HMAC-SHA2-256-ETM@O.C(C)@A6A           
SMF_119SSH_MAC_SHA2_512_EC EQU X'600A' - HMAC-SHA2-512-ETM@O.C(C)@A6A           
*                                                                               
SMF_119SSH_Comp          DS BL2     Compression method being used               
SMF_119SSH_CompUnknown     EQU X'0000' - Unknown                                
SMF_119SSH_CompNone        EQU X'0001' - None (no)                              
SMF_119SSH_CompZlib        EQU X'0002' - zlib (yes)                             
SMF_119SSH_CompZlibOpenSSH EQU X'0003' - zlib@openssh.com (delayed)             
SMF_119SSH_AuthMethod2   DS BL2          If not 0, the last      @A2A           
*                                   additional Auth method used  @A2A           
SMF_119SSH_FIPSMODE      DS BL2     Running in FIPS mode         @A4A           
SMF_119SSH_FIPSMODE_Disabled EQU X'0000' - Not running in FIPS mode             
SMF_119SSH_FIPSMODE_Enabled  EQU X'0001' - Running in FIPS       @A4A           
SMF_119SSH_KexMethod     DS BL2     Key exchange method          @A4A           
SMF_119SSH_KexMethod_Unknown    EQU X'0000'                      @A4A           
*                               Key exchange method unknown      @A4A           
SMF_119SSH_KexMethod_None       EQU X'0001'                      @A4A           
*                               Key exchange method None         @A4A           
SMF_119SSH_KexMethod_DHGESHA256 EQU X'0002'                      @A4A           
*                             diffie-hellman-group-exchange-sha256              
SMF_119SSH_KexMethod_DHGESHA1   EQU X'0003'                      @A4A           
*                             diffie-hellman-group-exchange-sha1 @A4A           
SMF_119SSH_KexMethod_DHG14SHA1  EQU X'0004'                      @A4A           
*                             diffie-hellman-group14-sha1        @A4A           
SMF_119SSH_KexMethod_DHG1SHA1   EQU X'0005'                      @A4A           
*                             diffie-hellman-group1-sha1         @A4A           
SMF_119SSH_KexMethod_ECDHSHA2NP256 EQU X'0006'                   @A4A           
*                             ecdh-sha2-nistp256                 @A4A           
SMF_119SSH_KexMethod_ECDHSHA2NP384 EQU X'0007'                   @A4A           
*                             ecdh-sha2-nistp384                 @A4A           
SMF_119SSH_KexMethod_ECDHSHA2NP521 EQU X'0008'                   @A4A           
*                             ecdh-sha2-nistp521                 @A4A           
SMF_119SSH_KexMethod_GSSGROUP1SHA1 EQU X'0009'                   @A4A           
*                             gss-group1-sha1-                   @A4A           
SMF_119SSH_KexMethod_GSSGROUP14SHA1    EQU X'000A'               @A4A           
*                             gss-group14-sha1-                  @A4A           
SMF_119SSH_KexMethod_GSSGEXSHA1       EQU X'000B'                @A4A           
*                             gss-gex-sha1-                      @A4A           
SMF_119SSH_KexMethod_DHG14SHA256   EQU X'000C'                   @A7A           
*                             diffie-hellman-group14-sha256      @A7A           
SMF_119SSH_KexMethod_DHG16SHA512   EQU X'000D'                   @A7A           
*                             diffie-hellman-group16-sha512      @A7A           
SMF_119SSH_KexMethod_DHG18SHA512   EQU X'000E'                   @A7A           
*                             diffie-hellman-group18-sha512      @A7A           
SMF_119SSH_KexMethod_C25519SHA256  EQU X'000F'                   @A7A           
*                             curve25519-sha256                  @A7A           
SMF_119SSH_KexMethod_DHGESHA256_I  EQU X'1002'                   @A4A           
*                             diffie-hellman-group-exchange-sha256 ICSF         
SMF_119SSH_KexMethod_DHGESHA1_I    EQU X'1003'                                  
*                             diffie-hellman-group-exchange-sha1   ICSF         
SMF_119SSH_KexMethod_DHG14SHA1_I   EQU X'1004'                                  
*                             diffie-hellman-group14-sha1          ICSF         
SMF_119SSH_KexMethod_DHG1SHA1_I    EQU X'1005'                                  
*                             diffie-hellman-group1-sha1           ICSF         
SMF_119SSH_KexMethod_ECDHSHA2NP256_I EQU X'1006'                                
*                             ecdh-sha2-nistp256            ICSF @A4A           
SMF_119SSH_KexMethod_ECDHSHA2NP384_I EQU X'1007'                 @A4A           
*                             ecdh-sha2-nistp384            ICSF @A4A           
SMF_119SSH_KexMethod_ECDHSHA2NP521_I EQU X'1008'                 @A4A           
*                             ecdh-sha2-nistp521            ICSF @A4A           
SMF_119SSH_KexMethod_GSSGROUP1SHA1_I EQU X'1009'                 @A4A           
*                             gss-group1-sha1-              ICSF @A4A           
SMF_119SSH_KexMethod_GSSGROUP14SHA1_I EQU X'100A'                @A4A           
*                             gss-group14-sha1-             ICSF @A4A           
SMF_119SSH_KexMethod_GSSGEXSHA1_I       EQU X'100B'              @A4A           
*                             gss-gex-sha1-                 ICSF @A4A           
*                                                                               
* Security Protocol version definitions                                         
*                                                                               
SMF_119SSH_ProtoVDefs   DSECT                                                   
SMF_119SSH_ProtoVSSHV1    DC C'SSHV1   '                                        
SMF_119SSH_ProtoVSSHV2    DC C'SSHV2   '                                        
