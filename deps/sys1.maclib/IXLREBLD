.********************************************************************   00575100
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00575200
.********************************************************************   00575300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00575400
                      LCLA  &ZOPN1         INVOCATION INDEX             00575500
                      LCLC  &ZOPL(4095)    LABEL                        00575600
                      LCLC  &ZOPO(4095)    OPERATION                    00575700
                      LCLC  &ZOPV(4095)    VALUE                        00575800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00575900
.*                                                                      00576000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00576100
.******************************************************************     00576200
.***AL.L10 IXLREBLD FMT=LIST MF=S!L PARAMETERS                          00576300
.******************************************************************     00576400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00576500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00576600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00576700
&XMFATTR              SETC  '0D'                                        00576800
.*                                                                      00576900
&ZOPN                 SETA  &ZOPN+1                                     00577000
&ZOPL(&ZOPN)          SETC  ' '                                         00577100
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00577200
&ZOPO(&ZOPN)          SETC  'B'                                         00577300
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00577400
                      AGO   .ZALVL6B                                    00577500
.ZALVL6A              ANOP                                              00577600
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00577700
&ZOPV(&ZOPN)          SETC  '4,8'                                       00577800
&ZOPN                 SETA  &ZOPN+1                                     00577900
&ZOPL(&ZOPN)          SETC  ' '                                         00578000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00578100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00578200
.ZALVL6B              ANOP                                              00578300
&ZOPD(&ZOPN)          SETC  '++ IXLREBLD ARG PARMS'                     00578400
.*                                                                      00578500
&ZOPN                 SETA  &ZOPN+1                                     00578600
&ZOPL(&ZOPN)          SETC  ' '                                         00578700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00578800
&ZOPV(&ZOPN)          SETC  '1'                                         00578900
&ZOPD(&ZOPN)          SETC  ' '                                         00579000
.*                                                                      00579100
.ZLMFSL2              ANOP  ,                                           00579200
.*-------------------------------------------------------------------   00579300
