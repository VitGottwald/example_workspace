                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00238900
.*-------------------------------------------------------------------   00239000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00239100
&ZMS            SETC  '"PLISTVER='                                      00239200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00239300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00239400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00239500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00239600
                AGO   .ZPVCK2                                           00239700
.ZPVCK3         ANOP  ,                                                 00239800
.*-------------------------------------------------------------------   00239900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00240000
&ZMS            SETC  '"PLISTVER='                                      00240100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00240200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00240300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00240400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00240500
                AGO   .ZPVCKE                                           00240600
.ZPVCK2         ANOP  ,                                                 00240700
.*-------------------------------------------------------------------   00240800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00240900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00241000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00241100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00241200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00241300
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00241400
.ZPVCK6         ANOP  ,                                                 00241500
&ZMS            SETC  '"PLISTVER='                                      00241600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00241700
&ZMS            SETC  '&ZMS THE RANGE IS "0-3"'                         00241800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00241900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00242000
&XPLISTVER      SETA  3                                                 00242100
                AGO   .ZPVCKE                                           00242200
.*-------------------------------------------------------------------   00242300
.ZPVCK4A        ANOP  ,                                                 00242400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00242500
&ZMS            SETC  'PLISTVER='                                       00242600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00242700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00242800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00242900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00243000
