.***AC.PLV01  PROCESS PLISTVER                                      *   00233400
.********************************************************************   00233500
&XPLISTVER      SETA  0                                                 00233600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00233700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00233800
&XPLISTVER      SETA  3                                                 00233900
.ZPLV0          ANOP  ,                                                 00234000
.*                                                                      00234100
                AIF   ('&LESSCONNACTION' EQ '').ZPLV1E                  00234200
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00234300
&XPLISTVER      SETA  1                                                 00234400
.ZPLV1E         ANOP  ,                                                 00234500
.*                                                                      00234600
                AIF   ('&LESSCONNACTION' EQ '').ZPLV2E                  00234700
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00234800
&XPLISTVER      SETA  1                                                 00234900
.ZPLV2E         ANOP  ,                                                 00235000
.*                                                                      00235100
                AIF   ('&POPULATECF' EQ '').ZPLV3E                      00235200
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00235300
&XPLISTVER      SETA  2                                                 00235400
.ZPLV3E         ANOP  ,                                                 00235500
.*                                                                      00235600
                AIF   ('&POPULATECF' EQ '').ZPLV4E                      00235700
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00235800
&XPLISTVER      SETA  2                                                 00235900
.ZPLV4E         ANOP  ,                                                 00236000
.*                                                                      00236100
                AIF   ('&USERTEXT' EQ '').ZPLV5E                        00236200
                AIF   (&XPLISTVER GE 3).ZPLV5E                          00236300
&XPLISTVER      SETA  3                                                 00236400
.ZPLV5E         ANOP  ,                                                 00236500
.*                                                                      00236600
                AIF   ('&WAITFORCONTOKEN' EQ '').ZPLV6E                 00236700
                AIF   (&XPLISTVER GE 3).ZPLV6E                          00236800
&XPLISTVER      SETA  3                                                 00236900
.ZPLV6E         ANOP  ,                                                 00237000
.*                                                                      00237100
                AIF   ('&WAITFORCONNAME' EQ '').ZPLV7E                  00237200
                AIF   (&XPLISTVER GE 3).ZPLV7E                          00237300
&XPLISTVER      SETA  3                                                 00237400
.ZPLV7E         ANOP  ,                                                 00237500
.*                                                                      00237600
                AIF   ('&WAITFORSYSNAME' EQ '').ZPLV8E                  00237700
                AIF   (&XPLISTVER GE 3).ZPLV8E                          00237800
&XPLISTVER      SETA  3                                                 00237900
.ZPLV8E         ANOP  ,                                                 00238000
.*                                                                      00238100
                AIF   ('&USERTEXT' EQ '').ZPLV9E                        00238200
                AIF   (&XPLISTVER GE 3).ZPLV9E                          00238300
&XPLISTVER      SETA  3                                                 00238400
.ZPLV9E         ANOP  ,                                                 00238500
.*                                                                      00238600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00238700
