*                          ,SSPMLEN=xsspmlen¨                         * 00017200
*                     Ý,COLLECTION=»xcollection!NO_COLLECTIONº        * 00017300
*                          ,COLLECTION_LEN                            * 00017400
*                                          =xcollection_len¨          * 00017500
*                     Ý,PLAN=»xplan!NO_PLANº¨                         * 00017600
*                     Ý,PACKAGE=»xpackage!NO_PACKAGEº¨                * 00017700
*                     Ý,CONNECTION=»xconnection!NO_CONNECTIONº¨       * 00017800
*                     Ý,CORRELATION=»xcorrelation!NO_CORRELATIONº     * 00017900
*                          ,CORR_LEN=xcorr_len¨                       * 00018000
*                     Ý,PERFORM=»xperform!NO_PERFORMº¨                * 00018100
*                     Ý,PRCNAME=»xprcname!NO_PRCNAMEº                 * 00018200
*                          ,PRCNAME_LEN=xprcname_len¨                 * 00018300
*                     Ý,PRIORITY=»xpriority!NO_PRIORITYº¨             * 00018400
*                     Ý,PROCESSNAME=»xprocessname!NOPROCESSNAMEº      * 00018500
*                          ,PROCESSNM_LEN=xprocessnm_len¨             * 00018600
*                      ,CONNTKN=xconntkn                              * 00018700
*                     Ý,SUBCOLN=»xsubcoln!NO_SUBCOLNº¨                * 00018800
*                     Ý,SCHEDENV=»xschedenv!NO_SCHEDENVº              * 00018900
*                         Ý,SCHEDENV_LEN=»xschedenv_len!16º¨¨         * 00019000
*                     Ý,EWLM_CORR=»xewlm_corr!NO_EWLM_CORRº¨          * 00019100
*                     Ý,EWLM_OUTCORR=xewlm_outcorr¨                   * 00019200
