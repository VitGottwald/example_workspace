                    AIF   (&ZMACRET NE 0).ZLX2  ERROR DETECTED          00511300
.*******************************************************************    00511400
.***AE.01  START OF IWMCLSFY MACRO CODE                                 00511500
.*******************************************************************    00511600
                    LCLC  &M0M0001                 NAME                 00511700
&M0M0001            SETC  '&XLABEL'                SET NAME             00511800
                    AIF   (K'&XLABEL NE 0).XLAB2                        00511900
&M0M0001            SETC  'M00M&SYSNDX'            DEFAULT NAME         00512000
.*                                                                      00512100
.XLAB2              ANOP  ,                                             00512200
                    AIF   ('&XMF' EQ 'L').XLAB3                         00512300
&M0M0001 DS    0H                             IWMCLSFY-8                00512400
.XLAB3              ANOP  ,                                             00512500
.*******************************************************************    00512600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00512700
.*******************************************************************    00512800
              PUSH  PRINT                                               00512900
                    AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON    00513000
                    PRINT OFF         ++ PR01                           00513100
                    AGO   .ZPRNTXX                                      00513200
.ZPRINTX            PRINT ON                                            00513300
.ZPRNTXX            ANOP  ,                                             00513400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00513500
         LCLC  &T1                                                      00513600
         LCLA  &E1                                                  @P3 00513700
         LCLA  &EWLMCNT                                             @P3 00513800
&E1      SETA  0                                                    @P3 00513900
&EWLMCNT SETA  0                                                    @P3 00514000
         AIF   ('&XSOURCELU' EQ '').SOURCELUOK                          00514100
         AIF   ('&XSOURCELU' EQ 'NO_SOURCELU').SOURCELUOK               00514200
.*                                                                      00514300
         AIF   ('&XNETID' EQ '').CHECKLUNAME                            00514400
         AIF   ('&XNETID' NE 'NO_NETID').SOURCELUMSG                    00514500
.CHECKLUNAME ANOP                                                       00514600
         AIF   ('&XLUNAME' EQ '').SOURCELUOK                            00514700
         AIF   ('&XLUNAME' EQ 'NO_LUNAME').SOURCELUOK                   00514800
.SOURCELUMSG   ANOP                                                     00514900
&T1      SETC  'NETID, LUNAME MAY NOT BE USED WITH SOURCELU'            00515000
&E1      SETA  8                                                    @P3 00515100
         MNOTE &E1,'&T1'                                            @P3 00515200
.*       MEXIT                                                      @P3 00515300
.SOURCELUOK    ANOP                                                     00515400
.*                                                                      00515500
         AIF   ('&XEWLM_CHCORR'  EQ '').ENDCHCO                     @P3 00515600
         AIF   ('&XEWLM_CHCORR'  EQ 'NO_EWLM_CHCORR').ENDCHCO       @P3 00515700
&EWLMCNT SETA  &EWLMCNT+1                                           @P3 00515800
.ENDCHCO ANOP                                                       @P3 00515900
         AIF   ('&XEWLM_CHCTKN'  EQ '').ENDCTKN                     @P3 00516000
         AIF   ('&XEWLM_CHCTKN'  EQ 'NO_EWLM_CHCTKN').ENDCTKN       @P3 00516100
&EWLMCNT SETA  &EWLMCNT+1                                           @P3 00516200
.ENDCTKN ANOP                                                       @P3 00516300
.CHCO1   AIF   ('&XEWLM_OUTCORR' EQ '').ENDOUTC                     @P3 00516400
         AIF   ('&XEWLM_OUTCORR' EQ 'NO_EWLM_OUTCORR').ENDOUTC      @P3 00516500
&EWLMCNT SETA  &EWLMCNT+1                                           @P3 00516600
.ENDOUTC AIF   (&EWLMCNT LE 1).EWLMOK                               @P3 00516700
&T1      SETC  'EWLM_CHCORR, EWLM_CHCTKN AND EWLM_OUTCORR'          @P3 00516800
&T1      SETC  '&T1 ARE MUTUALLY EXCLUSIVE PARAMETERS'              @P3 00516900
&E1      SETA  8                                                    @P3 00517000
         MNOTE &E1,'&T1'                                            @P3 00517100
.EWLMOK  ANOP                                                       @P3 00517200
         AIF   (&E1 LT 8).CONT1                                     @P3 00517300
         MEXIT                                                      @P3 00517400
.CONT1   ANOP                                                       @P3 00517500
.*                                                                      00517600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00517700
.********************************************************************   00517800
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00517900
.********************************************************************   00518000
