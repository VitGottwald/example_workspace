.***AC.PLV01  PROCESS PLISTVER                                      *   00135800
.********************************************************************   00135900
&XPLISTVER      SETA  0                                                 00136000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00136100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00136200
&XPLISTVER      SETA  8                                                 00136300
.ZPLV0          ANOP  ,                                                 00136400
.*                                                                      00136500
                AIF   ('&PERFORM' EQ '').ZPLV1E                         00136600
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00136700
&XPLISTVER      SETA  1                                                 00136800
.ZPLV1E         ANOP  ,                                                 00136900
.*                                                                      00137000
                AIF   ('&PRCNAME' EQ '').ZPLV2E                         00137100
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00137200
&XPLISTVER      SETA  1                                                 00137300
.ZPLV2E         ANOP  ,                                                 00137400
.*                                                                      00137500
                AIF   ('&PRCNAME_LEN' EQ '').ZPLV3E                     00137600
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00137700
&XPLISTVER      SETA  1                                                 00137800
.ZPLV3E         ANOP  ,                                                 00137900
.*                                                                      00138000
                AIF   ('&PRIORITY' EQ '').ZPLV4E                        00138100
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00138200
&XPLISTVER      SETA  2                                                 00138300
.ZPLV4E         ANOP  ,                                                 00138400
.*                                                                      00138500
                AIF   ('&PROCESSNAME' EQ '').ZPLV5E                     00138600
                AIF   (&XPLISTVER GE 3).ZPLV5E                          00138700
