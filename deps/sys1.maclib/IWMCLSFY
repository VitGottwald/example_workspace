.*******************************************************************    00723900
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00724000
.*******************************************************************    00724100
.ZMFLX                ANOP  ,              MF=L PROCESSING              00724200
.*******************************************************************    00724300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00724400
.*******************************************************************    00724500
                      ACTR  10000                                       00724600
.*                                                                      00724700
                      LCLA  &ZLC           COLUMN POSITION              00724800
                      LCLA  &ZOPX          LIST TABLE INDEX             00724900
                      LCLC  &ZLL           LABEL                        00725000
                      LCLC  &ZLOP          OPERATION                    00725100
                      LCLC  &ZLV           VALUE                        00725200
                      LCLC  &ZLD           DESCRIPTION                  00725300
.*                                                                      00725400
&ZOPX                 SETA  1                                           00725500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00725600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00725700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00725800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00725900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00726000
                      AIF   (K'&ZLD LE 0).ZL5                           00726100
&ZLC                  SETA  35             COMMENT COLUMN               00726200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00726300
&ZLC                  SETA  33             COMMMENT COLUMN              00726400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00726500
&ZLV                  SETC  '&ZLV'.' '                                  00726600
                      AGO   .ZL3                                        00726700
.*                                                                      00726800
.ZL4                  ANOP  ,                                           00726900
&ZLV                  SETC  '&ZLV'.' '                                  00727000
.ZL5                  ANOP  ,                                           00727100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00727200
.*                                                                      00727300
&ZLL     &ZLOP &ZLV                                                     00727400
.*                                                                      00727500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00727600
                      AGO   .ZL2           CONTINUE LOOPING             00727700
.*                                                                      00727800
.ZLX                  ANOP  ,                                           00727900
                      AIF   ('&XMF' EQ 'L').ZASMMX  MF=L                00728000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00728100
         AIF   ('&XSOURCELU' EQ '').ENDSOURCELU                         00728200
         AIF   ('&XSOURCELU' EQ 'NO_SOURCELU').ENDSOURCELU              00728300
.*                                                                      00728400
.*                                                                      00728500
&T1      SETC  '&SOURCELU'                                              00728600
.*                                                                      00728700
.*       REGISTER FORM SPECIFICATION OF SOURCELU IS ASSUMED TO BE       00728800
.*       CHARACTER(17) - SO TAKE THE BRANCH TO SOURCELUMVI17            00728900
.*                                                                      00729000
         AIF   ('&SOURCELU'(1,1) EQ '(').SOURCELUMVI17                  00729100
.*                                                                      00729200
.* &T1 IS  A VALID BASE-DISPLACEMENT EXPRESSION                         00729300
         MVI   31(1),L'&T1                                              00729400
         AGO   .ENDSOURCELU                                             00729500
.SOURCELUMVI17  ANOP                                                    00729600
         MVI   31(1),17                                                 00729700
.ENDSOURCELU   ANOP                                                     00729800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00729900
.ZASMMX               ANOP  ,                                           00730000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00730100
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00730200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00730300
         ICM   14,15,60(1)             GET CLSFY TOKEN                  00730400
         BNZ   WLM&SYSNDX                                               00730500
         LA    0,IWMRSNCODENOCONN      SET REASON CODE = NO CONNECT     00730600
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00730700
         B     END&SYSNDX              BRANCH TO END LABEL              00730800
*                                                                       00730900
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00731000
         CLC   0(4,14),=CL4'WCDA'                                       00731100
         BE    CONNOK&SYSNDX           BRANCH WHEN CONNECT TKN PASSES   00731200
         LA    0,IWMRSNCODEBADCONN     SET REASON CODE = BAD CONNECTION 00731300
         LA    15,IWMRETCODEINVOCERROR SET INVOCATION ERROR RETURN CODE 00731400
         B     END&SYSNDX              BRANCH TO END LABEL              00731500
CONNOK&SYSNDX DS    0H                 BRANCH LABEL FOR CONNECT OK      00731600
.*                                                                      00731700
.*                                                                      00731800
.*                                                                      00731900
         L     15,16(14,0)             GET CLASSIFY RTN ADDRESS         00732000
         BASR  14,15                   CALL CLASSIFY ROUTINE            00732100
END&SYSNDX DS    0H                    END LABEL                        00732200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00732300
.ZLINKX               ANOP  ,                                           00732400
.*******************************************************************    00732500
.***AP.X01  FORMAT GENERATED CODE                                       00732600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00732700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00732800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00732900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00733000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00733100
                      AIF   (K'&ZLD LE 0).ZL51                          00733200
&ZLC                  SETA  35             COMMENT COLUMN               00733300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00733400
&ZLC                  SETA  33             COMMENT COLUMN               00733500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00733600
&ZLV                  SETC  '&ZLV'.' '                                  00733700
                      AGO   .ZL31                                       00733800
.*                                                                      00733900
.ZL41                 ANOP  ,                                           00734000
&ZLV                  SETC  '&ZLV'.' '                                  00734100
.ZL51                 ANOP  ,                                           00734200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00734300
.*                                                                      00734400
&ZLL     &ZLOP &ZLV                                                     00734500
.*                                                                      00734600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00734700
                      AGO   .ZL21          CONTINUE LOOPING             00734800
.*                                                                      00734900
.ZLX1                 ANOP  ,                                           00735000
.*******************************************************************    00735100
.***AP.X02  END OF IWMCLSFY MACRO                                       00735200
&ZIWMCLSFY            SETC  'YES'          SET IWMCLSFY USED            00735300
         POP   PRINT                                                    00735400
*                                            IWMCLSFY-8                 00735500
         SPACE 1                                                        00735600
.ZLX2                 ANOP  ,                                           00735700
         MEND                                                           00735800
