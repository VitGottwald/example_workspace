.*-------------------------------------------------------------------   00075600
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00075700
.ZG20             ANOP  ,                                               00075800
&ZTLC             SETC 'S'                                              00075900
                  AIF   ('&MF(1)' NE '&ZTLC').ZG21                      00076000
&XMF              SETC  'S'         SET KEYWORD                         00076100
                  AIF   (N'&MF LE 1).ZG20A      CK NUM                  00076200
&ZMS              SETC  '"MF='                                          00076300
&ZMS              SETC  '&ZMS.&MF"'                                     00076400
&ZMS              SETC  '&ZMS "MF"'                                     00076500
                  MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'             00076600
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00076700
.ZG20A            ANOP  ,                                               00076800
.*-------------------------------------------------------------------   00076900
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00077000
.ZG21             ANOP  ,                                               00077100
&ZTLC             SETC 'L'                                              00077200
                  AIF   ('&MF(1)' NE '&ZTLC').ZG28                      00077300
&XMF              SETC  'L'         SET KEYWORD                         00077400
                  AIF   (N'&MF LE 3).ZG21A      CK NUM                  00077500
&ZMS              SETC  '"MF='                                          00077600
&ZMS              SETC  '&ZMS.&MF"'                                     00077700
&ZMS              SETC  '&ZMS "MF"'                                     00077800
                  MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'            00077900
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00078000
.ZG21A            ANOP  ,                                               00078100
.*-------------------------------------------------------------------   00078200
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00078300
                  AIF   (N'&SYSLIST NE 0).ZG21B  POSITIONALS?           00078400
                  AIF   ('&REQTYPE' NE '').ZG21B  KEYS?                 00078500
                  AIF   ('&DESCRIPTIONS' NE '').ZG21B  KEYS?            00078600
                  AIF   ('&RETCODE' NE '').ZG21B  KEYS?                 00078700
                  AIF   ('&RSNCODE' NE '').ZG21B  KEYS?                 00078800
                  AGO    .ZG21C     INVOCATION OKAY                     00078900
.ZG21B            ANOP  ,                                               00079000
&ZMS              SETC  'THE "L" FORM OF "MF"'                          00079100
&ZMS              SETC  '&ZMS CAN ONLY HAVE'                            00079200
&ZMS              SETC  '&ZMS THE "PLISTVER" KEY'                       00079300
                  MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'          00079400
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00079500
.ZG21C            ANOP  ,                                               00079600
.*-------------------------------------------------------------------   00079700
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00079800
