.***AC.PLV01  PROCESS PLISTVER                                      *   00069100
.********************************************************************   00069200
&XPLISTVER      SETA  0                                                 00069300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00069400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00069500
&XPLISTVER      SETA  0                                                 00069600
.ZPLV0          ANOP  ,                                                 00069700
.*                                                                      00069800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00069900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00070000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00070100
.*-------------------------------------------------------------------   00070200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00070300
&ZMS            SETC  '"PLISTVER='                                      00070400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00070500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00070600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00070700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070800
                AGO   .ZPVCK2                                           00070900
.ZPVCK3         ANOP  ,                                                 00071000
.*-------------------------------------------------------------------   00071100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00071200
&ZMS            SETC  '"PLISTVER='                                      00071300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00071500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00071600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071700
                AGO   .ZPVCKE                                           00071800
.ZPVCK2         ANOP  ,                                                 00071900
.*-------------------------------------------------------------------   00072000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00072100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00072200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00072300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00072400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00072500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00072600
.ZPVCK6         ANOP  ,                                                 00072700
&ZMS            SETC  '"PLISTVER='                                      00072800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00072900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00073000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00073100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00073200
&XPLISTVER      SETA  0                                                 00073300
                AGO   .ZPVCKE                                           00073400
.*-------------------------------------------------------------------   00073500
.ZPVCK4A        ANOP  ,                                                 00073600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00073700
&ZMS            SETC  'PLISTVER='                                       00073800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00073900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00074000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00074100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00074200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00074300
                AGO   .ZPVCK5                                           00074400
.ZPVCK4         ANOP  ,                                                 00074500
&XPLISTVER      SETA  &PLISTVER(1)                                      00074600
.ZPVCK5         ANOP  ,                                                 00074700
.ZPVCKE         ANOP  ,                                                 00074800
.********************************************************************   00074900
.***AG.MF01 PROCESS MF MACRO FORMS                                      00075000
.********************************************************************   00075100
                  ACTR  10000                                           00075200
