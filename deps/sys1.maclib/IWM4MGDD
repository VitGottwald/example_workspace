&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00149000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00149100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00149200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00149300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00149400
