.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00154000
.*******************************************************************    00154100
                      ACTR  10000                                       00154200
.*                                                                      00154300
                      LCLA  &ZLC           COLUMN POSITION              00154400
                      LCLA  &ZOPX          LIST TABLE INDEX             00154500
                      LCLC  &ZLL           LABEL                        00154600
                      LCLC  &ZLOP          OPERATION                    00154700
                      LCLC  &ZLV           VALUE                        00154800
                      LCLC  &ZLD           DESCRIPTION                  00154900
.*                                                                      00155000
&ZOPX                 SETA  1                                           00155100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00155200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00155300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00155400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00155500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00155600
                      AIF   (K'&ZLD LE 0).ZL5                           00155700
&ZLC                  SETA  35             COMMENT COLUMN               00155800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00155900
&ZLC                  SETA  33             COMMMENT COLUMN              00156000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00156100
&ZLV                  SETC  '&ZLV'.' '                                  00156200
                      AGO   .ZL3                                        00156300
.*                                                                      00156400
.ZL4                  ANOP  ,                                           00156500
&ZLV                  SETC  '&ZLV'.' '                                  00156600
.ZL5                  ANOP  ,                                           00156700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00156800
.*                                                                      00156900
&ZLL     &ZLOP &ZLV                                                     00157000
.*                                                                      00157100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00157200
                      AGO   .ZL2           CONTINUE LOOPING             00157300
.*                                                                      00157400
.ZLX                  ANOP  ,                                           00157500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00157600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00157700
         L     14,16(0,0)              GET CVT ADDRESS                  00157800
.*------------------------------------------------------------------    00157900
         TM    CVTOSLV3-CVT(14),CVTH7750  test for HBB7750 presence     00158000
         BNZ   VEROK&SYSNDX               running on HBB7750            00158100
         LA    0,IWMRSNCODEBADVERSION     HBB7750 is required           00158200
         LA    15,IWMRETCODEINVOCERROR    set return code               00158300
         B     END&SYSNDX                 skip service invocation       00158400
VEROK&SYSNDX DS    0H                     version check successful      00158500
         AIF   ('&SYSAM64' NE 'YES').AMODE31                            00158600
         LLGTR 14,14                   CLEAR HIGH HALF IN 64BIT MODE    00158700
.AMODE31 ANOP  ,                                                        00158800
.*-------------------------------------------------------------------   00158900
         L     14,772(14,0)            GET SFT ADDRESS                  00159000
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00159100
         LA    15,121                  GET EX FOR SERVICE ROUTINE       00159200
         OR    14,15                   GET PC NUMBER                    00159300
         PC    0(14)                                                    00159400
END&SYSNDX DS    0H                    END LABEL                        00159500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00159600
.ZLINKX               ANOP  ,                                           00159700
