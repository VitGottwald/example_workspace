&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00163300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00163400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00163500
                      AIF   (K'&ZLD LE 0).ZL51                          00163600
&ZLC                  SETA  35             COMMENT COLUMN               00163700
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00163800
&ZLC                  SETA  33             COMMENT COLUMN               00163900
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00164000
&ZLV                  SETC  '&ZLV'.' '                                  00164100
                      AGO   .ZL31                                       00164200
.*                                                                      00164300
.ZL41                 ANOP  ,                                           00164400
&ZLV                  SETC  '&ZLV'.' '                                  00164500
.ZL51                 ANOP  ,                                           00164600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00164700
.*                                                                      00164800
&ZLL     &ZLOP &ZLV                                                     00164900
.*                                                                      00165000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00165100
                      AGO   .ZL21          CONTINUE LOOPING             00165200
.*                                                                      00165300
.ZLX1                 ANOP  ,                                           00165400
.*******************************************************************    00165500
.***AP.X02  END OF IXZXIXCN MACRO                                       00165600
&ZIXZXIXCN            SETC  'YES'          SET IXZXIXCN USED            00165700
         POP   PRINT                                                    00165800
*                                            IXZXIXCN-1                 00165900
         SPACE 1                                                        00166000
.ZLX2                 ANOP  ,                                           00166100
         MEND                                                           00166200
**********************************************************************/ 00166300
