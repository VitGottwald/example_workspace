*% /*                                                                   00007000
         MACRO                                                          00008000
         IWMSVDCR &DSECT=YES,&LIST=YES,&TITLE=YES,&SVDCRHDR=YES,       *00009000
               &SVDCRSST=COND,&SVDCRRUL=COND,&SVDCRGRP=COND,           *00010000
               &SVDCRGVS=COND,&SVDCREXT=COND                            00011000
         GBLC  &IWMSVDCR_INCLUDED                                       00012000
         GBLC  &ZCBPRINT                                                00013000
