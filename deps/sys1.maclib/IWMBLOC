.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00195600
.*******************************************************************    00195700
                      ACTR  10000                                       00195800
.*                                                                      00195900
                      LCLA  &ZLC           COLUMN POSITION              00196000
                      LCLA  &ZOPX          LIST TABLE INDEX             00196100
                      LCLC  &ZLL           LABEL                        00196200
                      LCLC  &ZLOP          OPERATION                    00196300
                      LCLC  &ZLV           VALUE                        00196400
                      LCLC  &ZLD           DESCRIPTION                  00196500
.*                                                                      00196600
&ZOPX                 SETA  1                                           00196700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00196800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00196900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00197000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00197100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00197200
                      AIF   (K'&ZLD LE 0).ZL5                           00197300
&ZLC                  SETA  35             COMMENT COLUMN               00197400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00197500
&ZLC                  SETA  33             COMMMENT COLUMN              00197600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00197700
&ZLV                  SETC  '&ZLV'.' '                                  00197800
                      AGO   .ZL3                                        00197900
.*                                                                      00198000
.ZL4                  ANOP  ,                                           00198100
&ZLV                  SETC  '&ZLV'.' '                                  00198200
.ZL5                  ANOP  ,                                           00198300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00198400
.*                                                                      00198500
&ZLL     &ZLOP &ZLV                                                     00198600
.*                                                                      00198700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00198800
                      AGO   .ZL2           CONTINUE LOOPING             00198900
.*                                                                      00199000
.ZLX                  ANOP  ,                                           00199100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00199200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00199300
         L     14,16(0,0)              GET CVT ADDRESS                  00199400
         L     14,772(14,0)            GET SFT ADDRESS                  00199500
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00199600
         LA    15,72                   GET EX FOR SERVICE ROUTINE       00199700
         OR    14,15                   GET PC NUMBER                    00199800
         PC    0(14)                                                    00199900
END&SYSNDX DS    0H                    END LABEL                        00200000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00200100
.ZLINKX               ANOP  ,                                           00200200
.*******************************************************************    00200300
.***AP.X01  FORMAT GENERATED CODE                                       00200400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00200500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00200600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00200700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00200800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00200900
                      AIF   (K'&ZLD LE 0).ZL51                          00201000
&ZLC                  SETA  35             COMMENT COLUMN               00201100
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00201200
&ZLC                  SETA  33             COMMENT COLUMN               00201300
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00201400
&ZLV                  SETC  '&ZLV'.' '                                  00201500
                      AGO   .ZL31                                       00201600
.*                                                                      00201700
.ZL41                 ANOP  ,                                           00201800
&ZLV                  SETC  '&ZLV'.' '                                  00201900
.ZL51                 ANOP  ,                                           00202000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00202100
.*                                                                      00202200
&ZLL     &ZLOP &ZLV                                                     00202300
.*                                                                      00202400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00202500
                      AGO   .ZL21          CONTINUE LOOPING             00202600
.*                                                                      00202700
.ZLX1                 ANOP  ,                                           00202800
