                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00162400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00162500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00162600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00162700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00162800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00162900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00163000
&ZOPN                 SETA  &ZOPN+1                                     00163100
&ZOPL(&ZOPN)          SETC  ' '                                         00163200
