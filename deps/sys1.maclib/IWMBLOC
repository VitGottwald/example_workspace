.ZSYS2        ANOP  ,                                                   00068700
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00068800
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00068900
              SYSSTATE TEST                                             00069000
              AGO   .ZSYS4                                              00069100
