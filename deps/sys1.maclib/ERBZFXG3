* %ERBZFXG3_INCLUDED = 'YES';                                                   
* %DCL ZCBPRINT CHAR EXT;                                                       
* %DEACTIVATE ZCBPRINT;                                                         
* %DCL ERBZFXG3_LIST CHAR EXT;                                                  
* %DEACTIVATE ERBZFXG3_LIST;                                                    
* %IF ERBZFXG3_LIST = 'NO' !                                                    
*    ZCBPRINT = 'NO' %THEN                                                      
