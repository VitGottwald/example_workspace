.***AC.PLV01  PROCESS PLISTVER                                      *   00177700
.********************************************************************   00177800
&XPLISTVER      SETA  0                                                 00177900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00178000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00178100
&XPLISTVER      SETA  4                                                 00178200
.ZPLV0          ANOP  ,                                                 00178300
.*                                                                      00178400
                AIF   ('&WORK_MANAGER' EQ '').ZPLV1E                    00178500
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00178600
&XPLISTVER      SETA  1                                                 00178700
.ZPLV1E         ANOP  ,                                                 00178800
.*                                                                      00178900
                AIF   ('&EWLM' EQ '').ZPLV2E                            00179000
                AIF   (&XPLISTVER GE 4).ZPLV2E                          00179100
&XPLISTVER      SETA  4                                                 00179200
.ZPLV2E         ANOP  ,                                                 00179300
.*                                                                      00179400
                AIF   ('&ROUTER' EQ '').ZPLV3E                          00179500
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00179600
&XPLISTVER      SETA  1                                                 00179700
.ZPLV3E         ANOP  ,                                                 00179800
.*                                                                      00179900
                AIF   ('&QUEUE_MANAGER' EQ '').ZPLV4E                   00180000
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00180100
&XPLISTVER      SETA  1                                                 00180200
.ZPLV4E         ANOP  ,                                                 00180300
.*                                                                      00180400
                AIF   ('&QMGR_EXIT@' EQ '').ZPLV5E                      00180500
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00180600
&XPLISTVER      SETA  1                                                 00180700
.ZPLV5E         ANOP  ,                                                 00180800
.*                                                                      00180900
                AIF   ('&SERVER_MANAGER' EQ '').ZPLV6E                  00181000
                AIF   (&XPLISTVER GE 1).ZPLV6E                          00181100
&XPLISTVER      SETA  1                                                 00181200
.ZPLV6E         ANOP  ,                                                 00181300
.*                                                                      00181400
                AIF   ('&APPLENV' EQ '').ZPLV7E                         00181500
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00181600
&XPLISTVER      SETA  1                                                 00181700
.ZPLV7E         ANOP  ,                                                 00181800
.*                                                                      00181900
                AIF   ('&DYNAMIC' EQ '').ZPLV8E                         00182000
                AIF   (&XPLISTVER GE 3).ZPLV8E                          00182100
&XPLISTVER      SETA  3                                                 00182200
.ZPLV8E         ANOP  ,                                                 00182300
.*                                                                      00182400
                AIF   ('&PARALLEL_EU' EQ '').ZPLV9E                     00182500
                AIF   (&XPLISTVER GE 1).ZPLV9E                          00182600
&XPLISTVER      SETA  1                                                 00182700
.ZPLV9E         ANOP  ,                                                 00182800
.*                                                                      00182900
                AIF   ('&SERVER_TYPE' EQ '').ZPLV10E                    00183000
                AIF   (&XPLISTVER GE 1).ZPLV10E                         00183100
&XPLISTVER      SETA  1                                                 00183200
.ZPLV10E        ANOP  ,                                                 00183300
.*                                                                      00183400
                AIF   ('&MANAGE_TASKS' EQ '').ZPLV11E                   00183500
                AIF   (&XPLISTVER GE 2).ZPLV11E                         00183600
&XPLISTVER      SETA  2                                                 00183700
.ZPLV11E        ANOP  ,                                                 00183800
.*                                                                      00183900
                AIF   ('&SERVER_LIMIT' EQ '').ZPLV12E                   00184000
                AIF   (&XPLISTVER GE 2).ZPLV12E                         00184100
&XPLISTVER      SETA  2                                                 00184200
.ZPLV12E        ANOP  ,                                                 00184300
.*                                                                      00184400
                AIF   ('&SERVER_DATA' EQ '').ZPLV13E                    00184500
                AIF   (&XPLISTVER GE 1).ZPLV13E                         00184600
&XPLISTVER      SETA  1                                                 00184700
.ZPLV13E        ANOP  ,                                                 00184800
.*                                                                      00184900
                AIF   ('&SRV_MGR_EXIT@' EQ '').ZPLV14E                  00185000
                AIF   (&XPLISTVER GE 1).ZPLV14E                         00185100
&XPLISTVER      SETA  1                                                 00185200
.ZPLV14E        ANOP  ,                                                 00185300
.*                                                                      00185400
                AIF   ('&EXPTIMPT' EQ '').ZPLV15E                       00185500
                AIF   (&XPLISTVER GE 1).ZPLV15E                         00185600
&XPLISTVER      SETA  1                                                 00185700
.ZPLV15E        ANOP  ,                                                 00185800
.*                                                                      00185900
                AIF   ('&NODENM' EQ '').ZPLV16E                         00186000
                AIF   (&XPLISTVER GE 3).ZPLV16E                         00186100
&XPLISTVER      SETA  3                                                 00186200
.ZPLV16E        ANOP  ,                                                 00186300
.*                                                                      00186400
                AIF   ('&GROUPNM' EQ '').ZPLV17E                        00186500
                AIF   (&XPLISTVER GE 4).ZPLV17E                         00186600
&XPLISTVER      SETA  4                                                 00186700
.ZPLV17E        ANOP  ,                                                 00186800
.*                                                                      00186900
                AIF   ('&GROUPNM_LEN' EQ '').ZPLV18E                    00187000
                AIF   (&XPLISTVER GE 4).ZPLV18E                         00187100
&XPLISTVER      SETA  4                                                 00187200
.ZPLV18E        ANOP  ,                                                 00187300
.*                                                                      00187400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00187500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00187600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00187700
.*-------------------------------------------------------------------   00187800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00187900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00188000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00188100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00188200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00188300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00188400
                AGO   .ZPVCK2                                           00188500
.ZPVCK3         ANOP  ,                                                 00188600
.*-------------------------------------------------------------------   00188700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00188800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00188900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00189000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00189100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00189200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00189300
                AGO   .ZPVCKE                                           00189400
.ZPVCK2         ANOP  ,                                                 00189500
.*-------------------------------------------------------------------   00189600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00189700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00189800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00189900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00190000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00190100
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00190200
.ZPVCK6         ANOP  ,                                                 00190300
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00190400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00190500
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00190600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00190700
