                    AIF   (&ZMACRET NE 0).ZLX2  ERROR DETECTED          00370200
.*******************************************************************    00370300
.***AE.01  START OF IWMCONN MACRO CODE                                  00370400
.*******************************************************************    00370500
                    LCLC  &M0M0001                 NAME                 00370600
&M0M0001            SETC  '&XLABEL'                SET NAME             00370700
                    AIF   (K'&XLABEL NE 0).XLAB2                        00370800
&M0M0001            SETC  'M00M&SYSNDX'            DEFAULT NAME         00370900
.*                                                                      00371000
.XLAB2              ANOP  ,                                             00371100
                    AIF   ('&XMF' EQ 'L').XLAB3                         00371200
&M0M0001 DS    0H                             IWMCONN-4                 00371300
.XLAB3              ANOP  ,                                             00371400
.*******************************************************************    00371500
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00371600
.*******************************************************************    00371700
              PUSH  PRINT                                               00371800
                    AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON    00371900
                    PRINT OFF         ++ PR01                           00372000
                    AGO   .ZPRNTXX                                      00372100
.ZPRINTX            PRINT ON                                            00372200
.ZPRNTXX            ANOP  ,                                             00372300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00372400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00372500
.********************************************************************   00372600
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00372700
.********************************************************************   00372800
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00372900
                      LCLA  &ZOPN1         INVOCATION INDEX             00373000
                      LCLC  &ZOPL(4095)    LABEL                        00373100
                      LCLC  &ZOPO(4095)    OPERATION                    00373200
                      LCLC  &ZOPV(4095)    VALUE                        00373300
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00373400
.*                                                                      00373500
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00373600
.******************************************************************     00373700
.***AL.L10 IWMCONN FMT=LIST MF=S!L PARAMETERS                           00373800
.******************************************************************     00373900
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00374000
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00374100
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00374200
&XMFATTR              SETC  '0D'                                        00374300
.*                                                                      00374400
&ZOPN                 SETA  &ZOPN+1                                     00374500
&ZOPL(&ZOPN)          SETC  ' '                                         00374600
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00374700
&ZOPO(&ZOPN)          SETC  'B'                                         00374800
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00374900
                      AGO   .ZALVL6B                                    00375000
.ZALVL6A              ANOP                                              00375100
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00375200
&ZOPV(&ZOPN)          SETC  '4,8'                                       00375300
&ZOPN                 SETA  &ZOPN+1                                     00375400
&ZOPL(&ZOPN)          SETC  ' '                                         00375500
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00375600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00375700
.ZALVL6B              ANOP                                              00375800
&ZOPD(&ZOPN)          SETC  '++ IWMCONN ARG PARMS'                      00375900
.*                                                                      00376000
&ZOPN                 SETA  &ZOPN+1                                     00376100
&ZOPL(&ZOPN)          SETC  ' '                                         00376200
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00376300
&ZOPV(&ZOPN)          SETC  '1'                                         00376400
&ZOPD(&ZOPN)          SETC  ' '                                         00376500
.*                                                                      00376600
.ZLMFSL2              ANOP  ,                                           00376700
