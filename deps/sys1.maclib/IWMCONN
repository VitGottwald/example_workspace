                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00430300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00430400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00430500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00430600
