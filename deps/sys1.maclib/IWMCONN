&ZOPL(&ZOPN)          SETC  ' '                                         00431900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00432000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00432100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00432200
