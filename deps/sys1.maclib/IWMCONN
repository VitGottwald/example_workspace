&ZOPL(&ZOPN)          SETC  ' '                                         00430800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00430900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00431000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00431100
                      AGO   .ZIMFNX                                     00431200
.*                                                                      00431300
