.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00513400
.*******************************************************************    00513500
                      ACTR  10000                                       00513600
.*                                                                      00513700
                      LCLA  &ZLC           COLUMN POSITION              00513800
                      LCLA  &ZOPX          LIST TABLE INDEX             00513900
                      LCLC  &ZLL           LABEL                        00514000
                      LCLC  &ZLOP          OPERATION                    00514100
                      LCLC  &ZLV           VALUE                        00514200
                      LCLC  &ZLD           DESCRIPTION                  00514300
.*                                                                      00514400
&ZOPX                 SETA  1                                           00514500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00514600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00514700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00514800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00514900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00515000
                      AIF   (K'&ZLD LE 0).ZL5                           00515100
&ZLC                  SETA  35             COMMENT COLUMN               00515200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00515300
&ZLC                  SETA  33             COMMMENT COLUMN              00515400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00515500
&ZLV                  SETC  '&ZLV'.' '                                  00515600
                      AGO   .ZL3                                        00515700
.*                                                                      00515800
.ZL4                  ANOP  ,                                           00515900
&ZLV                  SETC  '&ZLV'.' '                                  00516000
.ZL5                  ANOP  ,                                           00516100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00516200
.*                                                                      00516300
&ZLL     &ZLOP &ZLV                                                     00516400
.*                                                                      00516500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00516600
                      AGO   .ZL2           CONTINUE LOOPING             00516700
.*                                                                      00516800
.ZLX                  ANOP  ,                                           00516900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00517000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00517100
         LCLC  &T1                       ERROR MESSAGE WORK AREA        00517200
         AIF   (&XPLISTVER LE 0).MGROK                                  00517300
         AIF   ('&XWORK_MANAGER' EQ 'YES').MGROK                        00517400
         AIF   ('&XROUTER' EQ 'YES').MGROK                              00517500
         AIF   ('&XQUEUE_MANAGER' EQ 'YES').MGROK                       00517600
         AIF   ('&XSERVER_MANAGER' EQ 'YES').MGROK                      00517700
         AIF   ('&XEXPTIMPT' EQ 'YES').MGROK                       @L7C 00517800
&T1      SETC  'AT LEAST ONE OF THE MANAGER OPTIONS'                    00517900
&T1      SETC  '&T1 MUST BE "YES"'                                      00518000
         MNOTE 8,'&T1'                                                  00518100
         MEXIT                                                          00518200
.MGROK   ANOP                                                           00518300
.*                                                                      00518400
         AIF   (&XPLISTVER LE 0).SRVOK                                  00518500
         AIF   ('&XSERVER_MANAGER' NE 'YES').SRVOK                      00518600
         AIF   ('&XROUTER' EQ 'YES').RTRCHK                             00518700
         AIF   ('&MANAGE_TASKS' EQ 'YES').TASKCHK                       00518800
         AIF   ('&XQUEUE_MANAGER' EQ 'YES').QMGRCHK                     00518900
         AGO   .SRVOK                                                   00519000
.RTRCHK  ANOP                                                           00519100
         AIF   ('&XSERVER_TYPE' EQ 'ROUTING').SRVOK                     00519200
&T1      SETC  'SERVER_MANAGER=YES WITH ROUTER=YES'                     00519300
&T1      SETC  '&T1 MUST USE SERVER_TYPE=ROUTING'                       00519400
         MNOTE 8,'&T1'                                                  00519500
         MEXIT                                                          00519600
.TASKCHK ANOP                                                           00519700
         AIF   ('&XSERVER_TYPE' EQ 'QUEUE').SRVOK                       00519800
&T1      SETC  'SERVER_MANAGER=YES WITH MANAGE_TASKS=YES'               00519900
&T1      SETC  '&T1 MUST USE SERVER_TYPE=QUEUE'                         00520000
         MNOTE 8,'&T1'                                                  00520100
         MEXIT                                                          00520200
.QMGRCHK ANOP                                                           00520300
         AIF   ('&XSERVER_TYPE' EQ 'QUEUE').SRVOK                       00520400
&T1      SETC  'SERVER_MANAGER=YES WITH QUEUE_MANAGER=YES'              00520500
&T1      SETC  '&T1 MUST USE SERVER_TYPE=QUEUE'                         00520600
         MNOTE 8,'&T1'                                                  00520700
         MEXIT                                                          00520800
.SRVOK   ANOP                                                           00520900
.*                                                                      00521000
         L     14,16(0,0)              GET CVT ADDRESS                  00521100
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00521200
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00521300
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00521400
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00521500
         B     END&SYSNDX              BRANCH TO END LABEL              00521600
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00521700
         L     14,772(14,0)            GET SFT ADDRESS                  00521800
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00521900
         LA    15,7                    GET EX FOR SERVICE ROUTINE       00522000
         OR    14,15                   GET PC NUMBER                    00522100
         PC    0(14)                                                    00522200
END&SYSNDX DS    0H                    END LABEL                        00522300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00522400
.ZLINKX               ANOP  ,                                           00522500
.*******************************************************************    00522600
