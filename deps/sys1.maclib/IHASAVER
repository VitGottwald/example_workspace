**/ IHASAVER_2:;                                                        00402000
* %IHASAVER_INCLUDED = 'YES';                                           00403000
* %DCL ZCBPRINT CHAR EXT;                                               00404000
* %DEACTIVATE ZCBPRINT;                                                 00405000
* %DCL IHASAVER_LIST CHAR EXT;                                          00406000
* %DEACTIVATE IHASAVER_LIST;                                            00407000
* %IF IHASAVER_LIST = 'NO' !                                            00408000
*    ZCBPRINT = 'NO' %THEN                                              00409000
* %DO;                                                                  00410000
*   @LIST PUSH NOECHO;                                                  00411000
*   @LIST NOASSEMBLE NOECHO;                                            00412000
*   @LIST OFF C NOECHO;                                                 00413000
* %END;                                                                 00414000
*/* Start of PL/X Source                                             */ 00415000
*        % IF SAVBASE = ''                                              00416000
*        %    THEN                                                      00417000
*        %      SAVBASE = 'BASED';                                      00418000
* DCL  1 SAVER SAVBASE BDY(WORD),                                       00419000
*        2 SAVPLI   PTR(31),            /* USED BY PL/I LANG. PRGM   */ 00420000
*        2 SAVPREV  PTR(31),            /* ADDR OF PREVIOUS SAVEAREA */ 00421000
*        2 SAVNEXT  PTR(31),            /* ADDR OF NEXT SAVE AREA    */ 00422000
*        2 SAVGRS14 PTR(31),            /* REGISTER 14               */ 00423000
