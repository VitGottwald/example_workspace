         MACRO                                                          00008000
         IHASAVER &DSECT=YES,&LIST=YES,&TITLE=YES,&SAVER=YES,          *00009000
               &SAVF4SA=COND,&SAVF5SA=COND,&SAVF7SA=COND,&SAVF8SA=COND  00010000
                GBLC  &IHASAVER_INCLUDED                                00011000
                GBLC  &ZCBPRINT                                         00012000
