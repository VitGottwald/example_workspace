*01* MODULE TYPE:  ASSEM MACRO                                        * 00247100
*                                                                     * 00247200
*02*   PROCESSOR:  ASSEM                                              * 00247300
*                                                                     * 00247400
*01* COMPONENT:  SC1CJ                                                * 00247500
*                                                                     * 00247600
*01* DISTRIBUTION LIBRARY: AMACLIB                                    * 00247700
*                                                                     * 00247800
*01* EXAMPLES:                                                        * 00247900
*                                                                     * 00248000
*    Example 1                                                        * 00248100
*                                                                     * 00248200
*    Operation:                                                       * 00248300
*                                                                     * 00248400
*                                                                     * 00248500
*    1.  Add a module to LPA                                          * 00248600
*                                                                     * 00248700
*    2.  Delete a module from LPA                                     * 00248800
*                                                                     * 00248900
*                                                                     * 00249000
*                                                                     * 00249100
*    The code is as follows.                                          * 00249200
*                                                                     * 00249300
*      *************************************************************  * 00249400
*      * Set up MODINFO area, for module "MYMODULE", indicating    *  * 00249500
*      * that the module is to be page-fixed.                      *  * 00249600
*      * Add the module to LPA, locating the module using          *  * 00249700
*      * data set 'SYS1.MYDS'.                                     *  * 00249800
*      *************************************************************  * 00249900
*               LA    2,ADDINFO                                       * 00250000
*               USING LPMEA,2                                         * 00250100
*               XC    ADDINFO(LPMEA_LEN),ADDINFO                      * 00250200
*               MVC   LPMEANAME,=CL8'MYMODULE'                        * 00250300
*               OI    LPMEAINPUTFLAGS0,LPMEAFIXED                     * 00250400
*               DROP  2                                               * 00250500
*               CSVDYLPA REQUEST=ADD,MODINFOTYPE=MEMBERLIST,        * * 00250600
*                     MODINFO=(2),NUMMOD=LMODN,                     * * 00250700
*                     DSNAME=LDS1,                                  * * 00250800
*                     REQUESTOR=LREQ,                               * * 00250900
*                     RETCODE=LRETCODE,RSNCODE=LRSNCODE,            * * 00251000
*                     MF=(E,DYLPAL)                                   * 00251100
*      *                                                              * 00251200
*      * Place code to check return/reason codes here                 * 00251300
*      *                                                              * 00251400
*       ... processing code here ...                                  * 00251500
*      *                                                              * 00251600
*      *************************************************************  * 00251700
*      * Set up MODINFO area, for module "MYMODULE", using the     *  * 00251800
*      * token returned on REQUEST=ADD.                            *  * 00251900
*      * Delete the module from LPA.                               *  * 00252000
*      *************************************************************  * 00252100
*               LA    3,ADDINFO                                       * 00252200
*               USING LPMEA,3                                         * 00252300
*               LA    2,DELINFO                                       * 00252400
*               USING LPMED,2                                         * 00252500
*               XC    DELINFO(LPMED_LEN),DELINFO                      * 00252600
*               MVC   LPMEDNAME,LPMEANAME                             * 00252700
*               MVC   LPMEDDELETETOKEN,LPMEADELETETOKEN               * 00252800
*               DROP  2,3                                             * 00252900
*               CSVDYLPA REQUEST=DELETE,MODINFO=(2),NUMMOD=LMODN,   * * 00253000
*                     RETCODE=LRETCODE,RSNCODE=LRSNCODE,            * * 00253100
*                     MF=(E,DYLPAL)                                   * 00253200
*      *                                                              * 00253300
*      * Place code to check return/reason codes here.                * 00253400
*      *                                                              * 00253500
*      LMODN    DC    F'1'                                            * 00253600
*      LDS1     DC    CL44'SYS1.MYDS'                                 * 00253700
*      LREQ     DC    CL16'CSVDYLPA EXAMPLE'                          * 00253800
*               CSVLPRET              Return code information         * 00253900
*      DYNAREA  DSECT                                                 * 00254000
*      ADDINFO  DS    0D                                              * 00254100
*               ORG   ADDINFO+LPMEA_LEN                               * 00254200
*      DELINFO  DS    0D                                              * 00254300
*               ORG   DELINFO+LPMED_LEN                               * 00254400
*      LRETCODE DS    F                                               * 00254500
*      LRSNCODE DS    F                                               * 00254600
*               CSVDYLPA MF=(L,DYLPAL)                                * 00254700
*                                                                     * 00254800
*                                                                     * 00254900
*    Example 2                                                        * 00255000
*                                                                     * 00255100
*    Operation:                                                       * 00255200
*                                                                     * 00255300
*                                                                     * 00255400
*    1.  Determine if deferred LPA processing is complete             * 00255500
*                                                                     * 00255600
*    2.  Wait for the completion of deferred LPA processing, if       * 00255700
*        needed                                                       * 00255800
*                                                                     * 00255900
*                                                                     * 00256000
*                                                                     * 00256100
*    The code is as follows.                                          * 00256200
*                                                                     * 00256300
*               CSVDYLPA REQUEST=QUERYDEFLPA,                       * * 00256400
*                     DEFLPASTATE=LDEFLPASTATE                        * 00256500
*               CLI   LDEFLPASTATE,CsvdylpaDefLPAState_Complete       * 00256600
*               JE    DEFLPA_COMPLETE                                 * 00256700
*               CSVDYLPA REQUEST=DEFLPAWAIT,                        * * 00256800
*                     RETCODE=LRETCODE,RSNCODE=LRSNCODE,            * * 00256900
*                     MF=(E,DYLPAL)                                   * 00257000
*      *                                                              * 00257100
*      * Place code to check return/reason codes here                 * 00257200
*      *                                                              * 00257300
*      DEFLPA_COMPLETE DS 0H                                          * 00257400
*       ... processing code here ...                                  * 00257500
*      DYNAREA  DSECT                                                 * 00257600
*      LRETCODE DS    F                                               * 00257700
*      LRSNCODE DS    F                                               * 00257800
*               CSVDYLPA MF=(L,DYLPAL)                                * 00257900
*      LDEFLPASTATE DS X                                              * 00258000
*               CSVLPRET                                              * 00258100
*                                                                     * 00258200
*                                                                     * 00258300
*01* MACRO USAGE NOTES:                                               * 00258400
*                                                                     * 00258500
*                                                                     * 00258600
*    1.  Printing of the macro expansion is controlled by the ZPRINT  * 00258700
*        global macro variable.  Any value other than 'NO' will       * 00258800
*        result in the macro expansion being printed.  The default is * 00258900
*        'YES'.  The following examples illustrate how the ZPRINT     * 00259000
*        variable should be set.                                      * 00259100
*                                                                     * 00259200
*                    GBLC &ZPRINT                                     * 00259300
*            &ZPRINT SETC 'YES'          &ZPRINT SETC 'NO'            * 00259400
*                                                                     * 00259500
*                                                                     * 00259600
*    2.  When using the NOCHECK option in conjunction with the Modify * 00259700
*        and/or  Execute  forms,  be  sure  that  it is preceded by a * 00259800
*        Modify   or  Execute  form  invocation  which  specifies  or * 00259900
*        defaults  to  the  COMPLETE  option.   This insures that the * 00260000
*        parameter  list  is  initialized completely.   The parameter * 00260100
*        list  field which identifies the version number of the macro * 00260200
*        is only set when the Standard form or the Complete option of * 00260300
*        the  Execute/Modify  form  is  used,  or  when  PLISTVER  is * 00260400
*        explicitly  specified.  Be sure that the resulting parameter * 00260500
*        list version number covers all the keys that you use.        * 00260600
*                                                                     * 00260700
*                                                                     * 00260800
*    3.  The CSVDYLPA macro supports multiple versions. The macro key * 00260900
*        list  below  contains the version level in which the key was * 00261000
*        first  introduced.  Positionals  are  identified by the word * 00261100
*        'Positional'  followed by the numeric value of its position. * 00261200
*        Nested  positionals  are  preceded  by their outer key.  The * 00261300
*        lowest value represents the original version and the highest * 00261400
*        value  represents  the  most  recent version of the CSVDYLPA * 00261500
*        macro.    When  specifying  PLISTVER,  be sure that it is at * 00261600
*        least  as high as the highest version number of all the keys * 00261700
*        being used.                                                  * 00261800
*                                                                     * 00261900
*        VERSION   KEY                                                * 00262000
*                                                                     * 00262100
*              0      AC1                                             * 00262200
*                     ADDALIAS                                        * 00262300
*                     APFREQUIRED                                     * 00262400
*                     BYADDR                                          * 00262500
*                     BYPATH                                          * 00262600
*                     CCHH                                            * 00262700
*                     CMDINFO                                         * 00262800
*                     DCB                                             * 00262900
*                     DCBPTR                                          * 00263000
*                     DDNAME                                          * 00263100
*                     DEFLPASTATE                                     * 00263200
*                     DSNAME                                          * 00263300
*                     DYNFUNC                                         * 00263400
*                     ERRORDATA                                       * 00263500
*                     LINKAGE                                         * 00263600
*                     LONGPARMOK                                      * 00263700
*                     LPMEAQ                                          * 00263800
*                     MASKDCB                                         * 00263900
*                     MASKDCBPTR                                      * 00264000
*                     MASKDDNAME                                      * 00264100
*                     MASKDSNAME                                      * 00264200
*                     MODINFO                                         * 00264300
*                     MODINFOTYPE                                     * 00264400
*                     MODINFOX                                        * 00264500
*                     MODPROB                                         * 00264600
*                     NUMMOD                                          * 00264700
*                     OUTAREALEN                                      * 00264800
*                     OUTAREANUM                                      * 00264900
*                     OUTAREAPTR                                      * 00265000
*                     OUTAREASP                                       * 00265100
*                     PATHNAME                                        * 00265200
*                     PATHNAMELEN                                     * 00265300
*                     QUERYONLY                                       * 00265400
*                     REQUEST                                         * 00265500
*                     REQUESTOR                                       * 00265600
*                     SECMODCHECK                                     * 00265700
*                     TYPE                                            * 00265800
*                     TYPEVALUE                                       * 00265900
*                     UCBADDR                                         * 00266000
*                     VOLUME                                          * 00266100
*              1      MODINFO64                                       * 00266200
*                     OUTAREA64PTR                                    * 00266300
*                                                                     * 00266400
*                                                                     * 00266500
*                                                                     * 00266600
*01* CHANGE ACTIVITY:                                                 * 00266700
*                                                                     * 00266800
*   Flag LineItem  FMID    Date   ID    Comment                       * 00266900
*                                                                     * 00267000
*    $L0=DYLPAAMG JBB6604 961001 PD00XB:  Dynamic LPA                 * 00267100
*    $L1=DYLPACSV JBB6604 961001 PD00XB:  Dynamic LPA                 * 00267200
*    $01=OW26531  JBB6604 970603 PD00XB:  Caller can hold ENQ         * 00267300
*    $P1=ME01563  HBB7720 040820 PD00XB:  QueryOnly                   * 00267400
*    $L2=ME01563  HBB7730 050201 PD00XB:  AddAlias                    * 00267500
*    $L3=ME11052  HBB7750 070901 PD00XB:  LPA pathname                * 00267600
*    $L4=CSV11    HBB7760 080101 PD00XB:  Pathname BYADDR=NO          * 00267700
*    $L5=ME14270  HBB7760 080801 PD00XB:  QUERYDEFLPA, DEFLPAWAIT     * 00267800
*    $L6=CSV12    HBB7770 081105 PD00XB:  Misc                        * 00267900
*    $L7=LONGPARM HBB7790 120601 PD00XB:  LongParm, AC1               * 00268000
*    $L8=ME27684  HBB77A0 140609 PD00XB:  VOLUME                      * 00268100
*    $L9=RMODE64  HBB77B0 150901 PD00XB:  RMODE 64                    * 00268200
****** END OF SPECIFICATIONS *****************************************/ 00268300
*/********************************************************************* 00268400
