.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01214700
.*******************************************************************    01214800
                      ACTR  10000                                       01214900
.*                                                                      01215000
                      LCLA  &ZLC           COLUMN POSITION              01215100
                      LCLA  &ZOPX          LIST TABLE INDEX             01215200
                      LCLC  &ZLL           LABEL                        01215300
                      LCLC  &ZLOP          OPERATION                    01215400
                      LCLC  &ZLV           VALUE                        01215500
                      LCLC  &ZLD           DESCRIPTION                  01215600
.*                                                                      01215700
&ZOPX                 SETA  1                                           01215800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01215900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01216000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01216100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01216200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01216300
                      AIF   (K'&ZLD LE 0).ZL5                           01216400
&ZLC                  SETA  35             COMMENT COLUMN               01216500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01216600
&ZLC                  SETA  33             COMMMENT COLUMN              01216700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01216800
&ZLV                  SETC  '&ZLV'.' '                                  01216900
                      AGO   .ZL3                                        01217000
.*                                                                      01217100
.ZL4                  ANOP  ,                                           01217200
&ZLV                  SETC  '&ZLV'.' '                                  01217300
.ZL5                  ANOP  ,                                           01217400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01217500
.*                                                                      01217600
&ZLL     &ZLOP &ZLV                                                     01217700
.*                                                                      01217800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01217900
                      AGO   .ZL2           CONTINUE LOOPING             01218000
.*                                                                      01218100
.ZLX                  ANOP  ,                                           01218200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01218300
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01218400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01218500
         L     14,16(0,0)              GET CVT ADDRESS             @L1A 01218600
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 01218700
         L     14,468(14,0)            GET LX/EX FOR SERVICE RTN   @L1A 01218800
         PC    0(14)                                               @L1A 01218900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01219000
