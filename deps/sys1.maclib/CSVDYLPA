                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01048200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01048300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01048400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01048500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01048600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01048700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01048800
&ZOPN                 SETA  &ZOPN+1                                     01048900
&ZOPL(&ZOPN)          SETC  ' '                                         01049000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01049100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01049200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01049300
                      AGO   .ZIMFNX                                     01049400
.*                                                                      01049500
.ZIMFN2               ANOP ,                                            01049600
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01049700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01049800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01049900
