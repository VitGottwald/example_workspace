                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00291700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00291800
                AGO   .ZPVCK2                                           00291900
.ZPVCK3         ANOP  ,                                                 00292000
.*-------------------------------------------------------------------   00292100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00292200
&ZMS            SETC  '"PLISTVER='                                      00292300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00292400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00292500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00292600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00292700
                AGO   .ZPVCKE                                           00292800
.ZPVCK2         ANOP  ,                                                 00292900
.*-------------------------------------------------------------------   00293000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00293100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00293200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00293300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00293400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00293500
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00293600
.ZPVCK6         ANOP  ,                                                 00293700
&ZMS            SETC  '"PLISTVER='                                      00293800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00293900
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00294000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00294100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00294200
&XPLISTVER      SETA  1                                                 00294300
                AGO   .ZPVCKE                                           00294400
.*-------------------------------------------------------------------   00294500
.ZPVCK4A        ANOP  ,                                                 00294600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00294700
&ZMS            SETC  'PLISTVER='                                       00294800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00294900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00295000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00295100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00295200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00295300
                AGO   .ZPVCK5                                           00295400
.ZPVCK4         ANOP  ,                                                 00295500
&XPLISTVER      SETA  &PLISTVER(1)                                      00295600
.ZPVCK5         ANOP  ,                                                 00295700
.ZPVCKE         ANOP  ,                                                 00295800
