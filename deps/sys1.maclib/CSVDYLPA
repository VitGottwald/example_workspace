.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00275500
.********************************************************************   00275600
                  GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL             00275700
                  GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE      00275800
                  GBLA  &SYSALVL    SYSSTATE ARCHLVL                    00275900
                  GBLC  &SYSAM64    SYSSTATE AMODE64                    00276000
.*                                                                      00276100
                  AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED      00276200
                  AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED       00276300
                  SPLEVEL  TEST                                         00276400
.ZSYS2            ANOP  ,                                               00276500
                  AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1           00276600
                  AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1           00276700
                  SYSSTATE TEST                                         00276800
                  AGO   .ZSYS4                                          00276900
.ZSYS3            ANOP  ,                                               00277000
&SYSASCE          SETC  'P'         ASSUME SYSSTATE PRIMARY             00277100
.ZSYS4            ANOP  ,                                               00277200
                  AIF   ('&SYSAM64' NE 'YES').ZSYSN64                   00277300
&ZMAC_LLGT        SETC 'LLGT'                                           00277400
&ZMAC_LGR         SETC 'LGR'                                            00277500
&ZMAC_LG          SETC 'LG'                                             00277600
&ZMAC_STG         SETC 'STG'                                            00277700
.ZSYSN64          ANOP  ,                                               00277800
.*******************************************************************    00277900
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00278000
.*******************************************************************    00278100
                  LCLC  &XREQUEST   ++ VALUE                            00278200
                  LCLC  &XMODINFOTYPE           ++ VALUE                00278300
                  LCLC  &XMODINFO   ++ NAME                             00278400
                  LCLC  &XMODINFOX  ++ NAME                             00278500
                  LCLC  &XMODINFO64             ++ NAME                 00278600
                  LCLC  &XNUMMOD    ++ NAME                             00278700
                  LCLC  &XBYADDR    ++ VALUE                            00278800
                  LCLC  &XDUMMY1    ++ NAME                             00278900
                  LCLC  &XDSNAME    ++ NAME                             00279000
                  LCLC  &XVOLUME    ++ NAME                             00279100
                  LCLC  &XDDNAME    ++ NAME                             00279200
                  LCLC  &XDCB       ++ NAME                             00279300
                  LCLC  &XDCBPTR    ++ NAME                             00279400
                  LCLC  &XPATHNAME  ++ NAME                             00279500
                  LCLC  &XPATHNAMELEN           ++ NAME                 00279600
                  LCLC  &XADDALIAS  ++ VALUE                            00279700
                  LCLC  &XOUTAREAPTR            ++ NAME                 00279800
                  LCLC  &XOUTAREA64PTR          ++ NAME                 00279900
                  LCLC  &XOUTAREALEN            ++ NAME                 00280000
                  LCLC  &XOUTAREANUM            ++ NAME                 00280100
                  LCLC  &XOUTAREASP             ++ NAME                 00280200
                  LCLC  &XBYPATH    ++ VALUE                            00280300
                  LCLC  &XUCBADDR   ++ NAME                             00280400
                  LCLC  &XCCHH      ++ NAME                             00280500
                  LCLC  &XAC1       ++ VALUE                            00280600
                  LCLC  &XLONGPARMOK            ++ VALUE                00280700
                  LCLC  &XMASKDSNAME            ++ NAME                 00280800
                  LCLC  &XMASKDDNAME            ++ NAME                 00280900
                  LCLC  &XMASKDCB   ++ NAME                             00281000
                  LCLC  &XMASKDCBPTR            ++ NAME                 00281100
                  LCLC  &XQUERYONLY             ++ VALUE                00281200
                  LCLC  &XLPMEAQ    ++ NAME                             00281300
