                  AIF   (N'&DYNFUNC GT 1).ZG169B  MULTI                 00657600
&XDYNFUNC         SETC  '&DYNFUNC'  SET XVARIABLE                       00657700
.ZG169B           AIF   (K'&DYNFUNC(1) GT 0).ZG169C                     00657800
&ZXDYNFUNC        SETC  ''          RESET IN/OUTPUT                     00657900
&ZMS              SETC  ' '                                             00658000
&ZMS              SETC  '&ZMS "DYNFUNC" KEY AND ITS'                    00658100
                  MNOTE 8,' &ZMS ARGUMENT ARE REQUIRED.'                00658200
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00658300
.ZG169C           ANOP  ,                                               00658400
.*-------------------------------------------------------------------   00658500
.***AG.KCK02 NAME, RQD OUTPUT, KEYS -GPR OUT OF RANGE                   00658600
                  AIF   (K'&ZXDYNFUNC LE 0).ZG169R  NOT I/O             00658700
                  AIF   ('&XDYNFUNC'(1,1) NE '(').ZG169R                00658800
&ZCGPR            SETC  '&XDYNFUNC'(2,K'&XDYNFUNC-2)                    00658900
                  AIF   ('&ZCGPR'(1,1) NE '0').ZG169P                   00659000
                  AIF   (K'&ZCGPR GT 2).ZG169Q                          00659100
                  AIF   ('&ZCGPR' EQ '0').ZG169Q                        00659200
&ZCGPR            SETC  '&ZCGPR'(2,1)                                   00659300
                  AIF   (T'&ZCGPR NE 'N').ZG169Q  INVALID REG NAME      00659400
.ZG169P           ANOP  ,                                               00659500
                  AIF   (T'&ZCGPR NE 'N').ZG169R  NON NUMERIC           00659600
                  AIF   ('&ZCGPR' LT '2').ZG169Q                        00659700
                  AIF   ('&ZCGPR' LE '12').ZG169R                       00659800
.ZG169Q           ANOP  ,                                               00659900
&ZMS              SETC  '"DYNFUNC='                                     00660000
&ZMS              SETC  '&ZMS.&DYNFUNC"'                                00660100
&ZMS              SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'       00660200
                  MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                00660300
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00660400
.ZG169R           ANOP  ,                                               00660500
