.*******************************************************************    00950900
.***AE.01  START OF CSVDYLPA MACRO CODE                                 00951000
.*******************************************************************    00951100
                  LCLC  &M0M0001                 NAME                   00951200
&M0M0001          SETC  '&XLABEL'                SET NAME               00951300
                  AIF   (K'&XLABEL NE 0).XLAB2                          00951400
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           00951500
.*                                                                      00951600
.XLAB2            ANOP  ,                                               00951700
                  AIF   ('&XMF' EQ 'L').XLAB3                           00951800
&M0M0001 DS    0H                             CSVDYLPA-1                00951900
.XLAB3            ANOP  ,                                               00952000
.*******************************************************************    00952100
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00952200
.*******************************************************************    00952300
              PUSH  PRINT                                               00952400
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00952500
                  PRINT OFF         ++ PR01                             00952600
                  AGO   .ZPRNTXX                                        00952700
.ZPRINTX          PRINT ON                                              00952800
.ZPRNTXX          ANOP  ,                                               00952900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00953000
         LCLC  &T2                                                 @L1A 00953100
         AIF   ('&REQUEST' NE 'QUERYDYN').NOQDYN                   @L5C 00953200
 AIF ('&MF' EQ '' AND '&RETCODE' EQ '' AND '&RSNCODE' EQ '').OKRET @L1A 00953300
&T2      SETC  'MF, RETCODE AND RSNCODE ARE NOT VALID WITH '       @L1A 00953400
&T2      SETC  '&T2.REQUEST=QUERYDYN'                              @L1A 00953500
         MNOTE 8,'&T2'                                             @L1A 00953600
         POP   PRINT                                               @L1A 00953700
         MEXIT                                                     @L1A 00953800
.OKRET   ANOP                                                      @L1A 00953900
         AIF   ('&DYNFUNC'(1,1) NE '(').LREG Not a reg, branch     @L1A 00954000
&T2      SETC  '0&DYNFUNC'             Target expression           @L1A 00954100
         AGO   .MREG                                               @L1A 00954200
.LREG    ANOP                          Not a reg                   @L1A 00954300
&T2      SETC  '&DYNFUNC(1)'           Target expression           @L1A 00954400
.MREG    ANOP                          Produce resulting statement @L1A 00954500
         L     15,16(0,0)              GET CVT                          00954600
         AIF   ('&SYSASCE' EQ 'P').NOAR                            @L1A 00954700
         LAE   15,0(15,0)              CLEAR AR15                       00954800
.NOAR    ANOP                                                      @L1A 00954900
         TM    X'4F2'(15),X'80'        Is MVS release correct      @L1A 00955000
         BNZ   A&SYSNDX                Yes, dynamic LPA available  @L1A 00955100
         MVI   &T2,0                   Function not available      @L1A 00955200
         B     B&SYSNDX                Skip other path             @L1A 00955300
A&SYSNDX DS    0H                      Function possibly available @L1A 00955400
         MVI   &T2,1                   Indicate available          @L1A 00955500
B&SYSNDX DS    0H                      End of QUERYDYN             @L1A 00955600
         POP   PRINT                                               @L1A 00955700
         MEXIT                                                     @L1A 00955800
.NOQDYN  ANOP                                                      @L5C 00955900
         AIF   ('&REQUEST' NE 'QUERYDEFLPA').NOQDEF                @L5A 00956000
 AIF ('&MF' EQ '' AND '&RETCODE' EQ '' AND '&RSNCODE' EQ '').OKQDL @L5A 00956100
&T2      SETC  'MF, RETCODE AND RSNCODE ARE NOT VALID WITH '       @L5A 00956200
&T2      SETC  '&T2.REQUEST=QUERYDEFLPA'                           @L5A 00956300
         MNOTE 8,'&T2'                                             @L5A 00956400
         POP   PRINT                                               @L5A 00956500
         MEXIT                                                     @L5A 00956600
.OKQDL   ANOP                                                      @L5A 00956700
         AIF   ('&DEFLPASTATE'(1,1) NE '(').LRG1 Not a reg, branch @L5A 00956800
&T2      SETC  '0&DEFLPASTATE'         Target expression           @L5A 00956900
         AGO   .MRG1                                               @L5A 00957000
.LRG1    ANOP                          Not a reg                   @L5A 00957100
&T2      SETC  '&DEFLPASTATE(1)'       Target expression           @L5A 00957200
.MRG1    ANOP                          Produce resulting statement @L5A 00957300
         L     15,16(0,0)              GET CVT                          00957400
         AIF   ('&SYSASCE' EQ 'P').NOARQDL                         @L5A 00957500
         LAE   15,0(15,0)              CLEAR AR15                       00957600
.NOARQDL ANOP                                                      @L5A 00957700
         L     15,X'8C'(15,0)          Locate ECVT                 @L5A 00957800
         L     15,X'E4'(15,0)          Locate DEFLPASTATE block    @L5A 00957900
         MVC   &T2.(1),X'2A'(15)       Copy DEFLPASTATE byte       @L5A 00958000
         POP   PRINT                                               @L5A 00958100
         MEXIT                                                     @L5A 00958200
