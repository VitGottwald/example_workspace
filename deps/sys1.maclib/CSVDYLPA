                  LCLC  &ZXMFSCK    ++ INPUT VALUE                      00288400
.********************************************************************   00288500
.***AC.PLV01  PROCESS PLISTVER                                      *   00288600
.********************************************************************   00288700
&XPLISTVER      SETA  0                                                 00288800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00288900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00289000
&XPLISTVER      SETA  1                                                 00289100
.ZPLV0          ANOP  ,                                                 00289200
.*                                                                      00289300
                AIF   ('&MODINFO64' EQ '').ZPLV1E                       00289400
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00289500
&XPLISTVER      SETA  1                                                 00289600
.ZPLV1E         ANOP  ,                                                 00289700
.*                                                                      00289800
                AIF   ('&OUTAREA64PTR' EQ '').ZPLV2E                    00289900
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00290000
&XPLISTVER      SETA  1                                                 00290100
.ZPLV2E         ANOP  ,                                                 00290200
.*                                                                      00290300
                AIF   ('&OUTAREA64PTR' EQ '').ZPLV3E                    00290400
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00290500
&XPLISTVER      SETA  1                                                 00290600
.ZPLV3E         ANOP  ,                                                 00290700
.*                                                                      00290800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00290900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00291000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00291100
.*-------------------------------------------------------------------   00291200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00291300
&ZMS            SETC  '"PLISTVER='                                      00291400
