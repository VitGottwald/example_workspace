&ZOPL(&ZOPN)          SETC  ' '                                         01050100
&ZOPO(&ZOPN)          SETC  'LAE'                                       01050200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01050300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01050400
                      AGO   .ZIMFNX                                     01050500
.ZIMFNX1              ANOP ,                                            01050600
