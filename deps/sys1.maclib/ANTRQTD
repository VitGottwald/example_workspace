.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               03280600
.*******************************************************************    03280700
                      ACTR  10000                                       03280800
.*                                                                      03280900
                      LCLA  &ZLC           COLUMN POSITION              03281000
                      LCLA  &ZOPX          LIST TABLE INDEX             03281100
                      LCLC  &ZLL           LABEL                        03281200
                      LCLC  &ZLOP          OPERATION                    03281300
                      LCLC  &ZLV           VALUE                        03281400
                      LCLC  &ZLD           DESCRIPTION                  03281500
.*                                                                      03281600
&ZOPX                 SETA  1                                           03281700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            03281800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              03281900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              03282000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              03282100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              03282200
                      AIF   (K'&ZLD LE 0).ZL5                           03282300
&ZLC                  SETA  35             COMMENT COLUMN               03282400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    03282500
&ZLC                  SETA  33             COMMMENT COLUMN              03282600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   03282700
&ZLV                  SETC  '&ZLV'.' '                                  03282800
                      AGO   .ZL3                                        03282900
.*                                                                      03283000
.ZL4                  ANOP  ,                                           03283100
&ZLV                  SETC  '&ZLV'.' '                                  03283200
.ZL5                  ANOP  ,                                           03283300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               03283400
.*                                                                      03283500
&ZLL     &ZLOP &ZLV                                                     03283600
.*                                                                      03283700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               03283800
                      AGO   .ZL2           CONTINUE LOOPING             03283900
.*                                                                      03284000
.ZLX                  ANOP  ,                                           03284100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                03284200
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                03284300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  03284400
         L     14,16(,0)                                                03284500
         L     14,1216(,14)                                             03284600
         L     14,44(,14)                                               03284700
