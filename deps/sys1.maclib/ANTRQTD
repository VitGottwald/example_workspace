                      AGO   .ZMRSNX                                     03278400
.*                                                                      03278500
.ZMRSN2               ANOP  ,                                           03278600
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                03278700
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 03278800
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                03278900
