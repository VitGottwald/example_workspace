                   AGO   .ZELIST2                                       02770900
.*                                                                      02771000
.ZELIST3           ANOP  ,                                              02771100
&ZCVAL             SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA             02771200
&ZMS               SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE       02771300
&ZMS               SETC  '&ZMS "&ZCVAL"'                                02771400
&ZMS               SETC  '&ZMS CANNOT BE USED.'                         02771500
&ZMS               SETC  '&ZMS NO POSITIONAL ARGUMENTS'                 02771600
                   MNOTE 8,' &ZMS ARE ALLOWED.'                         02771700
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        02771800
.ZELISTX           ANOP  ,                                              02771900
&ZASYSLNDX         SETA  0                                              02772000
.*-------------------------------------------------------------------   02772100
.***AE.MF02 MF=(SLME)                                                   02772200
.ZEMFX             ANOP  ,           END OF PARTIAL SYNTAX CHECK        02772300
.*******************************************************************    02772400
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       02772500
.*******************************************************************    02772600
                   AIF   (&ZMACRET NE 0).ZLX2   ERROR DETECTED          02772700
.*******************************************************************    02772800
.***AE.01  START OF ANTRQTD MACRO CODE                                  02772900
.*******************************************************************    02773000
                   LCLC  &M0M0001                 NAME                  02773100
&M0M0001           SETC  '&XLABEL'                SET NAME              02773200
                   AIF   (K'&XLABEL NE 0).XLAB2                         02773300
&M0M0001           SETC  'M00M&SYSNDX'            DEFAULT NAME          02773400
.*                                                                      02773500
.XLAB2             ANOP  ,                                              02773600
                   AIF   ('&XMF' EQ 'L').XLAB3                          02773700
&M0M0001 DS    0H                             ANTRQTD-4                 02773800
.XLAB3             ANOP  ,                                              02773900
.*******************************************************************    02774000
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              02774100
.*******************************************************************    02774200
              PUSH  PRINT                                               02774300
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     02774400
                   PRINT OFF         ++ PR01                            02774500
                   AGO   .ZPRNTXX                                       02774600
.ZPRINTX           PRINT ON                                             02774700
.ZPRNTXX           ANOP  ,                                              02774800
