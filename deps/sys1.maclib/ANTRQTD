&ZOPO(&ZOPN)          SETC  'LR'                                        03276300
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 03276400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        03276500
.ZMRETX               ANOP  ,                                           03276600
.*******************************************************************    03276700
.***AO.X03  REASON CODE PROCESSING                                      03276800
.*******************************************************************    03276900
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    03277000
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            03277100
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              03277200
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             03277300
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              03277400
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             03277500
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                03277600
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 03277700
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                03277800
