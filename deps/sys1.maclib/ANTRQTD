                      LCLC  &ZOPV(4095)    VALUE                        02775800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  02775900
.*                                                                      02776000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     02776100
.******************************************************************     02776200
.***AL.L10 ANTRQTD FMT=LIST MF=S!L PARAMETERS                           02776300
.******************************************************************     02776400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     02776500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     02776600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     02776700
&XMFATTR              SETC  '0D'                                        02776800
.*                                                                      02776900
