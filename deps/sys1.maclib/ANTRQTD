.***AC.PLV01  PROCESS PLISTVER                                      *   00602200
.********************************************************************   00602300
&XPLISTVER      SETA  0                                                 00602400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00602500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00602600
&XPLISTVER      SETA  4                                                 00602700
.ZPLV0          ANOP  ,                                                 00602800
.*                                                                      00602900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00603000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00603100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00603200
.*-------------------------------------------------------------------   00603300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00603400
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00603500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00603600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00603700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00603800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00603900
                AGO   .ZPVCK2                                           00604000
.ZPVCK3         ANOP  ,                                                 00604100
.*-------------------------------------------------------------------   00604200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00604300
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00604400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00604500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00604600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00604700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00604800
                AGO   .ZPVCKE                                           00604900
.ZPVCK2         ANOP  ,                                                 00605000
.*-------------------------------------------------------------------   00605100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00605200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00605300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00605400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00605500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00605600
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00605700
.ZPVCK6         ANOP  ,                                                 00605800
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00605900
