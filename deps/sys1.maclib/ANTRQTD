         L     14,88(,14)                                               03284900
         ICM   14,15,324(14)                                            03285000
         BNZ   A&SYSNDX                                                 03285100
         L     15,=A(RQST_PC_NUMBER_ZERO)                               03285200
         SLR   0,0                                                      03285300
         B     B&SYSNDX                                                 03285400
A&SYSNDX DS    0H                                                       03285500
         PC    0(14)                                                    03285600
B&SYSNDX DS    0H                                                       03285700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  03285800
.ZLINKX               ANOP  ,                                           03285900
.*******************************************************************    03286000
.***AP.X01  FORMAT GENERATED CODE                                       03286100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            03286200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              03286300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              03286400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              03286500
