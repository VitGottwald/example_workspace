                      AIF   (&SYSALVL NE 0).ZALVL6A                     02777200
&ZOPO(&ZOPN)          SETC  'B'                                         02777300
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                02777400
                      AGO   .ZALVL6B                                    02777500
.ZALVL6A              ANOP                                              02777600
&ZOPO(&ZOPN)          SETC  'BRAS'                                      02777700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              02777800
.ZALVL6B              ANOP                                              02777900
&ZOPD(&ZOPN)          SETC  '++ ANTRQTD ARG PARMS'                      02778000
.*                                                                      02778100
