.***AO.X02  RETURN CODE PROCESSING                                      03274000
.*******************************************************************    03274100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    03274200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            03274300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             03274400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             03274500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               03274600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                03274700
&ZOPN                 SETA  &ZOPN+1                                     03274800
&ZOPL(&ZOPN)          SETC  ' '                                         03274900
&ZOPO(&ZOPN)          SETC  'ST'                                        03275000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              03275100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        03275200
                      AGO   .ZMRETX                                     03275300
.*                                                                      03275400
.ZMRET2               ANOP  ,                                           03275500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                03275600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                03275700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                03275800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  03275900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   03276000
