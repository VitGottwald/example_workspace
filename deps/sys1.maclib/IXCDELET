.***AC.PLV01  PROCESS PLISTVER                                      *   00046300
.********************************************************************   00046400
&XPLISTVER      SETA  0                                                 00046500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00046600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00046700
&XPLISTVER      SETA  0                                                 00046800
.ZPLV0          ANOP  ,                                                 00046900
.*                                                                      00047000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00047100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00047200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00047300
.*-------------------------------------------------------------------   00047400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00047500
&ZMS            SETC  '"PLISTVER='                                      00047600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00047700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00047800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00047900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048000
                AGO   .ZPVCK2                                           00048100
.ZPVCK3         ANOP  ,                                                 00048200
.*-------------------------------------------------------------------   00048300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00048400
&ZMS            SETC  '"PLISTVER='                                      00048500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00048600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00048700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00048800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048900
                AGO   .ZPVCKE                                           00049000
.ZPVCK2         ANOP  ,                                                 00049100
.*-------------------------------------------------------------------   00049200
