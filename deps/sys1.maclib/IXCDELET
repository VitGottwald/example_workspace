.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00118600
.*******************************************************************    00118700
                      ACTR  10000                                       00118800
.*                                                                      00118900
                      LCLA  &ZLC           COLUMN POSITION              00119000
                      LCLA  &ZOPX          LIST TABLE INDEX             00119100
                      LCLC  &ZLL           LABEL                        00119200
                      LCLC  &ZLOP          OPERATION                    00119300
                      LCLC  &ZLV           VALUE                        00119400
                      LCLC  &ZLD           DESCRIPTION                  00119500
.*                                                                      00119600
&ZOPX                 SETA  1                                           00119700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00119800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00119900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00120000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00120100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00120200
                      AIF   (K'&ZLD LE 0).ZL5                           00120300
&ZLC                  SETA  35             COMMENT COLUMN               00120400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00120500
&ZLC                  SETA  33             COMMMENT COLUMN              00120600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00120700
&ZLV                  SETC  '&ZLV'.' '                                  00120800
                      AGO   .ZL3                                        00120900
.*                                                                      00121000
.ZL4                  ANOP  ,                                           00121100
&ZLV                  SETC  '&ZLV'.' '                                  00121200
.ZL5                  ANOP  ,                                           00121300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00121400
.*                                                                      00121500
&ZLL     &ZLOP &ZLV                                                     00121600
.*                                                                      00121700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00121800
                      AGO   .ZL2           CONTINUE LOOPING             00121900
.*                                                                      00122000
.ZLX                  ANOP  ,                                           00122100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00122200
