.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00041600
.********************************************************************   00041700
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00041800
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00041900
              GBLA  &SYSALVL    SYSSTATE ARCHLVL                        00042000
              GBLC  &SYSAM64    SYSSTATE AMODE64                        00042100
.*                                                                      00042200
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00042300
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00042400
              SPLEVEL  TEST                                             00042500
.ZSYS2        ANOP  ,                                                   00042600
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00042700
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00042800
              SYSSTATE TEST                                             00042900
              AGO   .ZSYS4                                              00043000
.ZSYS3        ANOP  ,                                                   00043100
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00043200
.ZSYS4        ANOP  ,                                                   00043300
              AIF   ('&SYSAM64' NE 'YES').ZSYSN64                       00043400
&ZMAC_LLGT    SETC 'LLGT'                                               00043500
&ZMAC_LGR     SETC 'LGR'                                                00043600
&ZMAC_LG      SETC 'LG'                                                 00043700
&ZMAC_STG     SETC 'STG'                                                00043800
.ZSYSN64      ANOP  ,                                                   00043900
