&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00122500
                      AGO   .ZMRETX                                     00122600
.*                                                                      00122700
.ZMRET2               ANOP  ,                                           00122800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00122900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00123000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00123100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00123200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00123300
&ZOPN                 SETA  &ZOPN+1                                     00123400
