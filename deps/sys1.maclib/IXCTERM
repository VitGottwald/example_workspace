              AIF   (T'&ZCGPR NE 'N').ZG8Q      INVALID REG NAME        00086100
.ZG8P         ANOP  ,                                                   00086200
              AIF   (T'&ZCGPR NE 'N').ZG8R      NON NUMERIC             00086300
              AIF   ('&ZCGPR' EQ '15').ZG8R                             00086400
              AIF   ('&ZCGPR' LT '2').ZG8Q                              00086500
              AIF   ('&ZCGPR' LE '12').ZG8R                             00086600
.ZG8Q         ANOP  ,                                                   00086700
&ZMS          SETC  '"RETCODE='                                         00086800
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00086900
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00087000
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12) OR (15).'            00087100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00087200
.ZG8R         ANOP  ,                                                   00087300
.*----------------------------------------------------------------***   00087400
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00087500
              AIF   (N'&RSNCODE LE 1).ZG11A     CK NUM                  00087600
&ZMS          SETC  '"RSNCODE='                                         00087700
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00087800
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00087900
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00088000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00088100
.ZG11A        ANOP  ,                                                   00088200
.*-------------------------------------------------------------------   00088300
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00088400
              AIF   (K'&RSNCODE(1) LE 0).ZG11C  NO PARM                 00088500
&ZXRSNCODE    SETC  'OUTPUT'    SET OUTPUT                              00088600
&XRSNCODE     SETC  '&RSNCODE(1)'               SET XVARIABLE           00088700
              AIF   (N'&RSNCODE GT 1).ZG11C                             00088800
&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00088900
.ZG11C        ANOP  ,                                                   00089000
.*-------------------------------------------------------------------   00089100
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00089200
              AIF   (K'&ZXRSNCODE LE 0).ZG11R   NOT I/O                 00089300
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG11R                     00089400
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00089500
              AIF   ('&ZCGPR'(1,1) NE '0').ZG11P                        00089600
              AIF   (K'&ZCGPR GT 2).ZG11Q                               00089700
              AIF   ('&ZCGPR' EQ '0').ZG11R                             00089800
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00089900
              AIF   (T'&ZCGPR NE 'N').ZG11Q     INVALID REG NAME        00090000
.ZG11P        ANOP  ,                                                   00090100
              AIF   (T'&ZCGPR NE 'N').ZG11R     NON NUMERIC             00090200
              AIF   ('&ZCGPR' EQ '0').ZG11R                             00090300
              AIF   ('&ZCGPR' LT '2').ZG11Q                             00090400
              AIF   ('&ZCGPR' LE '12').ZG11R                            00090500
.ZG11Q        ANOP  ,                                                   00090600
&ZMS          SETC  '"RSNCODE='                                         00090700
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00090800
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00090900
              MNOTE 8,' &ZMS IN THE RANGE (0) OR (2)-(12).'             00091000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00091100
.ZG11R        ANOP  ,                                                   00091200
.******************************************************************     00091300
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00091400
.******************************************************************     00091500
              ACTR  10000                                               00091600
.*----------------------------------------------------------------***   00091700
.***AH.P02 EXTRA POSITIONALS                                            00091800
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00091900
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00092000
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00092100
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00092200
.ZGLISTX1     ANOP  ,                                                   00092300
&ZCVAL        SETC  ''          ERROR FIELD                             00092400
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00092500
.ZGLIST2      ANOP  ,                                                   00092600
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00092700
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00092800
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00092900
              AGO   .ZGLIST2                                            00093000
.*                                                                      00093100
.ZGLIST3      ANOP  ,                                                   00093200
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00093300
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'                           00093400
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00093500
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00093600
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00093700
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00093800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00093900
.ZGLISTX      ANOP  ,                                                   00094000
&ZASYSLNDX    SETA  0                                                   00094100
