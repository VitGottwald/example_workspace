*/*01* MACRO NAME = IEFJFCBN                                         */ 00080000
*/*                                                                  */ 00090000
*/*01* DESCRIPTIVE NAME = Job File Control Block                     */ 00100000
*/*                                                                  */ 00110000
*/*02*  ACRONYM = JFCB                                               */ 00120000
*/*                                                                  */ 00130000
*/*01* PROPRIETARY STATEMENT=                                        */ 00140000
*/***PROPRIETARY_STATEMENT********************************************/ 00150000
*/*                                                                  */ 00170000
*/*     LICENSED MATERIALS - PROPERTY OF IBM                         */ 00180000
*/*     5650-ZOS COPYRIGHT IBM CORP. 1977, 2021                      */ 00190005
*/*                                                                  */ 00200000
