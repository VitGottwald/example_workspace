*/*          JFCNOCC  JFCNSL   JFCNTM   JFCNWRIT JFCOLD              */ 00610000
*/*          JFCONE   JFCONTIG JFCOPTJ  JFCOPTQ  JFCORGAM            */ 00620000
*/*          JFCORGCX JFCORGDA JFCORGGS JFCORGIS JFCORGPO            */ 00630000
*/*          JFCORGPS JFCORGU  JFCOUTLI JFCOUTOP JFCOVER             */ 00640000
*/*          JFCPCI   JFCPCIA1 JFCPCIA2 JFCPCIBT JFCPCIN1            */ 00650000
*/*          JFCPCIN2 JFCPCIR1 JFCPCIR2 JFCPCIX1 JFCPCIX2            */ 00660000
*/*          JFCPDS   JFCPOSID JFCPRTSP JFCRBIDC JFCRBIDO            */ 00670000
*/*          JFCRCFM  JFCRECFM JFCRECV  JFCREDUC JFCREL              */ 00680000
*/*          JFCREORG JFCRFB   JFCRFO   JFCRFS   JFCRKP              */ 00690000
*/*          JFCRLSE  JFCROUND JFCSDNAM JFCSEND  JFCSHARE            */ 00700000
*/*          JFCSIM   JFCSKP   JFCSL    JFCSPNO  JFCSPONE            */ 00710000
*/*          JFCSPTHR JFCSPTWO JFCSTACK JFCSUL   JFCTHRSH            */ 00720000
