*%GOTO IOCMX2;    /*                                                    03680002
.IOC000  ANOP                                                           03690002
         SPACE 2                                                        03700002
         AIF   (T'&CSECT NE 'O').IOC001                                 03710002
&OP      SETC  'DS'                      CSECT not specified,           03720002
.*                                       use DS to map storage          03730002
.*                                                                      03740002
&L       SETC  '  '                      No parenthesis                 03750002
&E       SETC  ''                                                       03760002
&VOILN   SETC  ''                                                       03770002
         AGO   .IOC002                                                  03780002
.IOC001  ANOP                                                           03790002
&OP      SETC  'DC'                      CSECT specified,               03800002
.*                                       allocate storage               03810002
&L       SETC  '('                                                      03820002
&E       SETC  ')'                                                      03830002
&VOILN   SETC  '''24'''                  VOID length                    03840002
         AIF   ('&CSECT' EQ 'WRITE').IOC006                             03850002
         AIF   ('&CSECT' EQ 'SYNCH').IOC009A                            03860002
         AIF   ('&CSECT' EQ 'DUMP').IOC0DMP                             03870002
         AIF   ('&CSECT' NE 'READ').ERCSECT                             03880002
*********************************************************************** 03890002
*      The IOCOM has a prefix area which is not mapped by the         * 03900002
*      DSECT.  The format is:                                         * 03910002
*         Acronym of 'IOCOM   ' at IOCOM-16                           * 03920002
*         Length of the read only section                             * 03930002
*           of the IOCOM (halfword) at IOCOM-8.                       * 03940002
*********************************************************************** 03950002
         SPACE 1                                                        03960002
IOCIOCOM DC    CL8'IOCOM   '             IOCOM acronym                  03970002
IOCLENR  DC    AL2(IOCOEND-IOCOM)        Length of read only section    03980002
*                                        of the IOCOM                   03990002
         DC    XL6'0'                    Reserved                       04000002
         SPACE 2                                                        04010002
.IOC002  ANOP                                                           04020002
*********************************************************************** 04030002
*        Read only section of IOCOM                                   * 04040002
