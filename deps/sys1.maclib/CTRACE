.***AM.MLM2 SAVED LOCATION COUNTER                                      00434300
                      AIF  (&XPLISTVER GE 1).ZL42LA                     00434400
                      AIF  ('&ZENDBASEPL' NE '').ZL42LA                 00434500
&ZENDBASEPL           SETC 'YES'                                        00434600
&ZOPN                 SETA &ZOPN+1                                      00434700
&ZOPL(&ZOPN)          SETC '&XMFCTRL._PL_END'                           00434800
&ZOPO(&ZOPN)          SETC 'EQU'                                        00434900
&ZOPV(&ZOPN)          SETC '*'                                          00435000
&ZOPD(&ZOPN)          SETC '++ END OF BASE PLIST'                       00435100
.ZL42LA               ANOP  ,                                           00435200
