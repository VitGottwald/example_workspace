.*******************************************************************    00557000
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00557100
.*******************************************************************    00557200
.ZMFLX                ANOP  ,              MF=L PROCESSING              00557300
.*******************************************************************    00557400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00557500
.*******************************************************************    00557600
                      ACTR  10000                                       00557700
.*                                                                      00557800
                      LCLA  &ZLC           COLUMN POSITION              00557900
                      LCLA  &ZOPX          LIST TABLE INDEX             00558000
                      LCLC  &ZLL           LABEL                        00558100
                      LCLC  &ZLOP          OPERATION                    00558200
                      LCLC  &ZLV           VALUE                        00558300
                      LCLC  &ZLD           DESCRIPTION                  00558400
.*                                                                      00558500
&ZOPX                 SETA  1                                           00558600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00558700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00558800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00558900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00559000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00559100
                      AIF   (K'&ZLD LE 0).ZL5                           00559200
&ZLC                  SETA  35             COMMENT COLUMN               00559300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00559400
&ZLC                  SETA  33             COMMMENT COLUMN              00559500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00559600
&ZLV                  SETC  '&ZLV'.' '                                  00559700
                      AGO   .ZL3                                        00559800
.*                                                                      00559900
.ZL4                  ANOP  ,                                           00560000
&ZLV                  SETC  '&ZLV'.' '                                  00560100
.ZL5                  ANOP  ,                                           00560200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00560300
.*                                                                      00560400
&ZLL     &ZLOP &ZLV                                                     00560500
.*                                                                      00560600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00560700
                      AGO   .ZL2           CONTINUE LOOPING             00560800
.*                                                                      00560900
.ZLX                  ANOP  ,                                           00561000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00561100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00561200
.*====================================================================  00561300
.***PDAC.MOD                                   Check sysstate           00561400
                      AIF   ('&SYSASCE' EQ 'P').ZCKSYSS SYSTATE PRI     00561500
     MNOTE 8,'THE CTRACE MACRO MAY ONLY BE USED IN PRIMARY ASC MODE.'   00561600
.ZCKSYSS              ANOP  ,                                           00561700
.*====================================================================  00561800
.***PDAC.MOD "MINOPS_LEN"   place length of minops string in plist.     00561900
                      AIF   ('&MINOPS' EQ '').ZMINLEN                   00562000
                      AIF   ('&ZXMINOPS' EQ '').ZMINLEN                 00562100
                      AIF   ('&ZXMINOPS' EQ 'OUTPUT').ZMINLEN           00562200
         LA    14,L'&XMINOPS      ++ OBTAIN LENGTH OF MINOPS            00562300
         STH   14,44(,1)          ++ INPUT  _XMINOPS_LEN HL2            00562400
.ZMINLEN              ANOP  ,                                           00562500
.*====================================================================  00562600
.***PDAC.MOD "SUB_LEN"   place length of SUB name in plist.             00562700
                      AIF   ('&SUB' EQ '').ZSUBLEN                      00562800
                      AIF   ('&ZXSUB' EQ '').ZSUBLEN                    00562900
                      AIF   ('&ZXSUB' EQ 'OUTPUT').ZSUBLEN              00563000
         LA    14,L'&XSUB         ++ OBTAIN LENGTH OF SUBNAME           00563100
         STH   14,80(,1)          ++ INPUT  _XSUB_LEN                   00563200
.ZSUBLEN              ANOP  ,                                           00563300
.*====================================================================  00563400
.***PDAC.MOD                        link to ctrace service routine.     00563500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZLINK1  NOT GPR    00563600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00563700
         LA    1,36(,&ZCGPR)      ++ ADDRESS OF LINKS PARM              00563800
         LINK  EP=ITTCTSER,PARAM=((&ZCGPR)),MF=(E,(1))                  00563900
                      AGO   .ZLINKX                                     00564000
.ZLINK1               ANOP  ,     RX ADDRESS OF LIST FORM               00564100
         LINK  EP=ITTCTSER,PARAM=(&XMFCTRL),MF=(E,&XMFCTRL+36)          00564200
         AIF   ('&SSRC' NE '').ZLINK2  START/STOP RETURN CODE      @01A 00564300
         AIF   ('&SSRSNC' EQ '').ZLINK3 NO START/STOP REASON CODE  @01A 00564400
.ZLINK2  L     1,0(0,1)           ADDRESS OF CTRACE LIST FORM      @02C 00564500
.ZLINK3  ANOP  ,                  NO NEED TO RELOAD GPR1           @01P 00564600
.*====================================================================  00564700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00564800
.ZLINKX               ANOP  ,                                           00564900
.*******************************************************************    00565000
.***AP.X01  FORMAT GENERATED CODE                                       00565100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00565200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00565300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00565400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00565500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00565600
                      AIF   (K'&ZLD LE 0).ZL51                          00565700
&ZLC                  SETA  35             COMMENT COLUMN               00565800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00565900
&ZLC                  SETA  33             COMMENT COLUMN               00566000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00566100
&ZLV                  SETC  '&ZLV'.' '                                  00566200
                      AGO   .ZL31                                       00566300
.*                                                                      00566400
.ZL41                 ANOP  ,                                           00566500
&ZLV                  SETC  '&ZLV'.' '                                  00566600
.ZL51                 ANOP  ,                                           00566700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00566800
.*                                                                      00566900
&ZLL     &ZLOP &ZLV                                                     00567000
.*                                                                      00567100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00567200
                      AGO   .ZL21          CONTINUE LOOPING             00567300
.*                                                                      00567400
.ZLX1                 ANOP  ,                                           00567500
.*******************************************************************    00567600
.***AP.X02  END OF CTRACE MACRO                                         00567700
&ZCTRACE              SETC  'YES'          SET CTRACE USED              00567800
         POP   PRINT                                                    00567900
*                                            CTRACE-3                   00568000
         SPACE 1                                                        00568100
.ZLX2                 ANOP  ,                                           00568200
         MEND                                                           00568300
**********************************************************************/ 00568400
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00568500
*%CTRACE: MACRO KEYS(DEFINE,NAME,STARTNAM,DISPNAM,ASIDS,JOBS,MINOPS,    00568600
