.*******************************************************************    00394000
.***AE.01  START OF CTRACE MACRO CODE                                   00394100
.*******************************************************************    00394200
               LCLC  &M0M0001                 NAME                      00394300
&M0M0001       SETC  '&XLABEL'                SET NAME                  00394400
               AIF   (K'&XLABEL NE 0).XLAB2                             00394500
&M0M0001       SETC  'M00M&SYSNDX'            DEFAULT NAME              00394600
.*                                                                      00394700
.XLAB2         ANOP  ,                                                  00394800
               AIF   ('&XMF' EQ 'L').XLAB3                              00394900
&M0M0001 DS    0H                             CTRACE-3                  00395000
.XLAB3         ANOP  ,                                                  00395100
.*******************************************************************    00395200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00395300
.*******************************************************************    00395400
              PUSH  PRINT                                               00395500
               AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON         00395600
               PRINT OFF         ++ PR01                                00395700
               AGO   .ZPRNTXX                                           00395800
.ZPRINTX       PRINT ON                                                 00395900
.ZPRNTXX       ANOP  ,                                                  00396000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00396100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00396200
.********************************************************************   00396300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00396400
.********************************************************************   00396500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00396600
                      LCLA  &ZOPN1         INVOCATION INDEX             00396700
                      LCLC  &ZOPL(4095)    LABEL                        00396800
                      LCLC  &ZOPO(4095)    OPERATION                    00396900
                      LCLC  &ZOPV(4095)    VALUE                        00397000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00397100
.*                                                                      00397200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00397300
.******************************************************************     00397400
.***AL.L10 CTRACE FMT=LIST MF=S!L PARAMETERS                            00397500
.******************************************************************     00397600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00397700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00397800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00397900
&XMFATTR              SETC  '0D'                                        00398000
.*                                                                      00398100
&ZOPN                 SETA  &ZOPN+1                                     00398200
&ZOPL(&ZOPN)          SETC  ' '                                         00398300
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00398400
&ZOPO(&ZOPN)          SETC  'B'                                         00398500
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00398600
                      AGO   .ZALVL6B                                    00398700
.ZALVL6A              ANOP                                              00398800
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00398900
&ZOPV(&ZOPN)          SETC  '4,8'                                       00399000
&ZOPN                 SETA  &ZOPN+1                                     00399100
&ZOPL(&ZOPN)          SETC  ' '                                         00399200
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00399300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00399400
.ZALVL6B              ANOP                                              00399500
&ZOPD(&ZOPN)          SETC  '++ CTRACE ARG PARMS'                       00399600
.*                                                                      00399700
&ZOPN                 SETA  &ZOPN+1                                     00399800
&ZOPL(&ZOPN)          SETC  ' '                                         00399900
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00400000
&ZOPV(&ZOPN)          SETC  '1'                                         00400100
&ZOPD(&ZOPN)          SETC  ' '                                         00400200
.*                                                                      00400300
.ZLMFSL2              ANOP  ,                                           00400400
.*-------------------------------------------------------------------   00400500
.***AM.L00  CTRACE FMT=LIST name                                        00400600
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00400700
&ZOPN                 SETA &ZOPN+1                                      00400800
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00400900
&ZOPO(&ZOPN)          SETC 'EQU'                                        00401000
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00401100
&ZOPD(&ZOPN)          SETC '++ CTRACE NAME'                             00401200
.ZLMFSL2M             ANOP  ,                                           00401300
.*-------------------------------------------------------------------   00401400
.***AM.L01  CTRACE FMT=LIST PARAMETERS                                  00401500
&ZOPN                 SETA &ZOPN+1                                      00401600
&ZOPL(&ZOPN)          SETC '&XMFCTRL'                                   00401700
&ZOPO(&ZOPN)          SETC 'DS'                                         00401800
&ZOPV(&ZOPN)          SETC '&XMFATTR'                                   00401900
&ZOPD(&ZOPN)          SETC '++ CTRACE PARM LIST'                        00402000
.*-------------------------------------------------------------------   00402100
