                      AIF   ('&XRC' EQ 'REG15').ZMRETX                  00551400
                      AIF   ('&XRC' EQ 'R15').ZMRETX                    00551500
                      AIF   ('&XRC' EQ '15').ZMRETX                     00551600
&ZOPN                 SETA  &ZOPN+1                                     00551700
&ZOPL(&ZOPN)          SETC  ' '                                         00551800
&ZOPO(&ZOPN)          SETC  'ST'                                        00551900
&ZOPV(&ZOPN)          SETC  '15,&XRC'                                   00552000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00552100
                      AGO   .ZMRETX                                     00552200
.*                                                                      00552300
.ZMRET2               ANOP  ,                                           00552400
&ZCGPR                SETC  '&XRC'(2,K'&XRC-2)                          00552500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00552600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00552700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00552800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00552900
&ZOPN                 SETA  &ZOPN+1                                     00553000
&ZOPL(&ZOPN)          SETC  ' '                                         00553100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00553200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00553300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00553400
.ZMRETX               ANOP  ,                                           00553500
.*******************************************************************    00553600
.***AO.X03  REASON CODE PROCESSING                                      00553700
.*******************************************************************    00553800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00553900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00554000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00554100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00554200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00554300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00554400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00554500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00554600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00554700
&ZOPN                 SETA  &ZOPN+1                                     00554800
&ZOPL(&ZOPN)          SETC  ' '                                         00554900
