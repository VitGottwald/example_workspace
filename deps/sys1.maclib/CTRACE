&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00555200
                      AGO   .ZMRSNX                                     00555300
.*                                                                      00555400
.ZMRSN2               ANOP  ,                                           00555500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00555600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00555700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00555800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00555900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00556000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00556100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00556200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00556300
&ZOPN                 SETA  &ZOPN+1                                     00556400
&ZOPL(&ZOPN)          SETC  ' '                                         00556500
