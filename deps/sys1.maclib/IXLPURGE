.********************************************************************   00048000
.***AC.PLV01  PROCESS PLISTVER                                      *   00048100
.********************************************************************   00048200
&XPLISTVER      SETA  0                                                 00048300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00048400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00048500
&XPLISTVER      SETA  0                                                 00048600
.ZPLV0          ANOP  ,                                                 00048700
.*                                                                      00048800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00048900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00049000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00049100
.*-------------------------------------------------------------------   00049200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00049300
&ZMS            SETC  '"PLISTVER='                                      00049400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00049500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00049600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00049700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00049800
                AGO   .ZPVCK2                                           00049900
.ZPVCK3         ANOP  ,                                                 00050000
.*-------------------------------------------------------------------   00050100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00050200
&ZMS            SETC  '"PLISTVER='                                      00050300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00050400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00050500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00050600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00050700
                AGO   .ZPVCKE                                           00050800
.ZPVCK2         ANOP  ,                                                 00050900
.*-------------------------------------------------------------------   00051000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00051100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00051200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00051300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00051400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00051500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00051600
.ZPVCK6         ANOP  ,                                                 00051700
&ZMS            SETC  '"PLISTVER='                                      00051800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00051900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00052000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00052100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00052200
&XPLISTVER      SETA  0                                                 00052300
                AGO   .ZPVCKE                                           00052400
.*-------------------------------------------------------------------   00052500
.ZPVCK4A        ANOP  ,                                                 00052600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00052700
&ZMS            SETC  'PLISTVER='                                       00052800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00052900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00053000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00053100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00053200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00053300
                AGO   .ZPVCK5                                           00053400
.ZPVCK4         ANOP  ,                                                 00053500
&XPLISTVER      SETA  &PLISTVER(1)                                      00053600
.ZPVCK5         ANOP  ,                                                 00053700
.ZPVCKE         ANOP  ,                                                 00053800
