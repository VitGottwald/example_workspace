.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00042600
.********************************************************************   00042700
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00042800
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00042900
              GBLA  &SYSALVL    SYSSTATE ARCHLVL                        00043000
              GBLC  &SYSAM64    SYSSTATE AMODE64                        00043100
.*                                                                      00043200
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00043300
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00043400
              SPLEVEL  TEST                                             00043500
.ZSYS2        ANOP  ,                                                   00043600
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00043700
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00043800
              SYSSTATE TEST                                             00043900
              AGO   .ZSYS4                                              00044000
.ZSYS3        ANOP  ,                                                   00044100
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00044200
.ZSYS4        ANOP  ,                                                   00044300
              AIF   ('&SYSAM64' NE 'YES').ZSYSN64                       00044400
&ZMAC_LLGT    SETC 'LLGT'                                               00044500
&ZMAC_LGR     SETC 'LGR'                                                00044600
&ZMAC_LG      SETC 'LG'                                                 00044700
&ZMAC_STG     SETC 'STG'                                                00044800
.ZSYSN64      ANOP  ,                                                   00044900
