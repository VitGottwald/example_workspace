&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00159100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00159200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00159300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159400
                      AIF   (K'&ZLD LE 0).ZL51                          00159500
&ZLC                  SETA  35             COMMENT COLUMN               00159600
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00159700
&ZLC                  SETA  33             COMMENT COLUMN               00159800
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00159900
&ZLV                  SETC  '&ZLV'.' '                                  00160000
                      AGO   .ZL31                                       00160100
.*                                                                      00160200
.ZL41                 ANOP  ,                                           00160300
&ZLV                  SETC  '&ZLV'.' '                                  00160400
.ZL51                 ANOP  ,                                           00160500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00160600
.*                                                                      00160700
&ZLL     &ZLOP &ZLV                                                     00160800
.*                                                                      00160900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00161000
                      AGO   .ZL21          CONTINUE LOOPING             00161100
.*                                                                      00161200
.ZLX1                 ANOP  ,                                           00161300
.*******************************************************************    00161400
.***AP.X02  END OF IXLPURGE MACRO                                       00161500
&ZIXLPURGE            SETC  'YES'          SET IXLPURGE USED            00161600
         POP   PRINT                                                    00161700
*                                            IXLPURGE-0                 00161800
         SPACE 1                                                        00161900
.ZLX2                 ANOP  ,                                           00162000
         MEND                                                           00162100
**********************************************************************/ 00162200
