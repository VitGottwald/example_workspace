.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00113800
.********************************************************************   00113900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00114000
                      LCLA  &ZOPN1         INVOCATION INDEX             00114100
                      LCLC  &ZOPL(4095)    LABEL                        00114200
                      LCLC  &ZOPO(4095)    OPERATION                    00114300
                      LCLC  &ZOPV(4095)    VALUE                        00114400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00114500
.*                                                                      00114600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00114700
.******************************************************************     00114800
.***AL.L10 IXLPURGE FMT=LIST MF=S!L PARAMETERS                          00114900
.******************************************************************     00115000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00115100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00115200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00115300
&XMFATTR              SETC  '0D'                                        00115400
.*                                                                      00115500
&ZOPN                 SETA  &ZOPN+1                                     00115600
&ZOPL(&ZOPN)          SETC  ' '                                         00115700
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00115800
&ZOPO(&ZOPN)          SETC  'B'                                         00115900
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00116000
                      AGO   .ZALVL6B                                    00116100
.ZALVL6A              ANOP                                              00116200
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00116300
&ZOPV(&ZOPN)          SETC  '4,8'                                       00116400
&ZOPN                 SETA  &ZOPN+1                                     00116500
&ZOPL(&ZOPN)          SETC  ' '                                         00116600
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00116700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00116800
.ZALVL6B              ANOP                                              00116900
&ZOPD(&ZOPN)          SETC  '++ IXLPURGE ARG PARMS'                     00117000
.*                                                                      00117100
&ZOPN                 SETA  &ZOPN+1                                     00117200
&ZOPL(&ZOPN)          SETC  ' '                                         00117300
