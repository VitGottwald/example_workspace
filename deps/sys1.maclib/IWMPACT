.***AC.PLV01  PROCESS PLISTVER                                      *   00078000
.********************************************************************   00078100
&XPLISTVER      SETA  0                                                 00078200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00078300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00078400
&XPLISTVER      SETA  1                                                 00078500
.ZPLV0          ANOP  ,                                                 00078600
.*                                                                      00078700
                AIF   ('&CHECKHISTORY' EQ '').ZPLV1E                    00078800
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00078900
&XPLISTVER      SETA  1                                                 00079000
.ZPLV1E         ANOP  ,                                                 00079100
.*                                                                      00079200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00079300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00079400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00079500
.*-------------------------------------------------------------------   00079600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00079700
&ZMS            SETC  '"PLISTVER='                                      00079800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00079900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00080000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00080100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00080200
                AGO   .ZPVCK2                                           00080300
.ZPVCK3         ANOP  ,                                                 00080400
.*-------------------------------------------------------------------   00080500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00080600
&ZMS            SETC  '"PLISTVER='                                      00080700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00080800
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00080900
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00081000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00081100
                AGO   .ZPVCKE                                           00081200
.ZPVCK2         ANOP  ,                                                 00081300
.*-------------------------------------------------------------------   00081400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00081500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00081600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00081700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00081800
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00081900
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00082000
.ZPVCK6         ANOP  ,                                                 00082100
&ZMS            SETC  '"PLISTVER='                                      00082200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00082300
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00082400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00082500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00082600
&XPLISTVER      SETA  1                                                 00082700
                AGO   .ZPVCKE                                           00082800
.*-------------------------------------------------------------------   00082900
.ZPVCK4A        ANOP  ,                                                 00083000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00083100
&ZMS            SETC  'PLISTVER='                                       00083200
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00083300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00083400
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00083500
                MNOTE 8,' &ZMS IS REQUIRED.'                            00083600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00083700
                AGO   .ZPVCK5                                           00083800
.ZPVCK4         ANOP  ,                                                 00083900
&XPLISTVER      SETA  &PLISTVER(1)                                      00084000
.ZPVCK5         ANOP  ,                                                 00084100
.ZPVCKE         ANOP  ,                                                 00084200
.********************************************************************   00084300
.***AG.MF01 PROCESS MF MACRO FORMS                                      00084400
