&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00170100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00170200
.ZMRETX               ANOP  ,                                           00170300
.*******************************************************************    00170400
.***AO.X03  REASON CODE PROCESSING                                      00170500
.*******************************************************************    00170600
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00170700
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00170800
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00170900
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00171000
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00171100
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00171200
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00171300
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00171400
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00171500
&ZOPN                 SETA  &ZOPN+1                                     00171600
&ZOPL(&ZOPN)          SETC  ' '                                         00171700
&ZOPO(&ZOPN)          SETC  'ST'                                        00171800
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00171900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00172000
                      AGO   .ZMRSNX                                     00172100
.*                                                                      00172200
.ZMRSN2               ANOP  ,                                           00172300
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00172400
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00172500
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00172600
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00172700
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00172800
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00172900
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00173000
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00173100
&ZOPN                 SETA  &ZOPN+1                                     00173200
&ZOPL(&ZOPN)          SETC  ' '                                         00173300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00173400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00173500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00173600
.ZMRSNX               ANOP  ,                                           00173700
.*******************************************************************    00173800
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00173900
.*******************************************************************    00174000
.ZMFLX                ANOP  ,              MF=L PROCESSING              00174100
.*******************************************************************    00174200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00174300
.*******************************************************************    00174400
                      ACTR  10000                                       00174500
.*                                                                      00174600
                      LCLA  &ZLC           COLUMN POSITION              00174700
                      LCLA  &ZOPX          LIST TABLE INDEX             00174800
                      LCLC  &ZLL           LABEL                        00174900
                      LCLC  &ZLOP          OPERATION                    00175000
                      LCLC  &ZLV           VALUE                        00175100
                      LCLC  &ZLD           DESCRIPTION                  00175200
.*                                                                      00175300
&ZOPX                 SETA  1                                           00175400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00175500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00175600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00175700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00175800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00175900
                      AIF   (K'&ZLD LE 0).ZL5                           00176000
&ZLC                  SETA  35             COMMENT COLUMN               00176100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00176200
&ZLC                  SETA  33             COMMMENT COLUMN              00176300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00176400
&ZLV                  SETC  '&ZLV'.' '                                  00176500
                      AGO   .ZL3                                        00176600
.*                                                                      00176700
.ZL4                  ANOP  ,                                           00176800
&ZLV                  SETC  '&ZLV'.' '                                  00176900
.ZL5                  ANOP  ,                                           00177000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00177100
.*                                                                      00177200
&ZLL     &ZLOP &ZLV                                                     00177300
.*                                                                      00177400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00177500
                      AGO   .ZL2           CONTINUE LOOPING             00177600
.*                                                                      00177700
.ZLX                  ANOP  ,                                           00177800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00177900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00178000
         L     14,16(0,0)              GET CVT ADDRESS                  00178100
         TM    CVTOSLV1-CVT(14),X'10'      TEST FOR SP520 RELEASE       00178200
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00178300
         LA    0,IWMRSNCODENOPOLMGT    SET REASON CODE = NO WLM         00178400
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00178500
         B     END&SYSNDX              BRANCH TO END LABEL              00178600
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00178700
         L     14,772(14,0)            GET SFT ADDRESS                  00178800
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00178900
         LA    15,34                   GET EX FOR SERVICE ROUTINE       00179000
         OR    14,15                   GET PC NUMBER                    00179100
         PC    0(14)                                                    00179200
END&SYSNDX DS    0H                    END LABEL                        00179300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00179400
.ZLINKX               ANOP  ,                                           00179500
.*******************************************************************    00179600
.***AP.X01  FORMAT GENERATED CODE                                       00179700
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00179800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00179900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00180000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00180100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00180200
                      AIF   (K'&ZLD LE 0).ZL51                          00180300
&ZLC                  SETA  35             COMMENT COLUMN               00180400
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00180500
&ZLC                  SETA  33             COMMENT COLUMN               00180600
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00180700
&ZLV                  SETC  '&ZLV'.' '                                  00180800
                      AGO   .ZL31                                       00180900
.*                                                                      00181000
.ZL41                 ANOP  ,                                           00181100
&ZLV                  SETC  '&ZLV'.' '                                  00181200
.ZL51                 ANOP  ,                                           00181300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00181400
.*                                                                      00181500
&ZLL     &ZLOP &ZLV                                                     00181600
.*                                                                      00181700
