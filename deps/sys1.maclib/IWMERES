.***AC.PLV01  PROCESS PLISTVER                                      *   00080400
.********************************************************************   00080500
&XPLISTVER      SETA  0                                                 00080600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00080700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00080800
&XPLISTVER      SETA  0                                                 00080900
.ZPLV0          ANOP  ,                                                 00081000
.*                                                                      00081100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00081200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00081300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00081400
.*-------------------------------------------------------------------   00081500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00081600
&ZMS            SETC  '"PLISTVER='                                      00081700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00081800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00081900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00082000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00082100
                AGO   .ZPVCK2                                           00082200
.ZPVCK3         ANOP  ,                                                 00082300
.*-------------------------------------------------------------------   00082400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00082500
&ZMS            SETC  '"PLISTVER='                                      00082600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00082700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00082800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00082900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00083000
                AGO   .ZPVCKE                                           00083100
.ZPVCK2         ANOP  ,                                                 00083200
.*-------------------------------------------------------------------   00083300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00083400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00083500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00083600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00083700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00083800
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00083900
.ZPVCK6         ANOP  ,                                                 00084000
&ZMS            SETC  '"PLISTVER='                                      00084100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00084200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00084300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00084400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00084500
&XPLISTVER      SETA  0                                                 00084600
                AGO   .ZPVCKE                                           00084700
.*-------------------------------------------------------------------   00084800
.ZPVCK4A        ANOP  ,                                                 00084900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00085000
&ZMS            SETC  'PLISTVER='                                       00085100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00085200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00085300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00085400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00085500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00085600
                AGO   .ZPVCK5                                           00085700
.ZPVCK4         ANOP  ,                                                 00085800
&XPLISTVER      SETA  &PLISTVER(1)                                      00085900
.ZPVCK5         ANOP  ,                                                 00086000
.ZPVCKE         ANOP  ,                                                 00086100
.********************************************************************   00086200
.***AG.MF01 PROCESS MF MACRO FORMS                                      00086300
.********************************************************************   00086400
              ACTR  10000                                               00086500
.*------------------------------------------------------------------    00086600
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00086700
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00086800
.*-------------------------------------------------------------------   00086900
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00087000
.ZG30         ANOP  ,                                                   00087100
&ZTLC         SETC 'S'                                                  00087200
              AIF   ('&MF(1)' NE '&ZTLC').ZG31                          00087300
&XMF          SETC  'S'         SET KEYWORD                             00087400
&XMFSCK       SETC  'COMPLETE'  SET XMFSCK                              00087500
              AIF   (N'&MF LE 1).ZG30A          CK NUM                  00087600
&ZMS          SETC  '"MF='                                              00087700
&ZMS          SETC  '&ZMS.&MF"'                                         00087800
&ZMS          SETC  '&ZMS "MF"'                                         00087900
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00088000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00088100
.ZG30A        ANOP  ,                                                   00088200
.*-------------------------------------------------------------------   00088300
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00088400
.ZG31         ANOP  ,                                                   00088500
&ZTLC         SETC 'L'                                                  00088600
              AIF   ('&MF(1)' NE '&ZTLC').ZG38                          00088700
&XMF          SETC  'L'         SET KEYWORD                             00088800
              AIF   (N'&MF LE 3).ZG31A          CK NUM                  00088900
&ZMS          SETC  '"MF='                                              00089000
&ZMS          SETC  '&ZMS.&MF"'                                         00089100
&ZMS          SETC  '&ZMS "MF"'                                         00089200
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00089300
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00089400
.ZG31A        ANOP  ,                                                   00089500
.*-------------------------------------------------------------------   00089600
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00089700
              AIF   (N'&SYSLIST NE 0).ZG31B     POSITIONALS?            00089800
              AIF   ('&ETOKEN' NE '').ZG31B     KEYS?                   00089900
              AIF   ('&FUNCTION' NE '').ZG31B   KEYS?                   00090000
              AIF   ('&SRVCLASS' NE '').ZG31B   KEYS?                   00090100
              AIF   ('&USERID' NE '').ZG31B     KEYS?                   00090200
              AIF   ('&PRODUCT' NE '').ZG31B    KEYS?                   00090300
              AIF   ('&RETCODE' NE '').ZG31B    KEYS?                   00090400
              AIF   ('&RSNCODE' NE '').ZG31B    KEYS?                   00090500
              AGO    .ZG31C     INVOCATION OKAY                         00090600
.ZG31B        ANOP  ,                                                   00090700
&ZMS          SETC  'THE "L" FORM OF "MF"'                              00090800
&ZMS          SETC  '&ZMS CAN ONLY HAVE'                                00090900
&ZMS          SETC  '&ZMS THE "PLISTVER" KEY'                           00091000
              MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'              00091100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00091200
.ZG31C        ANOP  ,                                                   00091300
.*-------------------------------------------------------------------   00091400
.***AG.KXVAR06 NAME, OPT RQD INPUT, POS(2) -XVARIABLE                   00091500
&ZXMFCTRL     SETC  'INPUT'     SET IN/OUTPUT                           00091600
&XMFCTRL      SETC  '&MF(2,1)'  SET XVARIABLE                           00091700
              AIF   (N'&MF(2) GT 1).ZG33B       MULTI PARMS             00091800
&XMFCTRL      SETC  '&MF(2)'    SET XVARIABLE                           00091900
.ZG33B        AIF   (K'&MF(2) GT 0).ZG33C                               00092000
&ZXMFCTRL     SETC  ''          RESET IN/OUTPUT                         00092100
&ZMS          SETC  'WHEN "L" IS SPECIFIED'                             00092200
&ZMS          SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'                 00092300
              MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                        00092400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00092500
.ZG33C        ANOP  ,                                                   00092600
.*-------------------------------------------------------------------   00092700
.***AG.KCK01 NAME, OPT RQD INPUT, POS(2) -GPR NOT ALLOWED               00092800
