*   DECLARE                                                             06240000
*     1 SMCABASE BASED(CVTSMCA) BDY(DWORD),                             06250000
*      2 SMCAOPT     CHAR(1),            /* SMFDEFLT OPTIONS SELECTED   06260000
*                                           AT INITIALIZATION TIME.     06270000
*                                           THE OPTIONS APPLY TO        06280000
*                                           BACKGROUND PROCESSING.      06290000
*                                           SMCAFOPT (OFFSET 82)        06300000
*                                           CONTAINS THE FOREGROUND     06310000
*                                           OPTIONS.                 */ 06320000
*       3 SMCAOPT1 BIT(1),               /* Job accounting       @0CC*/ 06330000
*       3 SMCAOPT2 BIT(1),               /* Step accounting      @0CC*/ 06340000
*       3 SMCAEXT  BIT(1),               /* Exits will be taken  @0CC*/ 06350000
*       3 SMCADSA  BIT(1),               /* DATA SET ACCOUNTING      */ 06360000
*       3 SMCAVOL  BIT(1),               /* VOLUME ACCOUNTING        */ 06370000
*       3 SMCAUDCS  BIT(1),         /* Usage Data Collection Services   06380000
*                                                                @01A*/ 06390000
*         5 SMCARS01 BIT(1),             /* RESERVED             @01C*/ 06400000
*       3 SMCATDS  BIT(1),               /* TYPE 17 RECORDS             06410000
*                                           MAINTAINED FOR TEMPORARY    06420000
*                                           DATA SETS (REC(PERM) OR     06430000
*                                           REC(CALL))               */ 06440000
*       3 SMCAFGND BIT(1),               /* SMF FOREGROUND OPTIONS      06450000
*                                           BIT.  IF 0, ABOVE BITS      06460000
*                                           DESCRIBE BACKGROUND         06470000
*                                           OPTIONS.  IF 1, ABOVE       06480000
*                                           BITS DESCRIBE FOREGROUND    06490000
*                                           OPTIONS             20011*/ 06500000
*      2 SMCAMISC    CHAR(1),            /* MISCELLANEOUS INDICATORS */ 06510000
*       3 SMCAUSER BIT(1),               /* SMF RECORDING REQUESTED  */ 06520000
*       3 SMCAMAN  BIT(1),               /* SYS1.MAN DATA SET           06530000
*                                           IS/IS NOT PRESENT.          06540000
*                                           BITS 0 AND 1 MEAN:          06550000
*                                           00 = NO SMF RECORDING       06560000
*                                                REQUESTED (MAN=NONE)   06570000
*                                           01 = ONLY USER RECORDS TO   06580000
*                                                BE RECORDED (MAN=USER) 06590000
*                                           10 = INVALID COMBINATION    06600000
*                                           11 = SMF AND USER           06610000
*                                                RECORDING REQUESTED    06620000
*                                                (MAN=ALL)           */ 06630000
*       3 SMCADSIC BIT(1),               /* DATA SET INITIALIZATION     06640000
*                                           COMPLETE BY IEEMB829        06650000
*                                           (1 = COMPLETE)       @Q4C*/ 06660000
*       3 SMCAFIRT BIT(1),               /* SMF DATA SET TO BE          06670000
*                                           OPENED                   */ 06680000
*       3 SMCAPSDP BIT(1),               /* PSEUDO-DUMP SWITCH          06690000
*                                           (DEVICE SWITCHING ONLY)  */ 06700000
*       3 SMCADBSY BIT(1),               /* DUMP IS BUSY (SMF WRITER)*/ 06710000
*       3 SMCABSW  BIT(1),               /* BUFFER SWITCH.  IF 0,       06720000
*                                           LEFT HALF OF BUFFER IN      06730000
*                                           USE.  IF 1, RIGHT HALF OF   06740000
*                                           BUFFER IN USE.           */ 06750000
*       3 SMCADUMP BIT(1),               /* DUMP BUSY                */ 06760000
