.*************************************************************          18440042
.*   USING THE INNER MACRO ICHMM , THE PARAMETERS ARE SET INTO          18450042
.*   THE PARAMETER LIST AT THE CORRECT OFFSETS.                         18460042
.*************************************************************          18470042
&ICHCT   SETA  8                    INITIALIZE PARAMETER COUNTER   @N5A 18480042
.******************************************************************@N5A 18490042
.* FOR MF=E OR M TOKENXTR WITH XMREQ SPECIFIED, SET REQTYPE        @N5A 18500042
.******************************************************************@N5A 18510042
         AIF   ('&ICHMF' NE 'E' OR &ICHLIST EQ 1).RTE0268          @N5A 18520042
         AIF   (&RQTYPE NE 10 AND &RQTYPE NE 17).RTE0268           @N5A 18530042
         AIF   (T'&XMREQ EQ 'O').RTE0268                           @N5A 18540042
         LA    15,&RQTYPE           REQUEST TYPE EXTRACT CODE      @N5A 18550042
         STH   15,12(1,0)                                          @R6C 18560042
.RTE0268 ANOP                                                      @N5A 18570042
&ICHCT   SETA 12                    INITIALIZE PARAMETER COUNTER   @N5A 18580042
.******************************************************************@N5A 18590042
.* FOR MF=E OR M TOKENMAP WITH XMREQ SPECIFIED, SET REQTYPE        @N5A 18600042
.******************************************************************@N5A 18610042
         AIF   ('&ICHMF' NE 'E' OR &ICHLIST EQ 1).RTE0269          @N5A 18620042
         AIF   (&RQTYPE NE 8 AND &RQTYPE NE 16).RTE0269            @N5A 18630042
         AIF   (T'&XMREQ EQ 'O').RTE0269                           @N5A 18640042
         LA    15,&RQTYPE           REQUEST TYPE EXTRACT CODE      @N5A 18650042
         STH   15,12(1,0)                                          @R6C 18660042
.RTE0269 ANOP                                                      @N5A 18670042
&ICHCT   SETA  4                    INITIALIZE PARAMETER COUNTER        18680042
.******************************************************************@P8A 18690042
.* FOR MF=E OR M EXTRACT WITH BRANCH SPECIFIED, SET REQTYPE        @P8A 18700042
.******************************************************************@P8A 18710042
         AIF   ('&ICHMF' NE 'E' OR &ICHLIST EQ 1).RTE0270          @P8A 18720042
         AIF   (&RQTYPE NE 12 AND &RQTYPE NE 6).RTE0270            @P8A 18730042
         AIF   ('&TYPE' EQ 'ENVRXTR').RTE027                       @LHA 18740042
         AIF   (T'&BRANCH EQ 'O').RTE0270                          @P8A 18750042
.RTE027  ANOP                                                      @LHA 18760042
         LA    15,&RQTYPE           REQUEST TYPE EXTRACT CODE      @P8A 18770042
         STH   15,12(1,0)                                          @R6C 18780042
.RTE0270 ANOP                                                      @P8A 18790042
         ICHMM &REQSTOR             REQUESTOR ADDRESS                   18800042
         ICHMM &SUBSYS              SUBSYSTEM ADDRESS                   18810042
         ICHMM &WORKA               WORK AREA ADDRESS                   18820042
         AIF   (NOT &ICHLIST).CONT04                                    18830042
         DC    A(0)                 MESSAGE RETURN ADDRESS              18840042
         DC    A(0)                 RESVD                               18850042
         DC    A(&ICHL-IHB&NX.A)    OFFSET TO RACF PARAMETER LIST       18860042
         AIF   ('&RELEASE' EQ '').CONT04A                          @D4A 18870042
         AIF   ('&RELEASE'(1,3) EQ 'PLV').CONT04X                  @MHA 18880042
         AIF   ('&RELEASE'(1,3) LT '1.9').CONT04A                  @D4C 18890042
.CONT04X ANOP                                                      @MHA 18900042
         DC    F'0'                 SAF RETURN CODE                @N2C 18910042
         DC    F'0'                 SAF REASON CODE                @N2C 18920042
         DC    AL2(64)              EXTENSION LENGTH               @N2A 18930042
         DC    AL2(0)               RESVD                          @M2A 18940042
         DC    A(0)                 RETURN DATA ADDRESS            @N2A 18950042
         DC    A(0)                 FLAT PLIST ADDRESS             @N2A 18960042
.CONT04  ANOP                                                           18970042
&ICHCT   SETA  15                   INITIALIZE PARAMETER COUNTER   @D4C 18980042
         ICHMM &ECB1                ECB1 ADDRESS                   @N2A 18990042
         ICHMM &ECB2                ECB2 ADDRESS                   @N2A 19000042
         AIF   (NOT &ICHLIST).CONT04B                              @N2A 19010042
         DC    A(0)                 PREVIOUS FLAT PLIST ADDRESS    @N2A 19020042
         DC    A(0)                 NEXT FLAT PLIST ADDRESS        @N2A 19030042
         DC    A(0)                 ORIGINAL PLIST ADDRESS         @N2A 19040042
         DC    F'0'                 FLAT PLIST LENGTH              @N2A 19050042
.CONT04B ANOP                                                      @N2A 19060042
&ICHCT   SETA  21                   INITIALIZE PARAMETER COUNTER   @D4C 19070042
         ICHMM &USERWRD             USERWORD                       @N2A 19080042
         ICHMM &PREEXI              PRE-PROCESSING VM EXIT         @N2A 19090042
         ICHMM &POSTEXI             PRE-PROCESSING VM EXIT         @N2A 19100042
         AIF   (NOT &ICHLIST).CONT04A                              @D4C 19110042
         DC    A(0)                 ASYNCHRONOUS ECB ADDRESS       @N2A 19120042
         DC    A(0)                 USED IN VM ENVIRONMENT         @N2A 19130042
.CONT04A ANOP                                                      @M2A 19140042
.***************************************************************        19150042
.*  AT THIS POINT, THE LIST AND EXECUTE FORMS ARE COMPLETED.  IF        19160042
.*  THE LIST PASS OF THE STANDARD FORM HAS JUST BEEN COMPLETED,         19170042
.*  RETURN FOR AN EXECUTE PASS.                                         19180042
.***************************************************************        19190042
         AIF   ('&ICHMF' NE 'S').CONT05                                 19200042
         AIF   (NOT &ICHLIST).CONT05                                    19210042
