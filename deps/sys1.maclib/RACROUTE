         AIF   ('&MSGSUPP' NE 'YES').MSUP10                      @Q3A   23260042
         OI    14(14),B'00100000' MSGSUPP=YES SPECIFIED          @Q3A   23270042
         AGO   .MSUP20                                           @Q3A   23280042
.MSUP10  ANOP  ,                                                 @Q3A   23290042
         NI    14(14),B'11011111' MSGSUPP=NO SPECIFIED           @Q3A   23300042
.MSUP20  ANOP  ,                                                 @Q3A   23310042
.* MODIFY MSGSP FIELD AS REQUESTED                               @Q3A   23320042
.MSPL01  ANOP  ,                                                 @Q3A   23330042
         AIF   ('&MSGSP' EQ '').MSPL20                           @Q3A   23340042
.MSPL10  ANOP  ,                                                 @Q3A   23350042
         MVI   15(14),&SP         MSGSP  SPECIFIED               @Q3A   23360042
.MSPL20  ANOP  ,                                                 @Q3A   23370042
.MFEM10  ANOP  ,                          NOT MF=E OR MF=M       @Q3A   23380042
         AIF   ('&MF(1)' EQ 'M').EROUTE                            @M3A 23390042
         AIF   (NOT &ICHLFS).RTE0420                              4@R2D 23400042
         L     15,24(0,14)          WORKAREA  ADDRS                @M2A 23410042
         XC    0(64,15),0(15)       CLEAR WORKAREA                 @M2A 23420042
         MVI   3(15),64             STORE WORK AREA LENGTH         @M2A 23430042
         MVC   4(1,15),8(1)         MOVE ATTR                      @M2A 23440042
         LR    0,1                                                 @M2A 23450042
         L     1,40(1,0)            POINT TO CLASS                 @R6C 23460042
         LTR   1,1                  CLASS SPECIFIED                @M2A 23470042
         BZ    *+10                                                @M2A 23480042
         MVC   7(9,15),0(1)         MOVE CLASS TO WORKAREA         @M2A 23490042
         LR    1,0                                                 @M2A 23500042
         L     1,60(1,0)            POINT TO USERID                @R6C 23510042
         LTR   1,1                  TEST IF USERID SPECIFIED       @M2A 23520042
         BZ    *+10                                                @M2A 23530042
         MVC   16(8,15),0(1)        MOVE USERID TO WORKAREA        @M2A 23540042
         LR    1,0                                                 @M2A 23550042
         L     1,36(1,0)            POINT TO ENTITY ADDRESS        @R6C 23560042
         LTR   1,1                  TEST IF ENTITY SPECIFIED       @M2A 23570042
         BZ    *+10                                                @M2A 23580042
         MVC   24(39,15),0(1)       MOVE ENTITY NAME               @M2A 23590042
         LR    1,15                 POINT TO WORKAREA              @M2A 23600042
         LA    15,32                SET DIAGNOSE SUBFUNCTION CODE  @M2A 23610042
         LA    0,64                 SET BUFFER LENGTH              @M2A 23620042
         DC    XL1'83',XL1'1F',XL2'00A0' ISSUE DIAGNOSE X'A0'      @M2A 23630042
.*                                                                 @M2A 23640042
.* SAVE THE RETURN CODE IN THE RACROUTE PARAMETER LIST             @M2A 23650042
.*                                                                 @M2A 23660042
         LA    0,84                                                @M2A 23670042
         CLR   0,15                                                @M2A 23680042
         BNE   *+12                                                @M2A 23690042
         LA    15,642                                              @D4C 23700042
         B     IHB&NX.G                                            @M2A 23710042
         LA    0,255                CHECK FOR SEVER                @M2A 23720042
         CLR   0,15                                                @M2A 23730042
         BNE   *+8                                                 @M2A 23740042
         LA    15,12                SEVER RETURN CODE              @R3C 23750042
         B     IHB&NX.G                                           2@R3D 23760042
IHB&NX.G DS    0F                                                  @M2A 23770042
         ST    15,0(14,0)           SAVE RACF RETURN CODE          @R6C 23780042
         XR    0,0                                                 @M2A 23790042
         XC    4(4,14),4(14)                                       @M2A 23800042
         AGO   .EROUTE                                             @M2A 23810042
.RTE0420 ANOP                                                      @M2A 23820042
         LR    1,14                 RESTORE ADDRESSABILITY TO PARM LIST 23830042
         L     15,16(0,0)           ADDRESS OF CVT                      23840042
         ICM   15,B'1111',248(15)   ADDRESS OF SAF VECTOR TABLE         23850042
         BNZ   IHB&NX.D                                                 23860042
         LA    15,4                 SAF ROUTER NOT AVAILABLE IN SYSTEM  23870042
         B     IHB&NX.E                                                 23880042
IHB&NX.D DS    0H                                                       23890042
         ICM   15,B'1111',12(15)    PICK UP ADDRESS OF SAF ROUTER  @R4C 23900042
         BNZ   IHB&NX.H                                            @R4A 23910042
         LA    15,4                 SAF ROUTER NOT AVAILABLE       @R4A 23920042
         B     IHB&NX.E                                                 23930042
IHB&NX.H DS    0H                                                  @R4A 23940042
         BALR  14,15                BRANCH TO SAF ROUTER                23950042
IHB&NX.E DS    0H                                                       23960042
         AGO   .EROUTE                                                  23970042
