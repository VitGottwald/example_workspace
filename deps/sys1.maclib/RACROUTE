               &ACEE=,  RACHK/AUD/DEF/DIR/INIT/LIST/XTRT/TKSRV,FRAC@LOC*02710042
               &ACEEALET=,    FRACHECK,DIRAUTH                     @LOC*02720042
               &ACTINFO=,     RACINIT                                  *02730042
               &APPL=,   RACHECK,FRACHECK,RACLIST,RACINIT,SIGNON   @N6C*02740042
               &ATTR=,        RACHECK,FRACHECK                         *02750042
               &AUDIT=,       RACDEF                                   *02760042
               &AUTHCHKS=,    FRACHECK                             @LVA*02770042
               &IDTA=,        RACINIT                              @MHA*02780045
