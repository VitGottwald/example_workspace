.ZLMFSLX              ANOP  ,                                           00111600
.*-------------------------------------------------------------------   00111700
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00111800
&ZPLLEN               SETA 12                                           00111900
.*                                                                      00112000
.******************************************************************     00112100
.***AI.L01 IWMMDELE FMT=LIST FILL IN PARAMETER AREA                     00112200
.******************************************************************     00112300
                      ACTR  10000                                       00112400
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00112500
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00112600
&ZCVAL                SETC  'LAE'          XAX MODE                     00112700
.ZIMFXA               ANOP ,                                            00112800
.*                                                                      00112900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00113000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00113100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00113200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00113300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00113400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00113500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00113600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00113700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00113800
&ZOPN                 SETA  &ZOPN+1                                     00113900
&ZOPL(&ZOPN)          SETC  ' '                                         00114000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00114100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00114200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00114300
                      AGO   .ZIMFNX                                     00114400
.*                                                                      00114500
.ZIMFN2               ANOP ,                                            00114600
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00114700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00114800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00114900
&ZOPN                 SETA  &ZOPN+1                                     00115000
&ZOPL(&ZOPN)          SETC  ' '                                         00115100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00115200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00115300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00115400
                      AGO   .ZIMFNX                                     00115500
.ZIMFNX1              ANOP ,                                            00115600
&ZOPN                 SETA  &ZOPN+1                                     00115700
&ZOPL(&ZOPN)          SETC  ' '                                         00115800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00115900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00116000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00116100
.ZIMFNX               ANOP ,                                            00116200
.********************************************************************   00116300
.***AI.L02 IWMMDELE FMT=LIST INITIALIZE PARAMETER AREA                  00116400
