&ZOPN1                SETA &ZOPN                                        00120200
.*******************************************************************    00120300
.***  PROCESS IWMMDELE OUTPUT PARAMETERS                                00120400
.*******************************************************************    00120500
                      ACTR  10000                                       00120600
.*******************************************************************    00120700
.***AO.X02  RETURN CODE PROCESSING                                      00120800
.*******************************************************************    00120900
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00121000
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00121100
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00121200
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00121300
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00121400
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00121500
&ZOPN                 SETA  &ZOPN+1                                     00121600
&ZOPL(&ZOPN)          SETC  ' '                                         00121700
&ZOPO(&ZOPN)          SETC  'ST'                                        00121800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00121900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00122000
                      AGO   .ZMRETX                                     00122100
.*                                                                      00122200
.ZMRET2               ANOP  ,                                           00122300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00122400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00122500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00122600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00122700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00122800
&ZOPN                 SETA  &ZOPN+1                                     00122900
&ZOPL(&ZOPN)          SETC  ' '                                         00123000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00123100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00123200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00123300
.ZMRETX               ANOP  ,                                           00123400
.*******************************************************************    00123500
.***AO.X03  REASON CODE PROCESSING                                      00123600
.*******************************************************************    00123700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00123800
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00123900
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00124000
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00124100
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00124200
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00124300
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00124400
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00124500
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00124600
