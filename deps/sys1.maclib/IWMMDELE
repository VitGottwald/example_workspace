.***AC.PLV01  PROCESS PLISTVER                                      *   00055100
.********************************************************************   00055200
&XPLISTVER      SETA  0                                                 00055300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00055400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00055500
&XPLISTVER      SETA  0                                                 00055600
.ZPLV0          ANOP  ,                                                 00055700
.*                                                                      00055800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00055900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00056000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00056100
.*-------------------------------------------------------------------   00056200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00056300
&ZMS            SETC  '"PLISTVER='                                      00056400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00056500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00056600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00056700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056800
                AGO   .ZPVCK2                                           00056900
.ZPVCK3         ANOP  ,                                                 00057000
.*-------------------------------------------------------------------   00057100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00057200
&ZMS            SETC  '"PLISTVER='                                      00057300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00057500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00057600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00057700
                AGO   .ZPVCKE                                           00057800
.ZPVCK2         ANOP  ,                                                 00057900
.*-------------------------------------------------------------------   00058000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00058100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00058200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00058300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00058400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00058500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00058600
.ZPVCK6         ANOP  ,                                                 00058700
&ZMS            SETC  '"PLISTVER='                                      00058800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00058900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00059000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00059100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059200
&XPLISTVER      SETA  0                                                 00059300
                AGO   .ZPVCKE                                           00059400
.*-------------------------------------------------------------------   00059500
.ZPVCK4A        ANOP  ,                                                 00059600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00059700
&ZMS            SETC  'PLISTVER='                                       00059800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00059900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00060000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00060100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00060200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00060300
                AGO   .ZPVCK5                                           00060400
.ZPVCK4         ANOP  ,                                                 00060500
&XPLISTVER      SETA  &PLISTVER(1)                                      00060600
.ZPVCK5         ANOP  ,                                                 00060700
.ZPVCKE         ANOP  ,                                                 00060800
.********************************************************************   00060900
