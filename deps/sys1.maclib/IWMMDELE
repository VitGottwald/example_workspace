&ZMS          SETC  '"RETCODE='                                         00086600
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00086700
&ZMS          SETC  '&ZMS "RETCODE"'                                    00086800
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00086900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00087000
.ZG5A         ANOP  ,                                                   00087100
.*-------------------------------------------------------------------   00087200
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00087300
              AIF   (K'&RETCODE(1) LE 0).ZG5C   NO PARM                 00087400
&ZXRETCODE    SETC  'OUTPUT'    SET OUTPUT                              00087500
&XRETCODE     SETC  '&RETCODE(1)'               SET XVARIABLE           00087600
              AIF   (N'&RETCODE GT 1).ZG5C                              00087700
&XRETCODE     SETC  '&RETCODE'  SET XVARIABLE                           00087800
.ZG5C         ANOP  ,                                                   00087900
.*-------------------------------------------------------------------   00088000
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00088100
              AIF   (K'&ZXRETCODE LE 0).ZG5R    NOT I/O                 00088200
              AIF   ('&XRETCODE'(1,1) NE '(').ZG5R                      00088300
&ZCGPR        SETC  '&XRETCODE'(2,K'&XRETCODE-2)                        00088400
              AIF   ('&ZCGPR'(1,1) NE '0').ZG5P                         00088500
              AIF   (K'&ZCGPR GT 2).ZG5Q                                00088600
              AIF   ('&ZCGPR' EQ '0').ZG5Q                              00088700
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00088800
              AIF   (T'&ZCGPR NE 'N').ZG5Q      INVALID REG NAME        00088900
.ZG5P         ANOP  ,                                                   00089000
              AIF   (T'&ZCGPR NE 'N').ZG5R      NON NUMERIC             00089100
              AIF   ('&ZCGPR' EQ '15').ZG5R                             00089200
              AIF   ('&ZCGPR' LT '2').ZG5Q                              00089300
              AIF   ('&ZCGPR' LE '12').ZG5R                             00089400
.ZG5Q         ANOP  ,                                                   00089500
&ZMS          SETC  '"RETCODE='                                         00089600
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00089700
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00089800
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12) OR (15).'            00089900
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00090000
.ZG5R         ANOP  ,                                                   00090100
.*----------------------------------------------------------------***   00090200
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00090300
              AIF   (N'&RSNCODE LE 1).ZG8A      CK NUM                  00090400
&ZMS          SETC  '"RSNCODE='                                         00090500
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00090600
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00090700
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00090800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00090900
.ZG8A         ANOP  ,                                                   00091000
.*-------------------------------------------------------------------   00091100
