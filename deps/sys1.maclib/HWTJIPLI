 /*  SearchType Constants for HWTJSRCH                               */         
 /*                                                                  */         
 /********************************************************************/         
 DECLARE (                                                                      
  HWTJ_SEARCHTYPE_GLOBAL  VALUE(1),                                             
  HWTJ_SEARCHTYPE_OBJECT  VALUE(2),                            /*@L5C*/         
  HWTJ_SEARCHTYPE_SHALLOW VALUE(3)                             /*@L5A*/         
 ) FIXED BINARY(31);                                                            
                                                                                
 /********************************************************************/         
 /*                                                                  */         
 /*  Mapping of PARAMETERS                                           */         
 /*                                                                  */         
 /********************************************************************/         
 DEFINE ALIAS HWTJ_PARSERHANDLE_TYPE   CHAR(12);                                
 DEFINE ALIAS HWTJ_HANDLE_TYPE         FIXED BINARY(31);                        
 DEFINE ALIAS HWTJ_JTYPE_TYPE          FIXED BINARY(32) UNSIGNED;               
 DEFINE ALIAS HWTJ_VALDESCRIPTOR_TYPE  FIXED BINARY(32) UNSIGNED;               
 DEFINE ALIAS HWTJ_BOOLEANVALUE_TYPE   BIT(8);                                  
 DEFINE ALIAS HWTJ_FORCEOPTION_TYPE    FIXED BINARY(31);                        
 DEFINE ALIAS HWTJ_ENTRYVALUETYPE_TYPE FIXED BINARY(31);                        
 DEFINE ALIAS HWTJ_ENCODEOPTION_TYPE   FIXED BINARY(31);      /* @L3A*/         
 DEFINE ALIAS HWTJ_OPTSOPTION_TYPE     FIXED BINARY(31);      /* @L6A*/         
                                                                                
 DEFINE STRUCTURE 1 HWTJ_DIAGAREA_TYPE                                          
          ,3 ReasonCode   FIXED BINARY(31)                                      
          ,3 ReasonDesc   CHAR(128);                                            
                                                                                
 /********************************************************************/         
 /*                                                                  */         
 /*  Entry Points                                                    */         
 /*                                                                  */         
 /********************************************************************/         
 /* HWTJCREN -  Create JSON Entry                                               
 */                                                                             
 DECLARE                                                                        
   HWTJCREN ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjectHandle         */         
    ,PTR                         INONLY      /* EntryNameAddr        */         
    ,FIXED BINARY(31)            INONLY      /* EntryNameLen         */         
    ,FIXED BINARY(31)            INONLY      /* EntryValueType       */         
    ,PTR                         INONLY      /* EntryValueAddr       */         
    ,FIXED BINARY(31)            INONLY      /* EntryValueLen        */         
    ,FIXED BINARY(31)            OUTONLY     /* NewEntryValueHandle  */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea             */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJDEL - JSON Delete                                                       
 */                                                                             
 DECLARE                                                                        
   HWTJDEL ENTRY(                                                               
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjectHandle         */         
    ,FIXED BINARY(31)            INONLY      /* EntryValueHandle     */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea             */         
   EXTERNAL OPTIONS(ASM INTER);                              /* @L2A */         
                                                                                
 /* HWTJGAEN - JSON Get Array Entry                                             
 */                                                                             
 DECLARE                                                                        
   HWTJGAEN ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjectHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ArrayEntryIndex      */         
    ,FIXED BINARY(31)            OUTONLY     /* EntryValueHandle     */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJGBOV - JSON Get Boolean Value                                           
 */                                                                             
 DECLARE                                                                        
   HWTJGBOV ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* EntryValueHandle     */         
    ,BIT(8) ALIGNED              OUTONLY     /* BooleanValue         */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJGENC - JSON Get Encoding                                                
 */                                                                             
 DECLARE                                                                        
   HWTJGENC ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,TYPE HWTJ_ENCODEOPTION_TYPE OUTONLY     /* Encoding             */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea             */         
   EXTERNAL OPTIONS(ASM INTER);                              /* @L3A */         
                                                                                
 /* HWTJGJST - JSON Get JSON Type                                               
 */                                                                             
 DECLARE                                                                        
   HWTJGJST ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjorEntryValueHandle*/         
    ,TYPE HWTJ_JTYPE_TYPE        OUTONLY     /* JSONType             */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJGNUE - JSON Number of Entries                                           
 */                                                                             
 DECLARE                                                                        
   HWTJGNUE ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjectHandle         */         
    ,FIXED BINARY(31)            OUTONLY     /* NumOfEntries         */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJGNUV - JSON Get Number Value                                            
 */                                                                             
 DECLARE                                                                        
   HWTJGNUV ENTRY(                                                              
     FIXED BINARY(31)             OUTONLY    /* Returncode         */           
    ,CHAR(12)                     INONLY     /* ParserHandle       */           
    ,FIXED BINARY(31)             INONLY     /* EntryValueHandle   */           
    ,PTR                          INONLY     /* ValueBufferAddr    */           
    ,FIXED BINARY(31)             INONLY     /* ValueBufferLen     */           
    ,TYPE HWTJ_VALDESCRIPTOR_TYPE OUTONLY    /* ValueDescriptor    */           
    ,TYPE HWTJ_DIAGAREA_TYPE      OUTONLY)   /* DiagArea Structure */           
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJGOEN - JSON Get Object Entry                                            
 */                                                                             
 DECLARE                                                                        
   HWTJGOEN ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjectHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ObjectEntryIndex     */         
    ,PTR                         INONLY      /* EntryNameBufferAddr  */         
    ,FIXED BINARY(31)            INONLY      /* EntryNameBufferLen   */         
    ,FIXED BINARY(31)            OUTONLY     /* EntryValueHandle     */         
    ,FIXED BINARY(31)            OUTONLY     /* ActualNameLenReturned*/         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJGVAL - JSON Get Value                                                   
 */                                                                             
 DECLARE                                                                        
   HWTJGVAL ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* EntryValueHandle     */         
    ,PTR                         OUTONLY     /* ValueLocationAddr    */         
    ,FIXED BINARY(31)            OUTONLY     /* ValueLen             */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJINIT - JSON Initialization                                              
 */                                                                             
 DECLARE                                                                        
   HWTJINIT ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,FIXED BINARY(31)            INONLY      /* MaxParserWorkAreaSize*/         
    ,TYPE HWTJ_PARSERHANDLE_TYPE OUTONLY     /* ParserHandle         */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJOPTS - JSON Set Misc Options                                            
 */                                                                             
 DECLARE                                                                        
   HWTJOPTS ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,TYPE HWTJ_OPTSOPTION_TYPE   INONLY      /* Option               */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                              /* @L6A */         
                                                                                
 /* HWTJPARS - JPARS function                                                   
 */                                                                             
 DECLARE                                                                        
   HWTJPARS ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,PTR                         INONLY      /* JsonTextAddr         */         
    ,FIXED BINARY(31)            INONLY      /* JsonTextLen          */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJSENC - JSON Set Encoding                                                
 */                                                                             
 DECLARE                                                                        
   HWTJSENC ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,TYPE HWTJ_ENCODEOPTION_TYPE INONLY      /* Encoding             */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea             */         
   EXTERNAL OPTIONS(ASM INTER);                              /* @L3A */         
                                                                                
 /* HWTJSERI -  Build JSON Text (Serialize)                                     
 */                                                                             
 DECLARE                                                                        
   HWTJSERI ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,PTR                         INONLY      /* NewJSONTextBufferAddr*/         
    ,FIXED BINARY(31)            INONLY      /* NewJSONTextBufferLen */         
    ,FIXED BINARY(31)            OUTONLY     /* ActualJSONTextLen    */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea             */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJSRCH - JSON Search                                                      
 */                                                                             
 DECLARE                                                                        
   HWTJSRCH ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* SearchType           */         
    ,PTR                         INONLY      /* SearchStringAddr     */         
    ,FIXED BINARY(31)            INONLY      /* SearchStringLen      */         
    ,FIXED BINARY(31)            INONLY      /* ObjectHandle         */         
    ,FIXED BINARY(31)            INONLY      /* StartingHandle       */         
    ,FIXED BINARY(31)            OUTONLY     /* SearchResultHandle   */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
 /* HWTJTERM - JSON Termination                                                 
 */                                                                             
 DECLARE                                                                        
   HWTJTERM ENTRY(                                                              
     FIXED BINARY(31)            OUTONLY     /* Returncode           */         
    ,TYPE HWTJ_PARSERHANDLE_TYPE INONLY      /* ParserHandle         */         
    ,FIXED BINARY(31)            INONLY      /* ForceOption          */         
    ,TYPE HWTJ_DIAGAREA_TYPE     OUTONLY)    /* DiagArea Structure   */         
   EXTERNAL OPTIONS(ASM INTER);                                                 
                                                                                
