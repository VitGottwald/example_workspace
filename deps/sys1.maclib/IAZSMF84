*/********************************************************************/ 34020000
*      4 * ,                      /* SVC WAIT entry                  */ 34030000
*       5 S84_%%SNUM  FIXED(31),  /* SVC NUMBER                      */ 34040000
*       5 S84_%%SNAM  CHAR(8),    /* SVC NAME                        */ 34050000
*       5 S84_%%SADR  FIXED(31),  /* ADDRESS OF WHERE THE SVC WAIT      34060000
*                                    OCCURED                         */ 34070000
*       5 S84_%%SMNAM CHAR(8),    /* MODULE NAME                     */ 34080000
*       5 S84_%%SMOFS FIXED(31),  /* OFFSET INTO THE MODULE          */ 34090000
*       5 S84_%%SCNT  FIXED(31),  /* SVC COUNT                       */ 34100000
*/********************************************************************/ 34110000
*/*   THE FOLLOWING FIELDS ARE APPLICABLE ONLY TO LOAD WAIT          */ 34120000
*/********************************************************************/ 34130000
*      4 * ,                      /* LOAD WAIT entry                 */ 34140000
*       5 S84_%%LMNAM CHAR(8),    /* LOADING MODULE NAME             */ 34150000
*       5 S84_%%LCNT  FIXED(31),  /* LOAD COUNT                      */ 34160000
*       5 *(5)        FIXED(31);  /* RESERVED                        */ 34170000
* DCL S84_%%WTPS1 FIXED(31) CONSTANT(Offset(S84_%%PCNT)+                34180000
*                                    Length(S84_%%PCNT)+12);            34190000
* DCL S84_%%WTSS1 FIXED(31) CONSTANT(Offset(S84_%%SCNT)+                34200000
*                                    Length(S84_%%SCNT)+0);             34210000
* DCL S84_%%WTLS1 FIXED(31) CONSTANT(Offset(S84_%%LCNT)+                34220000
*                                    Length(S84_%%LCNT)+20);            34230000
*                                                                       34240000
*% IF IAZSMF84_SUBTYPE='ALL'    %THEN %GOTO SUBTYP3;                    34250000
*% IF IAZSMF84_SUBTYPE='ALLJ3'  %THEN %GOTO SUBTYP3;                    34260000
*% IF IAZSMF84_SUBTYPE='ALLJES' %THEN %GOTO SUBTYP3;                    34270000
*% IF IAZSMF84_SUBTYPE=''       %THEN %GOTO SUBTYP3;                    34280000
*% GOTO SUBDONE;                                                        34290000
*%SUBTYP3:;                                                             34300000
* @EJECT;                                                               34310000
*/********************************************************************/ 34320000
*/*                                                                  */ 34330000
*/*   RELOCATE BLOCK FOR SUBTYPE 3 - SPOOL DATA MANAGEMENT SECTION   */ 34340000
*/*                                                                  */ 34350000
*/*   THERE ARE 8 DATA SECTIONS:  1) GENERAL SDM DATA SECTION,       */ 34360000
*/*   2) SPOOL DATA DESCRIBTION SECTION, 3) SPOOL PARTITION DESCRIP- */ 34370000
*/*   TION SECTION, 4) SPOOL SPACE UTILIZATION SNAPSHOT SECTION,     */ 34380000
