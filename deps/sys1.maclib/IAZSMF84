*/*   BE MAPPED BY THE FOLLOWING ENTRY.                              */ 43160000
*/********************************************************************/ 43170000
* DCL 1 S84_%%JFSUM BASED,                                              43180000
*     3 S84_%%JSNAM CHAR(33),     /* JES3 FUNCTION NAME OR DSP NAME  */ 43190000
*     3 *           CHAR(3),      /* RESERVED FOR DEVELOPMENT        */ 43200000
*     3 S84_%%JSJSR FIXED(8),     /* RQJSTAT SCHEDULING BYPASS CODE  */ 43210000
*/*                          X'01' - INSUFFICIENT STORAGE            */ 43220000
*/*                          X'02' - TLIMIT EXCEEDED                 */ 43230000
*/*                          X'03' - I/O RATE INAPPROPRIATE          */ 43240000
*/*                          X'04' - NO MATCH ON RQMAINS             */ 43250000
*/*                          X'05' - CHOICE INAPPROPRIATE            */ 43260000
*/*                          X'06' - NO MATCH ON CONTROL PROG. TYPE  */ 43270000
*/*                          X'07' - JOB CLASS NOT ENABLED           */ 43280000
*/*                          X'08' - LSTOR UPDATE PENDING            */ 43290000
*/*                          X'09' - CLASS CAN'T RUN ON THIS MAIN    */ 43300000
*/*                          X'0A' - MIX INAPPROPRIATE (1)           */ 43310000
*/*                          X'0B' - FIT INAPPROPRIATE (1)           */ 43320000
*/*                          X'0C' - FIT INAPPROPRIATE (2)           */ 43330000
*/*                          X'0D' - MIX INAPPROPRIATE (2)           */ 43340000
*/*                          X'0E' - JOB IN HOLD OR ACTIVE           */ 43350000
*/*                          X'0F' - RQINDEX NOT IN SELECT           */ 43360000
*/*                          X'10' - JOB IS IN DJC HOLD              */ 43370000
*/*                          X'11' - MINPART NOT AVAILABLE           */ 43380000
*/*                          X'12' - GROUP DISABLED ON THIS MAIN     */ 43390000
*/*                          X'13' - MAIN IS OFFLINE                 */ 43400000
*/*                          X'14' - TDEPTH EXCEEDED                 */ 43410000
*/*      THE REMAINING GMS BYPASS CODES ARE SET BY JMF               */ 43420000
*/*                          X'7E' - NO GROUP INITIATORS STARTED     */ 43430000
*/*                          X'7F' - ALL GROUP INITIATORS IN USE     */ 43440000
*/*      STORAGE DECLARATIONS FOR THE 'MDS SCHEDULING BYPASS REASONS'*/ 43450000
*/*                          X'80' - RESOURCE UPDATE ONLY            */ 43460000
*/*                          X'81' - JOB HOLD STATUS                 */ 43470000
*/*                          X'82' - MAX REGION SIZE EXCEEDED        */ 43480000
*/*                          X'83' - RESTART JOB PASS ONLY           */ 43490000
*/*                          X'84' - ELIGIBLE MAIN NOT ONLINE OR     */ 43500000
*/*                                  IPLED                           */ 43510000
*/*                          X'85' - INCORRECT CONTROL PROGRAM TYPE  */ 43520000
*/*                          X'86' - GMS GROUP OR CLASS NOT ENABLED  */ 43530000
*/*                          X'87' - REQUIRED RESOURCE NOT AVAILABLE */ 43540000
*/*                          X'88' - DEVICE POOL FENCE NOT BUILT     */ 43550000
*/*                          X'89' - MAIN SETUP DEPTH EXCEEDED       */ 43560000
*/*                          X'8A' - JOB CLASS SETUP DEPTH EXCEEDED  */ 43570000
*/*                          X'8B' - FAILED PREALLOCATION SCAN       */ 43580000
*/*                          X'FF' - JOB SELECTED FOR A MAIN         */ 43590000
*     3 *           CHAR(3),      /* RESERVED FOR DEVELOPMENT        */ 43600000
*     3 S84_%%JSAV  FIXED(31),    /* AVERAGE FUNCTION TIME IN SECONDS*/ 43610000
*     3 S84_%%JSCNT FIXED(31),    /* ACTIVE JOB COUNT                */ 43620000
*     3 S84_%%JSMIN FIXED(31),    /* MINIMUM FUNCTION TIME IN SECONDS*/ 43630000
*     3 S84_%%JSMAX FIXED(31),    /* MAXIMUM FUNCTION TIME IN SECONDS*/ 43640000
*     3 S84_%%JSMNJ CHAR(8),      /* JOB ID FOR THE MINIMUM FUNCTION    43650000
