*                                                                       25660000
*%DCL IAZSMF84_SUBTYPE CHAR EXTERNAL; /* Parameter used to determine    25670000
*                                        which subtypes to generate. */ 25680000
*%IF IBM_CBGENC = 'YES' %THEN                                           25690000
*%DO;                                                                   25700000
*  %S84_    = 'SMF84_';                                                 25710000
*  %S84U_   = 'SMF84_';                                                 25720000
*  %SUBTYPE = 'ALLJES';                                                 25730000
*%END;                                                                  25740000
*%ELSE %DO;                                                             25750000
*  %S84_    = 'R84';                                                    25760000
*  %S84U_   = 'R84_';                                                   25770000
*%END;                                                                  25780000
*                                                                       25790000
*   DCL 1 SMF84HDR BASED(SMF84PTR) BDY(WORD),                           25800000
*        3 SMF84LEN FIXED(15),    /* RECORD LENGTH                   */ 25810000
*        3 SMF84SEG FIXED(15),    /* SEGMENT DESCRIPTOR              */ 25820000
*        3 SMF84FLG BIT(8),       /* SYSTEM INDICATOR                */ 25830000
*          5 *        BIT(1),     /*   Reserved                      */ 25840000
*          5 SMF84STI BIT(1),     /*   SUBTYPE INDICATOR INDICATES      25850000
*                                       WHICH SUBSYSTEM BEING USED   */ 25860000
*          5 *        BIT(3),     /*   Reserved                      */ 25870000
*          5 *        BIT(1),     /*   MVS/XA                        */ 25880000
*          5 *        BIT(1),     /*   VS2                           */ 25890000
*          5 *        BIT(1),     /*   VS1                           */ 25900000
*        3 SMF84RTY FIXED(8),     /* RECORD TYPE (84 OR X'54')       */ 25910000
*        3 SMF84TME FIXED(32)     /* TIME, IN HUNDREDS OF A SECOND   */ 25920000
*                   BDY(BYTE),    /* WHEN RECORD WAS WRITTEN            25930000
*                                    (SET BY SMF)                    */ 25940000
*        3 SMF84DTE FIXED(32)     /* DATE RECORD WAS WRITTEN, IN THE */ 25950000
*                   BDY(BYTE),    /* FORM 00YYDDDF (SET BY SMF)      */ 25960000
*        3 SMF84SID CHAR(4),      /* SYSTEM IDENTIFIER (SET BY SMF)  */ 25970000
*        3 SMF84SBS FIXED(16),    /* SUBSYSTEM IDENTIFICATION        */ 25980000
*        3 SMF84SGN FIXED(16),    /* SEGMENT NUMBER                  */ 25990000
*        3 SMF84FL1 BIT(8),       /* FLAG BYTE 1 FOR SMF TYPE 84     */ 26000000
*          5 SMF84LST BIT(1),     /*   LAST SMF SEGMENT INDICATOR    */ 26010000
*          5 *        BIT(7),     /*   Reserved                      */ 26020000
*        3 SMF84VER FIXED(8),     /* SMF84 VERSION NUMBER            */ 26030000
*        3 SMF84STY FIXED(16),    /* RECORD SUBTYPE                  */ 26040000
*        3 SMF84TRN FIXED(16),    /* NUMBER OF TRIPLETS IN RECORD    */ 26050000
*  @SPACE (1);                                                          26060000
*        3 SMF84PRS FIXED(31),    /* OFFSET TO PRODUCT SECTION       */ 26070000
*        3 SMF84PRL FIXED(16),    /* LENGTH OF PRODUCT SECTION       */ 26080000
*        3 SMF84PRN FIXED(16),    /* NUMBER OF PRODUCT SECTIONS      */ 26090000
*        3 SMF84GNS FIXED(31),    /* OFFSET TO GENERAL SECTION       */ 26100000
*        3 SMF84GNL FIXED(16),    /* LENGTH OF GENERAL SECTION       */ 26110000
*        3 SMF84GNN FIXED(16),    /* NUMBER OF GENERAL SECTIONS      */ 26120000
*        3 SMF84J1O FIXED(31),    /* OFFSET TO DATA SECTION          */ 26130000
*        3 SMF84J1L FIXED(16),    /* LENGTH OF DATA SECTION          */ 26140000
*        3 SMF84J1N FIXED(16);    /* NUMBER OF DATA SECTIONS         */ 26150000
*                                                                       26160000
* DCL SMF84SZ1 FIXED(31) CONSTANT(Length(SMF84HDR));  /*                26170000
*                                           Len of heading section   */ 26180000
*% IF IAZSMF84_SUBTYPE=''       %THEN %GOTO PROSEC;                     26190000
*% IF IAZSMF84_SUBTYPE='ALL'    %THEN %GOTO PROSEC;                     26200000
*% IF IAZSMF84_SUBTYPE='ALLJ2'  %THEN %GOTO PROSEC;                     26210000
*% IF IAZSMF84_SUBTYPE='ALLJ3'  %THEN %GOTO PROSEC;                     26220000
*% IF IAZSMF84_SUBTYPE='ALLJES' %THEN %GOTO PROSEC;                     26230000
*% IF IAZSMF84_SUBTYPE='0'      %THEN %GOTO PROSEC;                     26240000
*% IF IAZSMF84_SUBTYPE='1'      %THEN %GOTO PROSEC;                     26250000
*% IF IAZSMF84_SUBTYPE='2'      %THEN %GOTO PROSEC;                     26260000
*% IF IAZSMF84_SUBTYPE='3'      %THEN %GOTO PROSEC;                     26270000
*% IF IAZSMF84_SUBTYPE='4'      %THEN %GOTO PROSEC;                     26280000
*% IF IAZSMF84_SUBTYPE='5'      %THEN %GOTO PROSEC;                     26290000
*% IF IAZSMF84_SUBTYPE='6'      %THEN %GOTO PROSEC;                     26300000
*% IF IAZSMF84_SUBTYPE='7'      %THEN %GOTO PROSEC;                     26310000
*% IF IAZSMF84_SUBTYPE='8'      %THEN %GOTO PROSEC;                     26320000
*% IF IAZSMF84_SUBTYPE='9'      %THEN %GOTO PROSEC;                     26330000
*% IF IAZSMF84_SUBTYPE='10'     %THEN %GOTO PROSEC;                     26340000
*% IF IAZSMF84_SUBTYPE='21'     %THEN %GOTO PROSEC;                     26350000
* @SPACE (1);                                                           26360000
*% NOTE ("IAZSMF84 - Bad subtype value specifed - " !! SUBTYPE)         26370000
*       Msglevel(8);                                                    26380000
*%PROSEC:;                                                              26390000
* @SPACE (1);                                                           26400000
*/********************************************************************/ 26410000
*/*                                                                  */ 26420000
*/*           SMF 84 PRODUCT SECTION                                 */ 26430000
*/*                                                                  */ 26440000
*/********************************************************************/ 26450000
*                                                                       26460000
*   DCL 1 SMF84PRO BASED(Addr(SMF84HDR)+SMF84PRS),                      26470000
*     3 S84_%%MFVER FIXED(16),    /* JMF VERSION NUMBER              */ 26480000
*     3 S84_%%PRDNM CHAR(8),      /* PRODUCT NAME (I.E., SC1BA    )  */ 26490000
*     3 S84_%%INTST FIXED(32)     /* TOD JMF MEASUREMENT INTERVAL    */ 26500000
*                   BDY(BYTE),    /* STARTED IN THE FORM HHMMSSTF    */ 26510000
*     3 S84_%%SDATE FIXED(32)     /* DATE JMF MEASUREMENT INTERVAL   */ 26520000
*                   BDY(BYTE),    /* STARTED IN THE FORM 00YYDDDF    */ 26530000
*     3 S84_%%INTEN FIXED(32)     /* TOD JMF MEASUREMENT INTERVAL    */ 26540000
*                   BDY(BYTE),    /* ENDED IN THE FORM HHMMSSTF      */ 26550000
*     3 S84_%%EDATE FIXED(32)     /* DATE JMF MEASUREMENT INTERVAL   */ 26560000
*                   BDY(BYTE),    /* ENDED IN THE FORM 00YYDDDF      */ 26570000
*     3 S84_%%INTER FIXED(32)     /* JMF INTERVAL LENGTH IN SECONDS  */ 26580000
*                   BDY(BYTE),                                          26590000
*     3 S84_%%MFCYC FIXED(32)     /* JMF SAMPLING CYCLE LENGTH IN THE*/ 26600000
*                   BDY(BYTE),    /* FORM OF 00SSSTTF, F = SIGN,        26610000
*                                    (TAKEN FROM THE CYCLE OPTION)   */ 26620000
*     3 *           CHAR(2),      /* RESERVED FOR DEVELOPMENT        */ 26630000
*     3 S84_%%SAMPL FIXED(31),    /* NUMBER OF JMF SAMPLES           */ 26640000
*     3 S84_%%MFCMD CHAR(80),     /* *CALL,JMF COMMAND               */ 26650000
*     3 S84_%%MVSRL CHAR(8),      /* MVS SOFTWARE LEVEL              */ 26660000
*     3 S84_%%JESRL CHAR(8),      /* JES3 RELEASE LEVEL              */ 26670000
*     3 S84_%%CPUM  CHAR(4),      /* CPU MODEL NUMBER                */ 26680000
*     3 S84_%%RSTO  FIXED(32),    /* Real storage size at IPL in KB -   26690000
*                                      set to X'FFFFFFFF' if the value  26692000
*                                      exceeds 4T-1 - see R84RST4K      26694000
*                                                            @Z25LSMF*/ 26696000
*     3 S84_%%CPUNM CHAR(8),      /* CPU SERIAL NUMBER               */ 26700000
*     3 S84_%%CPUID CHAR(4),      /* JES3 CPU ID                     */ 26710000
*     3 S84_%%MPNAM CHAR(8),      /* MAIN PROCESSOR NAME             */ 26720000
*     3 S84_%%J3FLG BIT(8),       /* JES3 STATUS INDICATOR FLAG      */ 26730000
*       5 S84_%%J3PRO BIT(1),     /*   JES3 PROCESSOR INDICATOR:        26740000
*                                        OFF MEANS JES3 GLOBAL          26750000
*                                        ON  MEANS JES3 LOCAL        */ 26760000
*       5 S84_%%J3APG BIT(1),     /*   JES3 IN THE APG PRIORITY LEVL */ 26770000
*       5 S84_%%J3SWP BIT(1),     /*   JES3 IS NON-SWAPPABLE         */ 26780000
*       5 *           BIT(5),     /*   Reserved                      */ 26790000
*     3 *           FIXED(8),     /* Reserved                        */ 26800000
*     3 S84_%%JPRTY FIXED(16),    /* JES3 DISPATCHING PRIORITY       */ 26810000
*     3 S84_%%JMFMN FIXED(31),    /* MIN. JMF OVERHEAD IN MICROSEC   */ 26820000
*     3 S84_%%JMFMX FIXED(31),    /* MAX. JMF OVERHEAD IN MICROSEC   */ 26830000
*     3 S84_%%JMFAV FIXED(31),    /* AVG. JMF OVERHEAD IN MICROSECONDS  26840000
*                                    % OF JMF INTERVAL TIME = JMF AVG   26850000
*                                    TIME * JMF SAMPLES / JMF INTERVAL  26860000
*                                    TIME IN MICROSECONDS * 100      */ 26870000
*     3 S84_%%MVSMN FIXED(31),    /* MIN. MVS OVERHEAD IN MICROSEC   */ 26880000
*     3 S84_%%MVSMX FIXED(31),    /* MAX. MVS OVERHEAD IN MICROSEC   */ 26890000
*     3 S84_%%MVSAV FIXED(31),    /* AVG. MVS OVERHEAD IN MICROSEC      26900000
*                                    % OF JMF INTERVAL TIME = MVS AVG   26910000
*                                    TIME * JMF SAMPLES / JMF INTERVAL  26920000
*                                    TIME IN MICROSECONDS * 100      */ 26930000
*     3 S84_%%RST4K FIXED(64);    /* Real storage size at IPL in 4K     26931000
*                                      pages                 @Z25LSMF*/ 26934000
* DCL S84_%%PSSZ1 FIXED(31) CONSTANT(Length(SMF84PRO));  /*             26940000
*                                           Len of product section   */ 26950000
* @SPACE (1);                                                           26960000
*/********************************************************************/ 26970000
