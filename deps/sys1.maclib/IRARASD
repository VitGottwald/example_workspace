         MACRO                                                          00011000
         IRARASD &DSECT=YES,&LIST=YES,&TITLE=YES,&RASD=YES              00012000
         GBLC  &IRARASD_INCLUDED                                        00013000
         GBLC  &ZCBPRINT                                                00014000
