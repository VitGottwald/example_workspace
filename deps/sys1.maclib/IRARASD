* %IRARASD_INCLUDED = 'YES';                                            00448000
* %DCL ZCBPRINT CHAR EXT;                                               00449000
* %DEACTIVATE ZCBPRINT;                                                 00450000
* %DCL IRARASD_LIST CHAR EXT;                                           00451000
* %DEACTIVATE IRARASD_LIST;                                             00452000
* %IF IRARASD_LIST = 'NO' !                                             00453000
*    ZCBPRINT = 'NO' %THEN                                              00454000
* %DO;                                                                  00455000
*   @LIST PUSH NOECHO;                                                  00456000
