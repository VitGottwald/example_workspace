         MACRO                                                          00008000
         IXLYAMDA &DSECT=YES,&LIST=YES,&TITLE=YES,&IXLYAMDAREA=YES,    *00009000
               &IXLYAMDHD=COND,&IXLYAMDCF=COND,&IXLYAMDCF1=COND,       *00010000
               &IXLYAMDSLL=COND,&IXLYAMDSLL1=COND,&IXLYAMDSLC=COND,    *00011000
               &IXLYAMDSLC1=COND,&IXLYAMDCFMI=COND,                    *00012000
               &IXLYAMDCFMINFO=COND,&IXLYAMDCFRF=COND,                 *00013000
