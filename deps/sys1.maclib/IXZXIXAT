         AIF   ('&XMFCTRL'(1,1) NE '(').IXAT100                         00213900
         LR    1,&XMFCTRL         Restore the parameter list address    00214000
         AGO   .IXAT150                                                 00214100
.IXAT100 ANOP                                                           00214200
         LA    1,&XMFCTRL         Restore the parameter list address    00214300
.IXAT150 ANOP                                                           00214400
