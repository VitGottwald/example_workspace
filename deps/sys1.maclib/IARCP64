         LG    1,&T                                                     00739400
         LG    14,72(0,0)              GET CVT ADDRESS             @L1A 00739500
         L     14,772(14,0)            GET SFT ADDRESS             @L1A 00739600
         L     14,208(14,0)            GET LX FOR SERVICE RTN      @L1A 00739700
         OILL  14,21                   Generate LX/EX                   00739800
         PC    0(14)                                               @L1A 00739900
.SAVERETRSN ANOP                                                        00740000
         AIF   ('&RSNCODE' EQ '').SAVENORSN                             00740100
         AIF   ('&RSNCODE'(1,1) NE '(').SAVERSN1A                       00740200
         LR    &RSNCODE(1),0                                            00740300
         AGO   .SAVERSN1B                                               00740400
.SAVERSN1A ANOP                                                         00740500
         ST    0,&RSNCODE                                               00740600
.SAVERSN1B ANOP                                                         00740700
.SAVENORSN ANOP                                                         00740800
         AIF   ('&RETCODE' EQ '').SAVENORET                             00740900
         AIF   ('&RETCODE'(1,1) NE '(').SAVERET1A                       00741000
         LR    &RETCODE(1),15                                           00741100
         AGO   .SAVERET1B                                               00741200
.SAVERET1A ANOP                                                         00741300
         ST    15,&RETCODE                                              00741400
.SAVERET1B ANOP                                                         00741500
.SAVENORET ANOP                                                         00741600
         POP   PRINT                                               @L1A 00741700
         MEXIT                                                     @L1A 00741800
.NOTDELETE ANOP                                                    @L1A 00741900
