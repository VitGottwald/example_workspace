         MACRO                                                          00188900
&XLABEL  IARCP64 &REQUEST=,&HEADER=,&CELLSIZE=,&OUTPUT_CPID=,          *00189000
               &SENSITIVE=,&COMMON=,&OWNINGTASK=,&MEMLIMIT=,&MOTKN=,   *00189100
               &LOCALSYSAREA=,&EXTENTSIZE=,&MULTIHDR=,&DUMP=,          *00189200
               &DUMPPRIO=,&OWNER=,&OWNINGASID=,&SADMP=,&FPROT=,&TYPE=, *00189300
               &CALLERKEY=,&KEY00TOF0=,&PRESERVE=,&TRAILER=,&FAILMODE=,*00189400
               &INPUT_CPID=,&CELLADDR=,&EXPAND=,&R0VALUE=,&TRACE=,     *00189500
               &REGS=,&CELLNAME=,&RETCODE=,&RSNCODE=,&PLISTVER=,&MF=    00189600
.*                                                                      00189700
*                  MACDATE -10/08/19-<0>                                00189800
.*                                                                      00189900
