                      AGO   .ZL2           CONTINUE LOOPING             00896400
.*                                                                      00896500
.ZLX                  ANOP  ,                                           00896600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00896700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00896800
