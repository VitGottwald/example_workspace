.***AI.L01 IARCP64 FMT=LIST FILL IN PARAMETER AREA                      00789000
.******************************************************************     00789100
                      ACTR  10000                                       00789200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00789300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00789400
&ZCVAL                SETC  'LAE'          XAX MODE                     00789500
.ZIMFXA               ANOP ,                                            00789600
.*                                                                      00789700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00789800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00789900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00790000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00790100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00790200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00790300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00790400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00790500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00790600
&ZOPN                 SETA  &ZOPN+1                                     00790700
&ZOPL(&ZOPN)          SETC  ' '                                         00790800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00790900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00791000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00791100
                      AGO   .ZIMFNX                                     00791200
.*                                                                      00791300
.ZIMFN2               ANOP ,                                            00791400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00791500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00791600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00791700
&ZOPN                 SETA  &ZOPN+1                                     00791800
&ZOPL(&ZOPN)          SETC  ' '                                         00791900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00792000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00792100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00792200
                      AGO   .ZIMFNX                                     00792300
.ZIMFNX1              ANOP ,                                            00792400
&ZOPN                 SETA  &ZOPN+1                                     00792500
&ZOPL(&ZOPN)          SETC  ' '                                         00792600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00792700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00792800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00792900
.ZIMFNX               ANOP ,                                            00793000
.********************************************************************   00793100
