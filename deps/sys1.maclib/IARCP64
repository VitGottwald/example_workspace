.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00195600
.********************************************************************   00195700
                  GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL             00195800
                  GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE      00195900
                  GBLA  &SYSALVL    SYSSTATE ARCHLVL                    00196000
                  GBLC  &SYSAM64    SYSSTATE AMODE64                    00196100
.*                                                                      00196200
                  AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED      00196300
                  AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED       00196400
                  SPLEVEL  TEST                                         00196500
.ZSYS2            ANOP  ,                                               00196600
                  AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1           00196700
                  AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1           00196800
                  SYSSTATE TEST                                         00196900
                  AGO   .ZSYS4                                          00197000
.ZSYS3            ANOP  ,                                               00197100
&SYSASCE          SETC  'P'         ASSUME SYSSTATE PRIMARY             00197200
.ZSYS4            ANOP  ,                                               00197300
                  AIF   ('&SYSAM64' NE 'YES').ZSYSN64                   00197400
&ZMAC_LLGT        SETC 'LLGT'                                           00197500
&ZMAC_LGR         SETC 'LGR'                                            00197600
&ZMAC_LG          SETC 'LG'                                             00197700
&ZMAC_STG         SETC 'STG'                                            00197800
.ZSYSN64          ANOP  ,                                               00197900
.*******************************************************************    00198000
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00198100
.*******************************************************************    00198200
                  LCLC  &XREQUEST   ++ VALUE                            00198300
                  LCLC  &XHEADER    ++ NAME                             00198400
                  LCLC  &XCELLSIZE  ++ NAME                             00198500
                  LCLC  &XOUTPUT_CPID           ++ NAME                 00198600
                  LCLC  &XSENSITIVE             ++ VALUE                00198700
                  LCLC  &XCOMMON    ++ VALUE                            00198800
                  LCLC  &XOWNINGTASK            ++ VALUE                00198900
                  LCLC  &XMEMLIMIT  ++ VALUE                            00199000
                  LCLC  &XMOTKN     ++ NAME                             00199100
                  LCLC  &XLOCALSYSAREA          ++ VALUE                00199200
                  LCLC  &XEXTENTSIZE            ++ VALUE                00199300
                  LCLC  &XMULTIHDR  ++ VALUE                            00199400
                  LCLC  &XDUMP      ++ VALUE                            00199500
                  LCLC  &XDUMPPRIO  ++ NAME                             00199600
                  LCLC  &XOWNER     ++ VALUE                            00199700
                  LCLC  &XOWNINGASID            ++ NAME                 00199800
                  LCLC  &XSADMP     ++ VALUE                            00199900
                  LCLC  &XFPROT     ++ VALUE                            00200000
                  LCLC  &XTYPE      ++ VALUE                            00200100
