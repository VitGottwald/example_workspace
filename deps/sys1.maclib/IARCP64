.***AO.X04  LABEL FOR MF=L PROCESSING                                   00892700
.*******************************************************************    00892800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00892900
.*******************************************************************    00893000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00893100
.*******************************************************************    00893200
                      ACTR  10000                                       00893300
.*                                                                      00893400
                      LCLA  &ZLC           COLUMN POSITION              00893500
                      LCLA  &ZOPX          LIST TABLE INDEX             00893600
                      LCLC  &ZLL           LABEL                        00893700
                      LCLC  &ZLOP          OPERATION                    00893800
                      LCLC  &ZLV           VALUE                        00893900
                      LCLC  &ZLD           DESCRIPTION                  00894000
.*                                                                      00894100
&ZOPX                 SETA  1                                           00894200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00894300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00894400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00894500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00894600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00894700
                      AIF   (K'&ZLD LE 0).ZL5                           00894800
&ZLC                  SETA  35             COMMENT COLUMN               00894900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00895000
&ZLC                  SETA  33             COMMMENT COLUMN              00895100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00895200
&ZLV                  SETC  '&ZLV'.' '                                  00895300
                      AGO   .ZL3                                        00895400
.*                                                                      00895500
.ZL4                  ANOP  ,                                           00895600
&ZLV                  SETC  '&ZLV'.' '                                  00895700
.ZL5                  ANOP  ,                                           00895800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00895900
.*                                                                      00896000
&ZLL     &ZLOP &ZLV                                                     00896100
.*                                                                      00896200
