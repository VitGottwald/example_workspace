.ZG8Q           ANOP  ,                                                 00084400
&ZMS            SETC  '"RTNCODE='               *KGPRA* MNOTE           00084500
&ZMS            SETC  '&ZMS.&RTNCODE"'                                  00084600
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00084700
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00084800
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00084900
.ZG8R           ANOP  ,                                                 00085000
.*----------------------------------------------------------------***   00085100
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00085200
                AIF   (N'&RSNCODE LE 1).ZG11A   CK NUM                  00085300
&ZMS            SETC  '"RSNCODE='               *KPOSA* MNOTE           00085400
&ZMS            SETC  '&ZMS.&RSNCODE"'                                  00085500
&ZMS            SETC  '&ZMS "RSNCODE"'                                  00085600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00085700
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00085800
.ZG11A          ANOP  ,                                                 00085900
.*-------------------------------------------------------------------   00086000
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00086100
                AIF   (K'&RSNCODE(1) LE 0).ZG11C  NO PARM               00086200
&ZXRSNCODE      SETC  'OUTPUT'    SET OUTPUT                            00086300
&XRSNCODE       SETC  '&RSNCODE(1)'             SET XVARIABLE           00086400
                AIF   (N'&RSNCODE GT 1).ZG11C                           00086500
&XRSNCODE       SETC  '&RSNCODE'  SET XVARIABLE                         00086600
.ZG11C          ANOP  ,                                                 00086700
.*-------------------------------------------------------------------   00086800
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00086900
                AIF   (K'&ZXRSNCODE LE 0).ZG11R  NOT I/O                00087000
                AIF   ('&XRSNCODE'(1,1) NE '(').ZG11R                   00087100
&ZCGPR          SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                      00087200
                AIF   ('&ZCGPR'(1,1) NE '0').ZG11P                      00087300
                AIF   (K'&ZCGPR GT 2).ZG11Q                             00087400
                AIF   ('&ZCGPR' EQ '0').ZG11Q                           00087500
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00087600
                AIF   (T'&ZCGPR NE 'N').ZG11Q   INVALID REG NAME        00087700
.ZG11P          ANOP  ,                                                 00087800
                AIF   (T'&ZCGPR NE 'N').ZG11R   NON NUMERIC             00087900
                AIF   ('&ZCGPR' LT '2').ZG11Q                           00088000
                AIF   ('&ZCGPR' LE '12').ZG11R                          00088100
.ZG11Q          ANOP  ,                                                 00088200
&ZMS            SETC  '"RSNCODE='               *KGPRA* MNOTE           00088300
&ZMS            SETC  '&ZMS.&RSNCODE"'                                  00088400
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00088500
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00088600
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00088700
.ZG11R          ANOP  ,                                                 00088800
.******************************************************************     00088900
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00089000
.******************************************************************     00089100
                ACTR  10000                                             00089200
.*----------------------------------------------------------------***   00089300
.***AH.P02 EXTRA POSITIONALS                                            00089400
                AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS              00089500
                AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL              00089600
                AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL          00089700
                AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL           00089800
.ZGLISTX1       ANOP  ,                                                 00089900
&ZCVAL          SETC  ''          ERROR FIELD                           00090000
&ZAWRK          SETA  0           POSITIONAL NUMBER                     00090100
.ZGLIST2        ANOP  ,                                                 00090200
&ZAWRK          SETA  &ZAWRK+1    NEXT POSITIONAL                       00090300
                AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE              00090400
&ZCVAL          SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                   00090500
                AGO   .ZGLIST2                                          00090600
.*                                                                      00090700
.ZGLIST3        ANOP  ,                                                 00090800
&ZCVAL          SETC '&ZCVAL'(2,K'&ZCVAL-1)     STRIP COMMA             00090900
&ZMS            SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE          00091000
&ZMS            SETC  '&ZMS "&ZCVAL"'                                   00091100
&ZMS            SETC  '&ZMS CANNOT BE USED.'                            00091200
&ZMS            SETC  '&ZMS NO POSITIONAL ARGUMENTS'                    00091300
                MNOTE 8,' &ZMS ARE ALLOWED.'                            00091400
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00091500
.ZGLISTX        ANOP  ,                                                 00091600
&ZASYSLNDX      SETA  0                                                 00091700
.*-------------------------------------------------------------------   00091800
.***AG.MF04 MF=(SLE)                                                    00091900
.ZGMFX          ANOP  ,           END OF FULL SYNTAX CHECK              00092000
.*******************************************************************    00092100
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00092200
.*******************************************************************    00092300
                AIF   (&ZMACRET NE 0).ZLX2      ERROR DETECTED          00092400
.*******************************************************************    00092500
.***AE.01  START OF IXZXIXCL MACRO CODE                                 00092600
.*******************************************************************    00092700
                LCLC  &M0M0001                 NAME                     00092800
&M0M0001        SETC  '&XLABEL'                SET NAME                 00092900
                AIF   (K'&XLABEL NE 0).XLAB2                            00093000
&M0M0001        SETC  'M00M&SYSNDX'            DEFAULT NAME             00093100
.*                                                                      00093200
.XLAB2          ANOP  ,                                                 00093300
                AIF   ('&XMF' EQ 'L').XLAB3                             00093400
&M0M0001 DS    0H                             IXZXIXCL-1                00093500
.XLAB3          ANOP  ,                                                 00093600
.*******************************************************************    00093700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00093800
.*******************************************************************    00093900
              PUSH  PRINT                                               00094000
                AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON        00094100
                PRINT OFF         ++ PR01                               00094200
                AGO   .ZPRNTXX                                          00094300
.ZPRINTX        PRINT ON                                                00094400
.ZPRNTXX        ANOP  ,                                                 00094500
.********************************************************************   00094600
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00094700
.********************************************************************   00094800
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00094900
                      LCLA  &ZOPN1         INVOCATION INDEX             00095000
                      LCLC  &ZOPL(4095)    LABEL                        00095100
                      LCLC  &ZOPO(4095)    OPERATION                    00095200
                      LCLC  &ZOPV(4095)    VALUE                        00095300
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00095400
.*                                                                      00095500
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00095600
.******************************************************************     00095700
.***AL.L10 IXZXIXCL FMT=LIST MF=S!L PARAMETERS                          00095800
.******************************************************************     00095900
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00096000
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00096100
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     00096200
&XMFATTR              SETC  '0D'                                        00096300
.*                                                                      00096400
&ZOPN                 SETA  &ZOPN+1                                     00096500
&ZOPL(&ZOPN)          SETC  ' '                                         00096600
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00096700
&ZOPO(&ZOPN)          SETC  'B'                                         00096800
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00096900
                      AGO   .ZALVL6B                                    00097000
.ZALVL6A              ANOP                                              00097100
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00097200
&ZOPV(&ZOPN)          SETC  '4,8'                                       00097300
&ZOPN                 SETA  &ZOPN+1                                     00097400
&ZOPL(&ZOPN)          SETC  ' '                                         00097500
