.***AC.PLV01  PROCESS PLISTVER                                      *   00045700
.********************************************************************   00045800
&XPLISTVER      SETA  1                                                 00045900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00046000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00046100
&XPLISTVER      SETA  1                                                 00046200
.ZPLV0          ANOP  ,                                                 00046300
.*                                                                      00046400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00046500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00046600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00046700
.*-------------------------------------------------------------------   00046800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00046900
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00047000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00047100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00047200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00047300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00047400
                AGO   .ZPVCK2                                           00047500
.ZPVCK3         ANOP  ,                                                 00047600
.*-------------------------------------------------------------------   00047700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00047800
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00047900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00048000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00048100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00048200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00048300
                AGO   .ZPVCKE                                           00048400
.ZPVCK2         ANOP  ,                                                 00048500
.*-------------------------------------------------------------------   00048600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00048700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00048800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00048900
