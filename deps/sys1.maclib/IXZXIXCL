.*******************************************************************    00120300
.***AO.X02  RETURN CODE PROCESSING                                      00120400
.*******************************************************************    00120500
                      AIF   (N'&RTNCODE LE 0).ZMRETX                    00120600
                      AIF   ('&XRTNCODE'(1,1) EQ '(').ZMRET2            00120700
                      AIF   ('&XRTNCODE' EQ 'GPR15').ZMRETX             00120800
                      AIF   ('&XRTNCODE' EQ 'REG15').ZMRETX             00120900
                      AIF   ('&XRTNCODE' EQ 'R15').ZMRETX               00121000
                      AIF   ('&XRTNCODE' EQ '15').ZMRETX                00121100
&ZOPN                 SETA  &ZOPN+1                                     00121200
&ZOPL(&ZOPN)          SETC  ' '                                         00121300
&ZOPO(&ZOPN)          SETC  'ST'                                        00121400
&ZOPV(&ZOPN)          SETC  '15,&XRTNCODE'                              00121500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00121600
                      AGO   .ZMRETX                                     00121700
.*                                                                      00121800
.ZMRET2               ANOP  ,                                           00121900
&ZCGPR                SETC  '&XRTNCODE'(2,K'&XRTNCODE-2)                00122000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00122100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00122200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00122300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00122400
&ZOPN                 SETA  &ZOPN+1                                     00122500
&ZOPL(&ZOPN)          SETC  ' '                                         00122600
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00122700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00122800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00122900
.ZMRETX               ANOP  ,                                           00123000
