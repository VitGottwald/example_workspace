                      LCLC  &ZLV           VALUE                        00127800
                      LCLC  &ZLD           DESCRIPTION                  00127900
.*                                                                      00128000
&ZOPX                 SETA  1                                           00128100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00128200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00128300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00128400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00128500
