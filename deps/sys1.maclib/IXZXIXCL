&ZOPD(&ZOPN)          SETC  '++ IXZXIXCL ARG PARMS'                     00097900
.*                                                                      00098000
&ZOPN                 SETA  &ZOPN+1                                     00098100
&ZOPL(&ZOPN)          SETC  ' '                                         00098200
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00098300
&ZOPV(&ZOPN)          SETC  '1'                                         00098400
&ZOPD(&ZOPN)          SETC  ' '                                         00098500
.*                                                                      00098600
.ZLMFSL2              ANOP  ,                                           00098700
.*-------------------------------------------------------------------   00098800
.***AM.L00  IXZXIXCL FMT=LIST name                                      00098900
