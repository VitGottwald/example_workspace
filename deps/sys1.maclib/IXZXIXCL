                      AGO   .ZIMFNX                                     00109000
.*                                                                      00109100
.ZIMFN2               ANOP ,                                            00109200
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00109300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00109400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00109500
