&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00109900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00110000
                      AGO   .ZIMFNX                                     00110100
.ZIMFNX1              ANOP ,                                            00110200
&ZOPN                 SETA  &ZOPN+1                                     00110300
&ZOPL(&ZOPN)          SETC  ' '                                         00110400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00110500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00110600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00110700
.ZIMFNX               ANOP ,                                            00110800
.********************************************************************   00110900
