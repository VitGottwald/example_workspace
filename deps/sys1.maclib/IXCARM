.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00346800
&ZPLLEN               SETA 128                                          00346900
.*                                                                      00347000
.******************************************************************     00347100
.***AI.L01 IXCARM FMT=LIST FILL IN PARAMETER AREA                       00347200
.******************************************************************     00347300
                      ACTR  10000                                       00347400
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00347500
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00347600
&ZCVAL                SETC  'LAE'          XAX MODE                     00347700
.ZIMFXA               ANOP ,                                            00347800
.*                                                                      00347900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00348000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00348100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00348200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00348300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00348400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00348500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00348600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00348700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00348800
&ZOPN                 SETA  &ZOPN+1                                     00348900
&ZOPL(&ZOPN)          SETC  ' '                                         00349000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00349100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00349200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00349300
                      AGO   .ZIMFNX                                     00349400
.*                                                                      00349500
.ZIMFN2               ANOP ,                                            00349600
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00349700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00349800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00349900
&ZOPN                 SETA  &ZOPN+1                                     00350000
&ZOPL(&ZOPN)          SETC  ' '                                         00350100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00350200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00350300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00350400
                      AGO   .ZIMFNX                                     00350500
.ZIMFNX1              ANOP ,                                            00350600
&ZOPN                 SETA  &ZOPN+1                                     00350700
&ZOPL(&ZOPN)          SETC  ' '                                         00350800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00350900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00351000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00351100
.ZIMFNX               ANOP ,                                            00351200
