.***  PROCESS IXCARM OUTPUT PARAMETERS                                  00401100
.*******************************************************************    00401200
                      ACTR  10000                                       00401300
.*******************************************************************    00401400
.***AO.X02  RETURN CODE PROCESSING                                      00401500
.*******************************************************************    00401600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00401700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00401800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00401900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00402000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00402100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00402200
&ZOPN                 SETA  &ZOPN+1                                     00402300
&ZOPL(&ZOPN)          SETC  ' '                                         00402400
&ZOPO(&ZOPN)          SETC  'ST'                                        00402500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00402600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00402700
                      AGO   .ZMRETX                                     00402800
.*                                                                      00402900
.ZMRET2               ANOP  ,                                           00403000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00403100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00403200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00403300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00403400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00403500
&ZOPN                 SETA  &ZOPN+1                                     00403600
&ZOPL(&ZOPN)          SETC  ' '                                         00403700
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00403800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00403900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00404000
.ZMRETX               ANOP  ,                                           00404100
.*******************************************************************    00404200
.***AO.X03  REASON CODE PROCESSING                                      00404300
.*******************************************************************    00404400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00404500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00404600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00404700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00404800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00404900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00405000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00405100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00405200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00405300
&ZOPN                 SETA  &ZOPN+1                                     00405400
&ZOPL(&ZOPN)          SETC  ' '                                         00405500
&ZOPO(&ZOPN)          SETC  'ST'                                        00405600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00405700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00405800
                      AGO   .ZMRSNX                                     00405900
.*                                                                      00406000
.ZMRSN2               ANOP  ,                                           00406100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00406200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00406300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00406400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00406500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00406600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00406700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00406800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00406900
&ZOPN                 SETA  &ZOPN+1                                     00407000
&ZOPL(&ZOPN)          SETC  ' '                                         00407100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00407200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00407300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00407400
.ZMRSNX               ANOP  ,                                           00407500
.*******************************************************************    00407600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00407700
.*******************************************************************    00407800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00407900
.*******************************************************************    00408000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00408100
.*******************************************************************    00408200
                      ACTR  10000                                       00408300
.*                                                                      00408400
                      LCLA  &ZLC           COLUMN POSITION              00408500
                      LCLA  &ZOPX          LIST TABLE INDEX             00408600
                      LCLC  &ZLL           LABEL                        00408700
                      LCLC  &ZLOP          OPERATION                    00408800
                      LCLC  &ZLV           VALUE                        00408900
                      LCLC  &ZLD           DESCRIPTION                  00409000
.*                                                                      00409100
&ZOPX                 SETA  1                                           00409200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00409300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00409400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00409500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00409600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00409700
                      AIF   (K'&ZLD LE 0).ZL5                           00409800
&ZLC                  SETA  35             COMMENT COLUMN               00409900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00410000
&ZLC                  SETA  33             COMMMENT COLUMN              00410100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00410200
&ZLV                  SETC  '&ZLV'.' '                                  00410300
                      AGO   .ZL3                                        00410400
.*                                                                      00410500
.ZL4                  ANOP  ,                                           00410600
&ZLV                  SETC  '&ZLV'.' '                                  00410700
.ZL5                  ANOP  ,                                           00410800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00410900
.*                                                                      00411000
&ZLL     &ZLOP &ZLV                                                     00411100
.*                                                                      00411200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00411300
                      AGO   .ZL2           CONTINUE LOOPING             00411400
.*                                                                      00411500
.ZLX                  ANOP  ,                                           00411600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00411700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00411800
         L     14,16(0,0)              GET CVT ADDRESS INTO REG 14      00411900
         TM    116(14),B'00001000'     TEST IF RELEASE BITS ARE IN CVT  00412000
         BZ    ARM1&SYSNDX             IF NOT, EXIT MACRO               00412100
         TM    1265(14),B'00010000'    TEST RELEASE BIT IN CVT          00412200
         BNZ   ARM2&SYSNDX             SYSTEM HAS FUNCTION.  CONTINUE   00412300
ARM1&SYSNDX DS 0H                                                       00412400
         LA    15,12                   PUT RETURN CODE 12 IN REG 15     00412500
         LA    0,4                     PUT REASON CODE 4 IN REG 0       00412600
         B     ARM3&SYSNDX             EXIT MACRO                       00412700
ARM2&SYSNDX DS 0H                                                       00412800
         L     14,16(0,0)              GET CVT ADDRESS                  00412900
         L     14,772(14,0)            GET SFT ADDRESS                  00413000
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00413100
         LA    15,48                   GET EX FOR SERVICE ROUTINE       00413200
         OR    14,15                   GET PC NUMBER                    00413300
         LCLC  &IXCR0VAL0              local macro variable             00413400
&IXCR0VAL0 SETC '0'                    set value                        00413500
         LA    0,&IXCR0VAL0.(0,0)      indicate IXCARM call             00413600
         PC    0(14)                   INVOKE PCT ROUTINE               00413700
ARM3&SYSNDX DS 0H                                                       00413800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00413900
.ZLINKX               ANOP  ,                                           00414000
.*******************************************************************    00414100
.***AP.X01  FORMAT GENERATED CODE                                       00414200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00414300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00414400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00414500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00414600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00414700
                      AIF   (K'&ZLD LE 0).ZL51                          00414800
&ZLC                  SETA  35             COMMENT COLUMN               00414900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00415000
&ZLC                  SETA  33             COMMENT COLUMN               00415100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00415200
&ZLV                  SETC  '&ZLV'.' '                                  00415300
                      AGO   .ZL31                                       00415400
.*                                                                      00415500
.ZL41                 ANOP  ,                                           00415600
&ZLV                  SETC  '&ZLV'.' '                                  00415700
.ZL51                 ANOP  ,                                           00415800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00415900
.*                                                                      00416000
&ZLL     &ZLOP &ZLV                                                     00416100
.*                                                                      00416200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00416300
                      AGO   .ZL21          CONTINUE LOOPING             00416400
.*                                                                      00416500
.ZLX1                 ANOP  ,                                           00416600
