*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00417600
*%IXCARM: MACRO KEYS(REQUEST,ELEMENT,EVENTEXIT,EVENTEXITPL,EXITPLLEN,   00417700
*                 STARTTXT,STARTTXTLEN,ELEMTYPE,TERMTYPE,ELEMBIND,      00417800
*                 RMTOKEN,RESTARTTIMEOUT,ANSAREA,TELEMENT,RETCODE,      00417900
*                 RSNCODE,PLISTVER,MF);                                 00418000
*  ANS('?'!!MACLABEL!!' '!!'IXCARMP '!!MACLIST!!                        00418100
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00418200
*%END IXCARM;                                                           00418300
