.***AC.PLV01  PROCESS PLISTVER                                      *   00164500
.********************************************************************   00164600
&XPLISTVER      SETA  1                                                 00164700
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00164800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00164900
&XPLISTVER      SETA  1                                                 00165000
.ZPLV0          ANOP  ,                                                 00165100
.*                                                                      00165200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00165300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00165400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00165500
.*-------------------------------------------------------------------   00165600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00165700
&ZMS            SETC  '"PLISTVER='                                      00165800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00165900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00166000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00166100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00166200
                AGO   .ZPVCK2                                           00166300
.ZPVCK3         ANOP  ,                                                 00166400
.*-------------------------------------------------------------------   00166500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00166600
&ZMS            SETC  '"PLISTVER='                                      00166700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00166800
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00166900
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00167000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00167100
                AGO   .ZPVCKE                                           00167200
.ZPVCK2         ANOP  ,                                                 00167300
.*-------------------------------------------------------------------   00167400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00167500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00167600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00167700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00167800
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00167900
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00168000
.ZPVCK6         ANOP  ,                                                 00168100
&ZMS            SETC  '"PLISTVER='                                      00168200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00168300
&ZMS            SETC  '&ZMS THE RANGE IS "1-1"'                         00168400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00168500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00168600
&XPLISTVER      SETA  1                                                 00168700
                AGO   .ZPVCKE                                           00168800
.*-------------------------------------------------------------------   00168900
.ZPVCK4A        ANOP  ,                                                 00169000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00169100
&ZMS            SETC  'PLISTVER='                                       00169200
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00169300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00169400
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00169500
                MNOTE 8,' &ZMS IS REQUIRED.'                            00169600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00169700
                AGO   .ZPVCK5                                           00169800
.ZPVCK4         ANOP  ,                                                 00169900
&XPLISTVER      SETA  &PLISTVER(1)                                      00170000
.ZPVCK5         ANOP  ,                                                 00170100
.ZPVCKE         ANOP  ,                                                 00170200
