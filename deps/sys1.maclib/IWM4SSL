.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00124200
.********************************************************************   00124300
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00124400
                      LCLA  &ZOPN1         INVOCATION INDEX             00124500
                      LCLC  &ZOPL(4095)    LABEL                        00124600
                      LCLC  &ZOPO(4095)    OPERATION                    00124700
                      LCLC  &ZOPV(4095)    VALUE                        00124800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00124900
.*                                                                      00125000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00125100
.******************************************************************     00125200
.***AL.L10 IWM4SSL FMT=LIST MF=S!L PARAMETERS                           00125300
.******************************************************************     00125400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00125500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00125600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00125700
&XMFATTR              SETC  '0D'                                        00125800
.*                                                                      00125900
&ZOPN                 SETA  &ZOPN+1                                     00126000
&ZOPL(&ZOPN)          SETC  ' '                                         00126100
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00126200
&ZOPV(&ZOPN)          SETC  '4,8'                                       00126300
&ZOPN                 SETA  &ZOPN+1                                     00126400
&ZOPL(&ZOPN)          SETC  ' '                                         00126500
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00126600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00126700
&ZOPD(&ZOPN)          SETC  '++ IWM4SSL ARG PARMS'                      00126800
.*                                                                      00126900
&ZOPN                 SETA  &ZOPN+1                                     00127000
&ZOPL(&ZOPN)          SETC  ' '                                         00127100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00127200
&ZOPV(&ZOPN)          SETC  '1'                                         00127300
&ZOPD(&ZOPN)          SETC  ' '                                         00127400
.*                                                                      00127500
.ZLMFSL2              ANOP  ,                                           00127600
.*-------------------------------------------------------------------   00127700
.***AM.L00  IWM4SSL FMT=LIST name                                       00127800
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00127900
&ZOPN                 SETA &ZOPN+1                                      00128000
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00128100
