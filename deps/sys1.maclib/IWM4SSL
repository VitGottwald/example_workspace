.*                                                                      00140000
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00140100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00140200
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00140300
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00140400
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00140500
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00140600
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00140700
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00140800
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00140900
&ZOPN                 SETA  &ZOPN+1                                     00141000
&ZOPL(&ZOPN)          SETC  ' '                                         00141100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00141200
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00141300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00141400
                      AGO   .ZIMFNX                                     00141500
.*                                                                      00141600
.ZIMFN2               ANOP ,                                            00141700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00141800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00141900
&ZOPN                 SETA  &ZOPN+1                                     00142000
&ZOPL(&ZOPN)          SETC  ' '                                         00142100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00142200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00142300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00142400
                      AGO   .ZIMFNX                                     00142500
.ZIMFNX1              ANOP ,                                            00142600
&ZOPN                 SETA  &ZOPN+1                                     00142700
&ZOPL(&ZOPN)          SETC  ' '                                         00142800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00142900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00143000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00143100
