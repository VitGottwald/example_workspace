.***AG.KKCTL04 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00090300
.ZG40             ANOP  ,                                               00090400
&ZXMF             SETC  'INPUT'     SET IN/OUTPUT                       00090500
                  AIF   (K'&XMF GT 0).ZG40E     VALID ARG               00090600
                  AIF   (T'&MF EQ 'O').ZG40D    OMITTED                 00090700
&ZASYSLNDX        SETA  0                                               00090800
.ZG40A            ANOP  ,           POSITIONAL MACRO KEY                00090900
&ZASYSLNDX        SETA  &ZASYSLNDX+1                                    00091000
                  AIF   (&ZASYSLNDX GT N'&SYSLIST).ZG40C                00091100
&ZTLC             SETC 'MF'                                             00091200
                  AIF   ('&SYSLIST(&ZASYSLNDX)' NE '&ZTLC').ZG40A       00091300
.ZG40B            ANOP  ,                                               00091400
&ZXMF             SETC  ''          RESET IN/OUTPUT                     00091500
&ZMS              SETC  'THE "MF" KEY'                                  00091600
