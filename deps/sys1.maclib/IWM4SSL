&XMFCTRL          SETC  '&MF(2,1)'  SET XVARIABLE                       00085100
                  AIF   (N'&MF(2) GT 1).ZG34B   MULTI PARMS             00085200
&XMFCTRL          SETC  '&MF(2)'    SET XVARIABLE                       00085300
.ZG34B            AIF   (K'&MF(2) GT 0).ZG34C                           00085400
&ZXMFCTRL         SETC  ''          RESET IN/OUTPUT                     00085500
&ZMS              SETC  'WHEN "E" IS SPECIFIED'                         00085600
&ZMS              SETC  '&ZMS FOR THE "MF" KEY, POSITIONAL'             00085700
                  MNOTE 8,' &ZMS ARG 2 IS REQUIRED.'                    00085800
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00085900
.ZG34C            ANOP  ,                                               00086000
.*-------------------------------------------------------------------   00086100
.***AG.KCK02 NAME, OPT RQD INPUT, POS(2) -GPR OUT OF RANGE              00086200
                  AIF   (K'&ZXMFCTRL LE 0).ZG34R  NOT I/O               00086300
                  AIF   ('&XMFCTRL'(1,1) NE '(').ZG34R                  00086400
&ZCGPR            SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)                      00086500
                  AIF   ('&ZCGPR'(1,1) NE '0').ZG34P                    00086600
                  AIF   (K'&ZCGPR GT 2).ZG34Q                           00086700
                  AIF   ('&ZCGPR' EQ '0').ZG34Q                         00086800
&ZCGPR            SETC  '&ZCGPR'(2,1)                                   00086900
                  AIF   (T'&ZCGPR NE 'N').ZG34Q  INVALID REG NAME       00087000
.ZG34P            ANOP  ,                                               00087100
                  AIF   (T'&ZCGPR NE 'N').ZG34R  NON NUMERIC            00087200
                  AIF   ('&ZCGPR' LT '1').ZG34Q                         00087300
                  AIF   ('&ZCGPR' LE '12').ZG34R                        00087400
.ZG34Q            ANOP  ,                                               00087500
&ZMS              SETC  '"MF='                                          00087600
&ZMS              SETC  '&ZMS.&MF"'                                     00087700
&ZMS              SETC  '&ZMS ARGUMENT 2 MUST SPECIFY A REGISTER'       00087800
                  MNOTE 8,' &ZMS IN THE RANGE (1)-(12).'                00087900
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00088000
.ZG34R            ANOP  ,                                               00088100
.*-------------------------------------------------------------------   00088200
.***AG.KKWRD02 VALUE, OPT INPUT, ME KEYWORD -SINGLE KEYWORD             00088300
