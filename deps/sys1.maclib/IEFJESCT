*                               SWA blocks routine, used by DFP during          
*                               checkpoint processing                           
*                               - Assigned by IEFQBINT at master                
*                               scheduler base initialization    @L7C*/         
*   2 JESFSICB PTR(31),      /* Address of FSI trace Control Block              
*                                                                @T3A*/         
*   2 JESSJTCL PTR(31),      /* Address of the SWBTU processor                  
*                               control routine IEFSJTCL:                       
*                               - initialized by IEFSJLOD during                
*                                 master scheduler base initialization          
*                               - Normally referenced through the               
*                                 SWBTUREQ macro                 @L8C*/         
*   2 JESPPTUS FIXED(31),    /* PPT concurrent table use count                  
*                               - Normally referenced through the               
*                                 IEFPPSCN macro                 @LBC*/         
*   2 JESPPTSC PTR(31),      /* PPT scan routine IEFPPTSC:                      
*                               - initialized by IEFSJINT during                
*                                 master scheduler base initialization          
*                               - Normally referenced through the               
*                                 IEFPPSCN macro                 @LBC*/         
*   2 JESDSNNO FIXED(31),    /* Counter for final qualifier of                  
*                               temporary dataset name           @P3C*/         
*   2 JESDSNID CHAR(2),      /* ID for temporary data sets on this              
*                               system.                          @P6A*/         
*   2 JESRSVEA FIXED(15),    /* Reserved for future use          @P6C*/         
*   2 JESSSIVT FIXED(31),    /* Token for SSI vector table                      
*                               - Initialized before MSTR subsystem             
*                                 initialization                 @LDA*/         
*   2 JESSSIPC FIXED(32),    /* PC number for Dynamic SSI subsystem             
*                               services                                        
*                               - Initialized during master scheduler           
*                                 initialization                                
*                               - Accessed through IEFSSI macro  @LDA*/         
*   2 JESVTPC  FIXED(32),    /* PC number for IEFSSVT macro                     
*                               - Initialized during master scheduler           
*                                 initialization                                
*                               - Accessed through IEFSSVT macro @LDA*/         
*   2 JESMSGT@ PTR(31),      /* SSI message table address                       
*                               - Initialized before MSTR subsystem             
*                                 initialization                 @LDA*/         
*   2 JESPALF   BIT(32),     /* JESPEXT flags reflecting ALLOCxx                
*                               SYSTEM statement settings                       
*                               - Set by Allocation Initialization              
*                               Routine IEFAB4I0 and updated by                 
*                               SETALLOC command processing      @02A*/         
*    3 JESPALF1  BIT(8),     /* 1st byte of ALLOCxx SYSTEM flags @02A*/         
*     4 JESBR14L BIT(1),     /* On when SYSTEM IEFBR14_DELMIGDS                 
*                               is set to LEGACY                 @02A*/         
*     4 JESBR14N BIT(1),     /* On when SYSTEM IEFBR14_DELMIGDS                 
*                               is set to NORECALL               @02A*/         
*     4 JESTLIBE BIT(1),     /* On when SYSTEM TAPELIB_PREF is                  
*                               set to EQUAL                     @02A*/         
*     4 JESTLIBD BIT(1),     /* On when SYSTEM TAPELIB_PREF is                  
*                               set to BYDEVICES                 @02A*/         
*     4 JESVUFL  BIT(1),     /* On when SYSTEM VERIFY_UNCAT is                  
*                               set to FAIL                      @02A*/         
*     4 JESVUTRK BIT(1),     /* On when SYSTEM VERIFY_UNCAT is                  
*                               set to TRACK only                @02A*/         
*     4 JESVUMTR BIT(1),     /* On when SYSTEM VERIFY_UNCAT is                  
*                               set to MSGTRACK                  @02A*/         
*     4 JESTDSFU BIT(1),     /* On when SYSTEM TEMPDSFORMAT is                  
*                               set to UNIQUE                    @02A*/         
*    3 JESPALF2  BIT(8),     /* 2nd byte of ALLOCxx SYSTEM flags @02A*/         
*     4 JESTDSFI BIT(1),     /* On when SYSTEM TEMPDSFORMAT is                  
*                               set to INCLUDELABEL              @02A*/         
*     4 JESDSEME BIT(1),     /* On when SYSTEM MEMDSENQMGMT is                  
*                               set to ENABLE                    @02A*/         
*     4 JESDSEMD BIT(1),     /* On when SYSTEM MEMDSENQMGMT is                  
*                               set to DISABLE                   @02A*/         
*     4 JESVULTR BIT(1),     /* On when SYSTEM VERIFY_UNCAT is                  
*                               set to LOGTRACK                  @03A*/         
*     4 JESBCHRS BIT(1),     /* On when SYSTEM BATCH_RCLMIGDS set to            
*                               SERIAL                           @L9A*/         
*     4 JESBCHRP BIT(1),     /* On when SYSTEM BATCH_RCLMIGDS set to            
*                               PARALLEL                         @L9A*/         
*     4 JESOPEXP BIT(1),     /* On when SYSTEM OPTCDB_SPLIT is                  
*                               set to EXPLICIT                  @04A*/         
*     4 JESOPCAT BIT(1),     /* On when SYSTEM OPTCDB_SPLIT is                  
*                               set to CATALOG                   @04A*/         
*    3 JESPALF3  BIT(8),     /* 3rd byte of ALLOCxx SYSTEM flags @LJA*/         
*     4 JESSSSWA BIT(1),     /* On when SYSTEM SWBSTORAGE is set to             
*                               SWA                              @LJA*/         
*     4 JESSSATB BIT(1),     /* On when SYSTEM SWBSTORAGE is set to             
*                               ATB                              @LJA*/         
*     4 *        BIT(6),     /* Reserved for future use          @LJA*/         
*    3 JESPALF4  BIT(8),     /* 4th byte, reserved for future use               
*                               for ALLOCxx SYSTEM keywords      @02A*/         
*   2 JESJ201D FIXED(31),    /* DOM ID for IEFJ201A              @05C*/         
*   2 JESRSVED FIXED(31),    /* Reserved for future use          @LGA*/         
*%IF SYSVERSION = '' %THEN %GOTO JESCT$1;                   /*   @LGA*/         
*%IF SYSRULESMOD='PL/S-III ' %THEN %GOTO JESCT$1;           /*   @LGA*/         
*?ASAXMAC ISITPLX(1,2,0);                                   /*   @LGA*/         
*%DCL ASAXMAC_ISITPLX CHAR EXT;                             /*   @LGA*/         
*%IF ASAXMAC_ISITPLX ^= 'YES' %THEN %GOTO JESCT$1;          /*   @LGA*/         
*   2 JESSCH_MODULETABLE(4) ptr, /* Module table for Scheduler, each            
*                                   word is a pointer to a subcomponent         
*                                   module table that is obtained by            
*                                   IEFSCHAI.  See IEFSCHMT      @LGA*/         
*   2 JESSMF_LIMITS_Policy_Ptr PTR(64), /* LIMIT table policy address           
*                                                                @LIA*/         
*   2 JESSMF_LIMITS_Policy_Cnt Fixed(31), /* LIMIT table use count              
*                                                                @LIA*/         
*%GOTO JESCT$1C;                                                                
*%JESCT$1:;                                                                     
*   2 JES$1RSV(4) PTR(31),   /* For older compilers, defined over               
*                               JESSCH_MODULETABLE               @LGA*/         
*   2 JES$2RSV CHAR(8),      /* For older compilers, defined over               
*                               JESSMF_LIMITS_Policy_Ptr         @LIA*/         
*   2 JES$3RSV Fixed(31),    /* For older compilers, defined over               
*                               JESSMF_LIMITS_Policy_Cnt         @LIA*/         
*%JESCT$1C:;                                                                    
    2 JESJWTOM PTR(31),      /* Address of IEFJWTOM              @LKA*/         
    2 JESJACTL PTR(31),      /* Address of IEFJACTL              @LKA*/         
    2 JESJWTOG PTR(31),      /* Address of IEFJWTOM glue         @LKA*/         
*   2 JES$4RSV CHAR(16),     /* Reserved and available           @LKC*/         
*   2 JESPEND CHAR(0);       /* End of JESCT pageable extension  @LGA*/         
*                                                                               
*/********************************************************************/         
*/*                                                                  */         
*/*  The JESSCVER field has had the following values in the past.    */         
*/*  - The version of the JESCT is only updated when the JESCT is    */         
*/*    physically changed (e.g. increase in length)                  */         
*/*                                                                  */         
*/*  This field is not intended to be used to indicate the level     */         
*/*  of MVS that is installed although it can be used to obtain      */         
*/*  that information in some cases.                                 */         
*/*                                                                  */         
*/*  JESSCVER = 1 - SP132,133,135                                    */         
*/*  JESSCVER = 2 - SP211,212,213,214,216,217                        */         
*/*  JESSCVER = 3 - SP220                                            */         
*/*  JESSCVER = 4 - SP223,310                                        */         
*/*  JESSCVER = 5 - Not used                                         */         
*/*  JESSCVER = 6 - SP410                                        @L8A*/         
*/*  JESSCVER = 7 - SP430                                        @P3A*/         
*/*  JESSCVER = 8 - SP520                                        @LDA*/         
*/*                                                                  */         
*/********************************************************************/         
*DCL JESSCVER FIXED(8) CONSTANT(8);    /* Current version level  @LDC*/         
*DCL JESSCID  CHAR(7)  CONSTANT('JESPEXT'); /* Identifier        @H1A*/         
*%IF JESCTEP = 'NO' %THEN %GOTO JESCTEND; /*                     @L7A*/         
*/********************************************************************/         
*/*                                                                  */         
*/* Start of declares for PLS entry points.                          */         
*/*                                                                  */         
*/* This may be bypassed by coding the following:                    */         
*/*        %JESCTEP = 'NO' before 'Including' this macro             */         
*/*                                                                  */         
*/* This option allows the exclusion of these entry point declares   */         
*/* because they can cause problems for PLS-PLAS and 370-XA          */         
*/* conversions.                                                     */         
*/*                                                                  */         
*/* Note:                                                            */         
*/*      By bypassing these entry points many executable macros      */         
*/*      which require the IEFJESCT mapping macro will now require   */         
*/*      an additional declare for the Entry Point.  By coding       */         
*/*      the entry declares separately the programmer accepts        */         
*/*      the responsibility for changes to the interface.            */         
*/*                                                                  */         
*/* To skip the DECLARE to IEFBB410, code                            */         
*/*             %JESCTBB = 'NO'  before including the macro          */         
*/* This option allows the exclusion of IEFBB410 EP declares.    @LFA*/         
*/********************************************************************/         
*DECLARE SWAMGR   ENTRY BASED(JESQB556) VALRG(*) OPTIONS(AMODE(*)); /*          
*                            To call the LOCATE mode SWA Manager use            
*                            the SWAREQ macro.  It will issue a                 
*                            'CALL SWAMGR'                       @L5C*/         
*DECLARE SSREQ    ENTRY BASED(JESSSREQ)                                         
*                       VALRG(*) OPTIONS(AMODE(31),VLIST); /* To call           
*                             the subsystem use the IEFSSREQ macro.             
*                             It will issue a 'CALL SSREQ'       @LGC*/         
*DECLARE IEFW21SD ENTRY BASED(JESALLOC) OPTIONS(AMODE(*)) VALRG(*); /*          
*                             To enter allocation issue 'CALL IEFW21SD'         
*                                                                @L3C*/         
*%IF JESCTBB = 'NO' %THEN %GOTO JESCTBNO; /*JESCTBB is NO?       @LFA*/         
*DECLARE IEFBB410 ENTRY BASED(JESUNALC) OPTIONS(AMODE(*)) VALRG(*); /*          
*                             To enter unallocation issue                       
*                             'CALL IEFBB410'                    @L3C*/         
*%JESCTBNO: ;                 /* Label after BB410 EP Dcl        @LFA*/         
*DECLARE IEFAB4F5 ENTRY BASED(JESCATL)  OPTIONS(AMODE(*)) VALRG(*); /*          
*                             To Enter private catalog allocation               
*                             issue 'CALL IEFAB4F5'              @L3C*/         
*DECLARE SJF      ENTRY BASED(JESSJCNL) VALRG(*);                   /*          
*                             To call the Scheduler JCL Facility                
*                             use the SJFREQ macro.              @H1A*/         
*DECLARE SWBTU    ENTRY BASED(JESSJTCL) OPTIONS(AMODE(31,*))                    
*                                       VALRG(*); /* To call the                
*                             Scheduler JCL Facility SWB text unit              
*                             service (IEFSJTCL) use the SWBTUREQ               
*                             macro.                             @L9C*/         
*DECLARE MSWAMGR  ENTRY BASED(JESQMGR)  VALRG(*); /* TO CALL THE MOVE           
*                             To call the MOVE mode SWA Manager use             
*                             the IEFQMREQ macro.  It will issue a              
*                             'CALL MSWAMGR'                     @L5A*/         
*DECLARE GETDDTOK ENTRY BASED(JESGDTOK) OPTIONS(AMODE(31)) VALRG(*); /*         
*                             To call the allocation get DD token               
*                             service issue 'CALL GETDDTOK'      @D4A*/         
*DECLARE SWADIR   ENTRY BASED(JESPQDIR) OPTIONS(AMODE(31)) VALRG(*); /*         
*                             To call the SWA Manager diagnostics               
*                             routine issue 'CALL SWADIR'        @L5A*/         
*DECLARE USWAMGR  ENTRY BASED(JESQBSVA) OPTIONS(AMODE(31)) VALRG(*); /*         
*                             To call the unauthorized SWA Manager              
*                             use the SWAREQ macro. It will issue a             
*                             'CALL USWAMGR'                     @T2A*/         
*DECLARE SJFCNL   ENTRY BASED(JESSJF) OPTIONS(AMODE(31,*)) VALRG(*); /*         
*                             To call the Scheduler JCL Facility                
*                             control routine directly from                     
*                             SJFREQ                             @P2C*/         
*DECLARE PPTSCAN  ENTRY BASED(JESPPTSC)                                         
*                 OPTIONS(AMODE(31,*)) VALRG(*); /* To call the                 
*                             PPT scan routine IEFPPTSC via the                 
*                             IEFPPSCN macro                     @LBA*/         
*%JESCTEND: ;                                                                   
