*        %GOTO JESCTBSL;     /*                              @G385P2N           
&NAM     SETC  'JESCT'           DEFAULT                                        
         AIF   ('&TYPE' EQ 'DSECT').DSECT                                       
         AIF   ('&TYPE' EQ 'DC').DC                                             
         AGO   .DSECT                  DEFAULT                                  
.DC      ANOP                                                                   
&NAM     DS    0F                                                               
         AGO   .J0                                                              
.DSECT   ANOP                                                                   
&NAM     DSECT                                                                  
.J0      ANOP                                                                   
JESCTID  DC    C'JEST'             ACRONYM: JEST               @G383P2J         
JESUNITS DC    A(0)                POINTER TO SYSRES UCB       @G389P2N         
JESWAA   DC    A(0)                ADDRESS OF THE SWA            Y02668         
*                                  MANAGER - LOCATE MODE         Y02668         
JESQMGR  DC    A(0)                ADDRESS OF SWA MANAGER        Y02668         
*                                  MOVE MODE                     Y02668         
*                                                                               
JESRESQM DC    A(0)                ENTRY POINT USED TO INTERFACE                
