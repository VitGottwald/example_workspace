.***AC.PLV01  PROCESS PLISTVER                                      *   00121100
.********************************************************************   00121200
&XPLISTVER      SETA  0                                                 00121300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00121400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00121500
&XPLISTVER      SETA  0                                                 00121600
.ZPLV0          ANOP  ,                                                 00121700
.*                                                                      00121800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00121900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00122000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00122100
.*-------------------------------------------------------------------   00122200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00122300
&ZMS            SETC  '"PLISTVER='                                      00122400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00122500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00122600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00122700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00122800
                AGO   .ZPVCK2                                           00122900
.ZPVCK3         ANOP  ,                                                 00123000
.*-------------------------------------------------------------------   00123100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00123200
&ZMS            SETC  '"PLISTVER='                                      00123300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00123400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00123500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00123600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00123700
                AGO   .ZPVCKE                                           00123800
.ZPVCK2         ANOP  ,                                                 00123900
.*-------------------------------------------------------------------   00124000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00124100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00124200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00124300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00124400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00124500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00124600
.ZPVCK6         ANOP  ,                                                 00124700
&ZMS            SETC  '"PLISTVER='                                      00124800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00124900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00125000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00125100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00125200
&XPLISTVER      SETA  0                                                 00125300
                AGO   .ZPVCKE                                           00125400
.*-------------------------------------------------------------------   00125500
.ZPVCK4A        ANOP  ,                                                 00125600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00125700
&ZMS            SETC  'PLISTVER='                                       00125800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00125900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00126000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00126100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00126200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00126300
                AGO   .ZPVCK5                                           00126400
.ZPVCK4         ANOP  ,                                                 00126500
&XPLISTVER      SETA  &PLISTVER(1)                                      00126600
.ZPVCK5         ANOP  ,                                                 00126700
.ZPVCKE         ANOP  ,                                                 00126800
.********************************************************************   00126900
.***AG.MF01 PROCESS MF MACRO FORMS                                      00127000
