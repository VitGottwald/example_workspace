.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00386900
&ZPLLEN               SETA 106                                          00387000
.*                                                                      00387100
.******************************************************************     00387200
.***AI.L01 IEFOPZQ FMT=LIST FILL IN PARAMETER AREA                      00387300
.******************************************************************     00387400
                      ACTR  10000                                       00387500
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00387600
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00387700
&ZCVAL                SETC  'LAE'          XAX MODE                     00387800
.ZIMFXA               ANOP ,                                            00387900
.*                                                                      00388000
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00388100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00388200
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00388300
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00388400
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00388500
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00388600
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00388700
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00388800
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00388900
&ZOPN                 SETA  &ZOPN+1                                     00389000
&ZOPL(&ZOPN)          SETC  ' '                                         00389100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00389200
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00389300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00389400
                      AGO   .ZIMFNX                                     00389500
.*                                                                      00389600
.ZIMFN2               ANOP ,                                            00389700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00389800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00389900
&ZOPN                 SETA  &ZOPN+1                                     00390000
&ZOPL(&ZOPN)          SETC  ' '                                         00390100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00390200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00390300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00390400
                      AGO   .ZIMFNX                                     00390500
.ZIMFNX1              ANOP ,                                            00390600
&ZOPN                 SETA  &ZOPN+1                                     00390700
&ZOPL(&ZOPN)          SETC  ' '                                         00390800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00390900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00391000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00391100
.ZIMFNX               ANOP ,                                            00391200
.********************************************************************   00391300
.***AI.L02 IEFOPZQ FMT=LIST INITIALIZE PARAMETER AREA                   00391400
.********************************************************************   00391500
.***AI.L03 MF=(SLME)                                                    00391600
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00391700
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            00391800
.*                                                                      00391900
&ZOPN                 SETA  &ZOPN+1                                     00392000
&ZOPL(&ZOPN)          SETC  ' '                                         00392100
&ZOPO(&ZOPN)          SETC  'XC'                                        00392200
&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00392300
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00392400
.ZIMFLX               ANOP  ,                                           00392500
