.***AE.MF02 MF=(SLME)                                                   00355800
.ZEMFX            ANOP  ,           END OF PARTIAL SYNTAX CHECK         00355900
.*******************************************************************    00356000
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00356100
.*******************************************************************    00356200
                  AIF   (&ZMACRET NE 0).ZLX2    ERROR DETECTED          00356300
.*******************************************************************    00356400
.***AE.01  START OF IEFOPZQ MACRO CODE                                  00356500
.*******************************************************************    00356600
                  LCLC  &M0M0001                 NAME                   00356700
&M0M0001          SETC  '&XLABEL'                SET NAME               00356800
                  AIF   (K'&XLABEL NE 0).XLAB2                          00356900
&M0M0001          SETC  'M00M&SYSNDX'            DEFAULT NAME           00357000
.*                                                                      00357100
.XLAB2            ANOP  ,                                               00357200
                  AIF   ('&XMF' EQ 'L').XLAB3                           00357300
&M0M0001 DS    0H                             IEFOPZQ-0                 00357400
.XLAB3            ANOP  ,                                               00357500
.*******************************************************************    00357600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00357700
.*******************************************************************    00357800
              PUSH  PRINT                                               00357900
                  AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON      00358000
                  PRINT OFF         ++ PR01                             00358100
                  AGO   .ZPRNTXX                                        00358200
.ZPRINTX          PRINT ON                                              00358300
.ZPRNTXX          ANOP  ,                                               00358400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00358500
         AIF   ('&XMF' NE 'M').NOTM                                @L1A 00358600
&ZMS     SETC  'MF=M MAY NOT BE SPECIFIED'                         @L1A 00358700
         MNOTE 8,'&ZMS'                                            @L1A 00358800
         POP   PRINT                                               @L1A 00358900
         MEXIT                                                     @L1A 00359000
.NOTM ANOP                                                         @L1A 00359100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00359200
.********************************************************************   00359300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00359400
.********************************************************************   00359500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00359600
                      LCLA  &ZOPN1         INVOCATION INDEX             00359700
                      LCLC  &ZOPL(4095)    LABEL                        00359800
                      LCLC  &ZOPO(4095)    OPERATION                    00359900
                      LCLC  &ZOPV(4095)    VALUE                        00360000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00360100
.*                                                                      00360200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00360300
.******************************************************************     00360400
.***AL.L10 IEFOPZQ FMT=LIST MF=S!L PARAMETERS                           00360500
.******************************************************************     00360600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00360700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00360800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00360900
&XMFATTR              SETC  '0D'                                        00361000
.*                                                                      00361100
&ZOPN                 SETA  &ZOPN+1                                     00361200
&ZOPL(&ZOPN)          SETC  ' '                                         00361300
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00361400
&ZOPV(&ZOPN)          SETC  '4,8'                                       00361500
&ZOPN                 SETA  &ZOPN+1                                     00361600
&ZOPL(&ZOPN)          SETC  ' '                                         00361700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00361800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00361900
&ZOPD(&ZOPN)          SETC  '++ IEFOPZQ ARG PARMS'                      00362000
.*                                                                      00362100
&ZOPN                 SETA  &ZOPN+1                                     00362200
&ZOPL(&ZOPN)          SETC  ' '                                         00362300
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00362400
&ZOPV(&ZOPN)          SETC  '1'                                         00362500
&ZOPD(&ZOPN)          SETC  ' '                                         00362600
.*                                                                      00362700
.ZLMFSL2              ANOP  ,                                           00362800
.*-------------------------------------------------------------------   00362900
.***AM.L00  IEFOPZQ FMT=LIST name                                       00363000
