.ZG79A            ANOP  ,                                               00130200
.*-------------------------------------------------------------------   00130300
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00130400
                  AIF   (N'&SYSLIST NE 0).ZG79B  POSITIONALS?           00130500
                  AIF   ('&REQUEST' NE '').ZG79B  KEYS?                 00130600
                  AIF   ('&DSNAME' NE '').ZG79B  KEYS?                  00130700
                  AIF   ('&VOLUME' NE '').ZG79B  KEYS?                  00130800
                  AIF   ('&ARCH' NE '').ZG79B   KEYS?                   00130900
                  AIF   ('&OWNER' NE '').ZG79B  KEYS?                   00131000
                  AIF   ('&MEMBERS' NE '').ZG79B  KEYS?                 00131100
                  AIF   ('&STATE' NE '').ZG79B  KEYS?                   00131200
                  AIF   ('&STATUSINFO' NE '').ZG79B  KEYS?              00131300
                  AIF   ('&DDNAME' NE '').ZG79B  KEYS?                  00131400
                  AIF   ('&JOBNAME' NE '').ZG79B  KEYS?                 00131500
                  AIF   ('&ANSAREA' NE '').ZG79B  KEYS?                 00131600
                  AIF   ('&ANSLEN' NE '').ZG79B  KEYS?                  00131700
                  AIF   ('&RETCODE' NE '').ZG79B  KEYS?                 00131800
                  AIF   ('&RSNCODE' NE '').ZG79B  KEYS?                 00131900
                  AGO    .ZG79C     INVOCATION OKAY                     00132000
.ZG79B            ANOP  ,                                               00132100
&ZMS              SETC  'THE "L" FORM OF "MF"'                          00132200
&ZMS              SETC  '&ZMS CAN ONLY HAVE'                            00132300
&ZMS              SETC  '&ZMS THE "PLISTVER" KEY'                       00132400
                  MNOTE 8,' &ZMS SPECIFIED ON THE INVOCATION.'          00132500
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00132600
.ZG79C            ANOP  ,                                               00132700
.*-------------------------------------------------------------------   00132800
