.***AI.L05 XREQUEST BL1                                                 00392700
&ZW                   SETC  ''                                          00392800
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00392900
&ZMS                  SETC  ''                                          00393000
&ZC                   SETC  ''                                          00393100
.*-------------------------------------------------------------------   00393200
.***AI.L08 XREQUEST "BY_OLD"                                            00393300
.***      BL1'1XXXXXXX'                                                 00393400
         AIF   ('&XREQUEST' NE 'BY_OLD').ZI3A                           00393500
&ZW                   SETC  '&ZW.1'                                     00393600
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00393700
&ZMS                  SETC  '&ZMS.1'                                    00393800
                      AGO   .ZI3C                                       00393900
.*                                                                      00394000
.ZI3A                 ANOP  ,                                           00394100
&ZW                   SETC  '&ZW.0'                                     00394200
         AIF   ('&ZXREQUEST' EQ '').ZI3B                                00394300
&ZMS                  SETC  '&ZMS.0'                                    00394400
&ZC                   SETC  'YES'          SET BIT OFF FLAG             00394500
                      AGO   .ZI3C                                       00394600
.*                                                                      00394700
.ZI3B                 ANOP  ,                                           00394800
&ZMS                  SETC  '&ZMS.1'                                    00394900
.ZI3C                 ANOP  ,                                           00395000
.*-------------------------------------------------------------------   00395100
.***AI.L08 XREQUEST "BY_NEW"                                            00395200
.***      BL1'X1XXXXXX'                                                 00395300
         AIF   ('&XREQUEST' NE 'BY_NEW').ZI4A                           00395400
&ZW                   SETC  '&ZW.1'                                     00395500
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00395600
&ZMS                  SETC  '&ZMS.1'                                    00395700
                      AGO   .ZI4C                                       00395800
.*                                                                      00395900
.ZI4A                 ANOP  ,                                           00396000
&ZW                   SETC  '&ZW.0'                                     00396100
         AIF   ('&ZXREQUEST' EQ '').ZI4B                                00396200
&ZMS                  SETC  '&ZMS.0'                                    00396300
&ZC                   SETC  'YES'          SET BIT OFF FLAG             00396400
                      AGO   .ZI4C                                       00396500
.*                                                                      00396600
.ZI4B                 ANOP  ,                                           00396700
&ZMS                  SETC  '&ZMS.1'                                    00396800
.ZI4C                 ANOP  ,                                           00396900
.*-------------------------------------------------------------------   00397000
