.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00438200
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00438300
.*******************************************************************    00438400
.***  PROCESS IEFOPZQ OUTPUT PARAMETERS                                 00438500
.*******************************************************************    00438600
                      ACTR  10000                                       00438700
.*******************************************************************    00438800
.***AO.X02  RETURN CODE PROCESSING                                      00438900
.*******************************************************************    00439000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00439100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00439200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00439300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00439400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00439500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00439600
&ZOPN                 SETA  &ZOPN+1                                     00439700
&ZOPL(&ZOPN)          SETC  ' '                                         00439800
&ZOPO(&ZOPN)          SETC  'ST'                                        00439900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00440000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00440100
                      AGO   .ZMRETX                                     00440200
.*                                                                      00440300
.ZMRET2               ANOP  ,                                           00440400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00440500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00440600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00440700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00440800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00440900
&ZOPN                 SETA  &ZOPN+1                                     00441000
&ZOPL(&ZOPN)          SETC  ' '                                         00441100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00441200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00441300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00441400
.ZMRETX               ANOP  ,                                           00441500
.*******************************************************************    00441600
.***AO.X03  REASON CODE PROCESSING                                      00441700
.*******************************************************************    00441800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00441900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00442000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00442100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00442200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00442300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00442400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00442500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00442600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00442700
&ZOPN                 SETA  &ZOPN+1                                     00442800
&ZOPL(&ZOPN)          SETC  ' '                                         00442900
&ZOPO(&ZOPN)          SETC  'ST'                                        00443000
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00443100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00443200
                      AGO   .ZMRSNX                                     00443300
.*                                                                      00443400
.ZMRSN2               ANOP  ,                                           00443500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00443600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00443700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00443800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00443900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00444000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00444100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00444200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00444300
&ZOPN                 SETA  &ZOPN+1                                     00444400
&ZOPL(&ZOPN)          SETC  ' '                                         00444500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00444600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00444700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00444800
.ZMRSNX               ANOP  ,                                           00444900
.*******************************************************************    00445000
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00445100
.*******************************************************************    00445200
.ZMFLX                ANOP  ,              MF=L PROCESSING              00445300
.*******************************************************************    00445400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00445500
.*******************************************************************    00445600
                      ACTR  10000                                       00445700
.*                                                                      00445800
                      LCLA  &ZLC           COLUMN POSITION              00445900
                      LCLA  &ZOPX          LIST TABLE INDEX             00446000
                      LCLC  &ZLL           LABEL                        00446100
                      LCLC  &ZLOP          OPERATION                    00446200
                      LCLC  &ZLV           VALUE                        00446300
                      LCLC  &ZLD           DESCRIPTION                  00446400
.*                                                                      00446500
&ZOPX                 SETA  1                                           00446600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00446700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00446800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00446900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00447000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00447100
                      AIF   (K'&ZLD LE 0).ZL5                           00447200
&ZLC                  SETA  35             COMMENT COLUMN               00447300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00447400
&ZLC                  SETA  33             COMMMENT COLUMN              00447500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00447600
&ZLV                  SETC  '&ZLV'.' '                                  00447700
                      AGO   .ZL3                                        00447800
.*                                                                      00447900
.ZL4                  ANOP  ,                                           00448000
&ZLV                  SETC  '&ZLV'.' '                                  00448100
.ZL5                  ANOP  ,                                           00448200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00448300
.*                                                                      00448400
&ZLL     &ZLOP &ZLV                                                     00448500
.*                                                                      00448600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00448700
                      AGO   .ZL2           CONTINUE LOOPING             00448800
.*                                                                      00448900
.ZLX                  ANOP  ,                                           00449000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00449100
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00449200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00449300
         &ZMAC_LLGT 15,16(0,0)      Get CVT address                     00449400
         L     15,772(15,0)         Get SFT address                     00449500
         L     15,632(15,0)         Get PC number                       00449600
         PC    0(15)                PC to handle IEFOPZQ                00449700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00449800
.ZLINKX               ANOP  ,                                           00449900
.*******************************************************************    00450000
.***AP.X01  FORMAT GENERATED CODE                                       00450100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00450200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00450300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00450400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00450500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00450600
                      AIF   (K'&ZLD LE 0).ZL51                          00450700
&ZLC                  SETA  35             COMMENT COLUMN               00450800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00450900
&ZLC                  SETA  33             COMMENT COLUMN               00451000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00451100
&ZLV                  SETC  '&ZLV'.' '                                  00451200
                      AGO   .ZL31                                       00451300
.*                                                                      00451400
.ZL41                 ANOP  ,                                           00451500
&ZLV                  SETC  '&ZLV'.' '                                  00451600
.ZL51                 ANOP  ,                                           00451700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00451800
.*                                                                      00451900
&ZLL     &ZLOP &ZLV                                                     00452000
.*                                                                      00452100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00452200
                      AGO   .ZL21          CONTINUE LOOPING             00452300
.*                                                                      00452400
.ZLX1                 ANOP  ,                                           00452500
.*******************************************************************    00452600
.***AP.X02  END OF IEFOPZQ MACRO                                        00452700
&ZIEFOPZQ             SETC  'YES'          SET IEFOPZQ USED             00452800
         POP   PRINT                                                    00452900
*                                            IEFOPZQ-0                  00453000
         SPACE 1                                                        00453100
.ZLX2                 ANOP  ,                                           00453200
         MEND                                                           00453300
