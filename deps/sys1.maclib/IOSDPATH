*/* Start of PL/X Source                                             */ 00348000
* %IF PathAttributeConstants = 'ONLY' %THEN                             00349000
*   %Goto PAConstantsOnly1;                                             00350000
*DCL 1  PATH BASED,           /* Path information mapping            */ 00351000
*      3 Path#CHPIDs FIXED(31), /* Number of valid installed Channel    00352000
*                                  Paths to the specific device.     */ 00353000
*      3 PathFlags Char(1),   /* Flags mapped like MapFlgs in IOSDMAP   00354000
*                                                                    */ 00355000
*        5 PathValph Bit(1),  /* If on, path validation has not been    00356000
*                    done. Reflects setting of UCBVALPH.             */ 00357000
*      3 PathFlags1 Char(1),  /* More Flags                          */ 00358000
*        5 PathIntTypeNotAvailable Bit(1), /* If on, no interface type  00359000
*                    information was available.                      */ 00360000
*        5 PathUaValid Bit(1), /* If on, the PathUa field contains      00361000
*                                 the device unit address        @05A*/ 00362000
*        5 PathFC   BIT(1),   /* If on, the device is connected to      00363000
*                                at least one FICON channel (i.e.,      00364000
*                                channel type is FICON POINT TO         00365000
*                                POINT, FICON SWITCHED, or FICON        00366000
*                                INCOMPLETE)                     @09A*/ 00367000
*        5 PathAttributesValid BIT(1), /* If on, path selection         00368000
*                                attribute information is supported     00369000
*                                (PathAttribute is valid).       @0BA*/ 00370000
*        5 *        BIT(4),   /* Reserved                        @0BC*/ 00371000
*      3 PathLPUM BIT(8),       /* Last Path used mask (LPUM)        */ 00372000
*      3 PathUa   BIT(16),    /* Device unit address             @05A*/ 00373000
*      3 * CHAR(23),          /* Reserved                        @05C*/ 00374000
*      3 PathChpidArray(1:8),     /* Array of up to 8 entries of        00375000
*                    path information. The last entry filled in is      00376000
*                    the entry corresponding to Path#Chpids. Note       00377000
*                    that Path#Chpids could be 0, in which case no      00378000
*                    entries are filled in.                          */ 00379000
*        5 PathChpid Fixed(16),        /* Channel Path ID number     */ 00380000
*        5 PathMask BIT(8),            /* This mask corresponds to the  00381000
*                    bit setting in the PIM for this channel path.   */ 00382000
*        5 PathBits BIT(8),            /* Mapped like MapBit in         00383000
*                                         IOSDMAP                    */ 00384000
*          7 * Bit(1),                 /* Reserved               @P4C*/ 00385000
*          7 PATHDCMVSW Bit(1),     /* Dynamic Chpid Management mask.   00386000
*                                      If on,iindicates that path is    00387000
*                                      offline due to a Vary Switch or  00388000
*                                     Config member(xx) request. @P4A*/ 00389000
*          7 PathCMM Bit(1),           /* Configuration management      00390000
*                    mask. If on, indicates that path is offline due    00391000
*                    to ESCM.                                        */ 00392000
*          7 PathOPM Bit(1),           /* Operator path mask. If on,    00393000
*                    indicates that path is offline due                 00394000
*                    to the operator.                                */ 00395000
*          7 PathCPM Bit(1),           /* Control unit recovery         00396000
*                    process path mask. If on, indicates that the path  00397000
*                    is offline due to control unit recovery process */ 00398000
*          7 PathLPM Bit(1),           /* Logically Available Mask:     00399000
*                    1 = Available, 0 = Not available                */ 00400000
*          7 PathPAM Bit(1),           /* Physically Available Mask:    00401000
*                    1 = Available, 0 = Not available                */ 00402000
*          7 PathVARY Bit(1),          /* If On, vary OFFLINE in        00403000
*                    progress                                        */ 00404000
*        5 PathIntType Fixed(8),       /* Interface type entry.         00405000
*                    Constants defining the possible values are below   00406000
*                    and begin with PathIntType_                     */ 00407000
*        5 PathAttribute Fixed(8),     /* Path attribute.  Constants    00408000
*                                         are defined below      @0BA*/ 00409000
*        5 * Char(22),                 /* Reserved               @0BC*/ 00410000
*      3 * CHAR(0) BDY(DWORD);   /*                                  */ 00411000
*DCL PathIntType_Unknown Fixed(8) Constant(0);  /* Channel path         00412000
*                                  description not known         @P1C*/ 00413000
*DCL PathIntType_Block_Mtpx Fixed(8) Constant(1);   /* Parallel block   00414000
*                                  multiplexer channel path      @P1C*/ 00415000
*DCL PathIntType_Byte_Mtpx Fixed(8) Constant(2);   /* Parallel byte     00416000
*                                  multiplexer channel path      @P1C*/ 00417000
*DCL PathIntType_Escon_PT_To_Pt Fixed(8) Constant(3);   /* ESCON        00418000
*                                  point to point channel path   @P1C*/ 00419000
*DCL PathIntType_Escon_Unknown Fixed(8) Constant(4);   /* ESCON channel 00420000
*                                  path                          @P1C*/ 00421000
*DCL PathIntType_Escon_Switch Fixed(8) Constant(5);   /* ESCON switch   00422000
*                                  point to point channel path   @P1C*/ 00423000
