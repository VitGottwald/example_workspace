* %IOSDPATH_INCLUDED = 'YES';                                           00336000
* %DCL ZCBPRINT CHAR EXT;                                               00337000
* %DEACTIVATE ZCBPRINT;                                                 00338000
* %DCL IOSDPATH_LIST CHAR EXT;                                          00339000
* %DEACTIVATE IOSDPATH_LIST;                                            00340000
