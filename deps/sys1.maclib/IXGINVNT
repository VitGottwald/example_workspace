.***AO.X02  RETURN CODE PROCESSING                                      01806100
.*******************************************************************    01806200
                      AIF   (N'&RETCODE LE 0).ZMRETX                    01806300
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            01806400
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             01806500
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             01806600
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               01806700
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                01806800
&ZOPN                 SETA  &ZOPN+1                                     01806900
&ZOPL(&ZOPN)          SETC  ' '                                         01807000
&ZOPO(&ZOPN)          SETC  'ST'                                        01807100
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              01807200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01807300
                      AGO   .ZMRETX                                     01807400
.*                                                                      01807500
.ZMRET2               ANOP  ,                                           01807600
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                01807700
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                01807800
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                01807900
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  01808000
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   01808100
&ZOPN                 SETA  &ZOPN+1                                     01808200
&ZOPL(&ZOPN)          SETC  ' '                                         01808300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  01808400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 01808500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01808600
.ZMRETX               ANOP  ,                                           01808700
.*******************************************************************    01808800
.***AO.X03  REASON CODE PROCESSING                                      01808900
.*******************************************************************    01809000
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    01809100
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            01809200
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              01809300
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             01809400
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              01809500
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             01809600
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                01809700
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 01809800
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                01809900
&ZOPN                 SETA  &ZOPN+1                                     01810000
&ZOPL(&ZOPN)          SETC  ' '                                         01810100
&ZOPO(&ZOPN)          SETC  'ST'                                        01810200
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               01810300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        01810400
                      AGO   .ZMRSNX                                     01810500
.*                                                                      01810600
.ZMRSN2               ANOP  ,                                           01810700
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                01810800
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 01810900
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                01811000
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 01811100
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                01811200
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   01811300
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    01811400
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   01811500
