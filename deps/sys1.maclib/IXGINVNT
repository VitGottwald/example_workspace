                AIF   ('&LOGGERDUPLEX' EQ '').ZPLV3E                    00641600
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00641700
&XPLISTVER      SETA  2                                                 00641800
.ZPLV3E         ANOP  ,                                                 00641900
