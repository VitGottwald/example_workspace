                AIF   ('&RMNAME' EQ '').ZPLV4E                          00642100
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00642200
&XPLISTVER      SETA  1                                                 00642300
.ZPLV4E         ANOP  ,                                                 00642400
