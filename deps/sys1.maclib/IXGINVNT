.********************************************************************   01606700
.***AI.L02 IXGINVNT FMT=LIST INITIALIZE PARAMETER AREA                  01606800
.********************************************************************   01606900
.***AI.L03 MF=(SLME)                                                    01607000
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      01607100
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            01607200
&ZAWRK                SETA  0                                           01607300
&ZN                   SETA  &ZPLLEN                                     01607400
.ZCLRCONT             AIF   (&ZN LE 256).ZCLRX                          01607500
.*                                                                      01607600
&ZOPN                 SETA  &ZOPN+1                                     01607700
&ZOPL(&ZOPN)          SETC  ' '                                         01607800
&ZOPO(&ZOPN)          SETC  'XC'                                        01607900
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(256,1),&ZAWRK.(1)'                 01608000
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             01608100
.*                                                                      01608200
&ZAWRK                SETA &ZAWRK+256                                   01608300
&ZN                   SETA  &ZN-256                                     01608400
                      AGO   .ZCLRCONT                                   01608500
.*                                                                      01608600
.ZCLRX                ANOP  ,                                           01608700
.*                                                                      01608800
&ZOPN                 SETA  &ZOPN+1                                     01608900
&ZOPL(&ZOPN)          SETC  ' '                                         01609000
&ZOPO(&ZOPN)          SETC  'XC'                                        01609100
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(&ZN,1),&ZAWRK.(1)'                 01609200
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             01609300
