&ZOPL(&ZOPN)          SETC  ' '                                         01811700
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  01811800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  01811900
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        01812000
.ZMRSNX               ANOP  ,                                           01812100
