.***AO.X04  LABEL FOR MF=L PROCESSING                                   01812300
.*******************************************************************    01812400
.ZMFLX                ANOP  ,              MF=L PROCESSING              01812500
.*******************************************************************    01812600
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01812700
.*******************************************************************    01812800
                      ACTR  10000                                       01812900
.*                                                                      01813000
                      LCLA  &ZLC           COLUMN POSITION              01813100
                      LCLA  &ZOPX          LIST TABLE INDEX             01813200
                      LCLC  &ZLL           LABEL                        01813300
                      LCLC  &ZLOP          OPERATION                    01813400
                      LCLC  &ZLV           VALUE                        01813500
                      LCLC  &ZLD           DESCRIPTION                  01813600
.*                                                                      01813700
&ZOPX                 SETA  1                                           01813800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01813900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01814000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01814100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01814200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01814300
                      AIF   (K'&ZLD LE 0).ZL5                           01814400
&ZLC                  SETA  35             COMMENT COLUMN               01814500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01814600
&ZLC                  SETA  33             COMMMENT COLUMN              01814700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01814800
&ZLV                  SETC  '&ZLV'.' '                                  01814900
                      AGO   .ZL3                                        01815000
.*                                                                      01815100
.ZL4                  ANOP  ,                                           01815200
&ZLV                  SETC  '&ZLV'.' '                                  01815300
.ZL5                  ANOP  ,                                           01815400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01815500
.*                                                                      01815600
&ZLL     &ZLOP &ZLV                                                     01815700
.*                                                                      01815800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01815900
                      AGO   .ZL2           CONTINUE LOOPING             01816000
.*                                                                      01816100
.ZLX                  ANOP  ,                                           01816200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01816300
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01816400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01816500
         AIF   ('&SYSASCE' EQ 'P').IVPRIM  SYSSTATE PRIMARY             01816600
         SLR   15,15                SET GPR 15 TO ZERO                  01816700
         SAR   15,15                ENSURE AR 15 IS ZERO FOR PRIMARY    01816800
.IVPRIM  ANOP                                                           01816900
         AIF   ('&SYSAM64' NE 'YES').IVA31 BRANCH IF AMODE 31           01817000
         LLGT  15,16(0,0)           GET CVT Address                     01817100
.*    Do not need to test Logger Avail out of line if AMODE 64.         01817200
.*    This was done for pre-520 compatability.                          01817300
         LLGT  15,140(15,0)         GET ECVT ADDRESS                    01817400
         LLGT  15,288(15,0)         GET LSAB ADDRESS                    01817500
         LLGT  15,124(15,0)         GET ENTRY POINT ADDRESS             01817600
         AGO   .IVCONT                                                  01817700
.IVA31   ANOP                                                           01817800
         L     15,16(0,0)           GET CVT ADDRESS                     01817900
.*   Do not need to test Logger Avail since releases now support it.    01818000
.*   This was done for pre-520 compatability.                      @PFC 01818100
         L     15,140(15,0)         GET ECVT ADDRESS                    01818200
         L     15,288(15,0)         GET LSAB ADDRESS                    01818300
         L     15,124(15,0)         GET ENTRY POINT ADDRESS             01818400
.IVCONT  ANOP                                                           01818500
         LA    0,8                  EX OF INVENTORY SERVICE             01818600
         BALR  14,15                INVOKE SERVICE ROUTINE              01818700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01818800
.ZLINKX               ANOP  ,                                           01818900
.*******************************************************************    01819000
.***AP.X01  FORMAT GENERATED CODE                                       01819100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            01819200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01819300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01819400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01819500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01819600
                      AIF   (K'&ZLD LE 0).ZL51                          01819700
&ZLC                  SETA  35             COMMENT COLUMN               01819800
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   01819900
&ZLC                  SETA  33             COMMENT COLUMN               01820000
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  01820100
&ZLV                  SETC  '&ZLV'.' '                                  01820200
                      AGO   .ZL31                                       01820300
.*                                                                      01820400
.ZL41                 ANOP  ,                                           01820500
&ZLV                  SETC  '&ZLV'.' '                                  01820600
.ZL51                 ANOP  ,                                           01820700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01820800
.*                                                                      01820900
