.******************************************************************     01602500
.***AI.L01 IXGINVNT FMT=LIST FILL IN PARAMETER AREA                     01602600
.******************************************************************     01602700
                      ACTR  10000                                       01602800
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01602900
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01603000
&ZCVAL                SETC  'LAE'          XAX MODE                     01603100
.ZIMFXA               ANOP ,                                            01603200
.*                                                                      01603300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01603400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01603500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01603600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01603700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01603800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01603900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01604000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01604100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01604200
&ZOPN                 SETA  &ZOPN+1                                     01604300
&ZOPL(&ZOPN)          SETC  ' '                                         01604400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01604500
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01604600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01604700
                      AGO   .ZIMFNX                                     01604800
.*                                                                      01604900
.ZIMFN2               ANOP ,                                            01605000
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01605100
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01605200
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01605300
&ZOPN                 SETA  &ZOPN+1                                     01605400
&ZOPL(&ZOPN)          SETC  ' '                                         01605500
&ZOPO(&ZOPN)          SETC  'LAE'                                       01605600
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01605700
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01605800
                      AGO   .ZIMFNX                                     01605900
.ZIMFNX1              ANOP ,                                            01606000
&ZOPN                 SETA  &ZOPN+1                                     01606100
