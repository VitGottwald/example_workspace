                AIF   ('&DASDONLY' EQ '').ZPLV2E                        00641100
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00641200
&XPLISTVER      SETA  2                                                 00641300
.ZPLV2E         ANOP  ,                                                 00641400
