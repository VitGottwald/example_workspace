                AIF   ('&ZCGPR' EQ '0').ZG8Q                            00573200
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00573300
                AIF   (T'&ZCGPR NE 'N').ZG8Q    INVALID REG NAME        00573400
.ZG8P           ANOP  ,                                                 00573500
                AIF   (T'&ZCGPR NE 'N').ZG8R    NON NUMERIC             00573600
                AIF   ('&ZCGPR' LT '2').ZG8Q                            00573700
                AIF   ('&ZCGPR' LE '12').ZG8R                           00573800
.ZG8Q           ANOP  ,                                                 00573900
&ZMS            SETC  '"RETINFO='               *KGPRA* MNOTE           00574000
&ZMS            SETC  '&ZMS.&RETINFO"'                                  00574100
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00574200
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00574300
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00574400
.ZG8R           ANOP  ,                                                 00574500
