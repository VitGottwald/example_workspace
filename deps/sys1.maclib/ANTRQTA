                      AIF   ('&XCLIP'(1,1) NE '(').ZI120B               02908700
&ZCGPR   SETC  '0('.'&XCLIP'(2,K'&XCLIP-2).')'                          02908800
.ZI120B               ANOP  ,                                           02908900
.*                                                                      02909000
&ZOPN                 SETA  &ZOPN+1                                     02909100
&ZOPL(&ZOPN)          SETC  ' '                                         02909200
&ZOPO(&ZOPN)          SETC  'MVC'                                       02909300
&ZOPV(&ZOPN)          SETC  '342(3,1),&ZCGPR'                           02909400
&ZOPD(&ZOPN)          SETC  '++ &ZXCLIP XCLIP'                          02909500
.*                                                                      02909600
.ZI120E               ANOP  ,                                           02909700
.*-------------------------------------------------------------------   02909800
.***AI.L56 XLASTBYTE XL1                                                02909900
&ZOPN                 SETA  &ZOPN+1                                     02910000
&ZOPL(&ZOPN)          SETC  ' '                                         02910100
&ZOPO(&ZOPN)          SETC  'MVI'                                       02910200
&ZOPV(&ZOPN)          SETC  '511(1),B''10111100'''                      02910300
&ZOPD(&ZOPN)          SETC  '++ XL1 CONSTANT'                           02910400
.*-------------------------------------------------------------------   02910500
&ZOPN1                SETA &ZOPN                                        02910600
.*-------------------------------------------------------------------   02910700
.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              02910800
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         02910900
.*******************************************************************    02911000
.***  PROCESS ANTRQTA OUTPUT PARAMETERS                                 02911100
.*******************************************************************    02911200
                      ACTR  10000                                       02911300
.*******************************************************************    02911400
.***AO.X02  RETURN CODE PROCESSING                                      02911500
.*******************************************************************    02911600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    02911700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            02911800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             02911900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             02912000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               02912100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                02912200
&ZOPN                 SETA  &ZOPN+1                                     02912300
&ZOPL(&ZOPN)          SETC  ' '                                         02912400
&ZOPO(&ZOPN)          SETC  'ST'                                        02912500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              02912600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        02912700
                      AGO   .ZMRETX                                     02912800
.*                                                                      02912900
.ZMRET2               ANOP  ,                                           02913000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                02913100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                02913200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                02913300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  02913400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   02913500
&ZOPN                 SETA  &ZOPN+1                                     02913600
&ZOPL(&ZOPN)          SETC  ' '                                         02913700
&ZOPO(&ZOPN)          SETC  'LR'                                        02913800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 02913900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        02914000
.ZMRETX               ANOP  ,                                           02914100
.*******************************************************************    02914200
.***AO.X03  REASON CODE PROCESSING                                      02914300
.*******************************************************************    02914400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    02914500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            02914600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              02914700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             02914800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              02914900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             02915000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                02915100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 02915200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                02915300
&ZOPN                 SETA  &ZOPN+1                                     02915400
&ZOPL(&ZOPN)          SETC  ' '                                         02915500
&ZOPO(&ZOPN)          SETC  'ST'                                        02915600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               02915700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        02915800
                      AGO   .ZMRSNX                                     02915900
.*                                                                      02916000
.ZMRSN2               ANOP  ,                                           02916100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                02916200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 02916300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                02916400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 02916500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                02916600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   02916700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    02916800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   02916900
&ZOPN                 SETA  &ZOPN+1                                     02917000
&ZOPL(&ZOPN)          SETC  ' '                                         02917100
&ZOPO(&ZOPN)          SETC  'LR'                                        02917200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  02917300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        02917400
.ZMRSNX               ANOP  ,                                           02917500
.*******************************************************************    02917600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   02917700
.*******************************************************************    02917800
.ZMFLX                ANOP  ,              MF=L PROCESSING              02917900
.*******************************************************************    02918000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               02918100
.*******************************************************************    02918200
                      ACTR  10000                                       02918300
.*                                                                      02918400
                      LCLA  &ZLC           COLUMN POSITION              02918500
                      LCLA  &ZOPX          LIST TABLE INDEX             02918600
                      LCLC  &ZLL           LABEL                        02918700
                      LCLC  &ZLOP          OPERATION                    02918800
                      LCLC  &ZLV           VALUE                        02918900
                      LCLC  &ZLD           DESCRIPTION                  02919000
.*                                                                      02919100
&ZOPX                 SETA  1                                           02919200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            02919300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              02919400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              02919500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              02919600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              02919700
                      AIF   (K'&ZLD LE 0).ZL5                           02919800
&ZLC                  SETA  35             COMMENT COLUMN               02919900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    02920000
&ZLC                  SETA  33             COMMMENT COLUMN              02920100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   02920200
&ZLV                  SETC  '&ZLV'.' '                                  02920300
                      AGO   .ZL3                                        02920400
.*                                                                      02920500
.ZL4                  ANOP  ,                                           02920600
&ZLV                  SETC  '&ZLV'.' '                                  02920700
.ZL5                  ANOP  ,                                           02920800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               02920900
.*                                                                      02921000
&ZLL     &ZLOP &ZLV                                                     02921100
.*                                                                      02921200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               02921300
                      AGO   .ZL2           CONTINUE LOOPING             02921400
.*                                                                      02921500
.ZLX                  ANOP  ,                                           02921600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                02921700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                02921800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02921900
         L     14,16(,0)                                                02922000
         L     14,1216(,14)                                             02922100
         L     14,44(,14)                                               02922200
         L     14,8(,14)                                                02922300
         L     14,88(,14)                                               02922400
         ICM   14,15,324(14)                                            02922500
         BNZ   A&SYSNDX                                                 02922600
         L     15,=A(RQST_PC_NUMBER_ZERO)                               02922700
         SLR   0,0                                                      02922800
         B     B&SYSNDX                                                 02922900
A&SYSNDX DS    0H                                                       02923000
         PC    0(14)                                                    02923100
B&SYSNDX DS    0H                                                       02923200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02923300
.ZLINKX               ANOP  ,                                           02923400
.*******************************************************************    02923500
.***AP.X01  FORMAT GENERATED CODE                                       02923600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            02923700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              02923800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              02923900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              02924000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              02924100
                      AIF   (K'&ZLD LE 0).ZL51                          02924200
&ZLC                  SETA  35             COMMENT COLUMN               02924300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   02924400
&ZLC                  SETA  33             COMMENT COLUMN               02924500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  02924600
&ZLV                  SETC  '&ZLV'.' '                                  02924700
                      AGO   .ZL31                                       02924800
.*                                                                      02924900
.ZL41                 ANOP  ,                                           02925000
&ZLV                  SETC  '&ZLV'.' '                                  02925100
.ZL51                 ANOP  ,                                           02925200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               02925300
.*                                                                      02925400
&ZLL     &ZLOP &ZLV                                                     02925500
.*                                                                      02925600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               02925700
                      AGO   .ZL21          CONTINUE LOOPING             02925800
.*                                                                      02925900
.ZLX1                 ANOP  ,                                           02926000
.*******************************************************************    02926100
