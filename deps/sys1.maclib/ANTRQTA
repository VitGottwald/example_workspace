.***AC.PLV01  PROCESS PLISTVER                                      *   00512800
.********************************************************************   00512900
&XPLISTVER      SETA  0                                                 00513000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00513100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00513200
&XPLISTVER      SETA  4                                                 00513300
.ZPLV0          ANOP  ,                                                 00513400
.*                                                                      00513500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00513600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00513700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00513800
.*-------------------------------------------------------------------   00513900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00514000
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00514100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00514200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00514300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00514400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00514500
                AGO   .ZPVCK2                                           00514600
.ZPVCK3         ANOP  ,                                                 00514700
.*-------------------------------------------------------------------   00514800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00514900
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00515000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00515100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00515200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00515300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00515400
                AGO   .ZPVCKE                                           00515500
.ZPVCK2         ANOP  ,                                                 00515600
.*-------------------------------------------------------------------   00515700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00515800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00515900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00516000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00516100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00516200
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00516300
.ZPVCK6         ANOP  ,                                                 00516400
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00516500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00516600
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00516700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00516800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00516900
&XPLISTVER      SETA  4                                                 00517000
                AGO   .ZPVCKE                                           00517100
.*-------------------------------------------------------------------   00517200
.ZPVCK4A        ANOP  ,                                                 00517300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00517400
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00517500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00517600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00517700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00517800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00517900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00518000
                AGO   .ZPVCK5                                           00518100
.ZPVCK4         ANOP  ,                                                 00518200
&XPLISTVER      SETA  &PLISTVER(1)                                      00518300
.ZPVCK5         ANOP  ,                                                 00518400
.ZPVCKE         ANOP  ,                                                 00518500
.********************************************************************   00518600
