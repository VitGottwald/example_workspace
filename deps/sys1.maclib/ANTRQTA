*               USING @PSTART,12                                      * 00442800
*               LA    15,0                                            * 00442900
*               L     0,@SIZDATD+4                                    * 00443000
*               GETMAIN  RU,LV=(0),SP=(15)                            * 00443100
*               LR    15,13                                           * 00443200
*               LR    13,01                                           * 00443300
*               USING @DATD,13                                        * 00443400
*               ST    15,4(,13)                                       * 00443500
*               ST    13,8(,15)                                       * 00443600
*               LM    15,01,16(15)                                    * 00443700
*               MVC   XRCSID,=CL8'TUCSON  '                           * 00443800
*               MVC   XASYNCH,=CL3'YES'                               * 00443900
*               XC    XECB,XECB                                       * 00444000
*               ANTRQST ILK=XRC,                     X                * 00444100
*                     REQUEST=XCONTIME,              X                * 00444200
*                     SID=XRCSID,                    X                * 00444300
*                     TIME=CONTIME,                  X                * 00444400
*                     RETINFO=XRETINFO,              X                * 00444500
*                     ASYNCH=XASYNCH,                X                * 00444600
*                     ECB=XECB,                      X                * 00444700
*                     RETCODE=RTNCD,                 X                * 00444800
*                     RSNCODE=RSNCD,                 X                * 00444900
*                     MF=(E,P_LIST)                                   * 00445000
*               CLC   RTNCD,=F'0'                                     * 00445100
*               BE    SKIP1                                           * 00445200
*               MVC   RTCD,RTNCD                                      * 00445300
*               MVC   RSCD,RSNCD                                      * 00445400
*               B     SKIP2                                           * 00445500
*      SKIP1    DS    0H                                              * 00445600
*               WAIT  ECB=XECB                                        * 00445700
*               NC    XECB,=XL4'3FFFFFFF'                             * 00445800
*               CLC   XECB,=F'0'                                      * 00445900
*               BE    SKIP1A                                          * 00446000
*               MVC   RTCD,XECB                                       * 00446100
*               MVC   RSCD,=F'0'                                      * 00446200
*               B     SKIP2                                           * 00446300
*      SKIP1A   DS    0H                                              * 00446400
*               MVC   RTCD,RTC                                        * 00446500
*               MVC   RSCD,RSC                                        * 00446600
*      SKIP2    DS    0H                                              * 00446700
*               CLC   RTCD,=A(RQST_PC_NUMBER_ZERO)                    * 00446800
*               BNE   SKIP3                                           * 00446900
*               WTO   'ANTAS000 NOT ACTIVE'                           * 00447000
*      SKIP3    DS    0H                                              * 00447100
*               L     2,RTCD                                          * 00447200
*               L     3,RSCD                                          * 00447300
*               LR    1,13                                            * 00447400
*               L     13,4(,13)                                       * 00447500
*               LA    15,0                                            * 00447600
*               L     0,@SIZDATD+4                                    * 00447700
*               FREEMAIN RU,LV=(0),A=(1),SP=(15)                      * 00447800
*               LR    15,2                                            * 00447900
*               LR    0,3                                             * 00448000
*               L     14,12(,13)                                      * 00448100
*               LM    1,12,24(13)                                     * 00448200
*               BR    14                                              * 00448300
*               LTORG                                                 * 00448400
*      @SIZDATD DS    0A                                              * 00448500
*               DC    AL1(0)                                          * 00448600
*               DC    AL3(@DYNSIZE)                                   * 00448700
*      @DATD    DSECT                                                 * 00448800
*               DS    18F                                             * 00448900
*      RTNCD    DS    F                                               * 00449000
*      RSNCD    DS    F                                               * 00449100
*      RTCD     DS    F                                               * 00449200
*      RSCD     DS    F                                               * 00449300
*      XECB     DS    F                                               * 00449400
*      XASYNCH  DS    CL3                                             * 00449500
*      XRCSID   DS    CL8                                             * 00449600
*      CONTIME  DS    CL8                                             * 00449700
*      XRETINFO DS    0F,CL100                                        * 00449800
*               ORG   XRETINFO                                        * 00449900
*      RTC      DS    F                                               * 00450000
*      RSC      DS    F                                               * 00450100
*               ORG   XRETINFO+100                                    * 00450200
*               ANTRQSTL NAME=P_LIST,BASE=0F                          * 00450300
*               ORG   *+1-(*-@DATD)/(*-@DATD)                         * 00450400
*      @ENDDATD DS    0X                                              * 00450500
*      @DYNSIZE EQU   ((@ENDDATD-@DATD+7)/8)*8                        * 00450600
*               END   TESTER                                          * 00450700
*                                                                     * 00450800
*                                                                     * 00450900
*    Operation:  Getting the current consistency time for             * 00451000
*    XRC session TUCSON in PL/X.                                      * 00451100
*                                                                     * 00451200
*       @PROCESS                                                      * 00451300
*         NOMSOURCE                                                   * 00451400
*         GENMARGINS(1,72)                                            * 00451500
*         OPTIMIZE(ADV,48)                                            * 00451600
*         ENVIRONMENT(MVS/ESA)                                        * 00451700
*         SOURCE(SEGMENT)                                             * 00451800
*         TITLE('TESTER: Test ANTRQST');                              * 00451900
*                                                                     * 00452000
*        TESTER: PROCEDURE                                            * 00452100
*                 OPTIONS(                                            * 00452200
*                         AMODE(31)                                   * 00452300
*                         CODEREG(12)                                 * 00452400
*                         DATAREG(13)                                 * 00452500
*                         REENTRANT                                   * 00452600
*                         RMODE(ANY)                                  * 00452700
*                         );                                          * 00452800
*                                                                     * 00452900
*       DCL                                                           * 00453000
*         1 XRETINFO CHAR(100) BDY(WORD),                             * 00453100
*           3 RTC FIXED(32),                                          * 00453200
*           3 RSC FIXED(32);                                          * 00453300
*       DCL XSID CHAR(8) CONSTANT('TUCSON  ');                        * 00453400
*       DCL XHLQ CHAR(8) CONSTANT('SYS1    ');                        * 00453500
*       DCL XTIME CHAR(8);                                            * 00453600
*       DCL RTCD FIXED(31);                                           * 00453700
*       DCL RSCD FIXED(31);                                           * 00453800
*       DCL RTNCD FIXED(31);                                          * 00453900
*       DCL RSNCD FIXED(31);                                          * 00454000
*                                                                     * 00454100
*         ?ANTRQST ILK(XRC)                                           * 00454200
*                  REQUEST(XCONTIME)                                  * 00454300
*                  SID(XSID)                                          * 00454400
*                  TIME(XTIME)                                        * 00454500
*                  HLQ(XHLQ)                                          * 00454600
*                  RETINFO(XRETINFO)                                  * 00454700
*                  RETCODE(RTNCD)                                     * 00454800
*                  RSNCODE(RSNCD)                                     * 00454900
*                  MF(E,PARMLIST);                                    * 00455000
*                                                                     * 00455100
*         IF RTNCD ^= 0 THEN                                          * 00455200
*           DO;                                                       * 00455300
*             RTCD = RTNCD;                                           * 00455400
*             RSCD = RSNCD;                                           * 00455500
*           END;                                                      * 00455600
*         ELSE                                                        * 00455700
*           DO;                                                       * 00455800
*             RTCD = RTC;                                             * 00455900
*             RSCD = RSC;                                             * 00456000
*           END;                                                      * 00456100
*                                                                     * 00456200
*         IF RTCD = RQST_PC_NUMBER_ZERO THEN                          * 00456300
*           ?WTO ('ANTAS000 NOT ACTIVE');                             * 00456400
*                                                                     * 00456500
*         RETURN CODE(RTCD,RSCD);                                     * 00456600
*                                                                     * 00456700
*         ?ANTRQSTL NAME(PARMLIST);                                   * 00456800
*                                                                     * 00456900
*         ?EPILOG;                                                    * 00457000
*                                                                     * 00457100
*       END TESTER;                                                   * 00457200
*                                                                     * 00457300
*                                                                     * 00457400
*                                                                     * 00457500
*01* MACRO USAGE NOTES:                                               * 00457600
*                                                                     * 00457700
*                                                                     * 00457800
*    1.  Printing of the macro expansion is controlled by the ZPRINT  * 00457900
*        global macro variable.  Any value other than 'NO' will       * 00458000
*        result in the macro expansion being printed.  The default is * 00458100
*        'YES'.  The following examples illustrate how the ZPRINT     * 00458200
*        variable should be set.                                      * 00458300
*                                                                     * 00458400
*                    GBLC &ZPRINT                                     * 00458500
*            &ZPRINT SETC 'YES'          &ZPRINT SETC 'NO'            * 00458600
*                                                                     * 00458700
*                                                                     * 00458800
*    2.  When using the NOCHECK option in conjunction with the Modify * 00458900
*        and/or  Execute  forms,  be  sure  that  it is preceded by a * 00459000
*        Modify   or  Execute  form  invocation  which  specifies  or * 00459100
*        defaults  to  the  COMPLETE  option.   This insures that the * 00459200
*        parameter  list  is  initialized completely.   The parameter * 00459300
*        list  field which identifies the version number of the macro * 00459400
*        is only set when the Standard form or the Complete option of * 00459500
*        the  Execute/Modify  form  is  used,  or  when  PLISTVER  is * 00459600
*        explicitly  specified.  Be sure that the resulting parameter * 00459700
*        list version number covers all the keys that you use.        * 00459800
*                                                                     * 00459900
*                                                                     * 00460000
*    3.  The  ANTRQTA macro supports multiple versions. The macro key * 00460100
*        list  below  contains the version level in which the key was * 00460200
*        first  introduced.  Positionals  are  identified by the word * 00460300
*        'Positional'  followed by the numeric value of its position. * 00460400
*        Nested  positionals  are  preceded  by their outer key.  The * 00460500
*        lowest value represents the original version and the highest * 00460600
*        value  represents  the  most  recent  version of the ANTRQTA * 00460700
*        macro.    When  specifying  PLISTVER,  be sure that it is at * 00460800
*        least  as high as the highest version number of all the keys * 00460900
*        being used.                                                  * 00461000
*                                                                     * 00461100
*        VERSION   KEY                                                * 00461200
*                                                                     * 00461300
*              0      ALET                                            * 00461400
*                     ASYNCH                                          * 00461500
*                     ATTIME                                          * 00461600
*                     CHECK                                           * 00461700
*                     CLIP                                            * 00461800
*                     COPY                                            * 00461900
*                     DEVN                                            * 00462000
*                     DEVNUM                                          * 00462100
*                     DONOTBLOCK                                      * 00462200
*                     DSDISP                                          * 00462300
*                     DSNAME                                          * 00462400
*                     DSPINFO                                         * 00462500
*                     DVCBLOCK                                        * 00462600
*                     ECB                                             * 00462700
*                     ERRLVL                                          * 00462800
*                     ETYPE                                           * 00462900
*                     FORCE                                           * 00463000
*                     HLQ                                             * 00463100
*                     ILK                                             * 00463200
*                     ILKNBR                                          * 00463300
*                     LOGPLUS                                         * 00463400
*                     MESSAGES                                        * 00463500
*                     MHLQ                                            * 00463600
*                     MID                                             * 00463700
*                     MODE                                            * 00463800
*                     MSGROUTE                                        * 00463900
*                     ONLINE                                          * 00464000
*                     OPTION                                          * 00464100
*                     PACTION                                         * 00464200
*                     PAGEFIX                                         * 00464300
*                     PDEVNUM                                         * 00464400
*                     PDSNAME                                         * 00464500
*                     PMEMBER                                         * 00464600
*                     PRIORITY                                        * 00464700
*                     PVOLSER                                         * 00464800
*                     QRYINFO                                         * 00464900
*                     QRYSIZE                                         * 00465000
*                     REFRESHP                                        * 00465100
*                     REFRESHS                                        * 00465200
*                     REQUEST                                         * 00465300
*                     RETINFO                                         * 00465400
*                     RFREQUENCY                                      * 00465500
*                     RTRACKS                                         * 00465600
*                     SCSESSION                                       * 00465700
*                     SCSYNCHP                                        * 00465800
*                     SCSYNCHS                                        * 00465900
*                     SDEVNUM                                         * 00466000
*                     SID                                             * 00466100
*                     SSID                                            * 00466200
*                     STYPE                                           * 00466300
*                     SUPMSG                                          * 00466400
*                     SUSLBUSY                                        * 00466500
*                     SUSPENDED                                       * 00466600
*                     SVOLSER                                         * 00466700
*                     SWAPDEV                                         * 00466800
*                     SYNCH                                           * 00466900
*                     TERTIARY                                        * 00467000
*                     TIME                                            * 00467100
*                     TIMEOUT                                         * 00467200
*                     UTILITY                                         * 00467300
*                     VOLLIST                                         * 00467400
*                     WAITTIME                                        * 00467500
*                                                                     * 00467600
*    4.  ANTRQST  Message  Buffer.   The format of an ANTRQST message * 00467700
*        buffer is:                                                   * 00467800
*                                                                     * 00467900
*                                                                     * 00468000
*        o   Bytes  1-4  contain  the  size  of  the  ANTRQST message * 00468100
*            buffer.                                                  * 00468200
*                                                                     * 00468300
*        o   Byte  5  contains  the  subpool  of  the ANTRQST message * 00468400
*            buffer.                                                  * 00468500
*                                                                     * 00468600
*        o   Byte 6 is not used.                                      * 00468700
*                                                                     * 00468800
*        o   Bytes  7-8  contain  the number of messages in the array * 00468900
*            that follows.                                            * 00469000
*                                                                     * 00469100
*        o   The  rest of the message buffer is an array of messages. * 00469200
*            Each message has the following format:                   * 00469300
*                                                                     * 00469400
*                                                                     * 00469500
*            -   Bytes  1-2  contain the size of the message in bytes * 00469600
*                3-x.                                                 * 00469700
*                                                                     * 00469800
*            -   Bytes 3-x contain the message.                       * 00469900
*                                                                     * 00470000
*                                                                     * 00470100
*                                                                     * 00470200
*                                                                     * 00470300
*                                                                     * 00470400
*        IT  IS  THE  RESPONSIBILITY  OF  THE CALLER TO FREEMAIN THIS * 00470500
*        BUFFER.                                                      * 00470600
*                                                                     * 00470700
*    5.  ANTRQST  XSCSTATS  Results Buffer.  The format of an ANTRQST * 00470800
*        XSCSTATS results buffer is:                                  * 00470900
*                                                                     * 00471000
*                                                                     * 00471100
*        o   Bytes 1-6 will contain the eye-catcher XSTATS.           * 00471200
*                                                                     * 00471300
*        o   Bytes  7-8  contain  the  number of storage control data * 00471400
*            mover  session  entries provided in the remainder of the * 00471500
*            results  buffer.    Statistics  for both concurrent copy * 00471600
*            (CC)  and extended remote copy (XRC) are provided in the * 00471700
*            results buffer.                                          * 00471800
*                                                                     * 00471900
*        o   The remainder of the results buffer depends on the ETYPE * 00472000
*            value  used  on  the  request.    If  ETYPE(SUMMARY) was * 00472100
*            specified  or  no ETYPE value was specified (the default * 00472200
*            is  SUMMARY),  the  remainder  of the results buffer are * 00472300
*            32-byte  entries  of statistics for each storage control * 00472400
*            session (as indicated by the number found in bytes 7-8). * 00472500
*            If  ETYPE(SINGLE)  was specified, a single 32-byte entry * 00472600
*            is  provided  followed by 8-byte entries for each of the * 00472700
*            devices in the single storage control session requested. * 00472800
*                                                                     * 00472900
*            Each   storage   control   session  entry  contains  the * 00473000
*            following 32 bytes of statistics:                        * 00473100
*                                                                     * 00473200
*                                                                     * 00473300
*            -   Bytes  1-2  contain  the Storage Subsytem Identifier * 00473400
*                (SSID).                                              * 00473500
*                                                                     * 00473600
*            -   Bytes   3-4  contain  the  storage  control  session * 00473700
*                identifier (scsession):                              * 00473800
*                                                                     * 00473900
*                                                                     * 00474000
*                --  Byte 3:                                          * 00474100
*                                                                     * 00474200
*                                                                     * 00474300
*                    o   Bit 1 - 0                                    * 00474400
*                                                                     * 00474500
*                    o   Bit 2 - 0                                    * 00474600
*                                                                     * 00474700
*                    o   Bit 3 - 0                                    * 00474800
*                                                                     * 00474900
*                    o   Bit 4:                                       * 00475000
*                                                                     * 00475100
*                                                                     * 00475200
*                        -   0 - Concurrent Copy (CC) session         * 00475300
*                                                                     * 00475400
*                        -   1 - Extended Remote Copy (XRC) session   * 00475500
*                                                                     * 00475600
*                                                                     * 00475700
*                                                                     * 00475800
*                    o   Bit  5  -  request for status came in on the * 00475900
*                        same path group that owns the session        * 00476000
*                                                                     * 00476100
*                    o   Bit 6 - internally terminated                * 00476200
*                                                                     * 00476300
*                    o   Bit 7 - suspended                            * 00476400
*                                                                     * 00476500
*                    o   Bit 8 - quiesced                             * 00476600
*                                                                     * 00476700
*                                                                     * 00476800
*                                                                     * 00476900
*                --  Byte 4 is the actual scsession number.           * 00477000
*                                                                     * 00477100
*                                                                     * 00477200
*                                                                     * 00477300
*            -   Bytes  5-6  contain  the  record  set  count for the * 00477400
*                scsession.                                           * 00477500
*                                                                     * 00477600
*            -   Byte 7 contains the Channel Connection Address (CCA) * 00477700
*                for the next utility device to use.                  * 00477800
*                                                                     * 00477900
*            -   Byte 8 contains scsession status information:        * 00478000
*                                                                     * 00478100
*                                                                     * 00478200
*                --  Bit 1 - scsession is suspended                   * 00478300
*                                                                     * 00478400
*                --  Bit 2 - in long busy because the sidefile is 60% * 00478500
*                    of cache                                         * 00478600
*                                                                     * 00478700
*                --  Bit  3  -  in long busy because residual count > * 00478800
*                    63K                                              * 00478900
*                                                                     * 00479000
*                --  Bit 4 - utility device is fixed                  * 00479100
*                                                                     * 00479200
*                --  Bits 5-8 - 0                                     * 00479300
*                                                                     * 00479400
*                                                                     * 00479500
*                                                                     * 00479600
*            -   Bytes  9-10  contain  the size of the largest record * 00479700
*                set.                                                 * 00479800
*                                                                     * 00479900
*            -   Bytes  11-14  contain  the number of 4K segements in * 00480000
*                the sidefile.                                        * 00480100
*                                                                     * 00480200
*            -   Bytes 15-18 contain the number of 4K segements which * 00480300
*                represents 60% of the cache size.                    * 00480400
*                                                                     * 00480500
*            -   Bytes  19-26 contain the scsession's current session * 00480600
*                time in TOD format.                                  * 00480700
*                                                                     * 00480800
*            -   Bytes  27-28  contain  the  time remaining until the * 00480900
*                scsession time's out.                                * 00481000
*                                                                     * 00481100
*            -   Bytes  29-30 contain the number of devices belonging * 00481200
*                to the SCSESSION.                                    * 00481300
*                                                                     * 00481400
*            -   Bytes  31-32  contain the number of devices that are * 00481500
*                in device blocking.                                  * 00481600
*                                                                     * 00481700
*            -   Byte 33.  If ETYPE(SINGLE) is specified, starting at * 00481800
*                byte  33,  statistics for each of the devices in the * 00481900
*                single   storage   control   session  requested  are * 00482000
*                provided.  Bytes 29-30 indicate the number of 8-byte * 00482100
*                areas  that  follow.   Each 8-byte device statistics * 00482200
*                area contains the following information:             * 00482300
*                                                                     * 00482400
*                                                                     * 00482500
*                --  Bytes  1-2  contain  the  device  number  of the * 00482600
*                    device.   The device number is only available if * 00482700
*                    the   device  is  online  to  the  system  which * 00482800
*                    processed the request.                           * 00482900
*                                                                     * 00483000
*                --  Bytes 3-4 contain the CCA of the device.         * 00483100
*                                                                     * 00483200
*                --  Bytes  5-6  contain  the threshold count for the * 00483300
*                    device.    The threshold count is only available * 00483400
*                    for XRC storage control sessions.                * 00483500
*                                                                     * 00483600
*                --  Bytes  7-8  contain the record set count for the * 00483700
*                    device.   The record set count is only available * 00483800
*                    for XRC storage control sessions.                * 00483900
*                                                                     * 00484000
*                                                                     * 00484100
*                                                                     * 00484200
*                                                                     * 00484300
*                                                                     * 00484400
*                                                                     * 00484500
*                                                                     * 00484600
*                                                                     * 00484700
*                                                                     * 00484800
*01* CHANGE ACTIVITY:                                                 * 00484900
*                                                                     * 00485000
*   Flag LineItem  FMID    Date   ID    Comment                       * 00485100
*                                                                     * 00485200
*    $00=OW30320  HDZ11C0 970531 TUCRNC:                              * 00485300
*    $01=OW29881  HDZ11C0 980309 TUCRNC:                              * 00485400
