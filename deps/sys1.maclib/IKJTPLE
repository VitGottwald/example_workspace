         MEND                                                           01800000
**/                                                                     01850000
* %TPLBSLE:                                                             01900000
* /* THE TPLE MAPPING CAN BE PLACED AT THE BEGINNING, MIDDLE,  */       01950000
* /* OR END OF ANY BSL STRUCTURE THROUGH THE USE OF THE        */       02000000
* /* COMPILE TIME OPTIONS 'TPLESLEV' AND/OR 'TPLEOPT1'.        */       02050000
* /*   TPLESLEV - THIS IS THE CHARACTER ('2' TO '7') TO BE     */       02100000
* /*             ASSIGNED TO THE HIGHEST LEVEL IF THE TPLE IS  */       02150000
* /*             NOT TO BE LOCATED AT THE BEGINNING OF THE     */       02200000
* /*             STRUCTURE.                                    */       02250000
* /*   TPLEOPT1 - IF A CHARACTER STRING 'CONTINUE' IS ASSIGNED */       02300000
* /*             TO THIS OPTION, A SEMI-COLON IS NOT PLACED AT */       02350000
* /*             THE END OF THE STRUCTURE SO THAT ADDITIONAL   */       02400000
* /*             STATEMENTS MAY BE ADDED TO THE STRUCTURE.     */       02450000
* /* IF NEITHER OPTION IS SPECIFIED, THE TPLE MAPPING WILL     */       02500000
* /* DEFAULT TO A STRUCTURE OF ITS OWN.                        */ ;     02550000
* %       DECLARE (XE,YE,ZE,WE) FIXED /* COMPILE TIME VARIABLES */ ;    02600000
* %       IF TPLESLEV<'2'                                               02650000
* %         THEN                                                        02700000
* %           GOTO TPLBSLE1;  /* SET LEVEL NUMBER TO 1 @E2115B7*/ ;     02750000
* %       IF TPLESLEV>'5'                                               02800000
* %         THEN                                                        02850000
* %           GOTO TPLBSLE1;  /* SET LEVEL NUMBER TO 1        */ ;      02900000
* %       IF TPLESLEV='2'                                               02950000
* %         THEN                                                        03000000
* %           XE=2           /* SET 1ST LEV AS SPECIFIED @E2115B7*/ ;   03050000
* %       IF TPLESLEV='3'                                               03100000
* %         THEN                                                        03150000
* %           XE=3           /* SET 1ST LEV AS SPECIFIED @E2115B7*/ ;   03200000
* %       IF TPLESLEV='4'                                               03250000
* %         THEN                                                        03300000
* %           XE=4           /* SET 1ST LEV AS SPECIFIED @E2115B7*/ ;   03350000
* %       IF TPLESLEV='5'                                               03400000
* %         THEN                                                        03450000
* %           XE=5           /* SET 1ST LEV AS SPECIFIED @E2115B7*/ ;   03500000
* %       GOTO TPLBSLE2;     /*                          @E2115B7*/     03550000
* %TPLBSLE1: /* BEGIN A BSL STRUCTURE - 'TPLESLEV' NOT SPECIFIED */ ;   03600000
* %       YE=2               /* USE THESE DEFAULT VALUES @E2115B7*/ ;   03650000
* %       ZE=3               /* USE THESE DEFAULT VALUES @E21D2I6*/ ;   03700000
* %       WE=4               /* USE THESE DEFAULT VALUES @E21D2I6*/ ;   03750000
* /*******************************************************************/ 03800000
