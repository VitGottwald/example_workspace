* %       IF TPLEOPT1='CONTINUE'     /*                      @E2115B7*/ 06200000
* %         THEN                                                        06250000
* %           GOTO TPLBSLE4;         /*                      @E2115B7*/ 06300000
* %         ELSE                                                        06350000
* %           GOTO TPLBSLE5;         /*                      @E2115B7*/ 06400000
* %TPLBSLE4: /* STRUCTURE IS TO BE CONTINUED - DO NOT CLOSE    */ ;     06450000
*       YE TPLERSVD CHAR(16),        /* RESERVED             @E21D2I6*/ 06500000
* %       GOTO TPLBSLE6;                                                06550000
* %TPLBSLE5: /* STRUCTURE IS COMPLETED - USE SEMI-COLON        */ ;     06600000
*       YE TPLERSVD CHAR(16);        /* RESERVED             @E21D2I6*/ 06650000
*                                                                       06700000
* %TPLBSLE6: /* DEACTIVATE THE COMPILE TIME VARIABLES          */ ;     06750000
* %       DEACTIVATE XE,YE,ZE,WE;   /*                       @E21D2I6*/ 06800000
