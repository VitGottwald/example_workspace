* DECLARE                                                               05100000
*   1 TPLE BASED(TPLTPLE),                                              05150000
* %       GOTO TPLBSLE3;             /*                      @E2115B7*/ 05200000
* %TPLBSLE2: /* CONTINUE  USING LEVEL SPECIFIED        @E2115B7*/ ;     05250000
* %       YE=XE+1        /* SET SECOND LEVEL ONE HIGHER  @E2115B7*/ ;   05300000
* %       ZE=YE+1        /* SET THIRD LEVEL ONE HIGHER   @E21D2I6*/ ;   05350000
* %       WE=ZE+1        /* SET FOURTH LEVEL ONE HIGHER  @E21D2I6*/ ;   05400000
*     XE TPLE,                       /*                      @E2115B7*/ 05450000
* %TPLBSLE3: /* THIS IS THE MAIN TPLE STRUCTURE        @E2115B7*/ ;     05500000
*       YE TPLETDCB PTR(31),         /* PTR TO THE TEST DCB  @E2115B7*/ 05550000
*       YE TPLEFLGS,                 /* TPLE FLAG FIELDS     @E21D2I6*/ 05600000
*          ZE TPLEFLG1,              /* TPLE FLAG1 FIELD     @E21D2I6*/ 05650000
*            WE TPLETSTA BIT(1),     /* TESTAUTH WAS THE COMMAND ENTERED05700000
*                                                            @E21D2I6*/ 05750000
*            WE *        BIT(7),     /* RESERVED FLAGS       @E21D2I6*/ 05800000
*          ZE *    CHAR(3),          /* TPLE RESERVED FLAGS  @E21D2I6*/ 05850000
*       YE TPLENCBF PTR(31),         /* PTR TO THE TESTAUTH INITIALIZA- 05900000
*                                       TION EXIT NEW COMMAND BUFFER    05950000
