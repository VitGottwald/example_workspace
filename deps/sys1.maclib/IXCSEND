.***AC.PLV01  PROCESS PLISTVER                                      *   00271000
.********************************************************************   00271100
&XPLISTVER      SETA  0                                                 00271200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00271300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00271400
&XPLISTVER      SETA  0                                                 00271500
.ZPLV0          ANOP  ,                                                 00271600
.*                                                                      00271700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00271800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00271900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00272000
.*-------------------------------------------------------------------   00272100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00272200
&ZMS            SETC  '"PLISTVER='                                      00272300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00272400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00272500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00272600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00272700
                AGO   .ZPVCK2                                           00272800
.ZPVCK3         ANOP  ,                                                 00272900
.*-------------------------------------------------------------------   00273000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00273100
&ZMS            SETC  '"PLISTVER='                                      00273200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00273300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00273400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00273500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00273600
                AGO   .ZPVCKE                                           00273700
.ZPVCK2         ANOP  ,                                                 00273800
.*-------------------------------------------------------------------   00273900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00274000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00274100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00274200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00274300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00274400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00274500
.ZPVCK6         ANOP  ,                                                 00274600
&ZMS            SETC  '"PLISTVER='                                      00274700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00274800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00274900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00275000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00275100
&XPLISTVER      SETA  0                                                 00275200
                AGO   .ZPVCKE                                           00275300
.*-------------------------------------------------------------------   00275400
.ZPVCK4A        ANOP  ,                                                 00275500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00275600
&ZMS            SETC  'PLISTVER='                                       00275700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00275800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00275900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00276000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00276100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00276200
                AGO   .ZPVCK5                                           00276300
.ZPVCK4         ANOP  ,                                                 00276400
&XPLISTVER      SETA  &PLISTVER(1)                                      00276500
.ZPVCK5         ANOP  ,                                                 00276600
.ZPVCKE         ANOP  ,                                                 00276700
.********************************************************************   00276800
