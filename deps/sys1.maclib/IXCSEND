.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00810700
.*******************************************************************    00810800
                      ACTR  10000                                       00810900
.*                                                                      00811000
                      LCLA  &ZLC           COLUMN POSITION              00811100
                      LCLA  &ZOPX          LIST TABLE INDEX             00811200
                      LCLC  &ZLL           LABEL                        00811300
                      LCLC  &ZLOP          OPERATION                    00811400
                      LCLC  &ZLV           VALUE                        00811500
                      LCLC  &ZLD           DESCRIPTION                  00811600
.*                                                                      00811700
&ZOPX                 SETA  1                                           00811800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00811900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00812000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00812100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00812200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00812300
                      AIF   (K'&ZLD LE 0).ZL5                           00812400
&ZLC                  SETA  35             COMMENT COLUMN               00812500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00812600
&ZLC                  SETA  33             COMMMENT COLUMN              00812700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00812800
&ZLV                  SETC  '&ZLV'.' '                                  00812900
                      AGO   .ZL3                                        00813000
.*                                                                      00813100
.ZL4                  ANOP  ,                                           00813200
&ZLV                  SETC  '&ZLV'.' '                                  00813300
.ZL5                  ANOP  ,                                           00813400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00813500
.*                                                                      00813600
&ZLL     &ZLOP &ZLV                                                     00813700
.*                                                                      00813800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00813900
                      AGO   .ZL2           CONTINUE LOOPING             00814000
.*                                                                      00814100
.ZLX                  ANOP  ,                                           00814200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00814300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00814400
         AIF   ('&SYSAM64' NE 'YES').NOTAM64                       @P1A 00814500
         LLGTR 14,14                   CLEAR HIGH HALF             @P1A 00814600
.NOTAM64 L     14,16(0,0)              GET CVT ADDRESS             @P1C 00814700
         L     14,772(14,0)            GET SFT ADDRESS                  00814800
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00814900
         LA    15,55                   GET EX FOR SERVICE ROUTINE       00815000
         OR    14,15                   GET PC NUMBER                    00815100
         PC    0(14)                                                    00815200
