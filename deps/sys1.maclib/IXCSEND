.*                                                                      00551600
&ZOPN                 SETA  &ZOPN+1                                     00551700
&ZOPL(&ZOPN)          SETC  ' '                                         00551800
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00551900
&ZOPV(&ZOPN)          SETC  '4,8'                                       00552000
&ZOPN                 SETA  &ZOPN+1                                     00552100
&ZOPL(&ZOPN)          SETC  ' '                                         00552200
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00552300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00552400
&ZOPD(&ZOPN)          SETC  '++ IXCSEND ARG PARMS'                      00552500
.*                                                                      00552600
&ZOPN                 SETA  &ZOPN+1                                     00552700
&ZOPL(&ZOPN)          SETC  ' '                                         00552800
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00552900
&ZOPV(&ZOPN)          SETC  '1'                                         00553000
&ZOPD(&ZOPN)          SETC  ' '                                         00553100
.*                                                                      00553200
.ZLMFSL2              ANOP  ,                                           00553300
