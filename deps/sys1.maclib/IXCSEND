.***AI.L01 IXCSEND FMT=LIST FILL IN PARAMETER AREA                      00634900
.******************************************************************     00635000
                      ACTR  10000                                       00635100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00635200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00635300
&ZCVAL                SETC  'LAE'          XAX MODE                     00635400
.ZIMFXA               ANOP ,                                            00635500
.*                                                                      00635600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00635700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00635800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00635900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00636000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00636100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00636200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00636300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00636400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00636500
&ZOPN                 SETA  &ZOPN+1                                     00636600
&ZOPL(&ZOPN)          SETC  ' '                                         00636700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00636800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00636900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00637000
                      AGO   .ZIMFNX                                     00637100
.*                                                                      00637200
.ZIMFN2               ANOP ,                                            00637300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00637400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00637500
&ZOPN                 SETA  &ZOPN+1                                     00637600
&ZOPL(&ZOPN)          SETC  ' '                                         00637700
&ZOPO(&ZOPN)          SETC  'LAE'                                       00637800
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00637900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00638000
                      AGO   .ZIMFNX                                     00638100
.ZIMFNX1              ANOP ,                                            00638200
&ZOPN                 SETA  &ZOPN+1                                     00638300
&ZOPL(&ZOPN)          SETC  ' '                                         00638400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00638500
