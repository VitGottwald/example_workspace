.*                                                                      00110400
.ZLMFSLX              ANOP  ,                                           00110500
.*-------------------------------------------------------------------   00110600
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00110700
&ZPLLEN               SETA 24                                           00110800
.*                                                                      00110900
.******************************************************************     00111000
.***AI.L01 IXCMOD FMT=LIST FILL IN PARAMETER AREA                       00111100
.******************************************************************     00111200
                      ACTR  10000                                       00111300
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00111400
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00111500
&ZCVAL                SETC  'LAE'          XAX MODE                     00111600
.ZIMFXA               ANOP ,                                            00111700
.*                                                                      00111800
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00111900
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00112000
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00112100
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00112200
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00112300
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00112400
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00112500
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00112600
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00112700
&ZOPN                 SETA  &ZOPN+1                                     00112800
&ZOPL(&ZOPN)          SETC  ' '                                         00112900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00113000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00113100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00113200
                      AGO   .ZIMFNX                                     00113300
.*                                                                      00113400
.ZIMFN2               ANOP ,                                            00113500
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00113600
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00113700
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00113800
&ZOPN                 SETA  &ZOPN+1                                     00113900
&ZOPL(&ZOPN)          SETC  ' '                                         00114000
&ZOPO(&ZOPN)          SETC  'LAE'                                       00114100
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00114200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00114300
                      AGO   .ZIMFNX                                     00114400
.ZIMFNX1              ANOP ,                                            00114500
&ZOPN                 SETA  &ZOPN+1                                     00114600
&ZOPL(&ZOPN)          SETC  ' '                                         00114700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00114800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00114900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00115000
.ZIMFNX               ANOP ,                                            00115100
.********************************************************************   00115200
.***AI.L02 IXCMOD FMT=LIST INITIALIZE PARAMETER AREA                    00115300
