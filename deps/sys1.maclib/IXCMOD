.ZALVL6B              ANOP                                              00101400
&ZOPD(&ZOPN)          SETC  '++ IXCMOD ARG PARMS'                       00101500
.*                                                                      00101600
&ZOPN                 SETA  &ZOPN+1                                     00101700
&ZOPL(&ZOPN)          SETC  ' '                                         00101800
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00101900
&ZOPV(&ZOPN)          SETC  '1'                                         00102000
&ZOPD(&ZOPN)          SETC  ' '                                         00102100
.*                                                                      00102200
.ZLMFSL2              ANOP  ,                                           00102300
.*-------------------------------------------------------------------   00102400
.***AM.L00  IXCMOD FMT=LIST name                                        00102500
