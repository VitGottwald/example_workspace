&ZCGPR        SETC  '&XRETCODE'(2,K'&XRETCODE-2)                        00086600
              AIF   ('&ZCGPR'(1,1) NE '0').ZG8P                         00086700
              AIF   (K'&ZCGPR GT 2).ZG8Q                                00086800
              AIF   ('&ZCGPR' EQ '0').ZG8Q                              00086900
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00087000
              AIF   (T'&ZCGPR NE 'N').ZG8Q      INVALID REG NAME        00087100
.ZG8P         ANOP  ,                                                   00087200
              AIF   (T'&ZCGPR NE 'N').ZG8R      NON NUMERIC             00087300
              AIF   ('&ZCGPR' EQ '15').ZG8R                             00087400
              AIF   ('&ZCGPR' LT '2').ZG8Q                              00087500
              AIF   ('&ZCGPR' LE '12').ZG8R                             00087600
.ZG8Q         ANOP  ,                                                   00087700
&ZMS          SETC  '"RETCODE='                                         00087800
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00087900
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00088000
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12) OR (15).'            00088100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00088200
.ZG8R         ANOP  ,                                                   00088300
.*----------------------------------------------------------------***   00088400
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00088500
              AIF   (N'&RSNCODE LE 1).ZG11A     CK NUM                  00088600
&ZMS          SETC  '"RSNCODE='                                         00088700
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00088800
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00088900
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00089000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00089100
.ZG11A        ANOP  ,                                                   00089200
.*-------------------------------------------------------------------   00089300
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00089400
              AIF   (K'&RSNCODE(1) LE 0).ZG11C  NO PARM                 00089500
&ZXRSNCODE    SETC  'OUTPUT'    SET OUTPUT                              00089600
&XRSNCODE     SETC  '&RSNCODE(1)'               SET XVARIABLE           00089700
              AIF   (N'&RSNCODE GT 1).ZG11C                             00089800
&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00089900
.ZG11C        ANOP  ,                                                   00090000
.*-------------------------------------------------------------------   00090100
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00090200
              AIF   (K'&ZXRSNCODE LE 0).ZG11R   NOT I/O                 00090300
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG11R                     00090400
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00090500
              AIF   ('&ZCGPR'(1,1) NE '0').ZG11P                        00090600
              AIF   (K'&ZCGPR GT 2).ZG11Q                               00090700
              AIF   ('&ZCGPR' EQ '0').ZG11R                             00090800
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00090900
              AIF   (T'&ZCGPR NE 'N').ZG11Q     INVALID REG NAME        00091000
.ZG11P        ANOP  ,                                                   00091100
              AIF   (T'&ZCGPR NE 'N').ZG11R     NON NUMERIC             00091200
              AIF   ('&ZCGPR' EQ '0').ZG11R                             00091300
              AIF   ('&ZCGPR' LT '2').ZG11Q                             00091400
              AIF   ('&ZCGPR' LE '12').ZG11R                            00091500
.ZG11Q        ANOP  ,                                                   00091600
&ZMS          SETC  '"RSNCODE='                                         00091700
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00091800
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00091900
              MNOTE 8,' &ZMS IN THE RANGE (0) OR (2)-(12).'             00092000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00092100
.ZG11R        ANOP  ,                                                   00092200
.******************************************************************     00092300
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00092400
.******************************************************************     00092500
              ACTR  10000                                               00092600
.*----------------------------------------------------------------***   00092700
.***AH.P02 EXTRA POSITIONALS                                            00092800
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00092900
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00093000
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00093100
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00093200
.ZGLISTX1     ANOP  ,                                                   00093300
&ZCVAL        SETC  ''          ERROR FIELD                             00093400
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00093500
.ZGLIST2      ANOP  ,                                                   00093600
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00093700
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00093800
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00093900
              AGO   .ZGLIST2                                            00094000
.*                                                                      00094100
.ZGLIST3      ANOP  ,                                                   00094200
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00094300
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'                           00094400
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00094500
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00094600
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00094700
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00094800
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00094900
.ZGLISTX      ANOP  ,                                                   00095000
&ZASYSLNDX    SETA  0                                                   00095100
.*-------------------------------------------------------------------   00095200
.***AG.MF04 MF=(SLE)                                                    00095300
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00095400
.*******************************************************************    00095500
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00095600
.*******************************************************************    00095700
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00095800
.*******************************************************************    00095900
.***AE.01  START OF IXCMOD MACRO CODE                                   00096000
.*******************************************************************    00096100
              LCLC  &M0M0001                 NAME                       00096200
&M0M0001      SETC  '&XLABEL'                SET NAME                   00096300
              AIF   (K'&XLABEL NE 0).XLAB2                              00096400
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00096500
.*                                                                      00096600
.XLAB2        ANOP  ,                                                   00096700
              AIF   ('&XMF' EQ 'L').XLAB3                               00096800
&M0M0001 DS    0H                             IXCMOD-0                  00096900
.XLAB3        ANOP  ,                                                   00097000
.*******************************************************************    00097100
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00097200
.*******************************************************************    00097300
              PUSH  PRINT                                               00097400
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00097500
              PRINT OFF         ++ PR01                                 00097600
              AGO   .ZPRNTXX                                            00097700
.ZPRINTX      PRINT ON                                                  00097800
.ZPRNTXX      ANOP  ,                                                   00097900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00098000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00098100
.********************************************************************   00098200
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00098300
.********************************************************************   00098400
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00098500
                      LCLA  &ZOPN1         INVOCATION INDEX             00098600
                      LCLC  &ZOPL(4095)    LABEL                        00098700
                      LCLC  &ZOPO(4095)    OPERATION                    00098800
                      LCLC  &ZOPV(4095)    VALUE                        00098900
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00099000
.*                                                                      00099100
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00099200
.******************************************************************     00099300
.***AL.L10 IXCMOD FMT=LIST MF=S!L PARAMETERS                            00099400
.******************************************************************     00099500
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00099600
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00099700
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00099800
&XMFATTR              SETC  '0D'                                        00099900
.*                                                                      00100000
&ZOPN                 SETA  &ZOPN+1                                     00100100
&ZOPL(&ZOPN)          SETC  ' '                                         00100200
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00100300
&ZOPO(&ZOPN)          SETC  'B'                                         00100400
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00100500
                      AGO   .ZALVL6B                                    00100600
.ZALVL6A              ANOP                                              00100700
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00100800
&ZOPV(&ZOPN)          SETC  '4,8'                                       00100900
&ZOPN                 SETA  &ZOPN+1                                     00101000
