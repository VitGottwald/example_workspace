.*******************************************************************    00122500
.***  PROCESS IXCMOD OUTPUT PARAMETERS                                  00122600
.*******************************************************************    00122700
                      ACTR  10000                                       00122800
.*******************************************************************    00122900
.***AO.X02  RETURN CODE PROCESSING                                      00123000
.*******************************************************************    00123100
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00123200
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00123300
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00123400
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00123500
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00123600
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00123700
&ZOPN                 SETA  &ZOPN+1                                     00123800
&ZOPL(&ZOPN)          SETC  ' '                                         00123900
&ZOPO(&ZOPN)          SETC  'ST'                                        00124000
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00124100
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00124200
                      AGO   .ZMRETX                                     00124300
.*                                                                      00124400
.ZMRET2               ANOP  ,                                           00124500
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00124600
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00124700
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00124800
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00124900
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00125000
&ZOPN                 SETA  &ZOPN+1                                     00125100
&ZOPL(&ZOPN)          SETC  ' '                                         00125200
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00125300
