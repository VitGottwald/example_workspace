                      ACTR  10000                                       00129800
.*                                                                      00129900
                      LCLA  &ZLC           COLUMN POSITION              00130000
                      LCLA  &ZOPX          LIST TABLE INDEX             00130100
                      LCLC  &ZLL           LABEL                        00130200
                      LCLC  &ZLOP          OPERATION                    00130300
                      LCLC  &ZLV           VALUE                        00130400
                      LCLC  &ZLD           DESCRIPTION                  00130500
.*                                                                      00130600
&ZOPX                 SETA  1                                           00130700
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00130800
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00130900
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00131000
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00131100
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00131200
                      AIF   (K'&ZLD LE 0).ZL5                           00131300
&ZLC                  SETA  35             COMMENT COLUMN               00131400
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00131500
&ZLC                  SETA  33             COMMMENT COLUMN              00131600
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00131700
&ZLV                  SETC  '&ZLV'.' '                                  00131800
                      AGO   .ZL3                                        00131900
.*                                                                      00132000
.ZL4                  ANOP  ,                                           00132100
&ZLV                  SETC  '&ZLV'.' '                                  00132200
.ZL5                  ANOP  ,                                           00132300
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00132400
.*                                                                      00132500
&ZLL     &ZLOP &ZLV                                                     00132600
.*                                                                      00132700
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00132800
                      AGO   .ZL2           CONTINUE LOOPING             00132900
.*                                                                      00133000
.ZLX                  ANOP  ,                                           00133100
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00133200
