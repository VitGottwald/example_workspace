.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00147300
.*******************************************************************    00147400
                      ACTR  10000                                       00147500
.*                                                                      00147600
                      LCLA  &ZLC           COLUMN POSITION              00147700
                      LCLA  &ZOPX          LIST TABLE INDEX             00147800
                      LCLC  &ZLL           LABEL                        00147900
                      LCLC  &ZLOP          OPERATION                    00148000
                      LCLC  &ZLV           VALUE                        00148100
                      LCLC  &ZLD           DESCRIPTION                  00148200
.*                                                                      00148300
&ZOPX                 SETA  1                                           00148400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00148500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00148600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00148700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00148800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00148900
                      AIF   (K'&ZLD LE 0).ZL5                           00149000
&ZLC                  SETA  35             COMMENT COLUMN               00149100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00149200
&ZLC                  SETA  33             COMMMENT COLUMN              00149300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00149400
&ZLV                  SETC  '&ZLV'.' '                                  00149500
                      AGO   .ZL3                                        00149600
.*                                                                      00149700
.ZL4                  ANOP  ,                                           00149800
&ZLV                  SETC  '&ZLV'.' '                                  00149900
.ZL5                  ANOP  ,                                           00150000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00150100
.*                                                                      00150200
&ZLL     &ZLOP &ZLV                                                     00150300
.*                                                                      00150400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00150500
                      AGO   .ZL2           CONTINUE LOOPING             00150600
.*                                                                      00150700
.ZLX                  ANOP  ,                                           00150800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00150900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00151000
         L     14,16(0,0)              GET CVT ADDRESS                  00151100
         L     14,772(14,0)            GET SFT ADDRESS                  00151200
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00151300
         LA    15,3                    GET EX FOR SERVICE ROUTINE       00151400
         OR    14,15                   GET PC NUMBER                    00151500
         PC    0(14)                                                    00151600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00151700
