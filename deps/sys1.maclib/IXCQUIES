&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00142300
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00142400
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00142500
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00142600
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00142700
