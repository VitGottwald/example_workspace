.***AC.PLV01  PROCESS PLISTVER                                      *   00054000
.********************************************************************   00054100
&XPLISTVER      SETA  0                                                 00054200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00054300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00054400
&XPLISTVER      SETA  0                                                 00054500
.ZPLV0          ANOP  ,                                                 00054600
.*                                                                      00054700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00054800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00054900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00055000
.*-------------------------------------------------------------------   00055100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00055200
&ZMS            SETC  '"PLISTVER='                                      00055300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00055400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00055500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00055600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055700
                AGO   .ZPVCK2                                           00055800
.ZPVCK3         ANOP  ,                                                 00055900
.*-------------------------------------------------------------------   00056000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00056100
&ZMS            SETC  '"PLISTVER='                                      00056200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00056300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00056400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00056500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00056600
                AGO   .ZPVCKE                                           00056700
.ZPVCK2         ANOP  ,                                                 00056800
.*-------------------------------------------------------------------   00056900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00057000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00057100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00057200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00057300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00057400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00057500
.ZPVCK6         ANOP  ,                                                 00057600
&ZMS            SETC  '"PLISTVER='                                      00057700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00057800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00057900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00058000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00058100
&XPLISTVER      SETA  0                                                 00058200
                AGO   .ZPVCKE                                           00058300
.*-------------------------------------------------------------------   00058400
.ZPVCK4A        ANOP  ,                                                 00058500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00058600
&ZMS            SETC  'PLISTVER='                                       00058700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00058800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00058900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00059000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00059100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00059200
                AGO   .ZPVCK5                                           00059300
.ZPVCK4         ANOP  ,                                                 00059400
&XPLISTVER      SETA  &PLISTVER(1)                                      00059500
.ZPVCK5         ANOP  ,                                                 00059600
.ZPVCKE         ANOP  ,                                                 00059700
