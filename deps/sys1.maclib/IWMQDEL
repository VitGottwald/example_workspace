.***AC.PLV01  PROCESS PLISTVER                                      *   00061900
.********************************************************************   00062000
&XPLISTVER      SETA  0                                                 00062100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00062200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00062300
&XPLISTVER      SETA  0                                                 00062400
.ZPLV0          ANOP  ,                                                 00062500
.*                                                                      00062600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00062700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00062800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00062900
.*-------------------------------------------------------------------   00063000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00063100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00063200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00063400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00063500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063600
                AGO   .ZPVCK2                                           00063700
.ZPVCK3         ANOP  ,                                                 00063800
.*-------------------------------------------------------------------   00063900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00064000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00064100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00064300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00064400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064500
                AGO   .ZPVCKE                                           00064600
.ZPVCK2         ANOP  ,                                                 00064700
.*-------------------------------------------------------------------   00064800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00064900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00065000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00065100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00065200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00065300
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00065400
.ZPVCK6         ANOP  ,                                                 00065500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00065600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065700
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00065800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00065900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066000
&XPLISTVER      SETA  0                                                 00066100
                AGO   .ZPVCKE                                           00066200
.*-------------------------------------------------------------------   00066300
.ZPVCK4A        ANOP  ,                                                 00066400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00066500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00066600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00066700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00066800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00066900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00067000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067100
                AGO   .ZPVCK5                                           00067200
.ZPVCK4         ANOP  ,                                                 00067300
&XPLISTVER      SETA  &PLISTVER(1)                                      00067400
.ZPVCK5         ANOP  ,                                                 00067500
.ZPVCKE         ANOP  ,                                                 00067600
.********************************************************************   00067700
