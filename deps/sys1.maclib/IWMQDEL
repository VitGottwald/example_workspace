.******************************************************************     00123800
.***AI.L01 IWMQDEL FMT=LIST FILL IN PARAMETER AREA                      00123900
.******************************************************************     00124000
                      ACTR  10000                                       00124100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00124200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00124300
&ZCVAL                SETC  'LAE'          XAX MODE                     00124400
.ZIMFXA               ANOP ,                                            00124500
.*                                                                      00124600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00124700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00124800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00124900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00125000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00125100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00125200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00125300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00125400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00125500
&ZOPN                 SETA  &ZOPN+1                                     00125600
&ZOPL(&ZOPN)          SETC  ' '                                         00125700
