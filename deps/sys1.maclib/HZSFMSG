&ZCVAL          SETC  '&PLISTVER(1)'                                    00360700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00360800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00360900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00361000
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00361100
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00361200
.ZPVCK6         ANOP  ,                                                 00361300
&ZMS            SETC  '"PLISTVER='                                      00361400
