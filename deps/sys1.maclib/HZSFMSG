*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  02058800
*%HZSFMSG: MACRO KEYS(REQUEST,MGBADDR,MGB,REMOTE,MSGTABLE,HANDLE,REXX,  02058900
*                 SEVERITY,SEVERITYVAL,REASON,ID,IDLEN,TEXT,TEXTLEN,    02059000
*                 EXPL,EXPLLEN,SYSACT,SYSACTLEN,ORESP,ORESPLEN,SPRESP,  02059100
*                 SPRESPLEN,PROBD,PROBDLEN,SOURCE,SOURCELEN,REFDOC,     02059200
*                 REFDOCLEN,AUTOMATION,AUTOMATIONLEN,DIAG,ABENDRESULT,  02059300
*                 MGBFORMAT,RETCODE,RSNCODE,PLISTVER,MF);               02059400
*  ANS('?'!!MACLABEL!!' '!!'HZSFMSGP '!!MACLIST!!                       02059500
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              02059600
*%END HZSFMSG;                                                          02059700
