         MACRO                                                          00322800
&XLABEL  HZSFMSG &REQUEST=,&MGBADDR=,&MGB=,&REMOTE=,&MSGTABLE=,        *00322900
               &HANDLE=,&REXX=,&SEVERITY=,&SEVERITYVAL=,&REASON=,&ID=, *00323000
               &IDLEN=,&TEXT=,&TEXTLEN=,&EXPL=,&EXPLLEN=,&SYSACT=,     *00323100
               &SYSACTLEN=,&ORESP=,&ORESPLEN=,&SPRESP=,&SPRESPLEN=,    *00323200
               &PROBD=,&PROBDLEN=,&SOURCE=,&SOURCELEN=,&REFDOC=,       *00323300
               &REFDOCLEN=,&AUTOMATION=,&AUTOMATIONLEN=,&DIAG=,        *00323400
               &ABENDRESULT=,&MGBFORMAT=,&RETCODE=,&RSNCODE=,          *00323500
               &PLISTVER=,&MF=                                          00323600
.*                                                                      00323700
*                    MACDATE -03/05/17-<2>                              00323800
.*                                                                      00323900
.*******************************************************************    00324000
.***AB.02  GLOBAL VARIABLES TO CONTROL PRINT AND CODE GENERATION        00324100
.*******************************************************************    00324200
                    GBLC  &ZPRINT     YES=PRINT NO=NO PRINT             00324300
                    GBLC  &ZTEST      YES=TEST NO=NO TEST               00324400
                    GBLC  &ZSYSTEM    PCCODE10, MVS, SAK, VM, XAX       00324500
.*                                                                      00324600
                    GBLC  &ZHZSFMSG   YES=MACRO USED AT LEAST ONCE      00324700
.*******************************************************************    00324800
.***AB.03  COMMONLY USED LOCAL MACRO VARIABLES                          00324900
.*******************************************************************    00325000
                    LCLC  &ZMS        ERROR MESSAGE WORK AREA           00325100
                    LCLC  &ZCNDX      SYSNDX CHARACTERS                 00325200
                    LCLC  &ZCLNG      USED WHEN LENGTH IS NEEDED        00325300
                    LCLC  &ZCGPR      USED WHEN VALUE IS IN A GPR       00325400
                    LCLC  &ZCVAL      USED TO CONTAIN FIXED VALUE       00325500
                    LCLA  &ZAWRK      USED AS A WORK AREA               00325600
                    LCLA  &ZASIZ      USED FOR SIZE OF VALUE            00325700
                    LCLA  &ZN         NUMERIC WORK VARIABLE             00325800
                    LCLC  &ZC         CHARACTER WORK VARIABLE           00325900
                    LCLC  &ZTLC       CHARACTER TOO LONG VARIABLE       00326000
                    LCLA  &ZTLA       VALUE TOO LONG VARIABLE           00326100
                    LCLC  &ZMAC_G     "G" or null                       00326200
&ZMAC_G             SETC  ''                                            00326300
                    LCLC  &ZMAC_LR                                      00326400
&ZMAC_LR            SETC  'LR'                                          00326500
                    LCLC  &ZMAC_L                                       00326600
&ZMAC_L             SETC  'L'                                           00326700
                    LCLC  &ZMAC_ST                                      00326800
&ZMAC_ST            SETC  'ST'                                          00326900
                    LCLC  &ZMAC_LLGT                                    00327000
&ZMAC_LLGT          SETC  'L'                                           00327100
                    LCLC  &ZMAC_LGR                                     00327200
&ZMAC_LGR           SETC  'LR'                                          00327300
                    LCLC  &ZMAC_LG                                      00327400
&ZMAC_LG            SETC  'L'                                           00327500
                    LCLC  &ZMAC_STG                                     00327600
&ZMAC_STG           SETC  'ST'                                          00327700
                    LCLA  &ZMACRET    MACRO RETURN CODE                 00327800
&ZMACRET            SETA  0           ASSUME NO ERRORS                  00327900
.*                                                                      00328000
                    LCLC  &ZCSYSLIST(255)       NONXKEY, POS ARRAY      00328100
                    LCLA  &ZASYSLNDX  WORK INDEX FOR ZCSYSLIST()        00328200
                    LCLA  &ZPLLEN     PARAMETER LIST LENGTH             00328300
                    LCLC  &ZENDBASEPL           End of base PL          00328400
