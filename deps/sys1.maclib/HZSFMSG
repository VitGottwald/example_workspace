.*******************************************************************    02047000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               02047100
.*******************************************************************    02047200
                      ACTR  10000                                       02047300
.*                                                                      02047400
                      LCLA  &ZLC           COLUMN POSITION              02047500
                      LCLA  &ZOPX          LIST TABLE INDEX             02047600
                      LCLC  &ZLL           LABEL                        02047700
                      LCLC  &ZLOP          OPERATION                    02047800
                      LCLC  &ZLV           VALUE                        02047900
                      LCLC  &ZLD           DESCRIPTION                  02048000
.*                                                                      02048100
&ZOPX                 SETA  1                                           02048200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            02048300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              02048400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              02048500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              02048600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              02048700
                      AIF   (K'&ZLD LE 0).ZL5                           02048800
&ZLC                  SETA  35             COMMENT COLUMN               02048900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    02049000
&ZLC                  SETA  33             COMMMENT COLUMN              02049100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   02049200
&ZLV                  SETC  '&ZLV'.' '                                  02049300
                      AGO   .ZL3                                        02049400
.*                                                                      02049500
.ZL4                  ANOP  ,                                           02049600
&ZLV                  SETC  '&ZLV'.' '                                  02049700
.ZL5                  ANOP  ,                                           02049800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               02049900
.*                                                                      02050000
&ZLL     &ZLOP &ZLV                                                     02050100
.*                                                                      02050200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               02050300
                      AGO   .ZL2           CONTINUE LOOPING             02050400
.*                                                                      02050500
.ZLX                  ANOP  ,                                           02050600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                02050700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                02050800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02050900
         AIF   ('&SYSASCE' EQ 'P').NOT_AR_1                             02051000
         LAE   15,0(15,0)           Clear AR 15                         02051100
.NOT_AR_1 ANOP                                                          02051200
         AIF   ('&SYSAM64' NE 'YES').NOT_AM64                           02051300
&ZMAC_L  SETC  'LLGT'                                                   02051400
.NOT_AM64 ANOP                                                          02051500
         &ZMAC_L 15,16(0,0)         Get CVT address                     02051600
         L     15,140(15,0)         Get ECVT address                    02051700
         L     15,204(15,0)         Get CTBL address                    02051800
         AIF   ('&XREMOTE' EQ 'YES').REM1A                              02051900
         L     15,300(15,0)         Get HZSTABLE address                02052000
         AGO   .REM1B                                                   02052100
.REM1A   ANOP                                                           02052200
         ICM   15,B'1111',300(15)   Get HZSTABLE address                02052300
         JZ    HZSFMSGNA_&SYSNDX    No HZSTABLE, get out now            02052400
         TM    1240(15),X'80'       Check if commands accepted          02052500
         JZ    HZSFMSGNA_&SYSNDX    No cmds accepted, get out now       02052600
.REM1B   ANOP                                                           02052700
         AIF   (&SYSALVL GE 2).IS_ZARCH_1                               02052800
         CLI   163(0),00            Z-Architecture?                     02052900
         JE    HZSFMSGESA390_&SYSNDX                                    02053000
.IS_ZARCH_1 ANOP                                                        02053100
         LMH   15,15,1372(15)                                           02053200
         AIF   (&SYSALVL GE 2).IS_ZARCH_2                               02053300
HZSFMSGESA390_&SYSNDX DS 0H                                             02053400
.IS_ZARCH_2 ANOP                                                        02053500
         AIF   ('&XREMOTE' EQ 'YES').REM2A                              02053600
         L     15,1308(15,0)        Get HZSFMSG  PC NUM                 02053700
         AGO   .REM2B                                                   02053800
.REM2A   ANOP                                                           02053900
         ICM   15,B'1111',1308(15)  Get HZSFMSG PC NUM                  02054000
         JZ    HZSFMSGNA_&SYSNDX    No PC Num, get out now              02054100
.REM2B   ANOP                                                           02054200
         PC    0(15)                PC to HZSFMSG Mod                   02054300
         AIF   ('&XREMOTE' NE 'YES').REM3B                              02054400
         J     HZSFMSGEND_&SYSNDX   Branch around seting bad RC         02054500
HZSFMSGNA_&SYSNDX DS 0H                                                 02054600
         LA    15,X'C'              Set RC (HC not active)              02054700
         LA    0,X'C01'             Set RSN not active                  02054800
HZSFMSGEND_&SYSNDX DS 0H                                                02054900
.REM3B   ANOP                                                           02055000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02055100
.ZLINKX               ANOP  ,                                           02055200
