.*******************************************************************    00328600
.***AB.04  MACRO VARIABLES FOR RANGES AND SETS OF ARGUMENTS             00328700
.*******************************************************************    00328800
                    LCLA  &ZI         INDEX FOR ARRAY                   00328900
                    LCLA  &ZJ         INDEX FOR PARMS                   00329000
                    LCLA  &ZK         INDEX FOR PARSE                   00329100
                    LCLA  &ZLO        LOW RANGE VALUE                   00329200
                    LCLA  &ZHI        HIGH RANGE VALUE                  00329300
                    LCLC  &ZW         WORK AREA FOR PARM                00329400
.********************************************************************   00329500
.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00329600
.********************************************************************   00329700
                    GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL           00329800
                    GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE    00329900
                    GBLA  &SYSALVL    SYSSTATE ARCHLVL                  00330000
                    GBLC  &SYSAM64    SYSSTATE AMODE64                  00330100
.*                                                                      00330200
                    AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED    00330300
                    AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED     00330400
                    SPLEVEL  TEST                                       00330500
.ZSYS2              ANOP  ,                                             00330600
                    AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1         00330700
                    AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1         00330800
                    SYSSTATE TEST                                       00330900
                    AGO   .ZSYS4                                        00331000
.ZSYS3              ANOP  ,                                             00331100
&SYSASCE            SETC  'P'         ASSUME SYSSTATE PRIMARY           00331200
.ZSYS4              ANOP  ,                                             00331300
                    AIF   ('&SYSAM64' NE 'YES').ZSYSN64                 00331400
&ZMAC_LLGT          SETC 'LLGT'                                         00331500
&ZMAC_LGR           SETC 'LGR'                                          00331600
&ZMAC_LG            SETC 'LG'                                           00331700
&ZMAC_STG           SETC 'STG'                                          00331800
.ZSYSN64            ANOP  ,                                             00331900
.*******************************************************************    00332000
.***AB.07  DEFINE ALL MACRO VARIABLES                                   00332100
.*******************************************************************    00332200
                    LCLC  &XREQUEST   ++ VALUE                          00332300
                    LCLC  &XDUMMY99   ++ NAME                           00332400
                    LCLC  &XMGBADDR   ++ NAME                           00332500
                    LCLC  &XMGB       ++ NAME                           00332600
                    LCLC  &XREMOTE    ++ VALUE                          00332700
                    LCLC  &XMSGTABLE  ++ NAME                           00332800
                    LCLC  &XHANDLE    ++ NAME                           00332900
                    LCLC  &XREXX      ++ VALUE                          00333000
                    LCLC  &XSEVERITY  ++ VALUE                          00333100
                    LCLC  &XSEVERITYVAL         ++ NAME                 00333200
                    LCLC  &XREASON    ++ VALUE                          00333300
                    LCLC  &XID        ++ NAME                           00333400
                    LCLC  &XIDLEN     ++ NAME                           00333500
                    LCLC  &XTEXT      ++ NAME                           00333600
                    LCLC  &XTEXTLEN   ++ NAME                           00333700
                    LCLC  &XEXPL      ++ NAME                           00333800
                    LCLC  &XEXPLLEN   ++ NAME                           00333900
                    LCLC  &XSYSACT    ++ NAME                           00334000
                    LCLC  &XSYSACTLEN           ++ NAME                 00334100
                    LCLC  &XORESP     ++ NAME                           00334200
                    LCLC  &XORESPLEN  ++ NAME                           00334300
                    LCLC  &XSPRESP    ++ NAME                           00334400
                    LCLC  &XSPRESPLEN           ++ NAME                 00334500
                    LCLC  &XPROBD     ++ NAME                           00334600
                    LCLC  &XPROBDLEN  ++ NAME                           00334700
                    LCLC  &XSOURCE    ++ NAME                           00334800
                    LCLC  &XSOURCELEN           ++ NAME                 00334900
                    LCLC  &XREFDOC    ++ NAME                           00335000
                    LCLC  &XREFDOCLEN           ++ NAME                 00335100
                    LCLC  &XAUTOMATION          ++ NAME                 00335200
                    LCLC  &XAUTOMATIONLEN       ++ NAME                 00335300
                    LCLC  &XDIAG      ++ NAME                           00335400
                    LCLC  &XDUMMY01   ++ NAME                           00335500
                    LCLC  &XDUMMY02   ++ NAME                           00335600
                    LCLC  &XDUMMY03   ++ NAME                           00335700
                    LCLC  &XDUMMY05   ++ NAME                           00335800
                    LCLC  &XDUMMY06   ++ NAME                           00335900
                    LCLC  &XDUMMY07   ++ NAME                           00336000
                    LCLC  &XDUMMY08   ++ NAME                           00336100
                    LCLC  &XDUMMY09   ++ NAME                           00336200
