                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01836100
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01836200
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01836300
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01836400
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01836500
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01836600
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01836700
&ZOPN                 SETA  &ZOPN+1                                     01836800
&ZOPL(&ZOPN)          SETC  ' '                                         01836900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01837000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01837100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01837200
                      AGO   .ZIMFNX                                     01837300
.*                                                                      01837400
.ZIMFN2               ANOP ,                                            01837500
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01837600
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01837700
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01837800
&ZOPN                 SETA  &ZOPN+1                                     01837900
&ZOPL(&ZOPN)          SETC  ' '                                         01838000
&ZOPO(&ZOPN)          SETC  'LAE'                                       01838100
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01838200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01838300
                      AGO   .ZIMFNX                                     01838400
.ZIMFNX1              ANOP ,                                            01838500
&ZOPN                 SETA  &ZOPN+1                                     01838600
&ZOPL(&ZOPN)          SETC  ' '                                         01838700
