.***AC.PLV01  PROCESS PLISTVER                                      *   00075400
.********************************************************************   00075500
&XPLISTVER      SETA  0                                                 00075600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00075700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00075800
&XPLISTVER      SETA  0                                                 00075900
.ZPLV0          ANOP  ,                                                 00076000
.*                                                                      00076100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00076200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00076300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00076400
.*-------------------------------------------------------------------   00076500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00076600
&ZMS            SETC  '"PLISTVER='                                      00076700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00076800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00076900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00077000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00077100
                AGO   .ZPVCK2                                           00077200
.ZPVCK3         ANOP  ,                                                 00077300
.*-------------------------------------------------------------------   00077400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00077500
&ZMS            SETC  '"PLISTVER='                                      00077600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00077700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00077800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00077900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00078000
                AGO   .ZPVCKE                                           00078100
.ZPVCK2         ANOP  ,                                                 00078200
.*-------------------------------------------------------------------   00078300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00078400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00078500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00078600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00078700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00078800
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00078900
.ZPVCK6         ANOP  ,                                                 00079000
&ZMS            SETC  '"PLISTVER='                                      00079100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00079200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00079300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00079400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00079500
&XPLISTVER      SETA  0                                                 00079600
                AGO   .ZPVCKE                                           00079700
.*-------------------------------------------------------------------   00079800
.ZPVCK4A        ANOP  ,                                                 00079900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00080000
&ZMS            SETC  'PLISTVER='                                       00080100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00080200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00080300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00080400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00080500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00080600
                AGO   .ZPVCK5                                           00080700
.ZPVCK4         ANOP  ,                                                 00080800
&XPLISTVER      SETA  &PLISTVER(1)                                      00080900
.ZPVCK5         ANOP  ,                                                 00081000
.ZPVCKE         ANOP  ,                                                 00081100
.********************************************************************   00081200
