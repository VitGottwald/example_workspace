.*-------------------------------------------------------------------   00206100
&ZOPN1                SETA &ZOPN                                        00206200
.*******************************************************************    00206300
.***  PROCESS IXLFORCE OUTPUT PARAMETERS                                00206400
.*******************************************************************    00206500
                      ACTR  10000                                       00206600
.*******************************************************************    00206700
.***AO.X02  RETURN CODE PROCESSING                                      00206800
.*******************************************************************    00206900
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00207000
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00207100
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00207200
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00207300
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00207400
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00207500
&ZOPN                 SETA  &ZOPN+1                                     00207600
&ZOPL(&ZOPN)          SETC  ' '                                         00207700
&ZOPO(&ZOPN)          SETC  'ST'                                        00207800
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00207900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00208000
                      AGO   .ZMRETX                                     00208100
.*                                                                      00208200
.ZMRET2               ANOP  ,                                           00208300
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00208400
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00208500
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00208600
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00208700
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00208800
&ZOPN                 SETA  &ZOPN+1                                     00208900
&ZOPL(&ZOPN)          SETC  ' '                                         00209000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00209100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00209200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00209300
.ZMRETX               ANOP  ,                                           00209400
.*******************************************************************    00209500
.***AO.X03  REASON CODE PROCESSING                                      00209600
.*******************************************************************    00209700
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00209800
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00209900
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00210000
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00210100
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00210200
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00210300
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00210400
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00210500
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00210600
&ZOPN                 SETA  &ZOPN+1                                     00210700
&ZOPL(&ZOPN)          SETC  ' '                                         00210800
&ZOPO(&ZOPN)          SETC  'ST'                                        00210900
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00211000
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00211100
                      AGO   .ZMRSNX                                     00211200
.*                                                                      00211300
.ZMRSN2               ANOP  ,                                           00211400
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00211500
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00211600
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00211700
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00211800
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00211900
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00212000
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00212100
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00212200
&ZOPN                 SETA  &ZOPN+1                                     00212300
&ZOPL(&ZOPN)          SETC  ' '                                         00212400
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00212500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00212600
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00212700
.ZMRSNX               ANOP  ,                                           00212800
.*******************************************************************    00212900
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00213000
.*******************************************************************    00213100
.ZMFLX                ANOP  ,              MF=L PROCESSING              00213200
.*******************************************************************    00213300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00213400
.*******************************************************************    00213500
                      ACTR  10000                                       00213600
.*                                                                      00213700
                      LCLA  &ZLC           COLUMN POSITION              00213800
                      LCLA  &ZOPX          LIST TABLE INDEX             00213900
                      LCLC  &ZLL           LABEL                        00214000
                      LCLC  &ZLOP          OPERATION                    00214100
                      LCLC  &ZLV           VALUE                        00214200
                      LCLC  &ZLD           DESCRIPTION                  00214300
.*                                                                      00214400
&ZOPX                 SETA  1                                           00214500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00214600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00214700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00214800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00214900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00215000
                      AIF   (K'&ZLD LE 0).ZL5                           00215100
&ZLC                  SETA  35             COMMENT COLUMN               00215200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00215300
&ZLC                  SETA  33             COMMMENT COLUMN              00215400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00215500
&ZLV                  SETC  '&ZLV'.' '                                  00215600
                      AGO   .ZL3                                        00215700
.*                                                                      00215800
.ZL4                  ANOP  ,                                           00215900
&ZLV                  SETC  '&ZLV'.' '                                  00216000
.ZL5                  ANOP  ,                                           00216100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00216200
.*                                                                      00216300
&ZLL     &ZLOP &ZLV                                                     00216400
.*                                                                      00216500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00216600
                      AGO   .ZL2           CONTINUE LOOPING             00216700
.*                                                                      00216800
.ZLX                  ANOP  ,                                           00216900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00217000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00217100
         L     14,16(0,0)              GET CVT ADDRESS                  00217200
         L     14,772(14,0)            GET SFT ADDRESS                  00217300
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00217400
         LA    15,45                   GET EX FOR SERVICE ROUTINE       00217500
         OR    14,15                   GET PC NUMBER                    00217600
         PC    0(14)                                                    00217700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00217800
.ZLINKX               ANOP  ,                                           00217900
.*******************************************************************    00218000
.***AP.X01  FORMAT GENERATED CODE                                       00218100
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00218200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00218300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00218400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00218500
