                      AIF   ('&XSTRDUMPID'(1,1) NE '(').ZI11B           00202400
&ZCGPR   SETC  '0('.'&XSTRDUMPID'(2,K'&XSTRDUMPID-2).')'                00202500
                      AGO   .ZI11D                                      00202600
.ZI11B                ANOP  ,                                           00202700
.*                                                                      00202800
&ZC                   SETC  '&XSTRDUMPID'(1,1)                          00202900
                      AIF   (T'&ZC NE 'N').ZI11D                        00203000
&ZC                   SETC  '&XSTRDUMPID'                               00203100
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI11D               00203200
&ZCGPR                SETC  '=AL2(&XSTRDUMPID)'                         00203300
.*                                                                      00203400
.ZI11D                ANOP  ,                                           00203500
.*                                                                      00203600
&ZOPN                 SETA  &ZOPN+1                                     00203700
&ZOPL(&ZOPN)          SETC  ' '                                         00203800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00203900
&ZOPV(&ZOPN)          SETC  '22(2,1),&ZCGPR'                            00204000
&ZOPD(&ZOPN)          SETC  '++ &ZXSTRDUMPID XSTRDUMPID'                00204100
.*                                                                      00204200
.ZI11E                ANOP  ,                                           00204300
.*-------------------------------------------------------------------   00204400
.***AI.L25  XSTRNAME CL16                                               00204500
                      AIF   ('&ZXSTRNAME' EQ '').ZI13E                  00204600
                      AIF   ('&ZXSTRNAME' EQ 'OUTPUT').ZI13E            00204700
&ZCGPR                SETC  '&XSTRNAME'                                 00204800
