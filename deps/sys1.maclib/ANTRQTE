                      AGO   .ZIMFNX                                     00827100
.*                                                                      00827200
.ZIMFN2               ANOP ,                                            00827300
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00827400
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00827500
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00827600
