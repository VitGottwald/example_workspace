.********************************************************************   00183300
.***AC.PLV01  PROCESS PLISTVER                                      *   00183400
.********************************************************************   00183500
&XPLISTVER      SETA  0                                                 00183600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00183700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00183800
&XPLISTVER      SETA  4                                                 00183900
.ZPLV0          ANOP  ,                                                 00184000
.*                                                                      00184100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00184200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00184300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00184400
.*-------------------------------------------------------------------   00184500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00184600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00184700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00184800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00184900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00185000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00185100
                AGO   .ZPVCK2                                           00185200
.ZPVCK3         ANOP  ,                                                 00185300
.*-------------------------------------------------------------------   00185400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00185500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00185600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00185700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00185800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00185900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00186000
                AGO   .ZPVCKE                                           00186100
.ZPVCK2         ANOP  ,                                                 00186200
.*-------------------------------------------------------------------   00186300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00186400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00186500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00186600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00186700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00186800
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00186900
.ZPVCK6         ANOP  ,                                                 00187000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00187100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00187200
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00187300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00187400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00187500
&XPLISTVER      SETA  4                                                 00187600
                AGO   .ZPVCKE                                           00187700
.*-------------------------------------------------------------------   00187800
.ZPVCK4A        ANOP  ,                                                 00187900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00188000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00188100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00188200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00188300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00188400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00188500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00188600
                AGO   .ZPVCK5                                           00188700
.ZPVCK4         ANOP  ,                                                 00188800
&XPLISTVER      SETA  &PLISTVER(1)                                      00188900
.ZPVCK5         ANOP  ,                                                 00189000
.ZPVCKE         ANOP  ,                                                 00189100
.********************************************************************   00189200
.***AG.MF01 PROCESS MF MACRO FORMS                                      00189300
.********************************************************************   00189400
                ACTR  10000                                             00189500
.*------------------------------------------------------------------    00189600
.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00189700
&XMF            SETC  ''          INITIALIZE XVARIABLE                  00189800
