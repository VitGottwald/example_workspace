&ZOPD(&ZOPN)          SETC  '++ XL1 CONSTANT'                           00889200
.*-------------------------------------------------------------------   00889300
&ZOPN1                SETA &ZOPN                                        00889400
.*-------------------------------------------------------------------   00889500
.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              00889600
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         00889700
.*******************************************************************    00889800
.***  PROCESS ANTRQTE OUTPUT PARAMETERS                                 00889900
.*******************************************************************    00890000
                      ACTR  10000                                       00890100
.*******************************************************************    00890200
.***AO.X02  RETURN CODE PROCESSING                                      00890300
.*******************************************************************    00890400
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00890500
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00890600
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00890700
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00890800
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00890900
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00891000
&ZOPN                 SETA  &ZOPN+1                                     00891100
&ZOPL(&ZOPN)          SETC  ' '                                         00891200
&ZOPO(&ZOPN)          SETC  'ST'                                        00891300
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00891400
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00891500
                      AGO   .ZMRETX                                     00891600
.*                                                                      00891700
.ZMRET2               ANOP  ,                                           00891800
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00891900
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00892000
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00892100
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00892200
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00892300
&ZOPN                 SETA  &ZOPN+1                                     00892400
&ZOPL(&ZOPN)          SETC  ' '                                         00892500
&ZOPO(&ZOPN)          SETC  'LR'                                        00892600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00892700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00892800
.ZMRETX               ANOP  ,                                           00892900
.*******************************************************************    00893000
.***AO.X03  REASON CODE PROCESSING                                      00893100
.*******************************************************************    00893200
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00893300
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00893400
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00893500
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00893600
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00893700
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00893800
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00893900
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00894000
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00894100
&ZOPN                 SETA  &ZOPN+1                                     00894200
&ZOPL(&ZOPN)          SETC  ' '                                         00894300
&ZOPO(&ZOPN)          SETC  'ST'                                        00894400
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00894500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00894600
                      AGO   .ZMRSNX                                     00894700
.*                                                                      00894800
.ZMRSN2               ANOP  ,                                           00894900
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00895000
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00895100
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00895200
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00895300
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00895400
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00895500
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00895600
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00895700
&ZOPN                 SETA  &ZOPN+1                                     00895800
&ZOPL(&ZOPN)          SETC  ' '                                         00895900
&ZOPO(&ZOPN)          SETC  'LR'                                        00896000
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00896100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00896200
.ZMRSNX               ANOP  ,                                           00896300
.*******************************************************************    00896400
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00896500
.*******************************************************************    00896600
.ZMFLX                ANOP  ,              MF=L PROCESSING              00896700
.*******************************************************************    00896800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00896900
.*******************************************************************    00897000
                      ACTR  10000                                       00897100
.*                                                                      00897200
                      LCLA  &ZLC           COLUMN POSITION              00897300
                      LCLA  &ZOPX          LIST TABLE INDEX             00897400
                      LCLC  &ZLL           LABEL                        00897500
                      LCLC  &ZLOP          OPERATION                    00897600
                      LCLC  &ZLV           VALUE                        00897700
                      LCLC  &ZLD           DESCRIPTION                  00897800
.*                                                                      00897900
&ZOPX                 SETA  1                                           00898000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00898100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00898200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00898300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00898400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00898500
                      AIF   (K'&ZLD LE 0).ZL5                           00898600
&ZLC                  SETA  35             COMMENT COLUMN               00898700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00898800
&ZLC                  SETA  33             COMMMENT COLUMN              00898900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00899000
&ZLV                  SETC  '&ZLV'.' '                                  00899100
                      AGO   .ZL3                                        00899200
.*                                                                      00899300
.ZL4                  ANOP  ,                                           00899400
&ZLV                  SETC  '&ZLV'.' '                                  00899500
.ZL5                  ANOP  ,                                           00899600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00899700
.*                                                                      00899800
&ZLL     &ZLOP &ZLV                                                     00899900
.*                                                                      00900000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00900100
                      AGO   .ZL2           CONTINUE LOOPING             00900200
.*                                                                      00900300
.ZLX                  ANOP  ,                                           00900400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00900500
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00900600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00900700
         L     14,16(,0)                                                00900800
         L     14,1216(,14)                                             00900900
         L     14,44(,14)                                               00901000
         L     14,8(,14)                                                00901100
         L     14,88(,14)                                               00901200
         ICM   14,15,324(14)                                            00901300
         BNZ   A&SYSNDX                                                 00901400
         L     15,=A(RQST_PC_NUMBER_ZERO)                               00901500
         SLR   0,0                                                      00901600
         B     B&SYSNDX                                                 00901700
A&SYSNDX DS    0H                                                       00901800
         PC    0(14)                                                    00901900
B&SYSNDX DS    0H                                                       00902000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00902100
.ZLINKX               ANOP  ,                                           00902200
.*******************************************************************    00902300
.***AP.X01  FORMAT GENERATED CODE                                       00902400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00902500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00902600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00902700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00902800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00902900
                      AIF   (K'&ZLD LE 0).ZL51                          00903000
&ZLC                  SETA  35             COMMENT COLUMN               00903100
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00903200
&ZLC                  SETA  33             COMMENT COLUMN               00903300
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00903400
&ZLV                  SETC  '&ZLV'.' '                                  00903500
                      AGO   .ZL31                                       00903600
.*                                                                      00903700
.ZL41                 ANOP  ,                                           00903800
&ZLV                  SETC  '&ZLV'.' '                                  00903900
.ZL51                 ANOP  ,                                           00904000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00904100
.*                                                                      00904200
&ZLL     &ZLOP &ZLV                                                     00904300
.*                                                                      00904400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00904500
                      AGO   .ZL21          CONTINUE LOOPING             00904600
.*                                                                      00904700
.ZLX1                 ANOP  ,                                           00904800
.*******************************************************************    00904900
.***AP.X02  END OF ANTRQTE MACRO                                        00905000
