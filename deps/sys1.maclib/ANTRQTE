                AIF   ('&XRSNCODE'(1,1) NE '(').ZE157R                  00774600
&ZCGPR          SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                      00774700
                AIF   ('&ZCGPR'(1,1) NE '0').ZE157P                     00774800
                AIF   (K'&ZCGPR GT 2).ZE157Q                            00774900
                AIF   ('&ZCGPR' EQ '0').ZE157Q                          00775000
&ZCGPR          SETC  '&ZCGPR'(2,1)                                     00775100
                AIF   (T'&ZCGPR NE 'N').ZE157Q  INVALID REG NAME        00775200
.ZE157P         ANOP  ,                                                 00775300
                AIF   (T'&ZCGPR NE 'N').ZE157R  NON NUMERIC             00775400
                AIF   ('&ZCGPR' LT '2').ZE157Q                          00775500
                AIF   ('&ZCGPR' LE '12').ZE157R                         00775600
.ZE157Q         ANOP  ,                                                 00775700
&ZMS            SETC  '"RSNCODE='               *KGPRA* MNOTE           00775800
&ZMS            SETC  '&ZMS.&RSNCODE"'                                  00775900
&ZMS            SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'         00776000
                MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                  00776100
&ZMACRET        SETA  8           SET MACRO ERROR RETURN CODE           00776200
.ZE157R         ANOP  ,                                                 00776300
.******************************************************************     00776400
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00776500
.******************************************************************     00776600
                ACTR  10000                                             00776700
