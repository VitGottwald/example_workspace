.ZMFLX                ANOP  ,              MF=L PROCESSING              00228300
.*******************************************************************    00228400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00228500
.*******************************************************************    00228600
                      ACTR  10000                                       00228700
.*                                                                      00228800
                      LCLA  &ZLC           COLUMN POSITION              00228900
                      LCLA  &ZOPX          LIST TABLE INDEX             00229000
                      LCLC  &ZLL           LABEL                        00229100
                      LCLC  &ZLOP          OPERATION                    00229200
                      LCLC  &ZLV           VALUE                        00229300
                      LCLC  &ZLD           DESCRIPTION                  00229400
.*                                                                      00229500
&ZOPX                 SETA  1                                           00229600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00229700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00229800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00229900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00230000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00230100
                      AIF   (K'&ZLD LE 0).ZL5                           00230200
&ZLC                  SETA  35             COMMENT COLUMN               00230300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00230400
&ZLC                  SETA  33             COMMMENT COLUMN              00230500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00230600
&ZLV                  SETC  '&ZLV'.' '                                  00230700
                      AGO   .ZL3                                        00230800
.*                                                                      00230900
.ZL4                  ANOP  ,                                           00231000
&ZLV                  SETC  '&ZLV'.' '                                  00231100
.ZL5                  ANOP  ,                                           00231200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00231300
.*                                                                      00231400
&ZLL     &ZLOP &ZLV                                                     00231500
.*                                                                      00231600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00231700
                      AGO   .ZL2           CONTINUE LOOPING             00231800
.*                                                                      00231900
.ZLX                  ANOP  ,                                           00232000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00232100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00232200
         L     14,16(0,0)              GET CVT ADDRESS                  00232300
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00232400
         LAE   14,0(14,0)              CLEAR AR 14                      00232500
.NOAR    ANOP                                                           00232600
         L     14,772(14,0)            GET SFT ADDRESS                  00232700
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00232800
         LA    15,14                   GET EX FOR SERVICE ROUTINE       00232900
         OR    14,15                   GET PC NUMBER                    00233000
         PC    0(14)                                                    00233100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00233200
.ZLINKX               ANOP  ,                                           00233300
.*******************************************************************    00233400
.***AP.X01  FORMAT GENERATED CODE                                       00233500
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00233600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00233700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00233800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00233900
