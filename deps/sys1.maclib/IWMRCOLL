.***AC.PLV01  PROCESS PLISTVER                                      *   00077800
.********************************************************************   00077900
&XPLISTVER      SETA  0                                                 00078000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00078100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00078200
&XPLISTVER      SETA  0                                                 00078300
.ZPLV0          ANOP  ,                                                 00078400
.*                                                                      00078500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00078600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00078700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00078800
.*-------------------------------------------------------------------   00078900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00079000
&ZMS            SETC  '"PLISTVER='                                      00079100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00079200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00079300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00079400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00079500
                AGO   .ZPVCK2                                           00079600
.ZPVCK3         ANOP  ,                                                 00079700
.*-------------------------------------------------------------------   00079800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00079900
&ZMS            SETC  '"PLISTVER='                                      00080000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00080100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00080200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00080300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00080400
                AGO   .ZPVCKE                                           00080500
.ZPVCK2         ANOP  ,                                                 00080600
.*-------------------------------------------------------------------   00080700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00080800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00080900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00081000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00081100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00081200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00081300
.ZPVCK6         ANOP  ,                                                 00081400
&ZMS            SETC  '"PLISTVER='                                      00081500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00081600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00081700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00081800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00081900
&XPLISTVER      SETA  0                                                 00082000
                AGO   .ZPVCKE                                           00082100
.*-------------------------------------------------------------------   00082200
.ZPVCK4A        ANOP  ,                                                 00082300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00082400
&ZMS            SETC  'PLISTVER='                                       00082500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00082600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00082700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00082800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00082900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00083000
                AGO   .ZPVCK5                                           00083100
.ZPVCK4         ANOP  ,                                                 00083200
&XPLISTVER      SETA  &PLISTVER(1)                                      00083300
.ZPVCK5         ANOP  ,                                                 00083400
.ZPVCKE         ANOP  ,                                                 00083500
.********************************************************************   00083600
