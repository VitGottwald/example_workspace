                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00184000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00184100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00184200
