.***AC.PLV01  PROCESS PLISTVER                                      *   00039800
.********************************************************************   00039900
&XPLISTVER      SETA  0                                                 00040000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00040100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00040200
&XPLISTVER      SETA  0                                                 00040300
.ZPLV0          ANOP  ,                                                 00040400
.*                                                                      00040500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00040600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00040700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00040800
.*-------------------------------------------------------------------   00040900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00041000
&ZMS            SETC  '"PLISTVER='                                      00041100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00041200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00041300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00041400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00041500
                AGO   .ZPVCK2                                           00041600
.ZPVCK3         ANOP  ,                                                 00041700
.*-------------------------------------------------------------------   00041800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00041900
&ZMS            SETC  '"PLISTVER='                                      00042000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00042100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00042200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00042300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00042400
                AGO   .ZPVCKE                                           00042500
.ZPVCK2         ANOP  ,                                                 00042600
.*-------------------------------------------------------------------   00042700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00042800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00042900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00043000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00043100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00043200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00043300
.ZPVCK6         ANOP  ,                                                 00043400
&ZMS            SETC  '"PLISTVER='                                      00043500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00043600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00043700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00043800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00043900
&XPLISTVER      SETA  0                                                 00044000
                AGO   .ZPVCKE                                           00044100
.*-------------------------------------------------------------------   00044200
.ZPVCK4A        ANOP  ,                                                 00044300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00044400
&ZMS            SETC  'PLISTVER='                                       00044500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00044600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00044700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00044800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00044900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00045000
                AGO   .ZPVCK5                                           00045100
.ZPVCK4         ANOP  ,                                                 00045200
&XPLISTVER      SETA  &PLISTVER(1)                                      00045300
.ZPVCK5         ANOP  ,                                                 00045400
.ZPVCKE         ANOP  ,                                                 00045500
.********************************************************************   00045600
.***AG.MF01 PROCESS MF MACRO FORMS                                      00045700
