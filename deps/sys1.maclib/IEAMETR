&ZOPL(&ZOPN)          SETC ' '                                          00086000
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00086100
&ZOPV(&ZOPN)          SETC '1'                                          00086200
&ZOPD(&ZOPN)          SETC ' '                                          00086300
.*-------------------------------------------------------------------   00086400
.***AL.L12 IEAMETR FMT=LIST MF=S!L PARAMETERS                           00086500
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00086600
.*                                                                      00086700
&ZOPN                 SETA  &ZOPN+1                                     00086800
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00086900
&ZOPO(&ZOPN)          SETC  'DC'                                        00087000
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00087100
&ZOPD(&ZOPN)          SETC  '++ IEAMETR ARG PARMS'                      00087200
.*                                                                      00087300
.ZLMFSLX              ANOP  ,                                           00087400
.*-------------------------------------------------------------------   00087500
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00087600
&ZPLLEN               SETA 32                                           00087700
.*                                                                      00087800
.******************************************************************     00087900
.***AI.L01 IEAMETR FMT=LIST FILL IN PARAMETER AREA                      00088000
.******************************************************************     00088100
                      ACTR  10000                                       00088200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00088300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00088400
&ZCVAL                SETC  'LAE'          XAX MODE                     00088500
.ZIMFXA               ANOP ,                                            00088600
.*                                                                      00088700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00088800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00088900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00089000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00089100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00089200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00089300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00089400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00089500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00089600
&ZOPN                 SETA  &ZOPN+1                                     00089700
&ZOPL(&ZOPN)          SETC  ' '                                         00089800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00089900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00090000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00090100
                      AGO   .ZIMFNX                                     00090200
.*                                                                      00090300
.ZIMFN2               ANOP ,                                            00090400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00090500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00090600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00090700
&ZOPN                 SETA  &ZOPN+1                                     00090800
&ZOPL(&ZOPN)          SETC  ' '                                         00090900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00091000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00091100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00091200
                      AGO   .ZIMFNX                                     00091300
.ZIMFNX1              ANOP ,                                            00091400
&ZOPN                 SETA  &ZOPN+1                                     00091500
&ZOPL(&ZOPN)          SETC  ' '                                         00091600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00091700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00091800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00091900
.ZIMFNX               ANOP ,                                            00092000
.********************************************************************   00092100
.***AI.L02 IEAMETR FMT=LIST INITIALIZE PARAMETER AREA                   00092200
.********************************************************************   00092300
.***AI.L03 MF=(SLE)                                                     00092400
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00092500
.*                                                                      00092600
&ZOPN                 SETA  &ZOPN+1                                     00092700
&ZOPL(&ZOPN)          SETC  ' '                                         00092800
&ZOPO(&ZOPN)          SETC  'XC'                                        00092900
&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00093000
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00093100
.ZIMFLX               ANOP  ,                                           00093200
.*-------------------------------------------------------------------   00093300
.***AI.L25  XOUTADDR AL4                                                00093400
                      AIF   ('&ZXOUTADDR' EQ '').ZI3E                   00093500
                      AIF   ('&ZXOUTADDR' EQ 'OUTPUT').ZI3E             00093600
&ZCGPR                SETC  '&XOUTADDR'                                 00093700
.*                                                                      00093800
                      AIF   ('&XOUTADDR'(1,1) NE '(').ZI3B              00093900
&ZCGPR   SETC  '0('.'&XOUTADDR'(2,K'&XOUTADDR-2).')'                    00094000
                      AGO   .ZI3D                                       00094100
.ZI3B                 ANOP  ,                                           00094200
.*                                                                      00094300
&ZC                   SETC  '&XOUTADDR'(1,1)                            00094400
                      AIF   (T'&ZC NE 'N').ZI3D                         00094500
&ZC                   SETC  '&XOUTADDR'                                 00094600
                      AIF   ('&ZC'(K'&ZC,1) EQ ')').ZI3D                00094700
&ZCGPR                SETC  '=AL4(&XOUTADDR)'                           00094800
.*                                                                      00094900
.ZI3D                 ANOP  ,                                           00095000
.*                                                                      00095100
&ZOPN                 SETA  &ZOPN+1                                     00095200
&ZOPL(&ZOPN)          SETC  ' '                                         00095300
&ZOPO(&ZOPN)          SETC  'MVC'                                       00095400
&ZOPV(&ZOPN)          SETC  '4(4,1),&ZCGPR'                             00095500
&ZOPD(&ZOPN)          SETC  '++ &ZXOUTADDR XOUTADDR'                    00095600
.*                                                                      00095700
.ZI3E                 ANOP  ,                                           00095800
.*-------------------------------------------------------------------   00095900
&ZOPN1                SETA &ZOPN                                        00096000
.*******************************************************************    00096100
.***  PROCESS IEAMETR OUTPUT PARAMETERS                                 00096200
.*******************************************************************    00096300
                      ACTR  10000                                       00096400
.*******************************************************************    00096500
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00096600
.*******************************************************************    00096700
.ZMFLX                ANOP  ,              MF=L PROCESSING              00096800
.*******************************************************************    00096900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00097000
.*******************************************************************    00097100
                      ACTR  10000                                       00097200
.*                                                                      00097300
                      LCLA  &ZLC           COLUMN POSITION              00097400
                      LCLA  &ZOPX          LIST TABLE INDEX             00097500
                      LCLC  &ZLL           LABEL                        00097600
                      LCLC  &ZLOP          OPERATION                    00097700
                      LCLC  &ZLV           VALUE                        00097800
                      LCLC  &ZLD           DESCRIPTION                  00097900
.*                                                                      00098000
&ZOPX                 SETA  1                                           00098100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00098200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00098300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00098400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00098500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00098600
                      AIF   (K'&ZLD LE 0).ZL5                           00098700
&ZLC                  SETA  35             COMMENT COLUMN               00098800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00098900
&ZLC                  SETA  33             COMMMENT COLUMN              00099000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00099100
&ZLV                  SETC  '&ZLV'.' '                                  00099200
                      AGO   .ZL3                                        00099300
.*                                                                      00099400
.ZL4                  ANOP  ,                                           00099500
&ZLV                  SETC  '&ZLV'.' '                                  00099600
.ZL5                  ANOP  ,                                           00099700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00099800
.*                                                                      00099900
&ZLL     &ZLOP &ZLV                                                     00100000
.*                                                                      00100100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00100200
                      AGO   .ZL2           CONTINUE LOOPING             00100300
.*                                                                      00100400
.ZLX                  ANOP  ,                                           00100500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00100600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00100700
         L     14,16(0,0)              CVT Address                      00100800
         L     14,772(14,0)            SFT Address                      00100900
         L     14,428(14,0)            LX/EX for service routine        00101000
         PC    0(14)                                                    00101100
         AIF   ('&SYSASCE' EQ 'P').ZIEAMETRP                            00101200
         LAE   14,0(0,0)               Zero AR14                        00101300
.ZIEAMETRP      ANOP ,                                                  00101400
         L     14,4(0,1)               Return area address              00101500
         MVC   0(24,14),8(1)           Move data to return area         00101600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00101700
.ZLINKX               ANOP  ,                                           00101800
.*******************************************************************    00101900
.***AP.X01  FORMAT GENERATED CODE                                       00102000
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00102100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00102200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00102300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00102400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00102500
                      AIF   (K'&ZLD LE 0).ZL51                          00102600
&ZLC                  SETA  35             COMMENT COLUMN               00102700
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00102800
&ZLC                  SETA  33             COMMENT COLUMN               00102900
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00103000
&ZLV                  SETC  '&ZLV'.' '                                  00103100
                      AGO   .ZL31                                       00103200
.*                                                                      00103300
.ZL41                 ANOP  ,                                           00103400
&ZLV                  SETC  '&ZLV'.' '                                  00103500
.ZL51                 ANOP  ,                                           00103600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00103700
.*                                                                      00103800
&ZLL     &ZLOP &ZLV                                                     00103900
.*                                                                      00104000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00104100
                      AGO   .ZL21          CONTINUE LOOPING             00104200
.*                                                                      00104300
.ZLX1                 ANOP  ,                                           00104400
.*******************************************************************    00104500
.***AP.X02  END OF IEAMETR MACRO                                        00104600
