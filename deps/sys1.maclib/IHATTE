*                                                                       20940000
*/*!PR - PROGRAM RETURN TTE                                          */ 20950000
*/*!                                                                 */ 20960000
*/*!                                                                 */ 20970000
*/*!FORMATTED PR TTE:                                                */ 20980000
*/*!                                                                 */ 20990000
*/*!PR ASID TCB PR    ... PSW ADDR U1                  PASD SASD     */ 21000000
*/*!                                                                 */ 21010000
*                                                                       21020000
*   DECLARE                                                             21030000
*     TTETPR   FIXED(8) CONSTANT(50), /* PR TTE TYPE (X'32').    @L2A*/ 21040000
*     TTEPRL   CONSTANT(12),        /* Length for type X'32'     @P4A*/ 21050000
*     TTETPR2  FIXED(8) CONSTANT(51), /* PR TTE TYPE (X'33').    @P3A*/ 21060000
*     TTEPR2L  CONSTANT(16),        /* Length for type X'33'     @P4A*/ 21070000
*     TTETPR3  FIXED(8) CONSTANT(52), /* PR TTE TYPE (X'34').    @P3A*/ 21080000
*     TTEPR3L  CONSTANT(20),        /* Length for type X'34'     @P4A*/ 21090000
*     1    TTEPR    IHATTE_DEF1 BOUNDARY(WORD), /* PR TRACE TABLE       21100000
*                                      ENTRY.                    @L2A*/ 21110000
*                                                                       21120000
*/*                                                                  */ 21130000
*/*    ------------------------------------------------------------- */ 21140000
*/*    All PR entries have an initial word having the same format    */ 21150000
*/*    ------------------------------------------------------------- */ 21160000
*/*                                                                  */ 21170000
*                                                                       21180000
*      2    TTEPRbase,              /* Common fields             @P4A*/ 21190000
*       3    TTEPRTYP FIXED(8),     /* PR TRACE TABLE ENTRY TYPE @L2A*/ 21200000
*       3    TTEPRPKY FIXED(8),     /* <PSW>. PSW KEY IN BITS 0-3.      21210000
*                                                                @L9C*/ 21220000
*        4    TTEPRKY  BIT(4),      /* PSW key                   @P4A*/ 21230000
*        4    TTEPRT   BIT(2),      /* To info                   @P4A*/ 21240000
*         5    TTEPR6M  BIT(1),     /* 64-bit mode               @P4A*/ 21250000
*         5    TTEPR6A  BIT(1),     /* 64-bits present in entry  @P4A*/ 21260000
*        4    TTEPRF   BIT(2) LIKE(TTEPRT), /* From info         @P4A*/ 21270000
*       3    TTEPRASD FIXED(16),    /* NEW ASID SPECIFIED BY THE PR     21280000
*                                      INSTRUCTION.              @L2A*/ 21290000
*                                                                       21300000
*/*                                                                  */ 21310000
*/*    ------------------------------------------------------------- */ 21320000
*/*    The remainder consists of two addresses that may be 31-bit    */ 21330000
*/*    or 64-bit addresses.  Addressing mode and problem state data  */ 21340000
*/*    also appears.                                                 */ 21350000
*/*    ------------------------------------------------------------- */ 21360000
*/*                                                                  */ 21370000
*                                                                       21380000
*      2    *        UNION BOUNDARY(WORD), /* 31/64-bit initial         21390000
*                                      address                   @P4A*/ 21400000
*                                                                       21410000
*/*                                                                  */ 21420000
*/*     ------------------------------------------------------------ */ 21430000
*/*     The older PR entries start with 31-bit addresses             */ 21440000
*/*     ------------------------------------------------------------ */ 21450000
*/*                                                                  */ 21460000
*                                                                       21470000
*       3    TTEPRF31 BOUNDARY(WORD), /* Starts with 31-bit addr @P4A*/ 21480000
*        4    TTEPRTAD POINTER(31), /* <ADDR>. NEW INSTRUCTION          21490000
*                                      ADDRESS AS UPDATED BY THE PR     21500000
*                                      INSTRUCTION.              @P4C*/ 21510000
*         5    TTEPRTB1 BIT(8),     /* BYTE 1 OF ADDRESS.        @L2A*/ 21520000
*           7    TTEPRAM1 BIT(1),   /* ADDRESSING MODE BIT FROM THE     21530000
*                                      PSW AS UPDATED BY THE PR         21540000
*                                      INSTRUCTION. BIT 0.       @L2A*/ 21550000
*         5    *        CHARACTER(2), /* BYTES 2 AND 3 OF ADDRESS.      21560000
*                                                                @L2A*/ 21570000
*         5    TTEPRTB4 BIT(8),     /* BYTE 4 OF ADDRESS.        @L2A*/ 21580000
*           7    *        BIT(7),   /* RESERVED.                 @L2A*/ 21590000
*           7    TTEPRPS  BIT(1),   /* PROBLEM STATE BIT AFTER          21600000
*                                      COMPLETION OF THE PR             21610000
*                                      INSTRUCTION. BIT 31.      @L2A*/ 21620000
*                                                                       21630000
*/*                                                                  */ 21640000
*/*      ----------------------------------------------------------- */ 21650000
*/*      Either a 32-bit or a 64-bit address may appear after the    */ 21660000
*/*      64-bit address in the older PR entries.                     */ 21670000
*/*      ----------------------------------------------------------- */ 21680000
*/*                                                                  */ 21690000
*                                                                       21700000
*        4    *        UNION BOUNDARY(WORD), /* 31/64-bit final         21710000
*                                      address                   @P4A*/ 21720000
*         5    TTEPRFAD POINTER(31), /* <U1>. ADDRESS OF THE LOCATION   21730000
*                                      FOLLOWING THE PR INSTRUCTION.    21740000
*                                                                @P4C*/ 21750000
*         5    TTEPRFA6 CHARACTER(8), /* <U1>. ADDRESS OF THE           21760000
*                                      LOCATION FOLLOWING THE PR        21770000
*                                      INSTRUCTION.              @P4C*/ 21780000
*                                                                       21790000
*/*                                                                  */ 21800000
*/*     ------------------------------------------------------------ */ 21810000
*/*     The newer PR entries start with 64-bit addresses.            */ 21820000
*/*     ------------------------------------------------------------ */ 21830000
*/*                                                                  */ 21840000
*                                                                       21850000
*       3    TTEPRF64 BOUNDARY(WORD), /* Starts with 64-bit addr @P4A*/ 21860000
*        4    TTEPRTA6 CHARACTER(8), /* <ADDR>. NEW INSTRUCTION         21870000
*                                      ADDRESS AS UPDATED BY THE PR     21880000
*                                      INSTRUCTION.              @P4C*/ 21890000
*         5    *        CHARACTER(7), /* BYTES 2 AND 3 OF ADDRESS.      21900000
*                                                                @L2A*/ 21910000
*         5    TTEPRTB8 BIT(8),     /* BYTE 4 OF ADDRESS.        @L2A*/ 21920000
*           7    *        BIT(7),   /* RESERVED.                 @L2A*/ 21930000
*           7    TTEPRPS8 BIT(1),   /* PROBLEM STATE BIT AFTER          21940000
*                                      COMPLETION OF THE PR             21950000
*                                      INSTRUCTION. BIT 31.      @L2A*/ 21960000
*                                                                       21970000
*/*                                                                  */ 21980000
*/*      ----------------------------------------------------------- */ 21990000
*/*      Either a 32-bit or a 64-bit address may appear after the    */ 22000000
*/*      64-bit address in the newer PR entries.                     */ 22010000
*/*      ----------------------------------------------------------- */ 22020000
*/*                                                                  */ 22030000
*                                                                       22040000
*        4    *        UNION BOUNDARY(WORD), /* 31/64-bit final         22050000
*                                      address                   @P4A*/ 22060000
* %IF IBM_CBGENC = 'YES' %THEN                                          22070000
*   %GOTO IHATTE_1A;                                                    22080000
*         5    TTEPRFA6 POINTER(31), /* <U1>. ADDRESS OF THE LOCATION   22090000
*                                      FOLLOWING THE PR INSTRUCTION.    22100000
*                                                                @P4C*/ 22110000
*         5    TTEPRF66 CHARACTER(8), /* <U1>. ADDRESS OF THE           22120000
*                                      LOCATION FOLLOWING THE PR        22130000
*                                      INSTRUCTION.              @P4C*/ 22140000
*   %GOTO IHATTE_1B;                                                    22150000
* %IHATTE_1A:;                                                          22160000
