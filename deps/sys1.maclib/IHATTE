*/*                                                                     02191000
*   SYSTEM TRACE TABLE ENTRY                                            02192000
*                                                                       02193000
         SPACE 1                                                        02194000
TTE      DSECT                          TRACE TABLE ENTRY.              02195000
         DS    0CL4                     MINIMUM TRACE TABLE ENTRY.      02196000
.*                                                                      02197000
         AIF   ('&BR' NE 'YES').ENDBR                              @P4C 02198000
         SPACE 3                                                        02199000
*                                                                       02200000
*   BR - BRANCH TTE no longer mapped                                    02210000
*                                                                       02220000
.ENDBR  ANOP                                                            02230000
.*                                                                      02240000
         AIF   ('&MODE' NE 'YES').ENDMODE                          @P4C 02250000
         SPACE 3                                                        02260000
*                                                                       02270000
*   MODE - MODE TTE - no longer mapped                                  02280000
*                                                                       02290000
.ENDMODE ANOP                                                      @L9A 02300000
.*                                                                      02310000
         AIF   ('&AS' NE 'YES').ENDAS                              @P4C 02320000
         SPACE 3                                                        02330000
*                                                                       02340000
*     Address space TTEs - no longer mapped                             02350000
*                                                                       02360000
.ENDAS   ANOP                                                           02370000
.*                                                                      02380000
         SPACE 3                                                        02390000
*                                                                       02400000
*     GENERAL EXPLICIT TTE                                              02410000
*                                                                       02420000
         ORG   TTE                                                      02430000
TTEXP    DS    0CL12                   EXPLICIT TRACE TABLE ENTRY.      02440000
TTETYPE  DS    XL1                     TRACE TABLE ENTRY TYPE (X'7N').  02450000
TTETEX   EQU   X'70'                   EXPLICIT TTE TYPE (X'7N').       02460000
TTEMEX   EQU   X'F0'                   MASK FOR EXPLICIT TTE TYPE.      02470000
TTEREGS  EQU   X'0F'                   MASK FOR ONE LESS THAN THE       02480000
*                                      NUMBER OF REGISTERS              02490000
*                                      IN THE TTE.                      02500000
TTEMBZ1  DS    XL1                     RESERVED. (MUST BE ZERO)         02510000
TTETOD   DS    XL6                     TOD-CLOCK BITS 16-63.            02520000
TTETOTE  DS    0XL4                    TRACE OPERAND TABLE ENTRY.       02530000
TTECPID  DS    XL2                     CP ID                       @LOC 02540000
TTEXPTYP DS    0H                      EXPLICIT ENTRY TYPE.             02550000
TTEXPSID DS    XL1                     EXPLICIT ENTRY SUB-ID.           02560000
TTEMSID  EQU   X'0F'                   MASK FOR EXPLICIT TTE SUB-ID.    02570000
TTEXPID  DS    XL1                     EXPLICIT ENTRY ID.               02580000
TTEUNQ   DS    0F                      UNIQUE ENTRY DESCRIPTIONS.       02590000
TTEEXCOM DS    0CL64                   EXPLICIT ENTRY COMMON FIELD      02600000
*                                      NAMES.                           02610000
TTEWRD0  DS    0CL4                    TTE WORD 0.                      02620000
TTETCB   DS    A                       TCB ADDRESS.                     02630000
TTEWRD1  DS    0CL4                    TTE WORD 1.                      02640000
TTEDEP   DS    H                       DEPENDENT ON EACH                02650000
*                                      EXPLICIT TTE ENTRY.              02660000
TTEASID  DS    H                       ASID.                            02670000
TTEWRD2  DS    CL4                     TTE WORD 2.                      02680000
TTEWRD3  DS    CL4                     TTE WORD 3.                      02690000
TTEWRD4  DS    CL4                     TTE WORD 4.                      02700000
TTEWRD5  DS    CL4                     TTE WORD 5.                      02710000
TTEWRD6  DS    CL4                     TTE WORD 6.                      02720000
TTEWRD7  DS    CL4                     TTE WORD 7.                      02730000
TTEWRD8  DS    CL4                     TTE WORD 8.                      02740000
TTEWRD9  DS    CL4                     TTE WORD 9.                      02750000
TTEWRDA  DS    CL4                     TTE WORD 10.                     02760000
TTEWRDB  DS    CL4                     TTE WORD 11.                     02770000
TTEWRDC  DS    CL4                     TTE WORD 12.                     02780000
TTEWRDD  DS    CL4                     TTE WORD 13.                     02790000
TTEWRDE  DS    CL4                     TTE WORD 14.                     02800000
TTEWRDF  DS    CL4                     TTE WORD 15.                     02810000
TTEXPEND DS    0F                      END OF MAXIMUM EXPLICIT TTE.     02820000
         SPACE 1                                                        02830000
TTETMAX  EQU   76                      MAXIMUM LENGTH OF EXPLICIT       02840000
*                                      ENTRIES.                         02850000
*                                                                       02860000
         ORG   TTE                                                      02870000
TTEEXP   DS    0CL16                   EXPLICIT TRACE TABLE ENTRY.      02880000
TTEETYPE DS    XL1                     TRACE TABLE ENTRY TYPE (X'7N').  02890000
TTEETEX  EQU   X'70'                   EXPLICIT TTE TYPE (X'7N').  @LGA 02900000
TTEEMEX  EQU   X'F0'                   MASK FOR EXPLICIT TTE TYPE. @LGA 02910000
TTEEREGS EQU   X'0F'                   MASK FOR ONE LESS THAN THE  @LGA 02920000
*                                      NUMBER OF REGISTERS              02930000
*                                      IN THE TTE.                      02940000
TTEEMBZ1 DS    X'80'                   MUST BE 10000000            @LGA 02950000
TTEETOD  DS    XL10                    TOD-CLOCK BITS 0-79.        @LGA 02960000
TTEETOTE DS    0XL4                    TRACE OPERAND TABLE ENTRY.  @LGA 02970000
TTEECPID DS    XL2                     CP ID                       @LOC 02980000
TTEEXPTP DS    0H                      EXPLICIT ENTRY TYPE.        @LGA 02990000
TTEEXPSD DS    XL1                     EXPLICIT ENTRY SUB-ID.      @LGA 03000000
TTEEMSID EQU   X'0F'                   MASK FOR EXPLICIT TTE SUB-ID.    03010000
TTEEXPID DS    XL1                     EXPLICIT ENTRY ID.          @LGA 03020000
TTEEUNQ  DS    0F                      UNIQUE ENTRY DESCRIPTIONS.  @LGA 03030000
TTEEECOM DS    0CL128                  EXPLICIT ENTRY COMMON FIELD @LGA 03040000
*                                      NAMES.                           03050000
TTEEWRD1 DS    0CL8                    TTE DOUBLE WORD1            @LQC 03070009
TTEETCB  DS    A                       TCB ADDRESS.                @LGA 03080000
TTEEDEP  DS    H                       DEPENDENT ON EACH           @LGA 03110000
