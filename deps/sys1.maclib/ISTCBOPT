&B       SETA  &CA+&CS+&RELEASE+&PASS+&NERASE+&EAU+&ERASE+&SPEC+&ANY+&N*        
               CONV+&CONV+&LBT+&LBM+&BLK+&COND+&UNCOND+&LOCK+&LGONMSG+&*        
               DEVCHAR+&TERMS+&APPSTAT+&COUNTS+&RNNAME+&CIDXLAT                 
&C       SETA  &TOPLGON+&BSCID+&DISPLAY+&RELRQ+&NRELRQ+&Q+&NQ+&PEND+&SE*        
               SSION+&ACTIVE+&UNCONDL+&QUIESCE+&START+&STOP+&ACCEPT+&AC*        
               QUIRE+&CONALL+&CONANY+&KEEP                                      
&D       SETA  &TRUNC+&NIBTK+&FMHDR+&NFMHDR+&SESSPRM+&SESSKEY+&NBUFLST+*        
               &BUFFLST+&CONTCHN+&NCNTCHN+&NLMPEO+&LMPEO+&USERRH+&NUSER*        
               RH+&QALL+&QSESS+&QNOTE                  D5B4747 @R504800         
&E       SETA  &BACKUP+&USERVAR+&FORCED+&NBACKUP+&SONCD+&UNBIND+&NSONCD*        
               +&SENSE+&NSENSE+&HOLD+&RSPQUED+&NRSPQUE+&MTS+&NMTS+&KPSR*        
               B+&NKPSRB+&STATUS                              @U2C @01C         
.*             &A CONTAINS THE NUMBER OF VSAM OPTIONS SPECIFIED                 
.*             &B, &C, &D, AND &E CONTAIN THE NUMBER OF VTAM OPTIONS            
.*             SPECS, &ERR CONTAINS THE NUMBER OF ERRORS       @R489301         
         AIF   (&A+&B+&C+&D+&E+&ERR EQ &I).T000200             @R489301         
.*             IF &A+&B+&C+&D+&E+&ERR EQUAL &I, THE LAST OPTION CHECKED         
.*             WAS A VALID VTAM OPTION. ELSE IT WAS INVALID OR                  
.*             DUPLICATED A PREVIOUSLY SPECIFIED OPTION.       @R489301         
         ISTERMAC 5,OPTCD,&OPTCD(&I)                           @ZA34940         
&IDACB01 SETB  1                                                                
&ERR     SETA  &ERR+1                   BUMP ERROR COUNT        X04SVHS         
         AGO   .T000100                                                         
.T000200 AIF   ('&AM' EQ 'VTAM').T000100                                        
         ISTERMAC 20,&OPTCD(&I),OPTCD                          @ZA34940         
&IDACB01 SETB  1                                                 X03004         
         AGO   .T000100                                                         
.**********************************************************************         
.*                                                                              
.*             THE FOLLOWING CODE CHECKS FOR MUTUALLY EXCLUSIVE OPTIONS         
.*             IF THEY EXIST, AN ERROR MSG IS ISSUED                            
.*                                                                              
.T000300 AIF   (NOT(&KEY AND &ADR)).T000400                                     
         ISTERMAC 8,OPTCD,KEY,ADR                              @ZA34940         
&IDACB01 SETB  1                                                                
.T000400 AIF   (NOT(&KEY AND &CNV)).T000500                                     
         ISTERMAC 8,OPTCD,KEY,CNV                              @ZA34940         
&IDACB01 SETB  1                                                                
.T000500 AIF   (NOT(&ADR AND &CNV)).T000600                                     
         ISTERMAC 8,OPTCD,ADR,CNV                              @ZA34940         
&IDACB01 SETB  1                                                                
.T000600 AIF   (NOT(&SEQ AND &DIR)).T000700                                     
         ISTERMAC 8,OPTCD,SEQ,DIR                              @ZA34940         
&IDACB01 SETB  1                                                                
.T000700 AIF   (NOT(&SEQ AND &SKP)).T000800                                     
         ISTERMAC 8,OPTCD,SEQ,SKP                              @ZA34940         
&IDACB01 SETB  1                                                                
.T000800 AIF   (NOT(&DIR AND &SKP)).T000900                                     
         ISTERMAC 8,OPTCD,DIR,SKP                              @ZA34940         
&IDACB01 SETB  1                                                                
.T000900 AIF   (NOT(&SYN AND &ASY)).T001000                                     
         ISTERMAC 8,OPTCD,SYN,ASY                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001000 AIF   (NOT(&NUP AND &UPD)).T001100                                     
         ISTERMAC 8,OPTCD,NUP,UPD                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001100 AIF   (NOT(&NUP AND &NSP)).T001150                                     
         ISTERMAC 8,OPTCD,NUP,NSP                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001150 AIF   (NOT(&UPD AND &NSP)).T001200                                     
         ISTERMAC 8,OPTCD,UPD,NSP                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001200 AIF   (NOT(&KEQ AND &KGE)).T001350                    @ZA07549         
         ISTERMAC 8,OPTCD,KEQ,KGE                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001350 AIF   (NOT(&WAITX AND &NWAITX)).T001400               @ZA07549         
         ISTERMAC 8,OPTCD,WAITX,NWAITX                         @ZA34940         
&IDACB01 SETB  1                                               @ZA07549         
.T001400 AIF   (NOT(&FKS AND &GEN)).T001500                                     
         ISTERMAC 8,OPTCD,FKS,GEN                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001500 AIF   (NOT(&MVE AND &LOC)).T001600                      X03004         
         ISTERMAC 8,OPTCD,MVE,LOC                              @ZA34940         
&IDACB01 SETB  1                                                                
.T001600 AIF   (NOT(&CA AND &CS)).T001800                      @Z40BHUC         
