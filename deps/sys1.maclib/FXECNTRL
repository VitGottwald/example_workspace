.***AC.PLV01  PROCESS PLISTVER                                      *   00170000
.********************************************************************   00170100
&XPLISTVER      SETA  0                                                 00170200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00170300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00170400
&XPLISTVER      SETA  0                                                 00170500
.ZPLV0          ANOP  ,                                                 00170600
.*                                                                      00170700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00170800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00170900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00171000
.*-------------------------------------------------------------------   00171100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00171200
&ZMS            SETC  '"PLISTVER='                                      00171300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00171400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00171500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00171600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00171700
                AGO   .ZPVCK2                                           00171800
.ZPVCK3         ANOP  ,                                                 00171900
.*-------------------------------------------------------------------   00172000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00172100
&ZMS            SETC  '"PLISTVER='                                      00172200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00172300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00172400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00172500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00172600
                AGO   .ZPVCKE                                           00172700
.ZPVCK2         ANOP  ,                                                 00172800
.*-------------------------------------------------------------------   00172900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00173000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00173100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00173200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00173300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00173400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00173500
.ZPVCK6         ANOP  ,                                                 00173600
&ZMS            SETC  '"PLISTVER='                                      00173700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00173800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00173900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00174000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00174100
&XPLISTVER      SETA  0                                                 00174200
                AGO   .ZPVCKE                                           00174300
.*-------------------------------------------------------------------   00174400
.ZPVCK4A        ANOP  ,                                                 00174500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00174600
&ZMS            SETC  'PLISTVER='                                       00174700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00174800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00174900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00175000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00175100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00175200
                AGO   .ZPVCK5                                           00175300
.ZPVCK4         ANOP  ,                                                 00175400
&XPLISTVER      SETA  &PLISTVER(1)                                      00175500
.ZPVCK5         ANOP  ,                                                 00175600
.ZPVCKE         ANOP  ,                                                 00175700
.********************************************************************   00175800
