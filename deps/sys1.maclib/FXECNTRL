.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00598500
&ZPLLEN               SETA 128                                          00598600
.*                                                                      00598700
.******************************************************************     00598800
.***AI.L01 FXECNTRL FMT=LIST FILL IN PARAMETER AREA                     00598900
.******************************************************************     00599000
                      ACTR  10000                                       00599100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00599200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00599300
&ZCVAL                SETC  'LAE'          XAX MODE                     00599400
.ZIMFXA               ANOP ,                                            00599500
.*                                                                      00599600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00599700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00599800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00599900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00600000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00600100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00600200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00600300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00600400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00600500
&ZOPN                 SETA  &ZOPN+1                                     00600600
&ZOPL(&ZOPN)          SETC  ' '                                         00600700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00600800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00600900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00601000
                      AGO   .ZIMFNX                                     00601100
.*                                                                      00601200
.ZIMFN2               ANOP ,                                            00601300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00601400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00601500
&ZOPN                 SETA  &ZOPN+1                                     00601600
&ZOPL(&ZOPN)          SETC  ' '                                         00601700
&ZOPO(&ZOPN)          SETC  'LAE'                                       00601800
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00601900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00602000
                      AGO   .ZIMFNX                                     00602100
.ZIMFNX1              ANOP ,                                            00602200
&ZOPN                 SETA  &ZOPN+1                                     00602300
&ZOPL(&ZOPN)          SETC  ' '                                         00602400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00602500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00602600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00602700
.ZIMFNX               ANOP ,                                            00602800
.********************************************************************   00602900
