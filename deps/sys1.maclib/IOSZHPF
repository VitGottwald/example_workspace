&ZOPD(&ZOPN)          SETC  '++ &ZXUCBPTR XUCBPTR'                      00119100
.*                                                                      00119200
.ZI7E                 ANOP  ,                                           00119300
.*-------------------------------------------------------------------   00119400
&ZOPN1                SETA &ZOPN                                        00119500
.*******************************************************************    00119600
.***  PROCESS IOSZHPF OUTPUT PARAMETERS                                 00119700
.*******************************************************************    00119800
                      ACTR  10000                                       00119900
.*******************************************************************    00120000
.***AO.X02  RETURN CODE PROCESSING                                      00120100
.*******************************************************************    00120200
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00120300
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00120400
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00120500
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00120600
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00120700
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00120800
&ZOPN                 SETA  &ZOPN+1                                     00120900
&ZOPL(&ZOPN)          SETC  ' '                                         00121000
&ZOPO(&ZOPN)          SETC  'ST'                                        00121100
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00121200
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00121300
                      AGO   .ZMRETX                                     00121400
.*                                                                      00121500
.ZMRET2               ANOP  ,                                           00121600
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00121700
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00121800
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00121900
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00122000
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00122100
&ZOPN                 SETA  &ZOPN+1                                     00122200
&ZOPL(&ZOPN)          SETC  ' '                                         00122300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00122400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00122500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00122600
.ZMRETX               ANOP  ,                                           00122700
.*******************************************************************    00122800
.***AO.X03  REASON CODE PROCESSING                                      00122900
.*******************************************************************    00123000
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00123100
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00123200
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00123300
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00123400
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00123500
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00123600
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00123700
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00123800
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00123900
&ZOPN                 SETA  &ZOPN+1                                     00124000
&ZOPL(&ZOPN)          SETC  ' '                                         00124100
&ZOPO(&ZOPN)          SETC  'ST'                                        00124200
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00124300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00124400
                      AGO   .ZMRSNX                                     00124500
.*                                                                      00124600
.ZMRSN2               ANOP  ,                                           00124700
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00124800
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00124900
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00125000
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00125100
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00125200
