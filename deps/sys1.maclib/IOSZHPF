&ZMS          SETC  '"RSNCODE='                                         00083400
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00083500
&ZMS          SETC  '&ZMS "RSNCODE"'                                    00083600
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00083700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00083800
.ZG21A        ANOP  ,                                                   00083900
.*-------------------------------------------------------------------   00084000
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00084100
              AIF   (K'&RSNCODE(1) LE 0).ZG21C  NO PARM                 00084200
&ZXRSNCODE    SETC  'OUTPUT'    SET OUTPUT                              00084300
&XRSNCODE     SETC  '&RSNCODE(1)'               SET XVARIABLE           00084400
              AIF   (N'&RSNCODE GT 1).ZG21C                             00084500
&XRSNCODE     SETC  '&RSNCODE'  SET XVARIABLE                           00084600
.ZG21C        ANOP  ,                                                   00084700
.*-------------------------------------------------------------------   00084800
.***AG.KCK02 NAME, OPT OUTPUT, KEYS -GPR OUT OF RANGE                   00084900
              AIF   (K'&ZXRSNCODE LE 0).ZG21R   NOT I/O                 00085000
              AIF   ('&XRSNCODE'(1,1) NE '(').ZG21R                     00085100
&ZCGPR        SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                        00085200
              AIF   ('&ZCGPR'(1,1) NE '0').ZG21P                        00085300
              AIF   (K'&ZCGPR GT 2).ZG21Q                               00085400
              AIF   ('&ZCGPR' EQ '0').ZG21R                             00085500
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00085600
              AIF   (T'&ZCGPR NE 'N').ZG21Q     INVALID REG NAME        00085700
.ZG21P        ANOP  ,                                                   00085800
              AIF   (T'&ZCGPR NE 'N').ZG21R     NON NUMERIC             00085900
              AIF   ('&ZCGPR' EQ '0').ZG21R                             00086000
              AIF   ('&ZCGPR' LT '2').ZG21Q                             00086100
              AIF   ('&ZCGPR' LE '12').ZG21R                            00086200
.ZG21Q        ANOP  ,                                                   00086300
&ZMS          SETC  '"RSNCODE='                                         00086400
&ZMS          SETC  '&ZMS.&RSNCODE"'                                    00086500
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00086600
              MNOTE 8,' &ZMS IN THE RANGE (0) OR (2)-(12).'             00086700
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00086800
.ZG21R        ANOP  ,                                                   00086900
.******************************************************************     00087000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00087100
.******************************************************************     00087200
              ACTR  10000                                               00087300
.*----------------------------------------------------------------***   00087400
.***AH.P02 EXTRA POSITIONALS                                            00087500
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00087600
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00087700
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00087800
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00087900
.ZGLISTX1     ANOP  ,                                                   00088000
&ZCVAL        SETC  ''          ERROR FIELD                             00088100
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00088200
.ZGLIST2      ANOP  ,                                                   00088300
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00088400
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00088500
&ZCVAL        SETC '&ZCVAL,&SYSLIST(&ZAWRK)'    EXTRA                   00088600
              AGO   .ZGLIST2                                            00088700
.*                                                                      00088800
.ZGLIST3      ANOP  ,                                                   00088900
&ZCVAL        SETC '&ZCVAL'(2,K'&ZCVAL-1)       STRIP COMMA             00089000
&ZMS          SETC  'POSITIONAL ARGUMENT(S),'                           00089100
&ZMS          SETC  '&ZMS "&ZCVAL"'                                     00089200
&ZMS          SETC  '&ZMS CANNOT BE USED.'                              00089300
&ZMS          SETC  '&ZMS NO POSITIONAL ARGUMENTS'                      00089400
              MNOTE 8,' &ZMS ARE ALLOWED.'                              00089500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00089600
.ZGLISTX      ANOP  ,                                                   00089700
&ZASYSLNDX    SETA  0                                                   00089800
.*-------------------------------------------------------------------   00089900
.***AG.MF04 MF=(SLE)                                                    00090000
.ZGMFX        ANOP  ,           END OF FULL SYNTAX CHECK                00090100
.*******************************************************************    00090200
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00090300
.*******************************************************************    00090400
              AIF   (&ZMACRET NE 0).ZLX2        ERROR DETECTED          00090500
.*******************************************************************    00090600
.***AE.01  START OF IOSZHPF MACRO CODE                                  00090700
.*******************************************************************    00090800
              LCLC  &M0M0001                 NAME                       00090900
&M0M0001      SETC  '&XLABEL'                SET NAME                   00091000
              AIF   (K'&XLABEL NE 0).XLAB2                              00091100
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00091200
.*                                                                      00091300
.XLAB2        ANOP  ,                                                   00091400
              AIF   ('&XMF' EQ 'L').XLAB3                               00091500
&M0M0001 DS    0H                             IOSZHPF-0                 00091600
.XLAB3        ANOP  ,                                                   00091700
.*******************************************************************    00091800
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00091900
.*******************************************************************    00092000
              PUSH  PRINT                                               00092100
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00092200
              PRINT OFF         ++ PR01                                 00092300
              AGO   .ZPRNTXX                                            00092400
.ZPRINTX      PRINT ON                                                  00092500
.ZPRNTXX      ANOP  ,                                                   00092600
.********************************************************************   00092700
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00092800
.********************************************************************   00092900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00093000
                      LCLA  &ZOPN1         INVOCATION INDEX             00093100
                      LCLC  &ZOPL(4095)    LABEL                        00093200
                      LCLC  &ZOPO(4095)    OPERATION                    00093300
                      LCLC  &ZOPV(4095)    VALUE                        00093400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00093500
.*                                                                      00093600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00093700
.******************************************************************     00093800
.***AL.L10 IOSZHPF FMT=LIST MF=S!L PARAMETERS                           00093900
.******************************************************************     00094000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00094100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00094200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00094300
&XMFATTR              SETC  '0D'                                        00094400
.*                                                                      00094500
&ZOPN                 SETA  &ZOPN+1                                     00094600
&ZOPL(&ZOPN)          SETC  ' '                                         00094700
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00094800
&ZOPO(&ZOPN)          SETC  'B'                                         00094900
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00095000
                      AGO   .ZALVL6B                                    00095100
.ZALVL6A              ANOP                                              00095200
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00095300
&ZOPV(&ZOPN)          SETC  '4,8'                                       00095400
&ZOPN                 SETA  &ZOPN+1                                     00095500
&ZOPL(&ZOPN)          SETC  ' '                                         00095600
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00095700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00095800
.ZALVL6B              ANOP                                              00095900
&ZOPD(&ZOPN)          SETC  '++ IOSZHPF ARG PARMS'                      00096000
.*                                                                      00096100
&ZOPN                 SETA  &ZOPN+1                                     00096200
&ZOPL(&ZOPN)          SETC  ' '                                         00096300
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00096400
&ZOPV(&ZOPN)          SETC  '1'                                         00096500
&ZOPD(&ZOPN)          SETC  ' '                                         00096600
.*                                                                      00096700
.ZLMFSL2              ANOP  ,                                           00096800
.*-------------------------------------------------------------------   00096900
.***AM.L00  IOSZHPF FMT=LIST name                                       00097000
