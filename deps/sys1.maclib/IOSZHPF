&ZOPV(&ZOPN)          SETC  '1(1),B''&ZW'''                             00113800
&ZOPD(&ZOPN)          SETC  '++ INPUT BL1'                              00113900
.ZI3E                 ANOP  ,                                           00114000
.*-------------------------------------------------------------------   00114100
.***AI.L19 ADDR XINFOAREA A                                             00114200
                      AIF   ('&ZXINFOAREA' EQ '').ZI5H                  00114300
&ZCLNG                SETC  '&XINFOAREA'                                00114400
&ZCGPR                SETC  '14'                                        00114500
         AIF   ('&XINFOAREA'(1,1) EQ '(').ZI5F  IN GPR                  00114600
.*                                                                      00114700
&ZOPN                 SETA  &ZOPN+1                                     00114800
&ZOPL(&ZOPN)          SETC  ' '                                         00114900
