.***AG.KKCTL01 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00042900
&XMF          SETC  ''          INITIALIZE XVARIABLE                    00043000
.*-------------------------------------------------------------------   00043100
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00043200
.ZG25         ANOP  ,                                                   00043300
&ZTLC         SETC 'S'                                                  00043400
              AIF   ('&MF(1)' NE '&ZTLC').ZG26                          00043500
&XMF          SETC  'S'         SET KEYWORD                             00043600
              AIF   (N'&MF LE 1).ZG25A          CK NUM                  00043700
&ZMS          SETC  '"MF='                                              00043800
&ZMS          SETC  '&ZMS.&MF"'                                         00043900
&ZMS          SETC  '&ZMS "MF"'                                         00044000
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00044100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00044200
.ZG25A        ANOP  ,                                                   00044300
.*-------------------------------------------------------------------   00044400
.***AG.KKCTL02 VALUE, OPT INPUT, ME KYWRDCTL -KEYWORD CONTROL           00044500
.ZG26         ANOP  ,                                                   00044600
&ZTLC         SETC 'L'                                                  00044700
              AIF   ('&MF(1)' NE '&ZTLC').ZG33                          00044800
&XMF          SETC  'L'         SET KEYWORD                             00044900
              AIF   (N'&MF LE 3).ZG26A          CK NUM                  00045000
&ZMS          SETC  '"MF='                                              00045100
&ZMS          SETC  '&ZMS.&MF"'                                         00045200
&ZMS          SETC  '&ZMS "MF"'                                         00045300
              MNOTE 8,' &ZMS CAN HAVE ONLY 3 ARGUMENTS.'                00045400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00045500
.ZG26A        ANOP  ,                                                   00045600
.*-------------------------------------------------------------------   00045700
.***AG.KKCTL08 -MF(L) SPECIAL CHECK                                     00045800
              AIF   (N'&SYSLIST NE 0).ZG26B     POSITIONALS?            00045900
              AIF   ('&INFOAREA' NE '').ZG26B   KEYS?                   00046000
              AIF   ('&UCBPTR' NE '').ZG26B     KEYS?                   00046100
              AIF   ('&DEVINFO' NE '').ZG26B    KEYS?                   00046200
              AIF   ('&LINKAGE' NE '').ZG26B    KEYS?                   00046300
              AIF   ('&RETCODE' NE '').ZG26B    KEYS?                   00046400
              AIF   ('&RSNCODE' NE '').ZG26B    KEYS?                   00046500
              AGO    .ZG26C     INVOCATION OKAY                         00046600
.ZG26B        ANOP  ,                                                   00046700
&ZMS          SETC  'THE "L" FORM OF "MF" CANNOT'                       00046800
&ZMS          SETC  '&ZMS HAVE ANY OTHER KEYWORDS OR'                   00046900
&ZMS          SETC  '&ZMS POSITIONAL PARAMETERS SPECIFIED'              00047000
              MNOTE 8,' &ZMS ON THE INVOCATION.'                        00047100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00047200
.ZG26C        ANOP  ,                                                   00047300
.*-------------------------------------------------------------------   00047400
