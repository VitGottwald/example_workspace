&ZOPD(&ZOPN)          SETC '++ INPUT XVERSION'                          00099100
.*-------------------------------------------------------------------   00099200
.***AM.L03 XFLAG1 BL.8                                                  00099300
&ZOPN                 SETA &ZOPN+1                                      00099400
&ZOPL(&ZOPN)          SETC '&XMFCTRL._XFLAG1'                           00099500
&ZOPO(&ZOPN)          SETC 'DS'                                         00099600
&ZOPV(&ZOPN)          SETC 'BL.8'                                       00099700
&ZOPD(&ZOPN)          SETC '++ FIELD_LABEL'                             00099800
.*                                                                      00099900
&ZOPN                 SETA  &ZOPN+1                                     00100000
&ZOPL(&ZOPN)          SETC  '&XMFCTRL._XDEVINFO_YES'                    00100100
&ZOPO(&ZOPN)          SETC  'EQU'                                       00100200
&ZOPV(&ZOPN)          SETC  'B''10000000'''                             00100300
&ZOPD(&ZOPN)          SETC  '++ XDEVINFO.'                              00100400
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN).YES'                          00100500
&ZOPD(&ZOPN)          SETC  '&ZOPD(&ZOPN) KEYWORD'                      00100600
.*-------------------------------------------------------------------   00100700
.***AM.L09  XRSVD1000 CL2                                               00100800
&ZOPN                 SETA &ZOPN+1                                      00100900
