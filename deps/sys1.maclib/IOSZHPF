&ZOPD(&ZOPN)          SETC '++ LENGTH OF PLIST'                         00104100
.*                                                                      00104200
&ZOPN                 SETA &ZOPN+1                                      00104300
&ZOPL(&ZOPN)          SETC ' '                                          00104400
&ZOPO(&ZOPN)          SETC 'SPACE'                                      00104500
&ZOPV(&ZOPN)          SETC '1'                                          00104600
&ZOPD(&ZOPN)          SETC ' '                                          00104700
.*-------------------------------------------------------------------   00104800
.***AL.L12 IOSZHPF FMT=LIST MF=S!L PARAMETERS                           00104900
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00105000
.*                                                                      00105100
&ZOPN                 SETA  &ZOPN+1                                     00105200
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00105300
&ZOPO(&ZOPN)          SETC  'DC'                                        00105400
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00105500
&ZOPD(&ZOPN)          SETC  '++ IOSZHPF ARG PARMS'                      00105600
.*                                                                      00105700
.ZLMFSLX              ANOP  ,                                           00105800
.*-------------------------------------------------------------------   00105900
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00106000
&ZPLLEN               SETA 16                                           00106100
.*                                                                      00106200
.******************************************************************     00106300
.***AI.L01 IOSZHPF FMT=LIST FILL IN PARAMETER AREA                      00106400
.******************************************************************     00106500
                      ACTR  10000                                       00106600
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00106700
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00106800
&ZCVAL                SETC  'LAE'          XAX MODE                     00106900
.ZIMFXA               ANOP ,                                            00107000
.*                                                                      00107100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00107200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00107300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00107400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00107500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00107600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00107700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00107800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00107900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00108000
&ZOPN                 SETA  &ZOPN+1                                     00108100
&ZOPL(&ZOPN)          SETC  ' '                                         00108200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00108300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00108400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00108500
                      AGO   .ZIMFNX                                     00108600
.*                                                                      00108700
.ZIMFN2               ANOP ,                                            00108800
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00108900
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00109000
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00109100
&ZOPN                 SETA  &ZOPN+1                                     00109200
&ZOPL(&ZOPN)          SETC  ' '                                         00109300
&ZOPO(&ZOPN)          SETC  'LAE'                                       00109400
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00109500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00109600
                      AGO   .ZIMFNX                                     00109700
.ZIMFNX1              ANOP ,                                            00109800
&ZOPN                 SETA  &ZOPN+1                                     00109900
&ZOPL(&ZOPN)          SETC  ' '                                         00110000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00110100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00110200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00110300
.ZIMFNX               ANOP ,                                            00110400
.********************************************************************   00110500
.***AI.L02 IOSZHPF FMT=LIST INITIALIZE PARAMETER AREA                   00110600
.********************************************************************   00110700
.***AI.L03 MF=(SLE)                                                     00110800
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00110900
.*                                                                      00111000
&ZOPN                 SETA  &ZOPN+1                                     00111100
&ZOPL(&ZOPN)          SETC  ' '                                         00111200
&ZOPO(&ZOPN)          SETC  'XC'                                        00111300
&ZOPV(&ZOPN)          SETC  '0(&ZPLLEN,1),0(1)'                         00111400
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00111500
.ZIMFLX               ANOP  ,                                           00111600
.*-------------------------------------------------------------------   00111700
.***AI.L05 XFLAG1 BL1                                                   00111800
&ZW                   SETC  ''                                          00111900
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00112000
.*-------------------------------------------------------------------   00112100
.***AI.L08 XFLAG1 "YES"                                                 00112200
.***      BL1'1XXXXXXX'                                                 00112300
         AIF   ('&XDEVINFO' NE 'YES').ZI3A                              00112400
&ZW                   SETC  '&ZW.10000000'                              00112500
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00112600
                      AGO   .ZI3C                                       00112700
.*                                                                      00112800
.ZI3A                 ANOP  ,                                           00112900
&ZW                   SETC  '&ZW.00000000'                              00113000
.ZI3C                 ANOP  ,                                           00113100
.*-------------------------------------------------------------------   00113200
.***AI.L11 XFLAG1 BL1                                                   00113300
                      AIF   ('&ZCLNG' NE 'YES').ZI3E  ALL ZERO BITS     00113400
&ZOPN                 SETA  &ZOPN+1                                     00113500
&ZOPL(&ZOPN)          SETC  ' '                                         00113600
