.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00126700
.*******************************************************************    00126800
                      ACTR  10000                                       00126900
.*                                                                      00127000
                      LCLA  &ZLC           COLUMN POSITION              00127100
                      LCLA  &ZOPX          LIST TABLE INDEX             00127200
                      LCLC  &ZLL           LABEL                        00127300
                      LCLC  &ZLOP          OPERATION                    00127400
                      LCLC  &ZLV           VALUE                        00127500
                      LCLC  &ZLD           DESCRIPTION                  00127600
.*                                                                      00127700
&ZOPX                 SETA  1                                           00127800
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00127900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00128000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00128100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00128200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00128300
                      AIF   (K'&ZLD LE 0).ZL5                           00128400
&ZLC                  SETA  35             COMMENT COLUMN               00128500
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00128600
&ZLC                  SETA  33             COMMMENT COLUMN              00128700
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00128800
&ZLV                  SETC  '&ZLV'.' '                                  00128900
                      AGO   .ZL3                                        00129000
.*                                                                      00129100
.ZL4                  ANOP  ,                                           00129200
&ZLV                  SETC  '&ZLV'.' '                                  00129300
.ZL5                  ANOP  ,                                           00129400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00129500
.*                                                                      00129600
&ZLL     &ZLOP &ZLV                                                     00129700
.*                                                                      00129800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00129900
                      AGO   .ZL2           CONTINUE LOOPING             00130000
.*                                                                      00130100
.ZLX                  ANOP  ,                                           00130200
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00130300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00130400
.*                                                                      00130500
         AIF   ('&xLINKAGE' EQ 'BRANCH').BRANCH                         00130600
         MNOTE 8,'IOSZHPF LINKAGE=SYSTEM not supported'                 00130700
         MEXIT ,                                                        00130800
.*                                                                      00130900
.BRANCH  ANOP  ,                                                        00131000
         L     15,16(0,0)                                               00131100
         L     15,124(15,0)                                             00131200
         L     15,208(15,0)                                             00131300
         L     15,336(15,0)                                             00131400
         BASR  14,15                                                    00131500
.*                                                                      00131600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00131700
.ZLINKX               ANOP  ,                                           00131800
.*******************************************************************    00131900
.***AP.X01  FORMAT GENERATED CODE                                       00132000
