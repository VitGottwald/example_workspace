.***AC.PLV01  PROCESS PLISTVER                                      *   00216900
.********************************************************************   00217000
&XPLISTVER      SETA  0                                                 00217100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00217200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00217300
&XPLISTVER      SETA  4                                                 00217400
.ZPLV0          ANOP  ,                                                 00217500
.*                                                                      00217600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00217700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00217800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00217900
.*-------------------------------------------------------------------   00218000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00218100
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00218200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00218300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00218400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00218500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00218600
                AGO   .ZPVCK2                                           00218700
.ZPVCK3         ANOP  ,                                                 00218800
.*-------------------------------------------------------------------   00218900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00219000
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00219100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00219200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00219300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00219400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00219500
                AGO   .ZPVCKE                                           00219600
.ZPVCK2         ANOP  ,                                                 00219700
.*-------------------------------------------------------------------   00219800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00219900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00220000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00220100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00220200
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00220300
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00220400
.ZPVCK6         ANOP  ,                                                 00220500
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00220600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00220700
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00220800
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00220900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00221000
&XPLISTVER      SETA  4                                                 00221100
                AGO   .ZPVCKE                                           00221200
.*-------------------------------------------------------------------   00221300
.ZPVCK4A        ANOP  ,                                                 00221400
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00221500
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00221600
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00221700
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00221800
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00221900
                MNOTE 8,' &ZMS IS REQUIRED.'                            00222000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00222100
                AGO   .ZPVCK5                                           00222200
.ZPVCK4         ANOP  ,                                                 00222300
&XPLISTVER      SETA  &PLISTVER(1)                                      00222400
.ZPVCK5         ANOP  ,                                                 00222500
.ZPVCKE         ANOP  ,                                                 00222600
.********************************************************************   00222700
