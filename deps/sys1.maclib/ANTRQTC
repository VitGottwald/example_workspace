.*-------------------------------------------------------------------   01044400
.***AI.L04B XVERSION XL1                                                01044500
                      AIF   (&XPLISTVER EQ 0).ZIMFVX                    01044600
                      AIF   ('&XMFSCK' EQ 'COMPLETE').ZIMFVY            01044700
.ZIMFVX               ANOP  ,                                           01044800
                      AIF   ('&PLISTVER' EQ '').ZIMFVZ                  01044900
                      AIF   ('&XMFSCK' EQ 'COMPLETE').ZIMFVZ            01045000
.ZIMFVY               ANOP  ,                                           01045100
&ZOPN                 SETA  &ZOPN+1                                     01045200
&ZOPL(&ZOPN)          SETC  ' '                                         01045300
&ZOPO(&ZOPN)          SETC  'MVI'                                       01045400
&ZOPV(&ZOPN)          SETC  '12(1),&XPLISTVER'                          01045500
&ZOPD(&ZOPN)          SETC  '++ INPUT XVERSION XL1'                     01045600
.ZIMFVZ               ANOP  ,                                           01045700
.*-------------------------------------------------------------------   01045800
.***AI.L18  XILKNBR XL1                                                 01045900
         AIF   ('&XILKNBR' NE '0').ZI4                                  01046000
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZI4  CK *         01046100
&ZXILKNBR             SETC  ''             RESET INPUT/OUTPUT           01046200
