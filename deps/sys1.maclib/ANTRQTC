.***AI.C00 MF=M CHECK TO BYPASS INVOCATION                              01161700
                      AIF   ('&XMF' EQ 'M').ZMFLX  DONE IF MF=M         01161800
.*******************************************************************    01161900
.***  PROCESS ANTRQTC OUTPUT PARAMETERS                                 01162000
.*******************************************************************    01162100
                      ACTR  10000                                       01162200
.*******************************************************************    01162300
.***AO.X02  RETURN CODE PROCESSING                                      01162400
.*******************************************************************    01162500
                      AIF   (N'&RETCODE LE 0).ZMRETX                    01162600
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            01162700
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             01162800
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             01162900
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               01163000
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                01163100
&ZOPN                 SETA  &ZOPN+1                                     01163200
&ZOPL(&ZOPN)          SETC  ' '                                         01163300
&ZOPO(&ZOPN)          SETC  'ST'                                        01163400
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              01163500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01163600
                      AGO   .ZMRETX                                     01163700
.*                                                                      01163800
.ZMRET2               ANOP  ,                                           01163900
