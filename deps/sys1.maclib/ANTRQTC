.*----------------------------------------------------------------***   00658400
.***AH.P02 EXTRA POSITIONALS                                            00658500
               AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS               00658600
               AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL               00658700
               AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL           00658800
               AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL            00658900
.ZGLISTX1      ANOP  ,                                                  00659000
&ZCVAL         SETC  ''          ERROR FIELD                            00659100
&ZAWRK         SETA  0           POSITIONAL NUMBER                      00659200
.ZGLIST2       ANOP  ,                                                  00659300
&ZAWRK         SETA  &ZAWRK+1    NEXT POSITIONAL                        00659400
               AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE               00659500
&ZCVAL         SETC '&ZCVAL,&SYSLIST(&ZAWRK)'   EXTRA                   00659600
               AGO   .ZGLIST2                                           00659700
.*                                                                      00659800
.ZGLIST3       ANOP  ,                                                  00659900
&ZCVAL         SETC '&ZCVAL'(2,K'&ZCVAL-1)      STRIP COMMA             00660000
&ZMS           SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE           00660100
&ZMS           SETC  '&ZMS "&ZCVAL"'                                    00660200
&ZMS           SETC  '&ZMS CANNOT BE USED.'                             00660300
&ZMS           SETC  '&ZMS NO POSITIONAL ARGUMENTS'                     00660400
               MNOTE 8,' &ZMS ARE ALLOWED.'                             00660500
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00660600
.ZGLISTX       ANOP  ,                                                  00660700
