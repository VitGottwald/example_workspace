.*-------------------------------------------------------------------   01034500
.***AL.L12 ANTRQTC FMT=LIST MF=S!L PARAMETERS                           01034600
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         01034700
.*                                                                      01034800
&ZOPN                 SETA  &ZOPN+1                                     01034900
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                01035000
&ZOPO(&ZOPN)          SETC  'DC'                                        01035100
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   01035200
&ZOPD(&ZOPN)          SETC  '++ ANTRQTC ARG PARMS'                      01035300
.*                                                                      01035400
.ZLMFSLX              ANOP  ,                                           01035500
.*-------------------------------------------------------------------   01035600
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          01035700
&ZPLLEN               SETA 512                                          01035800
.*                                                                      01035900
.******************************************************************     01036000
.***AI.L01 ANTRQTC FMT=LIST FILL IN PARAMETER AREA                      01036100
.******************************************************************     01036200
                      ACTR  10000                                       01036300
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01036400
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01036500
&ZCVAL                SETC  'LAE'          XAX MODE                     01036600
.ZIMFXA               ANOP ,                                            01036700
.*                                                                      01036800
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01036900
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01037000
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01037100
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01037200
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01037300
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01037400
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01037500
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01037600
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01037700
&ZOPN                 SETA  &ZOPN+1                                     01037800
&ZOPL(&ZOPN)          SETC  ' '                                         01037900
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01038000
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01038100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01038200
                      AGO   .ZIMFNX                                     01038300
.*                                                                      01038400
.ZIMFN2               ANOP ,                                            01038500
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01038600
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01038700
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01038800
&ZOPN                 SETA  &ZOPN+1                                     01038900
&ZOPL(&ZOPN)          SETC  ' '                                         01039000
&ZOPO(&ZOPN)          SETC  'LAE'                                       01039100
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01039200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01039300
                      AGO   .ZIMFNX                                     01039400
.ZIMFNX1              ANOP ,                                            01039500
&ZOPN                 SETA  &ZOPN+1                                     01039600
&ZOPL(&ZOPN)          SETC  ' '                                         01039700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01039800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                01039900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01040000
.ZIMFNX               ANOP ,                                            01040100
.********************************************************************   01040200
.***AI.L02 ANTRQTC FMT=LIST INITIALIZE PARAMETER AREA                   01040300
.********************************************************************   01040400
.***AI.L03 MF=(SLME)                                                    01040500
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      01040600
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            01040700
&ZAWRK                SETA  0                                           01040800
&ZN                   SETA  &ZPLLEN                                     01040900
.ZCLRCONT             AIF   (&ZN LE 256).ZCLRX                          01041000
.*                                                                      01041100
&ZOPN                 SETA  &ZOPN+1                                     01041200
&ZOPL(&ZOPN)          SETC  ' '                                         01041300
&ZOPO(&ZOPN)          SETC  'XC'                                        01041400
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(256,1),&ZAWRK.(1)'                 01041500
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             01041600
.*                                                                      01041700
&ZAWRK                SETA &ZAWRK+256                                   01041800
&ZN                   SETA  &ZN-256                                     01041900
                      AGO   .ZCLRCONT                                   01042000
.*                                                                      01042100
.ZCLRX                ANOP  ,                                           01042200
.*                                                                      01042300
&ZOPN                 SETA  &ZOPN+1                                     01042400
&ZOPL(&ZOPN)          SETC  ' '                                         01042500
&ZOPO(&ZOPN)          SETC  'XC'                                        01042600
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(&ZN,1),&ZAWRK.(1)'                 01042700
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             01042800
.ZIMFLX               ANOP  ,                                           01042900
.*-------------------------------------------------------------------   01043000
.***AI.L60 XEYECATCH CL8                                                01043100
&ZOPN                 SETA  &ZOPN+1                                     01043200
&ZOPL(&ZOPN)          SETC  ' '                                         01043300
&ZOPO(&ZOPN)          SETC  'MVC'                                       01043400
&ZOPV(&ZOPN)          SETC  '0(8,1),=CL8''ANTRQSTL'''                   01043500
&ZOPD(&ZOPN)          SETC  '++ CL8 CONSTANT'                           01043600
.*-------------------------------------------------------------------   01043700
.***AI.L60 XEYEVERSN CL4                                                01043800
&ZOPN                 SETA  &ZOPN+1                                     01043900
&ZOPL(&ZOPN)          SETC  ' '                                         01044000
