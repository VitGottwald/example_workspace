.***AC.PLV01  PROCESS PLISTVER                                      *   00137400
.********************************************************************   00137500
&XPLISTVER      SETA  0                                                 00137600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00137700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00137800
&XPLISTVER      SETA  1                                                 00137900
.ZPLV0          ANOP  ,                                                 00138000
.*                                                                      00138100
                AIF   ('&FORCE' EQ '').ZPLV1E                           00138200
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00138300
&XPLISTVER      SETA  1                                                 00138400
.ZPLV1E         ANOP  ,                                                 00138500
.*                                                                      00138600
                AIF   ('&OBLOCKID' EQ '').ZPLV2E                        00138700
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00138800
&XPLISTVER      SETA  1                                                 00138900
.ZPLV2E         ANOP  ,                                                 00139000
.*                                                                      00139100
                AIF   ('&REQDATA' EQ '').ZPLV3E                         00139200
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00139300
&XPLISTVER      SETA  1                                                 00139400
.ZPLV3E         ANOP  ,                                                 00139500
.*                                                                      00139600
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00139700
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00139800
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00139900
.*-------------------------------------------------------------------   00140000
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00140100
&ZMS            SETC  '"PLISTVER='                                      00140200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00140300
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00140400
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00140500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00140600
                AGO   .ZPVCK2                                           00140700
.ZPVCK3         ANOP  ,                                                 00140800
.*-------------------------------------------------------------------   00140900
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00141000
&ZMS            SETC  '"PLISTVER='                                      00141100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00141200
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00141300
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00141400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00141500
                AGO   .ZPVCKE                                           00141600
.ZPVCK2         ANOP  ,                                                 00141700
.*-------------------------------------------------------------------   00141800
&ZCVAL          SETC  '&PLISTVER(1)'                                    00141900
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00142000
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00142100
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00142200
