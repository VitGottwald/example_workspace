.ZMFLX                ANOP  ,              MF=L PROCESSING              00401700
.*******************************************************************    00401800
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00401900
.*******************************************************************    00402000
                      ACTR  10000                                       00402100
.*                                                                      00402200
                      LCLA  &ZLC           COLUMN POSITION              00402300
                      LCLA  &ZOPX          LIST TABLE INDEX             00402400
                      LCLC  &ZLL           LABEL                        00402500
                      LCLC  &ZLOP          OPERATION                    00402600
                      LCLC  &ZLV           VALUE                        00402700
                      LCLC  &ZLD           DESCRIPTION                  00402800
.*                                                                      00402900
&ZOPX                 SETA  1                                           00403000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00403100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00403200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00403300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00403400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00403500
                      AIF   (K'&ZLD LE 0).ZL5                           00403600
&ZLC                  SETA  35             COMMENT COLUMN               00403700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00403800
&ZLC                  SETA  33             COMMMENT COLUMN              00403900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00404000
&ZLV                  SETC  '&ZLV'.' '                                  00404100
                      AGO   .ZL3                                        00404200
.*                                                                      00404300
.ZL4                  ANOP  ,                                           00404400
&ZLV                  SETC  '&ZLV'.' '                                  00404500
.ZL5                  ANOP  ,                                           00404600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00404700
.*                                                                      00404800
&ZLL     &ZLOP &ZLV                                                     00404900
.*                                                                      00405000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00405100
                      AGO   .ZL2           CONTINUE LOOPING             00405200
.*                                                                      00405300
.ZLX                  ANOP  ,                                           00405400
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00405500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00405600
         L     14,16(0,0)              GET CVT ADDRESS                  00405700
         TM    CVTOSLV1-CVT(14),X'40'      TEST FOR WLM PRESENCE        00405800
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00405900
         AIF   ('&XTRAXFRPT' EQ 'YES').RPTSYS BRANCH IF SYSEVENT NEEDED 00406000
         LA    0,IwmRsnCodeCompatNoSyseventRqd SET REASON CODE = NO WLM 00406100
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00406200
         B     END&SYSNDX              BRANCH TO END LABEL              00406300
         AGO    .RPTWLM                                                 00406400
.RPTSYS  ANOP                                                           00406500
         L     1,4(1,0)                ESTABLISH THE SYSEVENT PL ADDR   00406600
         SYSEVENT TRAXFRPT,ENTRY=BRANCH                                 00406700
         LTR   15,15                                                    00406800
         BZ    END&SYSNDX              BRANCH TO END WHEN SUCCESSFUL    00406900
         LA    0,IWMRETCODEENVERROR                                     00407000
         CR    15,0                                                     00407100
         BE    R12&SYSNDX              BRANCH WHEN SYSEVENT RC=12       00407200
         DS    0H                      SYSEVENT RC = 8 (NO WORK ELT)    00407300
         LR    15,0                    SET RC = IWMRETCODEENVERROR      00407400
         LA    0,IWMRSNCODESYSEVENTNOWORKELT  SET REASONCODE            00407500
         B     END&SYSNDX              BRANCH TO END LABEL              00407600
R12&SYSNDX DS    0H                    SYSEVENT RC=12 (REPORTING SUSP)  00407700
         LA    0,IWMRSNCODEREPORTINGSUSP      SET REASONCODE            00407800
         B     END&SYSNDX              BRANCH TO END LABEL              00407900
.RPTWLM   ANOP                         CODE GENERATION FOR WLM PRESENT  00408000
*                                                                       00408100
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00408200
         AIF   ('&XENDTIME' NE 'CURRENT').RPTENDTIME                    00408300
         STCK  40(1)                   SAVE TOD IN P.L.                 00408400
         BC    8,IWMRPT_TIMEOK&SYSNDX  BRANCH WHEN STCK CC=0            00408500
         LA    0,IWMRSNCODENOENDTIME   SET REASONCODE                   00408600
         LA    15,IWMRETCODEENVERROR   SET RC = IWMRETCODEENVERROR      00408700
         B     IWMRPT_FINIS&SYSNDX     BRANCH TO THE END                00408800
IWMRPT_TIMEOK&SYSNDX DS   0H           BRANCH LABEL FOR STCK CC=0       00408900
.RPTENDTIME ANOP                       BRANCH LABEL WHEN ENDTIME GIVEN  00409000
         AIF   ('&XPSWKEYP' NE 'CURRENT').RPTNKEY BRANCH IF NOT PSWKEY  00409100
         LR    0,2                     SAVE GPR 2                       00409200
         SLR   2,2                                                      00409300
         IPK                           SET BITS 24-27 OF GPR2 TO PSWKEY 00409400
         SRL   2,4(0)                  SHIFT PSWKEY TO BITS 28-31       00409500
         STC   2,3(1,0)                SAVE THE SHIFTED PSWKEY IN P.L.  00409600
         LR    2,0                     RESTORE GPR 2 TO ORIGINAL VALUE  00409700
.RPTNKEY ANOP                                                           00409800
         L     14,X'8C'(14,0)                                           00409900
         L     14,X'C4'(14,0)                                           00410000
         L     15,X'100'(14,0)         GET REPORT RTN FOR MONTKN INPUT  00410100
         BASR  14,15                   CALL REPORT ROUTINE              00410200
         AIF   ('&XENDTIME' NE 'CURRENT').RPTEND                        00410300
IWMRPT_FINIS&SYSNDX DS   0H            BRANCH LABEL FOR STCK NON-0 CC   00410400
.RPTEND      ANOP                                                       00410500
END&SYSNDX DS    0H                    END LABEL                        00410600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00410700
