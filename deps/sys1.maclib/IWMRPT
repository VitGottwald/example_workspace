.***AC.PLV01  PROCESS PLISTVER                                      *   00121200
.********************************************************************   00121300
&XPLISTVER      SETA  0                                                 00121400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00121500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00121600
&XPLISTVER      SETA  1                                                 00121700
.ZPLV0          ANOP  ,                                                 00121800
.*                                                                      00121900
                AIF   ('&EWLM_CHCORR' EQ '').ZPLV1E                     00122000
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00122100
&XPLISTVER      SETA  1                                                 00122200
.ZPLV1E         ANOP  ,                                                 00122300
.*                                                                      00122400
                AIF   ('&EWLM_PACORR' EQ '').ZPLV2E                     00122500
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00122600
&XPLISTVER      SETA  1                                                 00122700
.ZPLV2E         ANOP  ,                                                 00122800
.*                                                                      00122900
                AIF   ('&EWLM_CHCTKN' EQ '').ZPLV3E                     00123000
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00123100
&XPLISTVER      SETA  1                                                 00123200
.ZPLV3E         ANOP  ,                                                 00123300
.*                                                                      00123400
                AIF   ('&BLOCK_TIME' EQ '').ZPLV4E                      00123500
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00123600
&XPLISTVER      SETA  1                                                 00123700
.ZPLV4E         ANOP  ,                                                 00123800
.*                                                                      00123900
                AIF   ('&WORK_AREA' EQ '').ZPLV5E                       00124000
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00124100
&XPLISTVER      SETA  1                                                 00124200
.ZPLV5E         ANOP  ,                                                 00124300
.*                                                                      00124400
                AIF   ('&WORKREQ_STA' EQ '').ZPLV6E                     00124500
                AIF   (&XPLISTVER GE 1).ZPLV6E                          00124600
&XPLISTVER      SETA  1                                                 00124700
.ZPLV6E         ANOP  ,                                                 00124800
.*                                                                      00124900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00125000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00125100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00125200
.*-------------------------------------------------------------------   00125300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00125400
&ZMS            SETC  '"PLISTVER='                                      00125500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00125600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00125700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00125800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00125900
                AGO   .ZPVCK2                                           00126000
.ZPVCK3         ANOP  ,                                                 00126100
.*-------------------------------------------------------------------   00126200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00126300
&ZMS            SETC  '"PLISTVER='                                      00126400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00126500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00126600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00126700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00126800
                AGO   .ZPVCKE                                           00126900
.ZPVCK2         ANOP  ,                                                 00127000
.*-------------------------------------------------------------------   00127100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00127200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00127300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00127400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00127500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00127600
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00127700
.ZPVCK6         ANOP  ,                                                 00127800
&ZMS            SETC  '"PLISTVER='                                      00127900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00128000
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00128100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00128200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00128300
&XPLISTVER      SETA  1                                                 00128400
                AGO   .ZPVCKE                                           00128500
.*-------------------------------------------------------------------   00128600
.ZPVCK4A        ANOP  ,                                                 00128700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00128800
&ZMS            SETC  'PLISTVER='                                       00128900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00129000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00129100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00129200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00129300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00129400
                AGO   .ZPVCK5                                           00129500
.ZPVCK4         ANOP  ,                                                 00129600
&XPLISTVER      SETA  &PLISTVER(1)                                      00129700
.ZPVCK5         ANOP  ,                                                 00129800
.ZPVCKE         ANOP  ,                                                 00129900
