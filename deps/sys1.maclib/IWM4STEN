.***AC.PLV01  PROCESS PLISTVER                                      *   00061000
.********************************************************************   00061100
&XPLISTVER      SETA  0                                                 00061200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00061300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00061400
&XPLISTVER      SETA  0                                                 00061500
.ZPLV0          ANOP  ,                                                 00061600
.*                                                                      00061700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00061800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00061900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00062000
.*-------------------------------------------------------------------   00062100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00062200
&ZMS            SETC  '"PLISTVER='                                      00062300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00062500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00062600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062700
                AGO   .ZPVCK2                                           00062800
.ZPVCK3         ANOP  ,                                                 00062900
.*-------------------------------------------------------------------   00063000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00063100
&ZMS            SETC  '"PLISTVER='                                      00063200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00063400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00063500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063600
                AGO   .ZPVCKE                                           00063700
.ZPVCK2         ANOP  ,                                                 00063800
.*-------------------------------------------------------------------   00063900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00064000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00064100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00064200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00064300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00064400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00064500
.ZPVCK6         ANOP  ,                                                 00064600
&ZMS            SETC  '"PLISTVER='                                      00064700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00064900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00065000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065100
&XPLISTVER      SETA  0                                                 00065200
                AGO   .ZPVCKE                                           00065300
.*-------------------------------------------------------------------   00065400
.ZPVCK4A        ANOP  ,                                                 00065500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00065600
&ZMS            SETC  'PLISTVER='                                       00065700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00065800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00065900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00066000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00066100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066200
                AGO   .ZPVCK5                                           00066300
.ZPVCK4         ANOP  ,                                                 00066400
&XPLISTVER      SETA  &PLISTVER(1)                                      00066500
.ZPVCK5         ANOP  ,                                                 00066600
.ZPVCKE         ANOP  ,                                                 00066700
.********************************************************************   00066800
