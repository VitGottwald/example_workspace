.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00135000
.*******************************************************************    00135100
                      ACTR  10000                                       00135200
.*                                                                      00135300
                      LCLA  &ZLC           COLUMN POSITION              00135400
                      LCLA  &ZOPX          LIST TABLE INDEX             00135500
                      LCLC  &ZLL           LABEL                        00135600
                      LCLC  &ZLOP          OPERATION                    00135700
                      LCLC  &ZLV           VALUE                        00135800
                      LCLC  &ZLD           DESCRIPTION                  00135900
.*                                                                      00136000
&ZOPX                 SETA  1                                           00136100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00136200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00136300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00136400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00136500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00136600
                      AIF   (K'&ZLD LE 0).ZL5                           00136700
&ZLC                  SETA  35             COMMENT COLUMN               00136800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00136900
&ZLC                  SETA  33             COMMMENT COLUMN              00137000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00137100
&ZLV                  SETC  '&ZLV'.' '                                  00137200
                      AGO   .ZL3                                        00137300
.*                                                                      00137400
.ZL4                  ANOP  ,                                           00137500
&ZLV                  SETC  '&ZLV'.' '                                  00137600
.ZL5                  ANOP  ,                                           00137700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00137800
.*                                                                      00137900
&ZLL     &ZLOP &ZLV                                                     00138000
.*                                                                      00138100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00138200
                      AGO   .ZL2           CONTINUE LOOPING             00138300
.*                                                                      00138400
.ZLX                  ANOP  ,                                           00138500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00138600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00138700
         AIF   ('&SYSAM64' NE 'YES').AMODE31                            00138800
         LLGTR 14,14                   CLEAR HIGH HALF IN 64BIT MODE    00138900
.AMODE31 L     14,16(0,0)              GET CVT ADDRESS                  00139000
.*-------------------------------------------------------------------   00139100
         TM    CVTOSLV3-CVT(14),CVTH7709  test for HBB7709 presence     00139200
         BNZ   VEROK&SYSNDX               running on HBB7709            00139300
         LA    0,IWMRSNCODEBADVERSION     HBB7709 is required           00139400
         LA    15,IWMRETCODEINVOCERROR    set return code               00139500
         B     END&SYSNDX                 skip service invocation       00139600
VEROK&SYSNDX DS    0H                     version check successful      00139700
.*-------------------------------------------------------------------   00139800
         L     14,772(14,0)            GET SFT ADDRESS                  00139900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00140000
         LA    15,48                   GET EX FOR SERVICE ROUTINE       00140100
         OR    14,15                   GET PC NUMBER                    00140200
         PC    0(14)                                                    00140300
END&SYSNDX DS    0H                    END LABEL                        00140400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00140500
.ZLINKX               ANOP  ,                                           00140600
