.***  PROCESS IWM4STEN OUTPUT PARAMETERS                                00128000
.*******************************************************************    00128100
                      ACTR  10000                                       00128200
.*******************************************************************    00128300
.***AO.X02  RETURN CODE PROCESSING                                      00128400
.*******************************************************************    00128500
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00128600
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00128700
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00128800
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00128900
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00129000
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00129100
&ZOPN                 SETA  &ZOPN+1                                     00129200
&ZOPL(&ZOPN)          SETC  ' '                                         00129300
&ZOPO(&ZOPN)          SETC  'ST'                                        00129400
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00129500
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00129600
                      AGO   .ZMRETX                                     00129700
.*                                                                      00129800
.ZMRET2               ANOP  ,                                           00129900
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00130000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00130100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00130200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00130300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00130400
&ZOPN                 SETA  &ZOPN+1                                     00130500
&ZOPL(&ZOPN)          SETC  ' '                                         00130600
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00130700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00130800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00130900
.ZMRETX               ANOP  ,                                           00131000
.*******************************************************************    00131100
