          IKJTMPWA &WORKA1=YES                                          00826000
          AIF   ('&WORKA1' EQ 'NO').WORKA2                              00829500
*********************************************************************/  00833000
**** TMPWRKA1 -- TMP PROBLEM PROGRAM STATE DYNAMIC WORK AREA        */  00836500
****                                                                */  00840000
****   THIS DYNAMIC WORK AREA IS GOTTEN FROM SUBPOOL 1, PP KEY, BY  */  00843500
****   IKJEFT01  DURING  TMP INITIALIZATION AND ANOTHER ONE         */  00847000
****   WILL BE GOTTEN IN IKJEFTSC IF A PARALLEL TMP IS NEEDED AND   */  00850500
****   THIS WORKAREA WILL BE FREED WHEN THE PARALLEL TMP IS FINISHED*/  00854000
****   WHILE THE ONE GOTTEN FOR THE NON-PARALLEL SIDE BY IKJEFT01   */  00857500
****   WILL NOT BE FREED UNTIL THE END OF THE TERMINAL SESSION.     */  00861000
****   DURING THE TMP SEVERITY 2 STAE RETRY ALL OF STORAGE IS FREED */  00864500
****   AND THIS WORKAREA MUST BE REINITIALIZED.                     */  00868000
****                                                        @E1113KJ*/  00871500
*********************************************************************/  00875000
         IKJTPL                                                         00878500
TMPWRKA1 EQU  TPL    WORK AREA BEGINS WITH TEST PARAMETER LIST          00882000
*     /* TMP COMMON VARIABLES AND WORK AREAS                        */  00885500
TMPNECB  DS    F                     /* ECB FOR STAI WAIT           */  00889000
TMPCECB  DS    F                     /* ECB FOR ATTACHED CP         */  00892500
TMPIECB  DS    F                     /* ECB FOR STAI POST           */  00896000
TMPAECB  DS    F                     /* ECB FOR ATTN POST           */  00899500
TMPCMDWT DS    F                     /* PTR TO CMD FROM ATTN EXIT   */  00903000
*                                                                       00906500
TMPSWS   DS    F                     /* TMP INTERNAL SWITCHES       */  00910000
TMPTEST  EQU   X'80'                 /* TEST PROGRAM IN CONTROL     */  00913500
TMPCMDW  EQU   X'40'                 /* COMMAND WAITING             */  00917000
TMPNFCMD EQU   X'20'                 /* FIRST COMMAND IS PROCESSED  */  00920500
TMPACTRL EQU   X'10'                 /* TMP ATTN EXIT IS IN CONTROL */  00924000
TMPSCTRL EQU   X'08'                 /* TMP STAI EXIT IS IN CONTROL */  00927500
ABND806  EQU   X'04'                 /* NO-MODULE FOUND BY FETCH    */  00931000
FRSTLAB  EQU   X'02'                 /* 1ST LEVEL ATTACHEE ABENDED  */  00934500
NONSCUR  EQU   X'01'                /* SECURITY AUTHORIZATION FAILS */  00938000
*                                                                       00941500
ATCHNOW  EQU   X'80'                 /* ABEND OCCURRED IN ATTACH    */  00945000
LOADNOW  EQU   X'40'                 /* ABEND OCCURRED IN LOAD      */  00948500
LINKNOW  EQU   X'20'                 /* ABEND OCCURRED IN LINK      */  00952000
FRSTEX   EQU   X'10'                /* FIRST EXPL/IMPLICIT EXEC TRY */  00955500
CALLNOW  EQU   X'08'                 /* CALL FUNCTION ACTIVE        */  00959000
TMP1TIME EQU   X'04'                 /* ESTAI ENTERED(TEST) @ZTM5046*/  00962500
T7TDONE  EQU   X'02'                 /* TSEVENT ISSUED      @ZA66275*/  00966001
SKPATTN  EQU   X'01'                 /* 1-BYPASS ATTN       @ZA89253*/  00969500
*                                                                       00973000
TMP1TSFE EQU   X'80'                 /* ERROR OCCURRED IN CLIST         00976500
*                                       WHILE IN TSF/CLIST MODE.        00980000
*                                                           @PBY0316*/  00983500
CALLSWS  EQU   TMPSWS+3              /* TMP-CALL INTERNAL SWITCHES  */  00987000
PDLPRES  EQU   X'80'                 /* PDL RETURNED BY PARSE       */  00990500
DSOPEN   EQU   X'40'                 /* DATA SET IS OPEN            */  00994000
BLANKB   EQU   X'10'                 /* DATA SET NAME PROCESSED     */  00997500
DORELS   EQU   X'08'                 /* RELEASE PDL NOW             */  01001000
GMBRNOW  EQU   X'04'                 /* GET MEMBER NAME             */  01004500
PCFDA    EQU   X'02'                 /* PCF DIRECT ATTACH   @YA18897*/  01008000
*        EQU   X'01'                 /* RESERVED FLAG               */  01011501
*     /* RESERVED AREAS                                             */  01015000
TMPT9ECB DS  A                       /* ECB USED FOR COMMUNICATION      01018500
*                                       BETWEEN IKJEFT09 AND IKJURPS    01022000
*                                                           @E24D2H3*/  01025500
TMPURPA  DS  A                       /* ANCHOR FOR URP REQUEST BLOCK    01029000
*                                       CHAIN FOR IKJEFT09  @E24D2H3*/  01032500
RESCOMM  DS     CL8                  /*                     @E24D2H3*/  01036000
RESCOM2  DS     CL16                                                    01039500
RESCOM3  DS     CL16                                                    01043000
RESCOM4  DS     CL16                                                    01046500
         DS     CL4                /* RESERVED WAS FLOFLGS  @ZA46174*/  01050000
CPPLPTR  DS     F                    /* PTR TO CP PARM LIST         */  01053500
CSOAPTR  DS     F                    /* PTR TO CMD SCAN PARM LIST   */  01057000
CSPLPTR  DS     F                    /* PTR TO CMD SCAN PARM LIST   */  01060500
DAPLPTR  DS     F                    /* PTR TO DAIR PARM LIST       */  01064000
GTPBPTR  DS     F                    /* PTR TO GETLINE PARM BLOCK   */  01067500
IOPLPTR  DS     F                    /* PTR TO I/O RTNS PARM LIST   */  01071000
PGPBPTR  DS     F                    /* PTR TO PUTGET PARM BLOCK    */  01074500
PPLPTR   DS     F                    /* PTR TO PARSE PARM LIST      */  01078000
PTPBPTR  DS     F                    /* PTR TO PUTLINE PARM BLOCK   */  01081500
STPLPTR  DS     F                    /* PTR TO STACK PARM LIST      */  01085000
ACEEPTR  DS     F                    /* ADDR OF ACEE                */  01088500
ASCANAP  DS     F                    /* ADDR OF ATTN SCAN ANSWER    */  01092000
ASRPLPTR DS     F                    /* ADDR OF ATTN SRPL           */  01095500
ATTCHPTR DS     F                    /* ADDR OF ATTACH PARM LIST    */  01099000
CDCBPTR  DS     F                    /* PTR TO CALL DCB             */  01102500
DCBPTR   DS     F                    /* PTR TO DCB                  */  01106000
DYNAPPTR DS     F                    /* PTR TO DYNALLOC PARM LIST   */  01109500
EBCDPTR  DS     F                    /* PTR TO TRANSLATE TABLE      */  01113000
READYPTR DS     F                    /* ADDR OF TMP MODE MESSAGE    */  01116500
SCANAP   DS     F                    /* ADDR OF SCAN ANSWER AREA    */  01120000
SRPLPTR  DS     F                    /* ADDR OF SRPL                */  01123500
         DS     F                    /* RESERVED            @ZA96882*/  01127000
STPBPTR  DS     F                    /* ADDR OF STACK PARM LIST     */  01130500
*     /* RESERVE SPACE FOR PARAMETER LISTS, BLOCKS                  */  01134000
         DS     0D                   /* ALIGN TO DOUBLEWORD         */  01137500
ABMSGSP  DS     CL41                  /* MESSAGE AREA       @PID0241*/  01141000
*     /* PUTLINE ACTIVE SEGMENT LIST    LIST                        */  01144500
ACTSL    DS     0F                   /* NAME OF LIST                */  01148000
ACTSEG   DS     28F                  /* SEGMENTS                    */  01151500
ACTSEGA  EQU    ACTSEG               /* FIRST HWORD OF SEGMENT      */  01155000
ACTSEGB  EQU    ACTSEG+2             /* SECOND HWORD OF SEGMENT     */  01158500
AMSGLIST DS     0F                   /* ATTN MESSAGE LIST           */  01162000
ANUMSEG  DS     F                    /* NUMBER OF MESSAGE SEGMENTS  */  01165500
AMSGSEG  DS     2F                   /* ARRAY OF SEGMENT PTRS       */  01169000
ARCODE   DS     F                    /* ATTN RETURN CODE SAVE AREA  */  01172500
ASCANFLG DS     F                    /* ATTN SCAN FLAGS             */  01176000
ASRPARM  DS     5F                   /* ATTN SR PARM AREA           */  01179500
ATTCHSP  DS     18F                  /* ATTACH PARM LIST SP @ZTY0371*/  01183000
BLDLLST  DS     0CL68                /* BLDL ENTRY                  */  01186500
XTRCLST  DS     0CL12                /* EXTRACT LIST                */  01190000
BLDLENT  DS     H                    /* NUM OF ENTRIES              */  01193500
BLDLELNG DS     H                    /* LENGTH OF ENTRY             */  01197000
BLDLNAME DS     CL8                  /* NAME OF COMMAND             */  01200500
BLDLTTRZ DS     CL56                 /* PAD TO FULL WORD    @ZA40795*/  01204000
         ORG    BLDLTTRZ             /* Redefine BLDLTTRZ   @WA36096*/  01207500
BLDLTMP_TT DS   CL2                  /* TT (relative track) returned    01211000
*                                       from BLDL           @WA36096*/  01214500
BLDLTMP_R DS    C                    /* R (record number) returned      01218000
*                                       from BLDL           @WA36096*/  01221500
         ORG    ,                    /* End of redefine     @WA36096*/  01225000
         DS     0D                   /* ALIGN TO DWORD              */  01228500
CDCBSP   DS     CL140                /* CALL DCB SPACE              */  01232000
CLOSESP  DS     CL12                 /* CLOSE PL SPACE              */  01235500
CPPLSP   DS     4F                   /* CPPL SPACE                  */  01239000
CSOASP   DS     2F                   /* CSOA SPACE                  */  01242500
CSOASP2  DS     2F                   /* 2ND CSOA SP (ATTN)          */  01246000
CSPLSP   DS     6F                   /* CSPL SPACE                  */  01249500
CSPLSP2  DS     6F                   /* 2ND CSPL SP (ATTN)          */  01253000
CTLBKSP  DS     0F                   /* NAME OF BLOCK SPACE         */  01256500
CTLBLKL  DS     F                    /* LENGTH OF BLOCK SPACE       */  01260000
CTLBLKA  DS     F                    /* LOC OF BLOCK SPACE          */  01263500
CTLBLKN  DS     F                    /* SUBPOOL                     */  01267000
DAPBSP   DS     21F                  /* DAIR PARM BLK SPACE         */  01270500
DAPLSP   DS     5F                   /* DAIR PARM LIST SPACE        */  01274000
         DS     0D                   /* ALIGN TO DOUBLEWORD         */  01277500
DCBSP    DS     CL140                /* DCB SPACE                   */  01281000
