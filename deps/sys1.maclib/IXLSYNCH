&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00139000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00139100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00139200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00139300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00139400
