.***AC.PLV01  PROCESS PLISTVER                                      *   00061200
.********************************************************************   00061300
&XPLISTVER      SETA  0                                                 00061400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00061500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00061600
&XPLISTVER      SETA  0                                                 00061700
.ZPLV0          ANOP  ,                                                 00061800
.*                                                                      00061900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00062000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00062100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00062200
.*-------------------------------------------------------------------   00062300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00062400
&ZMS            SETC  '"PLISTVER='                                      00062500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00062700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00062800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062900
                AGO   .ZPVCK2                                           00063000
.ZPVCK3         ANOP  ,                                                 00063100
.*-------------------------------------------------------------------   00063200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00063300
&ZMS            SETC  '"PLISTVER='                                      00063400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00063600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00063700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063800
                AGO   .ZPVCKE                                           00063900
.ZPVCK2         ANOP  ,                                                 00064000
.*-------------------------------------------------------------------   00064100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00064200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00064300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00064400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00064500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00064600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00064700
.ZPVCK6         ANOP  ,                                                 00064800
&ZMS            SETC  '"PLISTVER='                                      00064900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00065100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00065200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065300
&XPLISTVER      SETA  0                                                 00065400
                AGO   .ZPVCKE                                           00065500
.*-------------------------------------------------------------------   00065600
.ZPVCK4A        ANOP  ,                                                 00065700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00065800
&ZMS            SETC  'PLISTVER='                                       00065900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00066000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00066100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00066200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00066300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066400
                AGO   .ZPVCK5                                           00066500
.ZPVCK4         ANOP  ,                                                 00066600
&XPLISTVER      SETA  &PLISTVER(1)                                      00066700
.ZPVCK5         ANOP  ,                                                 00066800
.ZPVCKE         ANOP  ,                                                 00066900
.********************************************************************   00067000
