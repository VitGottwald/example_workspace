.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00122800
&ZPLLEN               SETA 8                                            00122900
.*                                                                      00123000
.******************************************************************     00123100
.***AI.L01 IXLSYNCH FMT=LIST FILL IN PARAMETER AREA                     00123200
.******************************************************************     00123300
                      ACTR  10000                                       00123400
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00123500
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00123600
&ZCVAL                SETC  'LAE'          XAX MODE                     00123700
.ZIMFXA               ANOP ,                                            00123800
.*                                                                      00123900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00124000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00124100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00124200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00124300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00124400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00124500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00124600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00124700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00124800
&ZOPN                 SETA  &ZOPN+1                                     00124900
&ZOPL(&ZOPN)          SETC  ' '                                         00125000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00125100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00125200
