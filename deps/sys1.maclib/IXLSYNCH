.*******************************************************************    00143900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00144000
.*******************************************************************    00144100
                      ACTR  10000                                       00144200
.*                                                                      00144300
                      LCLA  &ZLC           COLUMN POSITION              00144400
                      LCLA  &ZOPX          LIST TABLE INDEX             00144500
                      LCLC  &ZLL           LABEL                        00144600
                      LCLC  &ZLOP          OPERATION                    00144700
                      LCLC  &ZLV           VALUE                        00144800
                      LCLC  &ZLD           DESCRIPTION                  00144900
.*                                                                      00145000
&ZOPX                 SETA  1                                           00145100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00145200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00145300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00145400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00145500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00145600
                      AIF   (K'&ZLD LE 0).ZL5                           00145700
&ZLC                  SETA  35             COMMENT COLUMN               00145800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00145900
&ZLC                  SETA  33             COMMMENT COLUMN              00146000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00146100
&ZLV                  SETC  '&ZLV'.' '                                  00146200
                      AGO   .ZL3                                        00146300
.*                                                                      00146400
.ZL4                  ANOP  ,                                           00146500
&ZLV                  SETC  '&ZLV'.' '                                  00146600
.ZL5                  ANOP  ,                                           00146700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00146800
.*                                                                      00146900
&ZLL     &ZLOP &ZLV                                                     00147000
.*                                                                      00147100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00147200
                      AGO   .ZL2           CONTINUE LOOPING             00147300
.*                                                                      00147400
.ZLX                  ANOP  ,                                           00147500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00147600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00147700
         L     15,16(0,0)              GET CVT ADDRESS             @D3C 00147800
         L     15,140(15,0)            GET ECVT ADDRESS            @D3C 00147900
         L     15,256(15,0)            GET XRAT ADDRESS            @D3C 00148000
         L     15,28(15,0)             IXLSYNCH Service Routine    @D3C 00148100
         AIF   ('&SYSASCE' NE 'P').BR1 CHECK FOR AR MODE                00148200
         LAE   1,0(1,0)                MAKE SURE AR1 IS CLEARED         00148300
.BR1     ANOP                                                           00148400
         BALR  14,15                   BRANCH TO ROUTINE           @D3C 00148500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00148600
.ZLINKX               ANOP  ,                                           00148700
.*******************************************************************    00148800
.***AP.X01  FORMAT GENERATED CODE                                       00148900
