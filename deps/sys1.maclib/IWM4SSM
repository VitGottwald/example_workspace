.ZG2B         AIF   (K'&USERDATA(1) GT 0).ZG2C                          00090600
&ZXUSERDATA   SETC  ''          RESET IN/OUTPUT                         00090700
&ZMS          SETC  ' '                                                 00090800
&ZMS          SETC  '&ZMS "USERDATA" KEY AND ITS'                       00090900
              MNOTE 8,' &ZMS ARGUMENT ARE REQUIRED.'                    00091000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00091100
.ZG2C         ANOP  ,                                                   00091200
.*-------------------------------------------------------------------   00091300
.***AG.KCK02 NAME, RQD OUTPUT, KEYS -GPR OUT OF RANGE                   00091400
              AIF   (K'&ZXUSERDATA LE 0).ZG2R   NOT I/O                 00091500
              AIF   ('&XUSERDATA'(1,1) NE '(').ZG2R                     00091600
&ZCGPR        SETC  '&XUSERDATA'(2,K'&XUSERDATA-2)                      00091700
              AIF   ('&ZCGPR'(1,1) NE '0').ZG2P                         00091800
              AIF   (K'&ZCGPR GT 2).ZG2Q                                00091900
              AIF   ('&ZCGPR' EQ '0').ZG2Q                              00092000
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00092100
              AIF   (T'&ZCGPR NE 'N').ZG2Q      INVALID REG NAME        00092200
.ZG2P         ANOP  ,                                                   00092300
              AIF   (T'&ZCGPR NE 'N').ZG2R      NON NUMERIC             00092400
              AIF   ('&ZCGPR' LT '2').ZG2Q                              00092500
              AIF   ('&ZCGPR' LE '12').ZG2R                             00092600
.ZG2Q         ANOP  ,                                                   00092700
&ZMS          SETC  '"USERDATA='                                        00092800
&ZMS          SETC  '&ZMS.&USERDATA"'                                   00092900
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00093000
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00093100
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00093200
.ZG2R         ANOP  ,                                                   00093300
.*----------------------------------------------------------------***   00093400
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00093500
              AIF   (N'&RETCODE LE 1).ZG5A      CK NUM                  00093600
&ZMS          SETC  '"RETCODE='                                         00093700
&ZMS          SETC  '&ZMS.&RETCODE"'                                    00093800
&ZMS          SETC  '&ZMS "RETCODE"'                                    00093900
              MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'                 00094000
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00094100
.ZG5A         ANOP  ,                                                   00094200
.*-------------------------------------------------------------------   00094300
.***AG.KXVAR14 NAME, OPT OUTPUT, KEYS -XVARIABLE                        00094400
