&ZOPD(&ZOPN)          SETC  '++ IWM4SSM ARG PARMS'                      00109600
.*                                                                      00109700
&ZOPN                 SETA  &ZOPN+1                                     00109800
&ZOPL(&ZOPN)          SETC  ' '                                         00109900
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00110000
&ZOPV(&ZOPN)          SETC  '1'                                         00110100
&ZOPD(&ZOPN)          SETC  ' '                                         00110200
.*                                                                      00110300
.ZLMFSL2              ANOP  ,                                           00110400
.*-------------------------------------------------------------------   00110500
.***AM.L00  IWM4SSM FMT=LIST name                                       00110600
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00110700
&ZOPN                 SETA &ZOPN+1                                      00110800
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00110900
