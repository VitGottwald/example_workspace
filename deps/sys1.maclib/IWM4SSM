.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00118900
&ZPLLEN               SETA 28                                           00119000
.*                                                                      00119100
.******************************************************************     00119200
.***AI.L01 IWM4SSM FMT=LIST FILL IN PARAMETER AREA                      00119300
.******************************************************************     00119400
                      ACTR  10000                                       00119500
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00119600
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00119700
&ZCVAL                SETC  'LAE'          XAX MODE                     00119800
.ZIMFXA               ANOP ,                                            00119900
.*                                                                      00120000
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00120100
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00120200
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00120300
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00120400
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00120500
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00120600
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00120700
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00120800
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00120900
&ZOPN                 SETA  &ZOPN+1                                     00121000
&ZOPL(&ZOPN)          SETC  ' '                                         00121100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00121200
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00121300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00121400
                      AGO   .ZIMFNX                                     00121500
.*                                                                      00121600
.ZIMFN2               ANOP ,                                            00121700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00121800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00121900
&ZOPN                 SETA  &ZOPN+1                                     00122000
&ZOPL(&ZOPN)          SETC  ' '                                         00122100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00122200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00122300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00122400
                      AGO   .ZIMFNX                                     00122500
.ZIMFNX1              ANOP ,                                            00122600
&ZOPN                 SETA  &ZOPN+1                                     00122700
&ZOPL(&ZOPN)          SETC  ' '                                         00122800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00122900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00123000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00123100
.ZIMFNX               ANOP ,                                            00123200
.********************************************************************   00123300
.***AI.L02 IWM4SSM FMT=LIST INITIALIZE PARAMETER AREA                   00123400
.********************************************************************   00123500
.***AI.L03 MF=(SLE)                                                     00123600
