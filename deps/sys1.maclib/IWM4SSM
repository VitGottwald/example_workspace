.******************************************************************     00101000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00101100
.******************************************************************     00101200
              ACTR  10000                                               00101300
.*----------------------------------------------------------------***   00101400
.***AH.P02 EXTRA POSITIONALS                                            00101500
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00101600
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00101700
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00101800
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00101900
.ZGLISTX1     ANOP  ,                                                   00102000
&ZCVAL        SETC  ''          ERROR FIELD                             00102100
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00102200
.ZGLIST2      ANOP  ,                                                   00102300
&ZAWRK        SETA  &ZAWRK+1    NEXT POSITIONAL                         00102400
              AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE                00102500
