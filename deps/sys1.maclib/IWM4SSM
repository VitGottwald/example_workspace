              AIF   ('&ZCGPR'(1,1) NE '0').ZG5P                         00095600
              AIF   (K'&ZCGPR GT 2).ZG5Q                                00095700
              AIF   ('&ZCGPR' EQ '0').ZG5Q                              00095800
