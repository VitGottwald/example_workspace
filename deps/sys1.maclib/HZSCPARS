         SYSSTATE POP                                              @L1A 00554400
         AGO .NOTAR1B                                              @L1A 00554500
.NOTAR1A ANOP                                                      @L1A 00554600
         AIF   ('&XMFCTRL'(1,1) EQ '(').PAREN2A                    @P1A 00554700
