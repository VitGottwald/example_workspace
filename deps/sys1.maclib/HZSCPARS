.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00549500
.*******************************************************************    00549600
                      ACTR  10000                                       00549700
.*                                                                      00549800
                      LCLA  &ZLC           COLUMN POSITION              00549900
                      LCLA  &ZOPX          LIST TABLE INDEX             00550000
                      LCLC  &ZLL           LABEL                        00550100
                      LCLC  &ZLOP          OPERATION                    00550200
                      LCLC  &ZLV           VALUE                        00550300
                      LCLC  &ZLD           DESCRIPTION                  00550400
.*                                                                      00550500
&ZOPX                 SETA  1                                           00550600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00550700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00550800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00550900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00551000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00551100
                      AIF   (K'&ZLD LE 0).ZL5                           00551200
&ZLC                  SETA  35             COMMENT COLUMN               00551300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00551400
&ZLC                  SETA  33             COMMMENT COLUMN              00551500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00551600
&ZLV                  SETC  '&ZLV'.' '                                  00551700
                      AGO   .ZL3                                        00551800
.*                                                                      00551900
.ZL4                  ANOP  ,                                           00552000
&ZLV                  SETC  '&ZLV'.' '                                  00552100
.ZL5                  ANOP  ,                                           00552200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00552300
.*                                                                      00552400
&ZLL     &ZLOP &ZLV                                                     00552500
.*                                                                      00552600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00552700
                      AGO   .ZL2           CONTINUE LOOPING             00552800
.*                                                                      00552900
.ZLX                  ANOP  ,                                           00553000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00553100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00553200
         AIF   ('&SYSASCE' EQ 'P').NOTAR1A                         @L1A 00553300
         SYSSTATE PUSH                                             @L1A 00553400
         SYSSTATE ASCENV=P                                         @L1A 00553500
         SAC   0                                                   @L1A 00553600
         AIF   ('&XMFCTRL'(1,1) EQ '(').PAREN1A                    @P1A 00553700
         LINK  EP=HZSPKCPM,SF=(E,&XMFCTRL._XLINKPLIST)             @L1A 00553800
         AGO   .PAREN1B                                            @P1A 00553900
