&ZCVAL              SETC  ''          ERROR FIELD                       00414400
&ZAWRK              SETA  0           POSITIONAL NUMBER                 00414500
.ZGLIST2            ANOP  ,                                             00414600
&ZAWRK              SETA  &ZAWRK+1    NEXT POSITIONAL                   00414700
                    AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE          00414800
&ZCVAL              SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA               00414900
                    AGO   .ZGLIST2                                      00415000
.*                                                                      00415100
.ZGLIST3            ANOP  ,                                             00415200
&ZCVAL              SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA            00415300
&ZMS                SETC  'POSITIONAL ARGUMENT(S),'                     00415400
&ZMS                SETC  '&ZMS "&ZCVAL"'                               00415500
&ZMS                SETC  '&ZMS CANNOT BE USED.'                        00415600
&ZMS                SETC  '&ZMS NO POSITIONAL ARGUMENTS'                00415700
                    MNOTE 8,' &ZMS ARE ALLOWED.'                        00415800
&ZMACRET            SETA  8           SET MACRO ERROR RETURN CODE       00415900
.ZGLISTX            ANOP  ,                                             00416000
&ZASYSLNDX          SETA  0                                             00416100
.*-------------------------------------------------------------------   00416200
.***AG.MF04 MF=(SLE)                                                    00416300
.ZGMFX              ANOP  ,           END OF FULL SYNTAX CHECK          00416400
.*******************************************************************    00416500
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00416600
.*******************************************************************    00416700
                    AIF   (&ZMACRET NE 0).ZLX2  ERROR DETECTED          00416800
.*******************************************************************    00416900
.***AE.01  START OF HZSCPARS MACRO CODE                                 00417000
.*******************************************************************    00417100
                    LCLC  &M0M0001                 NAME                 00417200
&M0M0001            SETC  '&XLABEL'                SET NAME             00417300
                    AIF   (K'&XLABEL NE 0).XLAB2                        00417400
&M0M0001            SETC  'M00M&SYSNDX'            DEFAULT NAME         00417500
.*                                                                      00417600
.XLAB2              ANOP  ,                                             00417700
                    AIF   ('&XMF' EQ 'L').XLAB3                         00417800
&M0M0001 DS    0H                             HZSCPARS-0                00417900
.XLAB3              ANOP  ,                                             00418000
.*******************************************************************    00418100
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00418200
.*******************************************************************    00418300
              PUSH  PRINT                                               00418400
                    AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON    00418500
                    PRINT OFF         ++ PR01                           00418600
                    AGO   .ZPRNTXX                                      00418700
.ZPRINTX            PRINT ON                                            00418800
.ZPRNTXX            ANOP  ,                                             00418900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00419000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00419100
.********************************************************************   00419200
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00419300
.********************************************************************   00419400
