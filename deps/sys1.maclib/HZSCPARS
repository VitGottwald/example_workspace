                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00467200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00467300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00467400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00467500
&ZOPN                 SETA  &ZOPN+1                                     00467600
&ZOPL(&ZOPN)          SETC  ' '                                         00467700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00467800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00467900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00468000
                      AGO   .ZIMFNX                                     00468100
.*                                                                      00468200
.ZIMFN2               ANOP ,                                            00468300
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00468400
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00468500
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00468600
