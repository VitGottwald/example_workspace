*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00642000
*%IEFDDSRV: MACRO KEYS(RETRIEVE,DEVENTRY,DDNAME,DSABPTR,DCBPTR,ACBPTR,  00642100
*                 SUBPOOL,DEVAREA,AREALEN,LOC,EXTRACT,TYPE,DEVIOAREA,   00642200
*                 MODIFY,NEWDDNAME,DSENQMGMT,SWBSTORAGE,TCBPTR,         00642300
*                 RETCODE,RSNCODE,PLISTVER,MF);                         00642400
*  ANS('?'!!MACLABEL!!' '!!'IEFDDSRP '!!MACLIST!!                       00642500
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00642600
*%END IEFDDSRV;                                                         00642700
