.*                                                                      00533000
.******************************************************************     00533100
.***AI.L01 IEFDDSRV FMT=LIST FILL IN PARAMETER AREA                     00533200
.******************************************************************     00533300
                      ACTR  10000                                       00533400
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00533500
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00533600
&ZCVAL                SETC  'LAE'          XAX MODE                     00533700
.ZIMFXA               ANOP ,                                            00533800
.*                                                                      00533900
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00534000
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00534100
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00534200
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00534300
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00534400
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00534500
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00534600
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00534700
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00534800
&ZOPN                 SETA  &ZOPN+1                                     00534900
&ZOPL(&ZOPN)          SETC  ' '                                         00535000
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00535100
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00535200
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00535300
                      AGO   .ZIMFNX                                     00535400
.*                                                                      00535500
.ZIMFN2               ANOP ,                                            00535600
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00535700
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00535800
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00535900
&ZOPN                 SETA  &ZOPN+1                                     00536000
&ZOPL(&ZOPN)          SETC  ' '                                         00536100
&ZOPO(&ZOPN)          SETC  'LAE'                                       00536200
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00536300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00536400
                      AGO   .ZIMFNX                                     00536500
.ZIMFNX1              ANOP ,                                            00536600
&ZOPN                 SETA  &ZOPN+1                                     00536700
&ZOPL(&ZOPN)          SETC  ' '                                         00536800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00536900
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00537000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00537100
.ZIMFNX               ANOP ,                                            00537200
.********************************************************************   00537300
.***AI.L02 IEFDDSRV FMT=LIST INITIALIZE PARAMETER AREA                  00537400
.********************************************************************   00537500
.***AI.L03 MF=(SLME)                                                    00537600
