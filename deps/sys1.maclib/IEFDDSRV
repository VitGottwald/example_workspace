&ZOPV(&ZOPN)          SETC  '0(4,&ZCGPR),60(1)'                         00622700
&ZOPD(&ZOPN)          SETC  '++ XDEVIOAREA A'                           00622800
                      AGO   .ZO42C                                      00622900
.*                                                                      00623000
.ZO42B                ANOP  ,                                           00623100
&ZAWRK   SETA  K'&XDEVIOAREA                                            00623200
         AIF   ('&XDEVIOAREA'(&ZAWRK,1) NE ')').ZO42G                   00623300
&ZN      SETA  2                                                        00623400
.ZO42H    ANOP                                                          00623500
         AIF   (&ZN GE &ZAWRK).ZO42G                                    00623600
         AIF   ('&XDEVIOAREA'(&ZN,1) NE '(').ZO42I                      00623700
&ZC      SETC  '&XDEVIOAREA'(1,&ZN).'4'.','                             00623800
&ZC      SETC  '&ZC'.'&XDEVIOAREA'(&ZN+1,&ZAWRK-&ZN).',60(1)'           00623900
         AGO   .ZO42J                                                   00624000
.ZO42I    ANOP                                                          00624100
&ZN      SETA  &ZN+1                                                    00624200
         AGO   .ZO42H                                                   00624300
.ZO42G    ANOP                                                          00624400
&ZC      SETC  '&XDEVIOAREA.(4),60(1)'                                  00624500
.ZO42J    ANOP                                                          00624600
&ZOPN                 SETA  &ZOPN+1                                     00624700
&ZOPL(&ZOPN)          SETC  ' '                                         00624800
&ZOPO(&ZOPN)          SETC  'MVC'                                       00624900
&ZOPV(&ZOPN)          SETC  '&ZC'                                       00625000
&ZOPD(&ZOPN)          SETC  '++ XDEVIOAREA A'                           00625100
.ZO42C                ANOP  ,                                           00625200
.*------------------------------------------------------------------    00625300
.***AO.LVER02                                                           00625400
.ZO42Z                ANOP  ,                                           00625500
.*******************************************************************    00625600
.***AO.X01  LABEL TO BYPASS INVOCATION                                  00625700
.*******************************************************************    00625800
&ZOPN                 SETA  &ZOPN+1                                     00625900
&ZOPL(&ZOPN)          SETC  'AMGE&SYSNDX'                               00626000
&ZOPO(&ZOPN)          SETC  'DS'                                        00626100
&ZOPV(&ZOPN)          SETC  '0H'                                        00626200
&ZOPD(&ZOPN)          SETC  '++ BYPASS INVOCATION'                      00626300
.*******************************************************************    00626400
.***AO.X02  RETURN CODE PROCESSING                                      00626500
.*******************************************************************    00626600
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00626700
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00626800
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00626900
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00627000
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00627100
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00627200
&ZOPN                 SETA  &ZOPN+1                                     00627300
&ZOPL(&ZOPN)          SETC  ' '                                         00627400
&ZOPO(&ZOPN)          SETC  'ST'                                        00627500
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00627600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00627700
                      AGO   .ZMRETX                                     00627800
.*                                                                      00627900
.ZMRET2               ANOP  ,                                           00628000
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00628100
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00628200
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00628300
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00628400
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00628500
&ZOPN                 SETA  &ZOPN+1                                     00628600
&ZOPL(&ZOPN)          SETC  ' '                                         00628700
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00628800
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00628900
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00629000
.ZMRETX               ANOP  ,                                           00629100
.*******************************************************************    00629200
.***AO.X03  REASON CODE PROCESSING                                      00629300
.*******************************************************************    00629400
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00629500
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00629600
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00629700
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00629800
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00629900
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00630000
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00630100
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00630200
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00630300
&ZOPN                 SETA  &ZOPN+1                                     00630400
&ZOPL(&ZOPN)          SETC  ' '                                         00630500
&ZOPO(&ZOPN)          SETC  'ST'                                        00630600
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00630700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00630800
                      AGO   .ZMRSNX                                     00630900
.*                                                                      00631000
.ZMRSN2               ANOP  ,                                           00631100
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00631200
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00631300
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00631400
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00631500
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00631600
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00631700
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00631800
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00631900
&ZOPN                 SETA  &ZOPN+1                                     00632000
&ZOPL(&ZOPN)          SETC  ' '                                         00632100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00632200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00632300
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00632400
.ZMRSNX               ANOP  ,                                           00632500
.*******************************************************************    00632600
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00632700
.*******************************************************************    00632800
.ZMFLX                ANOP  ,              MF=L PROCESSING              00632900
.*******************************************************************    00633000
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00633100
.*******************************************************************    00633200
                      ACTR  10000                                       00633300
.*                                                                      00633400
                      LCLA  &ZLC           COLUMN POSITION              00633500
                      LCLA  &ZOPX          LIST TABLE INDEX             00633600
                      LCLC  &ZLL           LABEL                        00633700
                      LCLC  &ZLOP          OPERATION                    00633800
                      LCLC  &ZLV           VALUE                        00633900
                      LCLC  &ZLD           DESCRIPTION                  00634000
.*                                                                      00634100
&ZOPX                 SETA  1                                           00634200
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00634300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00634400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00634500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00634600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00634700
                      AIF   (K'&ZLD LE 0).ZL5                           00634800
&ZLC                  SETA  35             COMMENT COLUMN               00634900
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00635000
&ZLC                  SETA  33             COMMMENT COLUMN              00635100
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00635200
&ZLV                  SETC  '&ZLV'.' '                                  00635300
                      AGO   .ZL3                                        00635400
.*                                                                      00635500
.ZL4                  ANOP  ,                                           00635600
&ZLV                  SETC  '&ZLV'.' '                                  00635700
.ZL5                  ANOP  ,                                           00635800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00635900
.*                                                                      00636000
&ZLL     &ZLOP &ZLV                                                     00636100
.*                                                                      00636200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00636300
                      AGO   .ZL2           CONTINUE LOOPING             00636400
.*                                                                      00636500
.ZLX                  ANOP  ,                                           00636600
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00636700
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00636800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00636900
         L     15,16(0,0)         ++ Get CVT address                    00637000
         L     15,772(15,0)       ++ Get SFT address                    00637100
.*********************************************************************  00637200
.*                                                                   *  00637300
.*  Get LX/EX for DD Services                                        *  00637400
.*                                                                   *  00637500
.*  Note: LX/EX is calculated by multiplying the PC number (107)     *  00637600
.*        times the length of a PC/AUTH table entry (4) and          *  00637700
.*        then subtracting 4.                                        *  00637800
.*                                                                   *  00637900
.*********************************************************************  00638000
         L     15,424(15,0)       ++ Get PC number                      00638100
         PC    0(15)              ++ PC to IEFDDSRV                     00638200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00638300
.ZLINKX               ANOP  ,                                           00638400
.*******************************************************************    00638500
.***AP.X01  FORMAT GENERATED CODE                                       00638600
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00638700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00638800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00638900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00639000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00639100
                      AIF   (K'&ZLD LE 0).ZL51                          00639200
&ZLC                  SETA  35             COMMENT COLUMN               00639300
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00639400
&ZLC                  SETA  33             COMMENT COLUMN               00639500
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00639600
&ZLV                  SETC  '&ZLV'.' '                                  00639700
                      AGO   .ZL31                                       00639800
.*                                                                      00639900
.ZL41                 ANOP  ,                                           00640000
&ZLV                  SETC  '&ZLV'.' '                                  00640100
.ZL51                 ANOP  ,                                           00640200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00640300
.*                                                                      00640400
&ZLL     &ZLOP &ZLV                                                     00640500
.*                                                                      00640600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00640700
                      AGO   .ZL21          CONTINUE LOOPING             00640800
.*                                                                      00640900
.ZLX1                 ANOP  ,                                           00641000
.*******************************************************************    00641100
.***AP.X02  END OF IEFDDSRV MACRO                                       00641200
&ZIEFDDSRV            SETC  'YES'          SET IEFDDSRV USED            00641300
         POP   PRINT                                                    00641400
*                                            IEFDDSRV-1                 00641500
         SPACE 1                                                        00641600
.ZLX2                 ANOP  ,                                           00641700
         MEND                                                           00641800
