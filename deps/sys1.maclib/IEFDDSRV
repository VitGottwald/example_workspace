.***AC.PLV01  PROCESS PLISTVER                                      *   00141200
.********************************************************************   00141300
&XPLISTVER      SETA  0                                                 00141400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00141500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00141600
&XPLISTVER      SETA  1                                                 00141700
.ZPLV0          ANOP  ,                                                 00141800
.*                                                                      00141900
                AIF   ('&AREALEN' EQ '').ZPLV1E                         00142000
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00142100
&XPLISTVER      SETA  1                                                 00142200
.ZPLV1E         ANOP  ,                                                 00142300
.*                                                                      00142400
                AIF   ('&TYPE' EQ '').ZPLV2E                            00142500
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00142600
&XPLISTVER      SETA  1                                                 00142700
.ZPLV2E         ANOP  ,                                                 00142800
.*                                                                      00142900
                AIF   ('&DEVIOAREA' EQ '').ZPLV3E                       00143000
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00143100
&XPLISTVER      SETA  1                                                 00143200
.ZPLV3E         ANOP  ,                                                 00143300
.*                                                                      00143400
                AIF   ('&AREALEN' EQ '').ZPLV4E                         00143500
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00143600
&XPLISTVER      SETA  1                                                 00143700
.ZPLV4E         ANOP  ,                                                 00143800
.*                                                                      00143900
                AIF   ('&TYPE' EQ '').ZPLV5E                            00144000
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00144100
&XPLISTVER      SETA  1                                                 00144200
.ZPLV5E         ANOP  ,                                                 00144300
.*                                                                      00144400
                AIF   ('&NEWDDNAME' EQ '').ZPLV6E                       00144500
                AIF   (&XPLISTVER GE 1).ZPLV6E                          00144600
&XPLISTVER      SETA  1                                                 00144700
.ZPLV6E         ANOP  ,                                                 00144800
.*                                                                      00144900
                AIF   ('&DSENQMGMT' EQ '').ZPLV7E                       00145000
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00145100
&XPLISTVER      SETA  1                                                 00145200
.ZPLV7E         ANOP  ,                                                 00145300
.*                                                                      00145400
                AIF   ('&SWBSTORAGE' EQ '').ZPLV8E                      00145500
                AIF   (&XPLISTVER GE 1).ZPLV8E                          00145600
