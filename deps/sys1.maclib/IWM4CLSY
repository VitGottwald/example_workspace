.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00918900
.*******************************************************************    00919000
                      ACTR  10000                                       00919100
.*                                                                      00919200
                      LCLA  &ZLC           COLUMN POSITION              00919300
                      LCLA  &ZOPX          LIST TABLE INDEX             00919400
                      LCLC  &ZLL           LABEL                        00919500
                      LCLC  &ZLOP          OPERATION                    00919600
                      LCLC  &ZLV           VALUE                        00919700
                      LCLC  &ZLD           DESCRIPTION                  00919800
.*                                                                      00919900
&ZOPX                 SETA  1                                           00920000
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00920100
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00920200
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00920300
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00920400
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00920500
                      AIF   (K'&ZLD LE 0).ZL5                           00920600
&ZLC                  SETA  35             COMMENT COLUMN               00920700
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00920800
&ZLC                  SETA  33             COMMMENT COLUMN              00920900
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00921000
&ZLV                  SETC  '&ZLV'.' '                                  00921100
                      AGO   .ZL3                                        00921200
.*                                                                      00921300
.ZL4                  ANOP  ,                                           00921400
&ZLV                  SETC  '&ZLV'.' '                                  00921500
.ZL5                  ANOP  ,                                           00921600
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00921700
.*                                                                      00921800
&ZLL     &ZLOP &ZLV                                                     00921900
.*                                                                      00922000
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00922100
                      AGO   .ZL2           CONTINUE LOOPING             00922200
.*                                                                      00922300
.ZLX                  ANOP  ,                                           00922400
                      AIF   ('&XMF' EQ 'L').ZASMMX  MF=L                00922500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00922600
.*                                                                      00922700
.* If a Client IP address is specified, we have to set also             00922800
.* the related length field                                             00922900
.*                                                                 @P0A 00923000
         AIF   ('&XCLIENTIPADDR' EQ '').Clsy_CIA                   @P0A 00923100
         AIF   ('&XCLIENTIPADDR' EQ 'NO_CLIENTIPADDR').Clsy_CIA    @P0A 00923200
         LHI   14,39                   Set Client IP Address ...   @P0A 00923300
         ST    14,408(1)               length to 39 character      @P0A 00923400
.Clsy_CIA ANOP                                                     @P0A 00923500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00923600
.ZASMMX               ANOP  ,                                           00923700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00923800
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00923900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00924000
.* Get Connect Token (xconntkn)                                         00924100
         LLGT  14,12(1)                Get Connect token                00924200
         LTGR  14,14                   Is the connect token set         00924300
         JNZ   Clsy&SYSNDX._Chk        Branch when Connect tkn set      00924400
         LGHI  0,IwmRsnCodeNoConn      Set reason code = No Connect     00924500
         LGHI  15,IwmRetCodeWarning    Set warning return code          00924600
         J     Clsy&SYSNDX._End        Branch to end label              00924700
.* Verify Connect Token (xconntkn)                                      00924800
Clsy&SYSNDX._Chk  DC    0H             Connect done                     00924900
         CLC   0(4,14),=CL4'WCDA'      Check Connect token ID           00925000
         JE    Clsy&SYSNDX._Ok         Branch when Connect tkn passes   00925100
         LGHI  0,IwmRsnCodeBadConn     Set reason code = Bad Connection 00925200
         LGHI  15,IwmRetCodeInvocError Set invocation error return code 00925300
         J     Clsy&SYSNDX._End        Branch to end label              00925400
.* Verify Classify Version    @P1A                                      00925500
Clsy&SYSNDX._Ok   DC    0H             Connect Ok                  @P1A 00925600
         LLGT  14,16(0,0)              Get CVT address             @P1A 00925700
         TM    CVTOSLV6-CVT(14),CVTH7790   Test for HBB7790        @P1A 00925800
         BO    Clsy&SYSNDX._Begin      Branch when Lvl is present  @P1A 00925900
         LA    0,IWMRSNCODEBADVERSION  HBB7790 is required         @P1A 00926000
         LA    15,IWMRETCODEINVOCERROR Set Return Code             @P1A 00926100
         B     Clsy&SYSNDX._End        Branch to end label         @P1A 00926200
.* Invoke Classify Service                                              00926300
Clsy&SYSNDX._Begin DC    0H            Level Ok                    @P1C 00926400
         LLGT  14,X'8C'(14,0)          Get ECVT Address            @P1A 00926500
         LLGT  14,X'C4'(14,0)          Get WMVT Address            @P1A 00926600
         AIF   ('&SYSAM64' NE 'YES').Clsy_Amode31                       00926700
         LLGT  15,X'14C'(14,0)         Get Clsy Entry Point Addr   @P1C 00926800
         BASR  14,15                   Call 64bit Classify Routine @P1C 00926900
         AGO   .Clsy_End                                                00927000
.Clsy_Amode31 ANOP                                                      00927100
         LLGTR 1,1                     Clear High Half of R1            00927200
         LLGT  15,X'148'(14,0)         Get Clsy Entry Point Addr   @P1C 00927300
         BASR  14,15                   Call 31bit Classify Routine @P1C 00927400
.Clsy_End ANOP                                                          00927500
Clsy&SYSNDX._End  DC    0H             End label                        00927600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00927700
