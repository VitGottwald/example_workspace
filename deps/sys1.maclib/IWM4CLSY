.***AC.PLV01  PROCESS PLISTVER                                      *   00144000
.********************************************************************   00144100
&XPLISTVER      SETA  0                                                 00144200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00144300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00144400
&XPLISTVER      SETA  0                                                 00144500
.ZPLV0          ANOP  ,                                                 00144600
.*                                                                      00144700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00144800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00144900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00145000
.*-------------------------------------------------------------------   00145100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00145200
&ZMS            SETC  '"PLISTVER='                                      00145300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00145400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00145500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00145600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00145700
                AGO   .ZPVCK2                                           00145800
.ZPVCK3         ANOP  ,                                                 00145900
.*-------------------------------------------------------------------   00146000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00146100
&ZMS            SETC  '"PLISTVER='                                      00146200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00146300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00146400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00146500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00146600
                AGO   .ZPVCKE                                           00146700
.ZPVCK2         ANOP  ,                                                 00146800
.*-------------------------------------------------------------------   00146900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00147000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00147100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00147200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00147300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00147400
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00147500
.ZPVCK6         ANOP  ,                                                 00147600
&ZMS            SETC  '"PLISTVER='                                      00147700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00147800
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00147900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00148000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00148100
&XPLISTVER      SETA  0                                                 00148200
                AGO   .ZPVCKE                                           00148300
.*-------------------------------------------------------------------   00148400
.ZPVCK4A        ANOP  ,                                                 00148500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00148600
&ZMS            SETC  'PLISTVER='                                       00148700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00148800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00148900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00149000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00149100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00149200
                AGO   .ZPVCK5                                           00149300
.ZPVCK4         ANOP  ,                                                 00149400
&XPLISTVER      SETA  &PLISTVER(1)                                      00149500
.ZPVCK5         ANOP  ,                                                 00149600
.ZPVCKE         ANOP  ,                                                 00149700
