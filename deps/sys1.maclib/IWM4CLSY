.******************************************************************     00606900
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00607000
.******************************************************************     00607100
                    ACTR  10000                                         00607200
.*----------------------------------------------------------------***   00607300
.***AF.P02 EXTRA POSITIONALS                                            00607400
                    AIF   (N'&SYSLIST LE 0).ZELISTX  NO EXTRAS          00607500
&ZCVAL              SETC  ''          ERROR FIELD                       00607600
&ZAWRK              SETA  0           POSITIONAL NUMBER                 00607700
.ZELIST2            ANOP  ,                                             00607800
&ZAWRK              SETA  &ZAWRK+1    NEXT POSITIONAL                   00607900
                    AIF   (&ZAWRK GT N'&SYSLIST).ZELIST3  DONE          00608000
&ZCVAL              SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA               00608100
                    AGO   .ZELIST2                                      00608200
.*                                                                      00608300
.ZELIST3            ANOP  ,                                             00608400
&ZCVAL              SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA            00608500
&ZMS                SETC  'POSITIONAL ARGUMENT(S),'                     00608600
&ZMS                SETC  '&ZMS "&ZCVAL"'                               00608700
&ZMS                SETC  '&ZMS CANNOT BE USED.'                        00608800
&ZMS                SETC  '&ZMS NO POSITIONAL ARGUMENTS'                00608900
                    MNOTE 8,' &ZMS ARE ALLOWED.'                        00609000
&ZMACRET            SETA  8           SET MACRO ERROR RETURN CODE       00609100
.ZELISTX            ANOP  ,                                             00609200
&ZASYSLNDX          SETA  0                                             00609300
.*-------------------------------------------------------------------   00609400
.***AE.MF02 MF=(SLME)                                                   00609500
.ZEMFX              ANOP  ,           END OF PARTIAL SYNTAX CHECK       00609600
.*******************************************************************    00609700
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00609800
.*******************************************************************    00609900
                    AIF   (&ZMACRET NE 0).ZLX2  ERROR DETECTED          00610000
.*******************************************************************    00610100
.***AE.01  START OF IWM4CLSY MACRO CODE                                 00610200
.*******************************************************************    00610300
                    LCLC  &M0M0001                 NAME                 00610400
&M0M0001            SETC  '&XLABEL'                SET NAME             00610500
                    AIF   (K'&XLABEL NE 0).XLAB2                        00610600
&M0M0001            SETC  'M00M&SYSNDX'            DEFAULT NAME         00610700
.*                                                                      00610800
.XLAB2              ANOP  ,                                             00610900
                    AIF   ('&XMF' EQ 'L').XLAB3                         00611000
&M0M0001 DS    0H                             IWM4CLSY-0                00611100
.XLAB3              ANOP  ,                                             00611200
.*******************************************************************    00611300
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00611400
.*******************************************************************    00611500
              PUSH  PRINT                                               00611600
                    AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON    00611700
                    PRINT OFF         ++ PR01                           00611800
                    AGO   .ZPRNTXX                                      00611900
.ZPRINTX            PRINT ON                                            00612000
.ZPRNTXX            ANOP  ,                                             00612100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00612200
         LCLC  &MText                                                   00612300
         LCLA  &MError                                                  00612400
         LCLA  &KwCount1                                                00612500
         LCLA  &KwCount2                                                00612600
&MError  SETA 0                                                         00612700
.*                                                                      00612800
.* SOURCELU, CLIENTIPADDR and NETID/LUNAME                              00612900
.* are mutually exclusive                                               00613000
.*                                                                      00613100
&KwCount1 SETA 0                                                        00613200
         AIF   ('&XSOURCELU' EQ '').Clsy_Net_CIpAddr                    00613300
         AIF   ('&XSOURCELU' EQ 'NO_SOURCELU').Clsy_Net_CIpAddr         00613400
&KwCount1 SETA &KwCount1+2                                              00613500
.Clsy_Net_CIpAddr ANOP                                                  00613600
         AIF   ('&XCLIENTIPADDR' EQ '').Clsy_Net_NetId                  00613700
         AIF   ('&XCLIENTIPADDR' EQ 'NO_CLIENTIPADDR').Clsy_Net_NetId   00613800
&KwCount1 SETA &KwCount1+2                                              00613900
.Clsy_Net_NetId ANOP                                                    00614000
         AIF   ('&XNETID' EQ '').Clsy_Net_LuName                        00614100
         AIF   ('&XNETID' EQ 'NO_NETID').Clsy_Net_LuName                00614200
&KwCount1 SETA &KwCount1+1                                              00614300
.Clsy_Net_LuName ANOP                                                   00614400
         AIF   ('&XLUNAME' EQ '').Clsy_Net_End                          00614500
         AIF   ('&XLUNAME' EQ 'NO_LUNAME').Clsy_Net_End                 00614600
&KwCount1 SETA &KwCount1+1                                              00614700
.Clsy_Net_End AIF (&KwCount1 LE 2).Clsy_Net_OK                          00614800
&MText   SETC  'SOURCELU, CLIENTIPADDR and NETID/LUNAME '               00614900
&MText   SETC  '&MText are mutually exclusive'                          00615000
&MError  SETA  8                                                        00615100
         MNOTE &MError,'&MText'                                         00615200
.Clsy_Net_OK ANOP                                                       00615300
                                                                        00615400
.*                                                                      00615500
.* Ensure EWLM_CHCORR, EWLM_CHCTKN AND EWLM_OUTCORR are                 00615600
.* mutually exclusive parameters                                        00615700
.*                                                                      00615800
&KwCount2 SETA 0                                                        00615900
         AIF   ('&XEWLM_CHCORR' EQ'').Clsy_EwlmCCor_End                 00616000
         AIF   ('&XEWLM_CHCORR' EQ'NO_EWLM_CHCORR').Clsy_EwlmCCor_End   00616100
&KwCount2 SETA &KwCount2+1                                              00616200
.Clsy_EwlmCCor_End ANOP                                                 00616300
         AIF   ('&XEWLM_CHCTKN' EQ'').Clsy_EwlmTkn_End                  00616400
         AIF   ('&XEWLM_CHCTKN' EQ'NO_EWLM_CHCTKN').Clsy_EwlmTkn_End    00616500
&KwCount2 SETA &KwCount2+1                                              00616600
.Clsy_EwlmTkn_End ANOP                                                  00616700
         AIF   ('&XEWLM_OUTCORR'EQ'').Clsy_EwlmOCor_End                 00616800
         AIF   ('&XEWLM_OUTCORR'EQ'NO_EWLM_OUTCORR').Clsy_EwlmOCor_End  00616900
&KwCount2 SETA &KwCount2+1                                              00617000
.Clsy_EwlmOCor_End AIF   (&KwCount2 LE 1).Clsy_Ewlm_OK                  00617100
&MText   SETC  'EWLM_CHCORR, EWLM_CHCTKN AND EWLM_OUTCORR'              00617200
&MText   SETC  '&MText ARE MUTUALLY EXCLUSIVE PARAMETERS'               00617300
&MError  SETA  8                                                        00617400
         MNOTE &MError,'&MText'                                         00617500
.Clsy_Ewlm_OK ANOP                                                      00617600
                                                                        00617700
.*                                                                      00617800
.* End processing if a Macro Error > 4 occurred                         00617900
.*                                                                      00618000
.Clsy_Exit ANOP                                                         00618100
         AIF   (&MError LT 8).Clsy_Exit_End                             00618200
         MEXIT                                                          00618300
.Clsy_Exit_End ANOP                                                     00618400
.*                                                                      00618500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00618600
.********************************************************************   00618700
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00618800
.********************************************************************   00618900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00619000
                      LCLA  &ZOPN1         INVOCATION INDEX             00619100
                      LCLC  &ZOPL(4095)    LABEL                        00619200
                      LCLC  &ZOPO(4095)    OPERATION                    00619300
                      LCLC  &ZOPV(4095)    VALUE                        00619400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00619500
.*                                                                      00619600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00619700
.******************************************************************     00619800
.***AL.L10 IWM4CLSY FMT=LIST MF=S!L PARAMETERS                          00619900
.******************************************************************     00620000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00620100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00620200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00620300
&XMFATTR              SETC  '0D'                                        00620400
.*                                                                      00620500
&ZOPN                 SETA  &ZOPN+1                                     00620600
&ZOPL(&ZOPN)          SETC  ' '                                         00620700
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00620800
&ZOPV(&ZOPN)          SETC  '4,8'                                       00620900
&ZOPN                 SETA  &ZOPN+1                                     00621000
&ZOPL(&ZOPN)          SETC  ' '                                         00621100
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00621200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00621300
&ZOPD(&ZOPN)          SETC  '++ IWM4CLSY ARG PARMS'                     00621400
.*                                                                      00621500
&ZOPN                 SETA  &ZOPN+1                                     00621600
&ZOPL(&ZOPN)          SETC  ' '                                         00621700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00621800
&ZOPV(&ZOPN)          SETC  '1'                                         00621900
&ZOPD(&ZOPN)          SETC  ' '                                         00622000
.*                                                                      00622100
.ZLMFSL2              ANOP  ,                                           00622200
.*-------------------------------------------------------------------   00622300
.***AM.L00  IWM4CLSY FMT=LIST name                                      00622400
                      AIF   ('&XMF' NE 'L').ZLMFSL2M                    00622500
&ZOPN                 SETA &ZOPN+1                                      00622600
&ZOPL(&ZOPN)          SETC '&M0M0001'                                   00622700
&ZOPO(&ZOPN)          SETC 'EQU'                                        00622800
&ZOPV(&ZOPN)          SETC '&XMFCTRL'                                   00622900
&ZOPD(&ZOPN)          SETC '++ IWM4CLSY NAME'                           00623000
.ZLMFSL2M             ANOP  ,                                           00623100
.*-------------------------------------------------------------------   00623200
.***AM.L01  IWM4CLSY FMT=LIST PARAMETERS                                00623300
