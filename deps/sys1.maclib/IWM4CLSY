.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00129500
.********************************************************************   00129600
                    GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL           00129700
                    GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE    00129800
                    GBLA  &SYSALVL    SYSSTATE ARCHLVL                  00129900
                    GBLC  &SYSAM64    SYSSTATE AMODE64                  00130000
.*                                                                      00130100
                    AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED    00130200
                    AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED     00130300
                    SPLEVEL  TEST                                       00130400
.ZSYS2              ANOP  ,                                             00130500
                    AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1         00130600
                    AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1         00130700
                    SYSSTATE TEST                                       00130800
                    AGO   .ZSYS4                                        00130900
.ZSYS3              ANOP  ,                                             00131000
&SYSASCE            SETC  'P'         ASSUME SYSSTATE PRIMARY           00131100
.ZSYS4              ANOP  ,                                             00131200
                    AIF   ('&SYSAM64' NE 'YES').ZSYSN64                 00131300
&ZMAC_LLGT          SETC 'LLGT'                                         00131400
&ZMAC_LGR           SETC 'LGR'                                          00131500
&ZMAC_LG            SETC 'LG'                                           00131600
&ZMAC_STG           SETC 'STG'                                          00131700
.ZSYSN64            ANOP  ,                                             00131800
&ZMAC_G             SETC 'G'                                            00131900
&ZMAC_LR            SETC 'LGR'                                          00132000
&ZMAC_L             SETC 'LG'                                           00132100
&ZMAC_ST            SETC 'STG'                                          00132200
