.***AL.L12 IWM4CLSY FMT=LIST MF=S!L PARAMETERS                          00688400
                      AIF   ('&XMF' EQ 'L').ZMFLX  DONE IF MF=L         00688500
.*                                                                      00688600
&ZOPN                 SETA  &ZOPN+1                                     00688700
&ZOPL(&ZOPN)          SETC  '&XMFCTRL.S'                                00688800
&ZOPO(&ZOPN)          SETC  'DC'                                        00688900
&ZOPV(&ZOPN)          SETC  '0H''0'''                                   00689000
&ZOPD(&ZOPN)          SETC  '++ IWM4CLSY ARG PARMS'                     00689100
.*                                                                      00689200
.ZLMFSLX              ANOP  ,                                           00689300
.*-------------------------------------------------------------------   00689400
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00689500
&ZPLLEN               SETA 480                                          00689600
.*                                                                      00689700
.******************************************************************     00689800
.***AI.L01 IWM4CLSY FMT=LIST FILL IN PARAMETER AREA                     00689900
.******************************************************************     00690000
                      ACTR  10000                                       00690100
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00690200
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00690300
&ZCVAL                SETC  'LAE'          XAX MODE                     00690400
.ZIMFXA               ANOP ,                                            00690500
.*                                                                      00690600
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00690700
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00690800
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00690900
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00691000
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00691100
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00691200
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00691300
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00691400
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00691500
&ZOPN                 SETA  &ZOPN+1                                     00691600
&ZOPL(&ZOPN)          SETC  ' '                                         00691700
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00691800
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00691900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00692000
                      AGO   .ZIMFNX                                     00692100
.*                                                                      00692200
.ZIMFN2               ANOP ,                                            00692300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00692400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00692500
&ZOPN                 SETA  &ZOPN+1                                     00692600
&ZOPL(&ZOPN)          SETC  ' '                                         00692700
&ZOPO(&ZOPN)          SETC  'LAE'                                       00692800
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00692900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00693000
                      AGO   .ZIMFNX                                     00693100
.ZIMFNX1              ANOP ,                                            00693200
&ZOPN                 SETA  &ZOPN+1                                     00693300
&ZOPL(&ZOPN)          SETC  ' '                                         00693400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00693500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00693600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00693700
.ZIMFNX               ANOP ,                                            00693800
.********************************************************************   00693900
.***AI.L02 IWM4CLSY FMT=LIST INITIALIZE PARAMETER AREA                  00694000
.********************************************************************   00694100
.***AI.L03 MF=(SLME)                                                    00694200
                      AIF   ('&XMF' EQ 'L').ZIMFLX                      00694300
                      AIF   ('&XMFSCK' NE 'COMPLETE').ZIMFLX            00694400
&ZAWRK                SETA  0                                           00694500
&ZN                   SETA  &ZPLLEN                                     00694600
.ZCLRCONT             AIF   (&ZN LE 256).ZCLRX                          00694700
.*                                                                      00694800
&ZOPN                 SETA  &ZOPN+1                                     00694900
&ZOPL(&ZOPN)          SETC  ' '                                         00695000
&ZOPO(&ZOPN)          SETC  'XC'                                        00695100
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(256,1),&ZAWRK.(1)'                 00695200
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00695300
.*                                                                      00695400
&ZAWRK                SETA &ZAWRK+256                                   00695500
&ZN                   SETA  &ZN-256                                     00695600
                      AGO   .ZCLRCONT                                   00695700
.*                                                                      00695800
.ZCLRX                ANOP  ,                                           00695900
.*                                                                      00696000
&ZOPN                 SETA  &ZOPN+1                                     00696100
&ZOPL(&ZOPN)          SETC  ' '                                         00696200
&ZOPO(&ZOPN)          SETC  'XC'                                        00696300
&ZOPV(&ZOPN)          SETC  '&ZAWRK.(&ZN,1),&ZAWRK.(1)'                 00696400
&ZOPD(&ZOPN)          SETC  '++ INITIALIZE'                             00696500
.ZIMFLX               ANOP  ,                                           00696600
.*-------------------------------------------------------------------   00696700
.***AI.L05 XOPTIONS BL1                                                 00696800
&ZW                   SETC  ''                                          00696900
&ZCLNG                SETC  ''             RESET BIT ON FLAG            00697000
&ZMS                  SETC  ''                                          00697100
&ZC                   SETC  ''                                          00697200
.*-------------------------------------------------------------------   00697300
.***AI.L08 XOPTIONS "IWM4CLSY"                                          00697400
.***      BL1'XXXXXXX1'                                                 00697500
&ZW                   SETC  '&ZW.00000001'                              00697600
&ZCLNG                SETC  'YES'          SET BIT ON FLAG              00697700
&ZMS                  SETC  '&ZMS.11111111'                             00697800
.*-------------------------------------------------------------------   00697900
.***AI.L11 XOPTIONS BL1                                                 00698000
                      AIF   ('&XMFSCK' NE 'NOCHECK').ZI3D               00698100
                      AIF   ('&ZC' NE 'YES').ZI3D                       00698200
&ZOPN                 SETA  &ZOPN+1                                     00698300
&ZOPL(&ZOPN)          SETC  ' '                                         00698400
&ZOPO(&ZOPN)          SETC  'NI'                                        00698500
&ZOPV(&ZOPN)          SETC  '1(1),B''&ZMS'''                            00698600
&ZOPD(&ZOPN)          SETC  '++ 182 BL1'                                00698700
.ZI3D                 ANOP  ,                                           00698800
                      AIF   ('&ZCLNG' NE 'YES').ZI3E  ALL ZERO BITS     00698900
&ZOPN                 SETA  &ZOPN+1                                     00699000
&ZOPL(&ZOPN)          SETC  ' '                                         00699100
&ZOPO(&ZOPN)          SETC  'OI'                                        00699200
&ZOPV(&ZOPN)          SETC  '1(1),B''&ZW'''                             00699300
&ZOPD(&ZOPN)          SETC  '++ 182 BL1'                                00699400
.ZI3E                 ANOP  ,                                           00699500
.*-------------------------------------------------------------------   00699600
.***AI.L04 XPLISTLEN XL2                                                00699700
&ZOPN                 SETA  &ZOPN+1                                     00699800
&ZOPL(&ZOPN)          SETC  ' '                                         00699900
&ZOPO(&ZOPN)          SETC  'MVC'                                       00700000
&ZOPV(&ZOPN)          SETC  '2(2,1),=AL2(&ZPLLEN)'                      00700100
&ZOPD(&ZOPN)          SETC  '++ INPUT XPLISTLEN XL2'                    00700200
.*-------------------------------------------------------------------   00700300
.***AI.L25  XCONNTKN XL4                                                00700400
                      AIF   ('&ZXCONNTKN' EQ '').ZI7E                   00700500
                      AIF   ('&ZXCONNTKN' EQ 'OUTPUT').ZI7E             00700600
&ZCGPR                SETC  '&XCONNTKN'                                 00700700
.*                                                                      00700800
