.***AC.PLV01  PROCESS PLISTVER                                      *   00082800
.********************************************************************   00082900
&XPLISTVER      SETA  0                                                 00083000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00083100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00083200
&XPLISTVER      SETA  2                                                 00083300
.ZPLV0          ANOP  ,                                                 00083400
.*                                                                      00083500
                AIF   ('&PROXYRESPONSE' EQ '').ZPLV1E                   00083600
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00083700
&XPLISTVER      SETA  1                                                 00083800
.ZPLV1E         ANOP  ,                                                 00083900
.*                                                                      00084000
                AIF   ('&PROXYRESPONSE' EQ '').ZPLV2E                   00084100
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00084200
&XPLISTVER      SETA  1                                                 00084300
.ZPLV2E         ANOP  ,                                                 00084400
.*                                                                      00084500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00084600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00084700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00084800
.*-------------------------------------------------------------------   00084900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00085000
&ZMS            SETC  '"PLISTVER='                                      00085100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00085200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00085300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00085400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00085500
                AGO   .ZPVCK2                                           00085600
.ZPVCK3         ANOP  ,                                                 00085700
.*-------------------------------------------------------------------   00085800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00085900
&ZMS            SETC  '"PLISTVER='                                      00086000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00086100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00086200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00086300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00086400
                AGO   .ZPVCKE                                           00086500
.ZPVCK2         ANOP  ,                                                 00086600
.*-------------------------------------------------------------------   00086700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00086800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00086900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00087000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00087100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00087200
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00087300
.ZPVCK6         ANOP  ,                                                 00087400
&ZMS            SETC  '"PLISTVER='                                      00087500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00087600
&ZMS            SETC  '&ZMS THE RANGE IS "0-2"'                         00087700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00087800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00087900
&XPLISTVER      SETA  2                                                 00088000
                AGO   .ZPVCKE                                           00088100
.*-------------------------------------------------------------------   00088200
.ZPVCK4A        ANOP  ,                                                 00088300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00088400
&ZMS            SETC  'PLISTVER='                                       00088500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00088600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00088700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00088800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00088900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00089000
                AGO   .ZPVCK5                                           00089100
.ZPVCK4         ANOP  ,                                                 00089200
&XPLISTVER      SETA  &PLISTVER(1)                                      00089300
.ZPVCK5         ANOP  ,                                                 00089400
.ZPVCKE         ANOP  ,                                                 00089500
