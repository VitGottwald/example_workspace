.***AO.X04  LABEL FOR MF=L PROCESSING                                   03330900
.*******************************************************************    03331000
.ZMFLX                ANOP  ,              MF=L PROCESSING              03331100
.*******************************************************************    03331200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               03331300
.*******************************************************************    03331400
                      ACTR  10000                                       03331500
.*                                                                      03331600
                      LCLA  &ZLC           COLUMN POSITION              03331700
                      LCLA  &ZOPX          LIST TABLE INDEX             03331800
                      LCLC  &ZLL           LABEL                        03331900
                      LCLC  &ZLOP          OPERATION                    03332000
                      LCLC  &ZLV           VALUE                        03332100
                      LCLC  &ZLD           DESCRIPTION                  03332200
.*                                                                      03332300
&ZOPX                 SETA  1                                           03332400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            03332500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              03332600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              03332700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              03332800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              03332900
                      AIF   (K'&ZLD LE 0).ZL5                           03333000
&ZLC                  SETA  35             COMMENT COLUMN               03333100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    03333200
&ZLC                  SETA  33             COMMMENT COLUMN              03333300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   03333400
&ZLV                  SETC  '&ZLV'.' '                                  03333500
                      AGO   .ZL3                                        03333600
.*                                                                      03333700
.ZL4                  ANOP  ,                                           03333800
&ZLV                  SETC  '&ZLV'.' '                                  03333900
.ZL5                  ANOP  ,                                           03334000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               03334100
.*                                                                      03334200
&ZLL     &ZLOP &ZLV                                                     03334300
.*                                                                      03334400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               03334500
                      AGO   .ZL2           CONTINUE LOOPING             03334600
.*                                                                      03334700
.ZLX                  ANOP  ,                                           03334800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                03334900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                03335000
