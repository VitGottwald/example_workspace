&ZI                  SETA  0           XVARIABLE ARRAY INDEX            02472400
&ZJ                  SETA  0           SUBLIST INDEX                    02472500
.ZE434A              ANOP  ,                                            02472600
&ZJ                  SETA  &ZJ+1       NEXT SUBLIST                     02472700
                     AIF   (&ZJ GT N'&ATTRIBUTE).ZE434C  DONE           02472800
                     AIF   (K'&ATTRIBUTE(&ZJ) LE 0).ZE434A  NULL        02472900
&ZI                  SETA  &ZI+1       NEXT ARRAY ENTRY                 02473000
&XATTRIBUTE(&ZI)   SETC  ''            NULL ENTRY                       02473100
&XATTRIBUTE(&ZI+1) SETC  ''            NULL AT ARRAY END                02473200
.ZE435B  AIF   ('&ATTRIBUTE(&ZJ)' NE 'DEFS').ZE436B                     02473300
&XATTRIBUTE(&ZI)   SETC  'DEFS'        SET KEYWORD                      02473400
.ZE436B  AIF   ('&ATTRIBUTE(&ZJ)' NE 'NOTOWNERGONE').ZE437B             02473500
&XATTRIBUTE(&ZI)   SETC  'NOTOWNERGONE'         SET KEYWORD             02473600
.ZE437B  AIF   ('&ATTRIBUTE(&ZJ)' NE 'OWNERGONE').ZE438B                02473700
&XATTRIBUTE(&ZI)   SETC  'OWNERGONE'   SET KEYWORD                      02473800
.ZE438B              AIF   ('&XATTRIBUTE(&ZI)' NE '').ZE434A            02473900
&ZI                  SETA  &ZI-1       BACKUP ARRAY                     02474000
&ZMS                 SETC  '"ATTRIBUTE='                                02474100
&ZMS                 SETC  '&ZMS.&ATTRIBUTE"'                           02474200
&ZMS                 SETC  '&ZMS ARGUMENT &ZJ'                          02474300
&ZMS                 SETC  '&ZMS IS AN UNKNOWN KEYWORD. '               02474400
&ZMS                 SETC  '&ZMS VALID KEYWORD CHOICE(S) ARE:'          02474500
