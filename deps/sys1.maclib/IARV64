.*******************************************************************    02797900
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              02798000
.*******************************************************************    02798100
              PUSH  PRINT                                               02798200
                     AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON   02798300
                     PRINT OFF         ++ PR01                          02798400
                     AGO   .ZPRNTXX                                     02798500
.ZPRINTX             PRINT ON                                           02798600
.ZPRNTXX             ANOP  ,                                            02798700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  02798800
.**------------------------------------------------------------------** 02798900
.**                                                                  ** 02799000
.**  Start of code to check for OBJECTTYPE being used in the         ** 02799100
.**  assembler form of the macro. OBJECTTYPE is an internal          ** 02799200
.**  parameter that should only be used in BCP pl/x code.            ** 02799300
.**                                                              @AGA** 02799400
.**------------------------------------------------------------------** 02799500
         AIF   ('&OBJECTTYPE' EQ '').NOOBJTYPE                     @AGA 02799600
&ZMS     SETC  'OBJECTTYPE MUST NOT BE SPECIFIED'                  @AGA 02799700
         MNOTE 8,'&ZMS'                                            @AGA 02799800
         POP   PRINT                                               @AGA 02799900
         MEXIT                                                     @AGA 02800000
.NOOBJTYPE ANOP                                                    @AGA 02800100
.**------------------------------------------------------------------** 02800200
.**  Start of code to set macro version number                       ** 02800300
.**                                                                  ** 02800400
.**  This code sets the macro version number for those keywords      ** 02800500
.**  which already existed in earlier macro versions but are used    ** 02800600
.**  for additional request types in later macro versions.           ** 02800700
.**                                                                  ** 02800800
.**  The keyword in question is PAGEFRAMESIZE.  Additional request   ** 02800900
.**  types were added in version 2 (PAGEABLE1MEG and DREF1MEG).      ** 02801000
.**  The following code will ensure that the macro version number    ** 02801100
.**  is set to at least 2 if PAGEABLE1MEG or DREF1MEG is specified.  ** 02801200
.**                                                                  ** 02801300
.**  Note: 1M and 2G were added in version 5.  In order to use       ** 02801400
.**        these request types, the UNITS keyword must be specified. ** 02801500
.**        UNITS is a version 5 keyword.                             ** 02801600
.**                                                                  ** 02801700
.**                                                              @PJA** 02801800
.**------------------------------------------------------------------** 02801900
         LCLA  &LISTPLVERS                                         @PJA 02802000
         AIF   ('&XMF' NE 'M').CHKPLVERS                           @0DA 02802100
         AIF   ('&TYPE(1)' EQ 'FIXED').SETPLV5                     @0DA 02802200
         AGO   .CHKPLVERS                                          @0DA 02802300
.SETPLV5            ANOP                                           @0DA 02802400
&LISTPLVERS SETA  5                                                @0DA 02802500
         AGO   .SETPLVERSEND                                       @0DA 02802600
.CHKPLVERS          ANOP                                           @0DA 02802700
         AIF   (&XPLISTVER GT 2).SETPLVERSGT2                      @PJA 02802800
         AGO   .CHKPLVERSTWO                                       @PJA 02802900
.SETPLVERSGT2    ANOP ,                                            @PJA 02803000
&LISTPLVERS SETA  &XPLISTVER                                       @PJA 02803100
         AGO   .SETPLVERSEND                                       @PJA 02803200
.CHKPLVERSTWO       ANOP                                           @PJA 02803300
         AIF   (&XPLISTVER EQ 2).SETPLV2                           @PJA 02803400
         AIF   ('&PAGEFRAMESIZE(1)' EQ 'PAGEABLE1MEG').SETPLV2     @PJA 02803500
         AIF   ('&PAGEFRAMESIZE(1)' EQ 'DREF1MEG').SETPLV2         @PJA 02803600
         AGO   .CHKPLVERSONE                                       @PJA 02803700
.SETPLV2            ANOP                                           @PJA 02803800
&LISTPLVERS SETA  2                                                @PJA 02803900
         AGO   .SETPLVERSEND                                       @PJA 02804000
.CHKPLVERSONE       ANOP                                           @PJA 02804100
         AIF   (&XPLISTVER EQ 1).SETPLVERSONE                      @PJA 02804200
         AGO   .SETPLVERSZER                                       @PJA 02804300
.SETPLVERSONE       ANOP                                           @PJA 02804400
&LISTPLVERS SETA  1                                                @PJA 02804500
         AGO   .SETPLVERSEND                                       @PJA 02804600
.SETPLVERSZER       ANOP                                           @PJA 02804700
&LISTPLVERS SETA  0                                                @PJA 02804800
.SETPLVERSEND       ANOP                                           @PJA 02804900
         AIF   ('&PLISTVER(1)' EQ '').SETPLVER                     @PJA 02805000
         AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').SETPLVER      @PJA 02805100
         AIF   (&XPLISTVER GE &LISTPLVERS).SETPLVERDONE            @0DC 02805200
         MNOTE 8,' PLISTVER="&PLISTVER(1)", BUT PLISTVER OF &LISTPLVERS*02805300
                IS REQUIRED.'                                      @PJA 02805400
&ZMACRET SETA  8                                                   @PJA 02805500
         AGO    .SETPLVERDONE                                      @PJA 02805600
.SETPLVER           ANOP                                           @PJA 02805700
&XPLISTVER  SETA  &LISTPLVERS                                      @PJA 02805800
.SETPLVERDONE       ANOP                                           @PJA 02805900
         AIF   ('&XMF' NE 'M').MODIFYFORMEND                       @0DA 02806000
         AIF   ('&UNITS(1)' NE '').MODIFYFORMEND                   @0DA 02806100
         AIF   ('&SEGMENTS(1)' NE '').MODIFYFORMEND                @0DA 02806200
         AIF   ('&TYPE(1)' EQ '').SKIPTYPE                         @0DA 02806300
         AIF   ('&TYPE(1)' EQ 'PAGEABLE').SETTYPEPAGEABLE          @0DA 02806400
         AIF   ('&TYPE(1)' EQ 'DREF').SETTYPEDREF                  @0DA 02806500
         AIF   ('&TYPE(1)' EQ 'FIXED').SETTYPEFIXED                @0DA 02806600
&ZXTYPE            SETC  ''          RESET KEYWORD                 @0DA 02806700
         MNOTE 8,' TYPE="&TYPE(1)" ARGUMENT 1 IS AN UNKNOWN KEYWORD. VA*02806800
                LID KEYWORD CHOICE(S) ARE: "PAGEABLE", "DREF", OR "FIXE*02806900
                D".'                                               @0DA 02807000
         AGO   .SKIPTYPE                                           @0DA 02807100
.SETTYPEPAGEABLE    ANOP                                           @0DA 02807200
&XTYPE             SETC  'PAGEABLE'  SET KEYWORD                   @0DA 02807300
         AGO   .SETTYPE                                            @0DA 02807400
.SETTYPEDREF        ANOP                                           @0DA 02807500
&XTYPE             SETC  'DREF'      SET KEYWORD                   @0DA 02807600
         AGO   .SETTYPE                                            @0DA 02807700
.SETTYPEFIXED       ANOP                                           @0DA 02807800
&XTYPE             SETC  'FIXED'     SET KEYWORD                   @0DA 02807900
         AGO   .SETTYPE                                            @0DA 02808000
.SETTYPE            ANOP                                           @0DA 02808100
&ZXTYPE            SETC  'INPUT'     SET IN/OUTPUT                 @0DA 02808200
         AGO   .SKIPTYPE                                           @0DA 02808300
.SKIPTYPE           ANOP                                           @0DA 02808400
.MODIFYFORMEND      ANOP                                           @0DA 02808500
