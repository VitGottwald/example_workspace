*                                                                       00001000
* %DCL IHAIPA_INCLUDED CHAR EXT;                                        00002000
* %DEACTIVATE IHAIPA_INCLUDED;                                          00003000
* %IF IHAIPA_INCLUDED = '' %THEN                                        00004000
* %DO;                                                                  00005000
*/* Macro made bi-lingual on 22120. CBGEN compile date 21138         */ 00006000
*% /*                                                                   00007000
         MACRO                                                          00008000
         IHAIPA &DSECT=YES,&LIST=YES,&TITLE=YES,&IPA=YES,&IPAPDE=COND, *00009000
               &IPAPLI=COND                                             00010000
         GBLC  &IHAIPA_INCLUDED                                         00011000
         GBLC  &ZCBPRINT                                                00012000
&IHAIPA_INCLUDED SETC 'YES'                                             00013000
         AIF   ('&LIST' EQ 'NONE').P0                                   00014000
         AIF   ('&TITLE' EQ 'NO').P5                                    00015000
 TITLE   'IHAIPA  - Initialization Parameter Area                   '   00016000
.P5      ANOP                                                           00017000
