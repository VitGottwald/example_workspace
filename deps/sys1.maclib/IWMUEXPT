.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00140000
.*******************************************************************    00140100
                      ACTR  10000                                       00140200
.*                                                                      00140300
                      LCLA  &ZLC           COLUMN POSITION              00140400
                      LCLA  &ZOPX          LIST TABLE INDEX             00140500
                      LCLC  &ZLL           LABEL                        00140600
                      LCLC  &ZLOP          OPERATION                    00140700
                      LCLC  &ZLV           VALUE                        00140800
                      LCLC  &ZLD           DESCRIPTION                  00140900
.*                                                                      00141000
&ZOPX                 SETA  1                                           00141100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00141200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00141300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00141400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00141500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00141600
                      AIF   (K'&ZLD LE 0).ZL5                           00141700
&ZLC                  SETA  35             COMMENT COLUMN               00141800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00141900
&ZLC                  SETA  33             COMMMENT COLUMN              00142000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00142100
&ZLV                  SETC  '&ZLV'.' '                                  00142200
                      AGO   .ZL3                                        00142300
.*                                                                      00142400
.ZL4                  ANOP  ,                                           00142500
&ZLV                  SETC  '&ZLV'.' '                                  00142600
.ZL5                  ANOP  ,                                           00142700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00142800
.*                                                                      00142900
&ZLL     &ZLOP &ZLV                                                     00143000
.*                                                                      00143100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00143200
                      AGO   .ZL2           CONTINUE LOOPING             00143300
.*                                                                      00143400
.ZLX                  ANOP  ,                                           00143500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00143600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00143700
         L     14,16(0,0)              GET CVT ADDRESS                  00143800
         L     14,772(14,0)            GET SFT ADDRESS                  00143900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00144000
         LA    15,88                   GET EX FOR SERVICE ROUTINE       00144100
         OR    14,15                   GET PC NUMBER                    00144200
         PC    0(14)                                                    00144300
END&SYSNDX DS    0H                    END LABEL                        00144400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00144500
