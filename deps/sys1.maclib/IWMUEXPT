.ZIMFXA               ANOP ,                                            00123000
.*                                                                      00123100
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00123200
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00123300
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00123400
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00123500
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00123600
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00123700
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00123800
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00123900
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00124000
&ZOPN                 SETA  &ZOPN+1                                     00124100
&ZOPL(&ZOPN)          SETC  ' '                                         00124200
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00124300
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00124400
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00124500
                      AGO   .ZIMFNX                                     00124600
.*                                                                      00124700
