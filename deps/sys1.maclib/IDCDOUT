*                               1=NOLIMIT, 0=SEE DSGGBKUF    @OY59794*/         
*     3 DSGFIMIG BIT(1),     /* INTERVAL MIGRATION                              
*                                 1 = YES 0 = NO             @OY59794*/         
*     3 DSGFPSM  BIT(1),     /* PRIMARY SPACE MGMT           @OW20434*/         
*                            /* 1 = YES 0 = NO               @OW20434*/         
*   2 DSGFLGDC BIT(8),       /* RESERVED                     @OW20434*/         
*     3 DSG32NAM BIT(1),     /* 0 = Use DSGFPRST, DSGCNFRM   @OW20434*/         
*                            /* 1 = Use DSGSSTAT             @OW20434*/         
*   2 *        CHAR(2),      /* RESERVED                   4@20D @20C*/         
*                                                                               
*   /*****************************************************************/         
*   /* STORAGE GROUP ATTRIBUTES                              @OY59794*/         
*   /*****************************************************************/         
*   2 DSGFTYPE FIXED(8),     /* STORAGE GROUP TYPE -- SEE                       
*                               CONSTANTS BELOW              @OY59794*/         
*   2 DSGFHTHR FIXED(8),     /* HIGH THRESHOLD - 0 TO 99 %   @OY59794*/         
*   2 DSGFLTHR FIXED(8),     /* LOW THRESHOLD - 0 TO 99 %    @OY59794*/         
*   2 *        CHAR(1),      /* RESERVED                     @OY59794*/         
