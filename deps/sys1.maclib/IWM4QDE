&ZCVAL                SETC  'LA'           ASSUME XA MODE               00122900
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00123000
&ZCVAL                SETC  'LAE'          XAX MODE                     00123100
.ZIMFXA               ANOP ,                                            00123200
.*                                                                      00123300
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00123400
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00123500
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00123600
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00123700
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00123800
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00123900
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00124000
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00124100
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00124200
&ZOPN                 SETA  &ZOPN+1                                     00124300
