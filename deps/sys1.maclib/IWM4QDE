.***AC.PLV01  PROCESS PLISTVER                                      *   00060400
.********************************************************************   00060500
&XPLISTVER      SETA  0                                                 00060600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00060700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00060800
&XPLISTVER      SETA  0                                                 00060900
.ZPLV0          ANOP  ,                                                 00061000
.*                                                                      00061100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00061200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00061300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00061400
.*-------------------------------------------------------------------   00061500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00061600
&ZMS            SETC  '"PLISTVER='                                      00061700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00061800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00061900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00062000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00062100
                AGO   .ZPVCK2                                           00062200
.ZPVCK3         ANOP  ,                                                 00062300
.*-------------------------------------------------------------------   00062400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00062500
&ZMS            SETC  '"PLISTVER='                                      00062600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00062700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00062800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00062900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063000
                AGO   .ZPVCKE                                           00063100
.ZPVCK2         ANOP  ,                                                 00063200
.*-------------------------------------------------------------------   00063300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00063400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00063500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00063600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00063700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00063800
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00063900
.ZPVCK6         ANOP  ,                                                 00064000
&ZMS            SETC  '"PLISTVER='                                      00064100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064200
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00064300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00064400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064500
&XPLISTVER      SETA  0                                                 00064600
                AGO   .ZPVCKE                                           00064700
.*-------------------------------------------------------------------   00064800
.ZPVCK4A        ANOP  ,                                                 00064900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00065000
&ZMS            SETC  'PLISTVER='                                       00065100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00065200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00065300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00065400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00065500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065600
                AGO   .ZPVCK5                                           00065700
.ZPVCK4         ANOP  ,                                                 00065800
&XPLISTVER      SETA  &PLISTVER(1)                                      00065900
.ZPVCK5         ANOP  ,                                                 00066000
.ZPVCKE         ANOP  ,                                                 00066100
.********************************************************************   00066200
