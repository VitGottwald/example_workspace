**/ ;                                                                   00860000
* %IWMSVPOL_INCLUDED = 'YES';                                           00861000
* %DCL ZCBPRINT CHAR EXT;                                               00862000
* %DEACTIVATE ZCBPRINT;                                                 00863000
* %DCL IWMSVPOL_LIST CHAR EXT;                                          00864000
* %DEACTIVATE IWMSVPOL_LIST;                                            00865000
* %IF IWMSVPOL_LIST = 'NO' !                                            00866000
*    ZCBPRINT = 'NO' %THEN                                              00867000
* %DO;                                                                  00868000
*   @LIST PUSH NOECHO;                                                  00869000
*   @LIST NOASSEMBLE NOECHO;                                            00870000
*   @LIST OFF C NOECHO;                                                 00871000
* %END;                                                                 00872000
*/* Start of PL/X Source                                             */ 00873000
*                                                                       00874000
*   %IF SVPOLVIA = 'YES'            /* Specified SVPOLVIA=YES        */ 00875000
*     %THEN %SVPOLVIA = 'VIA';      /* Cause mapping to be VIA       */ 00876000
*     %ELSE %SVPOLVIA = '';         /* Otherwise do not use VIA      */ 00877000
*   DECLARE                                                             00878000
*                                                                       00879000
*     /***************************************************************/ 00880000
*     /*                                                             */ 00881000
*     /* Service policy header information                           */ 00882000
*     /*                                                             */ 00883000
*     /***************************************************************/ 00884000
*                                                                       00885000
*     1    SVPOLHD  SVPOLVIA BASED BDY(DWORD), /* IWMPQRY Answer area*/ 00886000
*           3    SVPOLNAM CHAR(4),   /* Eyecatcher - SVPO            */ 00887000
*           3    SVPOLLVL FIXED(8),  /* Functionality level of the      00888000
*                                       SVPOL. The functionality level  00889000
*                                       defines the highest level of    00890000
*                                       the WLM function that exists    00891000
