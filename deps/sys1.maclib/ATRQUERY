.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01070800
.*******************************************************************    01070900
                      ACTR  10000                                       01071000
.*                                                                      01071100
                      LCLA  &ZLC           COLUMN POSITION              01071200
                      LCLA  &ZOPX          LIST TABLE INDEX             01071300
                      LCLC  &ZLL           LABEL                        01071400
                      LCLC  &ZLOP          OPERATION                    01071500
                      LCLC  &ZLV           VALUE                        01071600
                      LCLC  &ZLD           DESCRIPTION                  01071700
.*                                                                      01071800
&ZOPX                 SETA  1                                           01071900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01072000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01072100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01072200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01072300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01072400
                      AIF   (K'&ZLD LE 0).ZL5                           01072500
&ZLC                  SETA  35             COMMENT COLUMN               01072600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01072700
&ZLC                  SETA  33             COMMMENT COLUMN              01072800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01072900
&ZLV                  SETC  '&ZLV'.' '                                  01073000
                      AGO   .ZL3                                        01073100
.*                                                                      01073200
.ZL4                  ANOP  ,                                           01073300
&ZLV                  SETC  '&ZLV'.' '                                  01073400
.ZL5                  ANOP  ,                                           01073500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01073600
.*                                                                      01073700
&ZLL     &ZLOP &ZLV                                                     01073800
.*                                                                      01073900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01074000
                      AGO   .ZL2           CONTINUE LOOPING             01074100
.*                                                                      01074200
.ZLX                  ANOP  ,                                           01074300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01074400
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01074500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01074600
         L     14,16(0,0)                                               01074700
         L     14,CVTECVT-CVTMAP(14)                                    01074800
         TM    ECVTRRSW-ECVT(14),B'10000000'                            01074900
         BNZ   NSRV&SYSNDX                                              01075000
         L     14,ECVTRRSW-ECVT(14)                                     01075100
         L     14,156(14)                                               01075200
         PC    0(14)                                                    01075300
         B     DONE&SYSNDX                                              01075400
NSRV&SYSNDX EQU *                                                       01075500
         LA    15,ATRQUERY_FAILURE                                      01075600
         LA    0,ATRQUERY_RRS_NOT_ACTIVE                                01075700
DONE&SYSNDX EQU *                                                       01075800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  01075900
.ZLINKX               ANOP  ,                                           01076000
