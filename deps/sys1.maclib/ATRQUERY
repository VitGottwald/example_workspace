&ZASIZ           SETA  1024                                             00404400
                 AIF   ('&ZCGPR' EQ 'K').ZG44T  K FACTOR                00404500
&ZASIZ           SETA  1024*1024                                        00404600
                 AIF   ('&ZCGPR' EQ 'M').ZG44T  M FACTOR                00404700
&ZASIZ           SETA  1024*1024*1024                                   00404800
                 AIF   ('&ZCGPR' NE 'G').ZG44U  NOT G FACTOR            00404900
.ZG44T           ANOP  ,                                                00405000
&ZCVAL           SETC  '&ZAWRK*&ZASIZ'          EQUIVALENT NUMBER       00405100
.ZG44TT          ANOP  ,                                                00405200
&ZN              SETA  2147483647/&ZASIZ        CONVERT                 00405300
                 AIF   (&ZN LT 0).ZG44U         TOO LOW                 00405400
                 AIF   (&ZAWRK GT &ZN).ZG44U    TOO HIGH                00405500
&XURTYPE         SETC '&ZCVAL'     SET XVARIABLE NUMBER                 00405600
                 AGO   .ZG44V                                           00405700
.*                                                                      00405800
