*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  01079600
*%ATRQUERY: MACRO KEYS(REQUEST,RMNAME,GNAME,SYSNAME,METADATA,WMNAME,    01079700
*                 DEFURONLY,EXCLMASK,ASID,URID,URIDSTR,SURID,SURIDSTR,  01079800
*                 URTYPE,URSTATE,URSTMASK,LUWID,LUWIDSTR,TID,TIDHIGH,   01079900
*                 TIDLOW,GTID,GTIDSTR,XID,XIDFORMATID,XIDGTRIDSTR,      01080000
*                 XIDBQUALSTR,TODBEG,TODEND,CURDUR,EXINFOMASK,SORTTAB,  01080100
*                 SORTNUM,AREAADDR,AREAALET,AREAOPT,AREALEN,COUNT,      01080200
*                 RCTABLE,RCNUM,RETCODE,RSNCODE,PLISTVER,MF);           01080300
*  ANS('?'!!MACLABEL!!' '!!'ATRQUERP '!!MACLIST!!                       01080400
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              01080500
*%END ATRQUERY;                                                         01080600
