*01* COMPONENT:  SCRRS                                                * 00188000
*                                                                     * 00188100
*01* DISTRIBUTION LIBRARY: AMACLIB                                    * 00188200
*                                                                     * 00188300
*01* EXAMPLES:                                                        * 00188400
*    EXAMPLE 1                                                        * 00188500
*                                                                     * 00188600
*    OPERATION: Obtain Resource Manager information                   * 00188700
*                                                                     * 00188800
*               ATRQUERY REQUEST=RMINFO,AREAADDR=AREA@,AREALEN=RETLEN * 00188900
*      ,        X                                                     * 00189000
*                     COUNT=RETCNT                                    * 00189100
*      *                                                              * 00189200
*      * INPUT VALUES                                                 * 00189300
*      *                                                              * 00189400
*      AREA@    DC     A(RETAREA)               ADDRESS OF RETURN ARE * 00189500
*      A                                                              * 00189600
*      RETLEN   DC     F'1024'                  LENGTH OF RETURN AREA * 00189700
*      *                                                              * 00189800
*      * OUTPUT AREAS                                                 * 00189900
*      *                                                              * 00190000
*      RETCNT   DS     F                        RETURNED RM COUNT     * 00190100
*      RETAREA  DS     1024C                    RETURN AREA           * 00190200
*      *                                                              * 00190300
*      * REQUIRED MAPPINGS                                            * 00190400
*      *                                                              * 00190500
*               CVT DSECT=YES                                         * 00190600
*               IHAECVT                                               * 00190700
*               ATRFZQRY                                              * 00190800
*                                                                     * 00190900
*    EXAMPLE 2                                                        * 00191000
*                                                                     * 00191100
*    OPERATION: Obtain Resource Manager information for the resource  * 00191200
*    manager with the name UTRICK                                     * 00191300
*                                                                     * 00191400
*               ATRQUERY REQUEST=RMINFO,AREAADDR=AREA@,AREALEN=RETLEN * 00191500
*      ,        X                                                     * 00191600
*                     COUNT=RETCNT,RMNAME=MYRM                        * 00191700
*      *                                                              * 00191800
*      * INPUT VALUES                                                 * 00191900
*      *                                                              * 00192000
*      AREA@    DC     A(RETAREA)               ADDRESS OF RETURN ARE * 00192100
*      A                                                              * 00192200
*      RETLEN   DC     F'1024'                  LENGTH OF RETURN AREA * 00192300
*      MYRM     DC     CL32'UTRICK'             RESOURCE MANAGER NAME * 00192400
*      *                                                              * 00192500
*      * OUTPUT AREAS                                                 * 00192600
*      *                                                              * 00192700
*      RETCNT   DS     F                        RETURNED RM COUNT     * 00192800
*      RETAREA  DS     1024C                    RETURN AREA           * 00192900
*      *                                                              * 00193000
*      * REQUIRED MAPPINGS                                            * 00193100
*      *                                                              * 00193200
*               CVT DSECT=YES                                         * 00193300
*               IHAECVT                                               * 00193400
*               ATRFZQRY                                              * 00193500
*                                                                     * 00193600
*    EXAMPLE 3                                                        * 00193700
*                                                                     * 00193800
*    OPERATION: Obtain Unit Of Recovery information                   * 00193900
*                                                                     * 00194000
*               ATRQUERY REQUEST=URINFO,AREAADDR=AREA@,AREALEN=RETLEN * 00194100
*      ,        X                                                     * 00194200
*                     COUNT=RETCNT                                    * 00194300
*      *                                                              * 00194400
*      * INPUT VALUES                                                 * 00194500
*      *                                                              * 00194600
*      AREA@    DC     A(RETAREA)               ADDRESS OF RETURN ARE * 00194700
*      A                                                              * 00194800
*      RETLEN   DC     F'1024'                  LENGTH OF RETURN AREA * 00194900
*      *                                                              * 00195000
*      * OUTPUT AREAS                                                 * 00195100
*      *                                                              * 00195200
*      RETCNT   DS     F                        RETURNED RM COUNT     * 00195300
*      RETAREA  DS     1024C                    RETURN AREA           * 00195400
*      *                                                              * 00195500
*      * REQUIRED MAPPINGS                                            * 00195600
*      *                                                              * 00195700
*               CVT DSECT=YES                                         * 00195800
*               IHAECVT                                               * 00195900
*               ATRFZQRY                                              * 00196000
*                                                                     * 00196100
*    EXAMPLE 4                                                        * 00196200
*                                                                     * 00196300
*    OPERATION: Obtain Unit Of Recovery information for protected     * 00196400
*    units of recovery (URs).                                         * 00196500
*                                                                     * 00196600
*               ATRQUERY REQUEST=URINFO,AREAADDR=AREA@,AREALEN=RETLEN * 00196700
*      ,        X                                                     * 00196800
*                     COUNT=RETCNT,URTYPE=ATR_PROTECTED               * 00196900
*      *                                                              * 00197000
*      * INPUT VALUES                                                 * 00197100
*      *                                                              * 00197200
*      AREA@    DC     A(RETAREA)               ADDRESS OF RETURN ARE * 00197300
*      A                                                              * 00197400
*      RETLEN   DC     F'1024'                  LENGTH OF RETURN AREA * 00197500
*      *                                                              * 00197600
*      * OUTPUT AREAS                                                 * 00197700
*      *                                                              * 00197800
*      RETCNT   DS     F                        RETURNED RM COUNT     * 00197900
*      RETAREA  DS     1024C                    RETURN AREA           * 00198000
*      *                                                              * 00198100
*      * REQUIRED MAPPINGS                                            * 00198200
*      *                                                              * 00198300
*               CVT DSECT=YES                                         * 00198400
*               IHAECVT                                               * 00198500
*               ATRFZQRY                                              * 00198600
*               ATRRASM                                               * 00198700
*                                                                     * 00198800
*    EXAMPLE 5                                                        * 00198900
*                                                                     * 00199000
*    OPERATION: Obtain Unit Of Recovery information for address space * 00199100
*    52                                                               * 00199200
*                                                                     * 00199300
*               ATRQUERY REQUEST=URINFO,AREAADDR=AREA@,AREALEN=RETLEN * 00199400
*      ,        X                                                     * 00199500
*                     COUNT=RETCNT,ASID=MYASID                        * 00199600
*      *                                                              * 00199700
*      * INPUT VALUES                                                 * 00199800
*      *                                                              * 00199900
*      AREA@    DC     A(RETAREA)               ADDRESS OF RETURN ARE * 00200000
*      A                                                              * 00200100
*      RETLEN   DC     F'1024'                  LENGTH OF RETURN AREA * 00200200
*      MYASID   DC     XL2'52'                  ADDRESS SPACE IDENTIF * 00200300
*      IER                                                            * 00200400
*      *                                                              * 00200500
*      * OUTPUT AREAS                                                 * 00200600
*      *                                                              * 00200700
*      RETCNT   DS     F                        RETURNED RM COUNT     * 00200800
*      RETAREA  DS     1024C                    RETURN AREA           * 00200900
*      *                                                              * 00201000
*      * REQUIRED MAPPINGS                                            * 00201100
*      *                                                              * 00201200
*               CVT DSECT=YES                                         * 00201300
*               IHAECVT                                               * 00201400
*               ATRFZQRY                                              * 00201500
*               ATRRASM                                               * 00201600
*                                                                     * 00201700
*                                                                     * 00201800
*01* MACRO USAGE NOTES:                                               * 00201900
*                                                                     * 00202000
*                                                                     * 00202100
*    1.  Printing of the macro expansion is controlled by the ZPRINT  * 00202200
*        global macro variable.  Any value other than 'NO' will       * 00202300
*        result in the macro expansion being printed.  The default is * 00202400
*        'YES'.  The following examples illustrate how the ZPRINT     * 00202500
*        variable should be set.                                      * 00202600
*                                                                     * 00202700
*                    GBLC &ZPRINT                                     * 00202800
*            &ZPRINT SETC 'YES'          &ZPRINT SETC 'NO'            * 00202900
*                                                                     * 00203000
*                                                                     * 00203100
*    2.  When using the NOCHECK option in conjunction with the Modify * 00203200
*        and/or  Execute  forms,  be  sure  that  it is preceded by a * 00203300
*        Modify   or  Execute  form  invocation  which  specifies  or * 00203400
*        defaults  to  the  COMPLETE  option.   This insures that the * 00203500
*        parameter  list  is  initialized completely.   The parameter * 00203600
*        list  field which identifies the version number of the macro * 00203700
*        is only set when the Standard form or the Complete option of * 00203800
*        the  Execute/Modify  form  is  used,  or  when  PLISTVER  is * 00203900
*        explicitly  specified.  Be sure that the resulting parameter * 00204000
*        list version number covers all the keys that you use.        * 00204100
*                                                                     * 00204200
*                                                                     * 00204300
*    3.  The ATRQUERY macro supports multiple versions. The macro key * 00204400
*        list  below  contains the version level in which the key was * 00204500
*        first  introduced.  Positionals  are  identified by the word * 00204600
*        'Positional'  followed by the numeric value of its position. * 00204700
*        Nested  positionals  are  preceded  by their outer key.  The * 00204800
*        lowest value represents the original version and the highest * 00204900
*        value  represents  the  most  recent version of the ATRQUERY * 00205000
*        macro.    When  specifying  PLISTVER,  be sure that it is at * 00205100
*        least  as high as the highest version number of all the keys * 00205200
*        being used.                                                  * 00205300
*                                                                     * 00205400
*        VERSION   KEY                                                * 00205500
*                                                                     * 00205600
*              1      AREAADDR                                        * 00205700
*                     AREALEN                                         * 00205800
*                     ASID                                            * 00205900
*                     COUNT                                           * 00206000
*                     GTID                                            * 00206100
*                     LUWID                                           * 00206200
*                     METADATA                                        * 00206300
*                     REQUEST                                         * 00206400
*                     RMNAME                                          * 00206500
*                     SYSNAME                                         * 00206600
*                     TID                                             * 00206700
*                     URID                                            * 00206800
*                     URSTATE                                         * 00206900
*                     URTYPE                                          * 00207000
*              2      DEFURONLY                                       * 00207100
*              3      CURDUR                                          * 00207200
*                     EXCLMASK                                        * 00207300
*                     EXINFOMASK                                      * 00207400
*                     GNAME                                           * 00207500
*                     GTIDSTR                                         * 00207600
*                     LUWIDSTR                                        * 00207700
*                     SORTNUM                                         * 00207800
*                     SORTTAB                                         * 00207900
*                     SYSNAME                                         * 00208000
*                     TIDHIGH                                         * 00208100
*                     TIDLOW                                          * 00208200
*                     TODBEG                                          * 00208300
*                     TODEND                                          * 00208400
*                     URIDSTR                                         * 00208500
*                     URSTMASK                                        * 00208600
*                     WMNAME                                          * 00208700
*              4      AREAALET                                        * 00208800
*                     AREAOPT                                         * 00208900
*                     GNAME                                           * 00209000
*                     RCNUM                                           * 00209100
*                     RCTABLE                                         * 00209200
*                     SYSNAME                                         * 00209300
*              5      XID                                             * 00209400
*                     XIDBQUALSTR                                     * 00209500
*                     XIDFORMATID                                     * 00209600
*                     XIDGTRIDSTR                                     * 00209700
*              6      SURID                                           * 00209800
*                     SURIDSTR                                        * 00209900
*                                                                     * 00210000
*                                                                     * 00210100
*                                                                     * 00210200
*01* CHANGE ACTIVITY:                                                 * 00210300
*                                                                     * 00210400
