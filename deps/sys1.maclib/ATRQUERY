.***AE.KCK06 VALUE, OPT INPUT, KEYS -KMG RANGE OR EQUATE                00653500
                 AIF   (K'&ZXURTYPE LE 0).ZE44W  NOT I/O                00653600
&ZCVAL           SETC  '&XURTYPE'                                       00653700
                 AIF   ('&ZCVAL'(1,1) EQ '(').ZE44W  IN GPR             00653800
                 AIF   (T'&ZCVAL EQ 'N').ZE44U  NUMBER                  00653900
&ZCLNG           SETC  '&ZCVAL'(1,K'&ZCVAL-1)   GET PREFIX              00654000
                 AIF   (T'&ZCLNG EQ 'N').ZE44S  NUMERIC                 00654100
&ZCGPR           SETC  '&ZCVAL'(1,1)            FIRST CHAR              00654200
                 AIF   (T'&ZCGPR EQ 'N').ZE44V  NUMERIC ERR             00654300
                 AGO               .ZE44W                               00654400
.*                                                                      00654500
.ZE44S           ANOP  ,                                                00654600
&ZAWRK           SETA  &ZCLNG      CONVERT                              00654700
&ZCGPR           SETC  '&ZCVAL'(K'&ZCVAL,1)     GET FACTOR              00654800
&ZASIZ           SETA  1024                                             00654900
                 AIF   ('&ZCGPR' EQ 'K').ZE44T  K FACTOR                00655000
&ZASIZ           SETA  1024*1024                                        00655100
                 AIF   ('&ZCGPR' EQ 'M').ZE44T  M FACTOR                00655200
&ZASIZ           SETA  1024*1024*1024                                   00655300
                 AIF   ('&ZCGPR' NE 'G').ZE44V  NOT G FACTOR            00655400
                 AIF   (&ZAWRK GT 2).ZE44V      NOT MAX G FACTOR        00655500
.ZE44T           ANOP  ,                                                00655600
&ZCVAL           SETC  '&ZASIZ*&ZAWRK'          EQUIVALENT NUMBER       00655700
.ZE44U           ANOP  ,                                                00655800
&ZASIZ           SETA  &ZCVAL      CONVERT                              00655900
                 AIF   (&ZASIZ LT 0).ZE44V      TOO LOW                 00656000
                 AIF   (&ZASIZ GT 2147483647).ZE44V  TOO HIGH           00656100
