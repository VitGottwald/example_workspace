                 AIF   ('&XURTYPE'(1,1) EQ '(').ZG44V  IN GPR           00402700
                 AIF   ('&XURTYPE'(1,1) EQ '-').ZG44U  INVALID ENTRY    00402800
                 AIF   (T'&XURTYPE NE 'N').ZG44S  NON NUMERIC           00402900
&ZAWRK           SETA  &XURTYPE    INITIALIZE ZAWRK                     00403000
&ZCVAL           SETC  '&XURTYPE'  INITIALIZE ZCVAL                     00403100
&ZASIZ           SETA  1                                                00403200
                 AGO   .ZG44TT                                          00403300
.ZG44S           ANOP ,                                                 00403400
&ZCLNG           SETC  '&XURTYPE'(1,K'&XURTYPE-1)                       00403500
                 AIF   (T'&ZCLNG EQ 'N').ZG44SS  NUMERIC                00403600
&ZCGPR           SETC  '&XURTYPE'(1,1)          FIRST CHAR              00403700
                 AIF   (T'&ZCGPR EQ 'N').ZG44U  NUMERIC ERR             00403800
                 AGO   .ZG44V                                           00403900
.*                                                                      00404000
.ZG44SS          ANOP ,                                                 00404100
&ZAWRK           SETA  &ZCLNG      CONVERT                              00404200
