.***AC.PLV01  PROCESS PLISTVER                                      *   00233900
.********************************************************************   00234000
&XPLISTVER      SETA  1                                                 00234100
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00234200
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00234300
&XPLISTVER      SETA  9                                                 00234400
.ZPLV0          ANOP  ,                                                 00234500
.*                                                                      00234600
                AIF   ('&GNAME' EQ '').ZPLV1E                           00234700
                AIF   (&XPLISTVER GE 4).ZPLV1E                          00234800
&XPLISTVER      SETA  4                                                 00234900
.ZPLV1E         ANOP  ,                                                 00235000
.*                                                                      00235100
                AIF   ('&WMNAME' EQ '').ZPLV2E                          00235200
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00235300
&XPLISTVER      SETA  3                                                 00235400
.ZPLV2E         ANOP  ,                                                 00235500
.*                                                                      00235600
                AIF   ('&GNAME' EQ '').ZPLV3E                           00235700
                AIF   (&XPLISTVER GE 4).ZPLV3E                          00235800
&XPLISTVER      SETA  4                                                 00235900
.ZPLV3E         ANOP  ,                                                 00236000
.*                                                                      00236100
