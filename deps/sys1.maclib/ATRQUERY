                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00893800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00893900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00894000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00894100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00894200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00894300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00894400
&ZOPN                 SETA  &ZOPN+1                                     00894500
&ZOPL(&ZOPN)          SETC  ' '                                         00894600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00894700
