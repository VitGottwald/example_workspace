&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00894900
                      AGO   .ZIMFNX                                     00895000
.*                                                                      00895100
.ZIMFN2               ANOP ,                                            00895200
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00895300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00895400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00895500
