                 AIF   (K'&ZCGPR GT 2).ZE137Q                           00798400
                 AIF   ('&ZCGPR' EQ '0').ZE137Q                         00798500
&ZCGPR           SETC  '&ZCGPR'(2,1)                                    00798600
                 AIF   (T'&ZCGPR NE 'N').ZE137Q  INVALID REG NAME       00798700
.ZE137P          ANOP  ,                                                00798800
                 AIF   (T'&ZCGPR NE 'N').ZE137R  NON NUMERIC            00798900
                 AIF   ('&ZCGPR' LT '2').ZE137Q                         00799000
                 AIF   ('&ZCGPR' LE '12').ZE137R                        00799100
.ZE137Q          ANOP  ,                                                00799200
&ZMS             SETC  '"RSNCODE='                                      00799300
&ZMS             SETC  '&ZMS.&RSNCODE"'                                 00799400
&ZMS             SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'        00799500
                 MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                 00799600
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00799700
.ZE137R          ANOP  ,                                                00799800
.******************************************************************     00799900
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00800000
.******************************************************************     00800100
                 ACTR  10000                                            00800200
.*-------------------------------------------------------------------   00800300
.***AF.P03 EXTRA POSITIONALS AMONG NON-X KEYS                           00800400
&ZCVAL           SETC  ''          ERROR FIELD                          00800500
&ZAWRK           SETA  0           POSITIONAL COUNTER                   00800600
&ZASYSLNDX       SETA  0           NON-X KEY COUNTER                    00800700
.ZELIST2         ANOP  ,                                                00800800
&ZASYSLNDX       SETA  &ZASYSLNDX+1             NEXT NON-X KEY          00800900
         AIF   (&ZASYSLNDX GT N'&SYSLIST).ZELIST3  DONE                 00801000
         AIF   ('&ZCSYSLIST(&ZASYSLNDX)' EQ 'NONXKEY').ZELIST2          00801100
&ZAWRK           SETA  &ZAWRK+1    NEXT POSITIONAL                      00801200
                 AIF   (&ZAWRK LE 0).ZELIST2                            00801300
&ZCVAL           SETC '&ZCVAL,&SYSLIST(&ZASYSLNDX)'  EXTRA              00801400
                 AGO   .ZELIST2                                         00801500
.*                                                                      00801600
.ZELIST3  AIF  (K'&ZCVAL LE 0).ZELISTX                                  00801700
&ZCVAL           SETC '&ZCVAL'(2,K'&ZCVAL-1)    STRIP COMMA             00801800
&ZMS             SETC  'POSITIONAL ARGUMENT(S),'                        00801900
&ZMS             SETC  '&ZMS "&ZCVAL"'                                  00802000
&ZMS             SETC  '&ZMS CANNOT BE USED.'                           00802100
&ZMS             SETC  '&ZMS NO POSITIONAL ARGUMENTS'                   00802200
                 MNOTE 8,' &ZMS ARE ALLOWED.'                           00802300
&ZMACRET         SETA  8           SET MACRO ERROR RETURN CODE          00802400
.ZELISTX         ANOP  ,                                                00802500
&ZASYSLNDX       SETA  0                                                00802600
.*-------------------------------------------------------------------   00802700
.***AE.MF02 MF=(SLME)                                                   00802800
.ZEMFX           ANOP  ,           END OF PARTIAL SYNTAX CHECK          00802900
.*******************************************************************    00803000
.***AE.ZMR01 NO CODE GENERATED IF ERRORS DETECTED                       00803100
.*******************************************************************    00803200
                 AIF   (&ZMACRET NE 0).ZLX2     ERROR DETECTED          00803300
.*******************************************************************    00803400
.***AE.01  START OF ATRQUERY MACRO CODE                                 00803500
.*******************************************************************    00803600
                 LCLC  &M0M0001                 NAME                    00803700
&M0M0001         SETC  '&XLABEL'                SET NAME                00803800
                 AIF   (K'&XLABEL NE 0).XLAB2                           00803900
&M0M0001         SETC  'M00M&SYSNDX'            DEFAULT NAME            00804000
.*                                                                      00804100
.XLAB2           ANOP  ,                                                00804200
                 AIF   ('&XMF' EQ 'L').XLAB3                            00804300
&M0M0001 DS    0H                             ATRQUERY-9                00804400
.XLAB3           ANOP  ,                                                00804500
.*******************************************************************    00804600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00804700
.*******************************************************************    00804800
              PUSH  PRINT                                               00804900
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00805000
                 PRINT OFF         ++ PR01                              00805100
                 AGO   .ZPRNTXX                                         00805200
.ZPRINTX         PRINT ON                                               00805300
.ZPRNTXX         ANOP  ,                                                00805400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00805500
              AIF   ('&TID' NE '').QRYA                                 00805600
              AGO   .QRYD INVOCATION OKAY                               00805700
.QRYA         ANOP  ,                                                   00805800
              AIF   ('&TIDHIGH' NE '' OR '&TIDLOW' NE '').QRYC          00805900
              AGO   .QRYD INVOCATION OKAY                               00806000
.QRYC         ANOP  ,                                                   00806100
&ZMS          SETC  '"TID" MAY NOT BE SPECIFIED '                       00806200
&ZMS          SETC  '&ZMS IF EITHER "TIDHIGH" OR "TIDLOW" '             00806300
              MNOTE 8,'&ZMS IS SPECIFIED'                               00806400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00806500
.QRYD         ANOP  ,                                                   00806600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00806700
