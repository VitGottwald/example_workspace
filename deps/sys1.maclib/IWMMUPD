.***AC.PLV01  PROCESS PLISTVER                                      *   00061800
.********************************************************************   00061900
&XPLISTVER      SETA  0                                                 00062000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00062100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00062200
&XPLISTVER      SETA  0                                                 00062300
.ZPLV0          ANOP  ,                                                 00062400
.*                                                                      00062500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00062600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00062700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00062800
.*-------------------------------------------------------------------   00062900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00063000
&ZMS            SETC  '"PLISTVER='                                      00063100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00063300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00063400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063500
                AGO   .ZPVCK2                                           00063600
.ZPVCK3         ANOP  ,                                                 00063700
.*-------------------------------------------------------------------   00063800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00063900
&ZMS            SETC  '"PLISTVER='                                      00064000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00064100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00064200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00064300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064400
                AGO   .ZPVCKE                                           00064500
.ZPVCK2         ANOP  ,                                                 00064600
.*-------------------------------------------------------------------   00064700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00064800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00064900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00065000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00065100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00065200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00065300
.ZPVCK6         ANOP  ,                                                 00065400
&ZMS            SETC  '"PLISTVER='                                      00065500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00065700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00065800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065900
&XPLISTVER      SETA  0                                                 00066000
                AGO   .ZPVCKE                                           00066100
.*-------------------------------------------------------------------   00066200
.ZPVCK4A        ANOP  ,                                                 00066300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00066400
&ZMS            SETC  'PLISTVER='                                       00066500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00066600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00066700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00066800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00066900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067000
                AGO   .ZPVCK5                                           00067100
.ZPVCK4         ANOP  ,                                                 00067200
&XPLISTVER      SETA  &PLISTVER(1)                                      00067300
.ZPVCK5         ANOP  ,                                                 00067400
.ZPVCKE         ANOP  ,                                                 00067500
.********************************************************************   00067600
