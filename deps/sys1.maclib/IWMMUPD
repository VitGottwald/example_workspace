         ICM   14,15,&T1               FETCH MONITORING TOKEN           00167000
         BNZ   MONENV&SYSNDX           BRANCH WHEN MON ENV IS PRESENT   00167100
         LA    0,IWMRSNCODENOMONENV    SET REASON CODE = NO MON ENV     00167200
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00167300
         B     END&SYSNDX              BRANCH TO END LABEL              00167400
MONENV&SYSNDX DS    0H                 BRANCH LABEL FOR MON ENV PRESENT 00167500
         CLC   PB_ID-PB(4,14),=CL4'PB  '                                00167600
         BE    MONENVOK&SYSNDX         BRANCH WHEN MON ENV PASSES       00167700
         LA    0,IWMRSNCODEBADMONENV   SET REASON CODE = BAD MON ENV    00167800
         LA    15,IWMRETCODEINVOCERROR SET INVOCATION ERROR RETURN CODE 00167900
         B     END&SYSNDX              BRANCH TO END LABEL              00168000
MONENVOK&SYSNDX DS    0H               BRANCH LABEL FOR MON ENV OK      00168100
         CLI   PB_VERSION-PB(14),PB_VERSION6                            00168200
         BH    VERSOK&SYSNDX           BRANCH WHEN VERSION IS GREATER   00168300
         SLR   0,0                     SET REASON CODE = 0              00168400
         SLR   15,15                   SET RETURN CODE = 0              00168500
         B     END&SYSNDX              BRANCH TO END LABEL              00168600
VERSOK&SYSNDX DS    0H                 BRANCH LABEL FOR VERSION OK      00168700
         ST    1,PB_EWLM_PARMLIST-PB(,14)    STORE ADDR OF PARMLIST     00168800
         MVI   PB_EWLM_REQUEST-PB(14),PB_EWLM_REQUEST_IWMMUPD           00168900
         LR    1,14                    LOAD ADDR OF PB INTO R01         00169000
*        USING PB,14                                                    00169100
         L     15,X'10'                ADDRESS THE CVT                  00169200
