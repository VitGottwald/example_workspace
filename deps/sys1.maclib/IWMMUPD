                      LCLA  &ZOPN1         INVOCATION INDEX             00123500
                      LCLC  &ZOPL(4095)    LABEL                        00123600
                      LCLC  &ZOPO(4095)    OPERATION                    00123700
                      LCLC  &ZOPV(4095)    VALUE                        00123800
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00123900
.*                                                                      00124000
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00124100
.******************************************************************     00124200
.***AL.L10 IWMMUPD FMT=LIST MF=S!L PARAMETERS                           00124300
.******************************************************************     00124400
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00124500
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00124600
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00124700
&XMFATTR              SETC  '0D'                                        00124800
.*                                                                      00124900
&ZOPN                 SETA  &ZOPN+1                                     00125000
&ZOPL(&ZOPN)          SETC  ' '                                         00125100
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00125200
