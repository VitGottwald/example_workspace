.***AO.X02  RETURN CODE PROCESSING                                      01138900
.*******************************************************************    01139000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    01139100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            01139200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             01139300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             01139400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               01139500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                01139600
&ZOPN                 SETA  &ZOPN+1                                     01139700
&ZOPL(&ZOPN)          SETC  ' '                                         01139800
&ZOPO(&ZOPN)          SETC  'ST'                                        01139900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              01140000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        01140100
                      AGO   .ZMRETX                                     01140200
.*                                                                      01140300
.ZMRET2               ANOP  ,                                           01140400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                01140500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                01140600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                01140700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  01140800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   01140900
