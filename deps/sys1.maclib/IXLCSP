.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               01145500
.*******************************************************************    01145600
                      ACTR  10000                                       01145700
.*                                                                      01145800
                      LCLA  &ZLC           COLUMN POSITION              01145900
                      LCLA  &ZOPX          LIST TABLE INDEX             01146000
                      LCLC  &ZLL           LABEL                        01146100
                      LCLC  &ZLOP          OPERATION                    01146200
                      LCLC  &ZLV           VALUE                        01146300
                      LCLC  &ZLD           DESCRIPTION                  01146400
.*                                                                      01146500
&ZOPX                 SETA  1                                           01146600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            01146700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              01146800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              01146900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              01147000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              01147100
                      AIF   (K'&ZLD LE 0).ZL5                           01147200
&ZLC                  SETA  35             COMMENT COLUMN               01147300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    01147400
&ZLC                  SETA  33             COMMMENT COLUMN              01147500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   01147600
&ZLV                  SETC  '&ZLV'.' '                                  01147700
                      AGO   .ZL3                                        01147800
.*                                                                      01147900
.ZL4                  ANOP  ,                                           01148000
&ZLV                  SETC  '&ZLV'.' '                                  01148100
.ZL5                  ANOP  ,                                           01148200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               01148300
.*                                                                      01148400
&ZLL     &ZLOP &ZLV                                                     01148500
.*                                                                      01148600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               01148700
                      AGO   .ZL2           CONTINUE LOOPING             01148800
.*                                                                      01148900
.ZLX                  ANOP  ,                                           01149000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                01149100
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                01149200
