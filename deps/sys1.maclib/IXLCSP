.******************************************************************     01008900
.***AI.L01 IXLCSP FMT=LIST FILL IN PARAMETER AREA                       01009000
.******************************************************************     01009100
                      ACTR  10000                                       01009200
&ZCVAL                SETC  'LA'           ASSUME XA MODE               01009300
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    01009400
&ZCVAL                SETC  'LAE'          XAX MODE                     01009500
.ZIMFXA               ANOP ,                                            01009600
.*                                                                      01009700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    01009800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         01009900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 01010000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                01010100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 01010200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                01010300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   01010400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    01010500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   01010600
&ZOPN                 SETA  &ZOPN+1                                     01010700
&ZOPL(&ZOPN)          SETC  ' '                                         01010800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01010900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              01011000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01011100
                      AGO   .ZIMFNX                                     01011200
.*                                                                      01011300
.ZIMFN2               ANOP ,                                            01011400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     01011500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     01011600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  01011700
&ZOPN                 SETA  &ZOPN+1                                     01011800
&ZOPL(&ZOPN)          SETC  ' '                                         01011900
&ZOPO(&ZOPN)          SETC  'LAE'                                       01012000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  01012100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01012200
                      AGO   .ZIMFNX                                     01012300
.ZIMFNX1              ANOP ,                                            01012400
&ZOPN                 SETA  &ZOPN+1                                     01012500
&ZOPL(&ZOPN)          SETC  ' '                                         01012600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    01012700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                01012800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       01012900
.ZIMFNX               ANOP ,                                            01013000
.********************************************************************   01013100
