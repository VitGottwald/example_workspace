.*******************************************************************    00277100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00277200
.*******************************************************************    00277300
                      ACTR  10000                                       00277400
.*                                                                      00277500
                      LCLA  &ZLC           COLUMN POSITION              00277600
                      LCLA  &ZOPX          LIST TABLE INDEX             00277700
                      LCLC  &ZLL           LABEL                        00277800
                      LCLC  &ZLOP          OPERATION                    00277900
                      LCLC  &ZLV           VALUE                        00278000
                      LCLC  &ZLD           DESCRIPTION                  00278100
.*                                                                      00278200
&ZOPX                 SETA  1                                           00278300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00278400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00278500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00278600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00278700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00278800
                      AIF   (K'&ZLD LE 0).ZL5                           00278900
&ZLC                  SETA  35             COMMENT COLUMN               00279000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00279100
&ZLC                  SETA  33             COMMMENT COLUMN              00279200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00279300
&ZLV                  SETC  '&ZLV'.' '                                  00279400
                      AGO   .ZL3                                        00279500
.*                                                                      00279600
.ZL4                  ANOP  ,                                           00279700
&ZLV                  SETC  '&ZLV'.' '                                  00279800
.ZL5                  ANOP  ,                                           00279900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00280000
.*                                                                      00280100
&ZLL     &ZLOP &ZLV                                                     00280200
.*                                                                      00280300
