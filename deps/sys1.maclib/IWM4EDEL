                      AGO   .ZL2           CONTINUE LOOPING             00280500
.*                                                                      00280600
.ZLX                  ANOP  ,                                           00280700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00280800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00280900
         AIF   ('&SYSAM64' NE 'YES').AMODE31                            00281000
         LLGTR 14,14                   CLEAR HIGH HALF IN 64BIT MODE    00281100
.AMODE31 L     14,16(0,0)              GET CVT ADDRESS                  00281200
.*-------------------------------------------------------------------   00281300
         TM    CVTOSLV3-CVT(14),CVTH7709  test for HBB7709 presence     00281400
