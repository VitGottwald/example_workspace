                      AGO   .ZL2           CONTINUE LOOPING             00183700
.*                                                                      00183800
.ZLX                  ANOP  ,                                           00183900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00184000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00184100
         AIF   ('&SYSAM64' NE 'YES').AMODE31                            00184200
         LLGTR 14,14                   CLEAR HIGH HALF IN 64BIT MODE    00184300
.AMODE31 L     14,16(0,0)              GET CVT ADDRESS                  00184400
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00184500
         LAE   14,0(14,0)              CLEAR AR 14                      00184600
.NOAR    ANOP                                                           00184700
         L     14,772(14,0)            GET SFT ADDRESS                  00184800
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00184900
         LA    15,123                  GET EX FOR SERVICE ROUTINE  @P1C 00185000
         OR    14,15                   GET PC NUMBER                    00185100
         PC    0(14)                                                    00185200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00185300
