.***AC.PLV01  PROCESS PLISTVER                                      *   00064200
.********************************************************************   00064300
&XPLISTVER      SETA  0                                                 00064400
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00064500
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00064600
&XPLISTVER      SETA  0                                                 00064700
.ZPLV0          ANOP  ,                                                 00064800
.*                                                                      00064900
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00065000
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00065100
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00065200
.*-------------------------------------------------------------------   00065300
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00065400
&ZMS            SETC  '"PLISTVER='                                      00065500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065600
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00065700
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00065800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065900
                AGO   .ZPVCK2                                           00066000
.ZPVCK3         ANOP  ,                                                 00066100
.*-------------------------------------------------------------------   00066200
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00066300
&ZMS            SETC  '"PLISTVER='                                      00066400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066500
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00066600
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00066700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066800
                AGO   .ZPVCKE                                           00066900
.ZPVCK2         ANOP  ,                                                 00067000
.*-------------------------------------------------------------------   00067100
&ZCVAL          SETC  '&PLISTVER(1)'                                    00067200
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00067300
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00067400
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00067500
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00067600
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00067700
.ZPVCK6         ANOP  ,                                                 00067800
&ZMS            SETC  '"PLISTVER='                                      00067900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068000
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00068100
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00068200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068300
&XPLISTVER      SETA  0                                                 00068400
                AGO   .ZPVCKE                                           00068500
.*-------------------------------------------------------------------   00068600
.ZPVCK4A        ANOP  ,                                                 00068700
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00068800
&ZMS            SETC  'PLISTVER='                                       00068900
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00069000
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00069100
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00069200
                MNOTE 8,' &ZMS IS REQUIRED.'                            00069300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069400
                AGO   .ZPVCK5                                           00069500
.ZPVCK4         ANOP  ,                                                 00069600
&XPLISTVER      SETA  &PLISTVER(1)                                      00069700
.ZPVCK5         ANOP  ,                                                 00069800
.ZPVCKE         ANOP  ,                                                 00069900
