.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00180400
.*******************************************************************    00180500
                      ACTR  10000                                       00180600
.*                                                                      00180700
                      LCLA  &ZLC           COLUMN POSITION              00180800
                      LCLA  &ZOPX          LIST TABLE INDEX             00180900
                      LCLC  &ZLL           LABEL                        00181000
                      LCLC  &ZLOP          OPERATION                    00181100
                      LCLC  &ZLV           VALUE                        00181200
                      LCLC  &ZLD           DESCRIPTION                  00181300
.*                                                                      00181400
&ZOPX                 SETA  1                                           00181500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00181600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00181700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00181800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00181900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00182000
                      AIF   (K'&ZLD LE 0).ZL5                           00182100
&ZLC                  SETA  35             COMMENT COLUMN               00182200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00182300
&ZLC                  SETA  33             COMMMENT COLUMN              00182400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00182500
&ZLV                  SETC  '&ZLV'.' '                                  00182600
                      AGO   .ZL3                                        00182700
.*                                                                      00182800
.ZL4                  ANOP  ,                                           00182900
