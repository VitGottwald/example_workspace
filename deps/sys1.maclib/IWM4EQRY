               ACTR  10000                                              00126000
.*----------------------------------------------------------------***   00126100
.***AH.P02 EXTRA POSITIONALS                                            00126200
               AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS               00126300
               AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL               00126400
               AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL           00126500
               AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL            00126600
.ZGLISTX1      ANOP  ,                                                  00126700
&ZCVAL         SETC  ''          ERROR FIELD                            00126800
&ZAWRK         SETA  0           POSITIONAL NUMBER                      00126900
.ZGLIST2       ANOP  ,                                                  00127000
&ZAWRK         SETA  &ZAWRK+1    NEXT POSITIONAL                        00127100
               AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE               00127200
&ZCVAL         SETC '&ZCVAL,&SYSLIST(&ZAWRK)'   EXTRA                   00127300
               AGO   .ZGLIST2                                           00127400
.*                                                                      00127500
.ZGLIST3       ANOP  ,                                                  00127600
&ZCVAL         SETC '&ZCVAL'(2,K'&ZCVAL-1)      STRIP COMMA             00127700
&ZMS           SETC  'POSITIONAL ARGUMENT(S),'                          00127800
&ZMS           SETC  '&ZMS "&ZCVAL"'                                    00127900
&ZMS           SETC  '&ZMS CANNOT BE USED.'                             00128000
&ZMS           SETC  '&ZMS NO POSITIONAL ARGUMENTS'                     00128100
               MNOTE 8,' &ZMS ARE ALLOWED.'                             00128200
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00128300
