*%IF ENFPDEF = '' %THEN                                                         
*  %ENFPDEF = 'BDY(WORD)';       /* DEFAULT THE PARM LIST TO WORD    */         
*  DECLARE                                                                      
*  1 ENFPM  ENFPDEF,                                                            
*    2 ENFPLEN  FIXED(15),       /* LENGTH OF ENF PARAMETER LIST     */         
*    2 ENFPACT  FIXED(15),       /* REQUESTED ENF ACTION             */         
*      3 ENFPACT1 FIXED(8),      /* RESERVED                         */         
*      3 ENFPACT2 FIXED(8),      /* DEFINITION BYTE                  */         
*    2 ENFPCODE CHAR(4),         /* HEXADECIMAL EVENT CODE                      
*                                   (RIGHT JUSTIFIED)                */         
*    2 ENFPFLG  BIT(8),          /* FLAG FIELD                       */         
*      3 ENFPASN  BIT(1),        /* ASYNCHRONOUS REQUEST             */         
*      3 ENFPDISA BIT(1),        /* Disable keyword              @L9A*/         
*      3 ENFPDISO BIT(1),        /* Disable keyword operational  @L9A*/         
*      3 ENFPXSYS BIT(1),        /* Signal request - set if signal              
*                                   is to be sent to other systems.             
*                                   Listen request - set if                     
*                                   listener will accept signals                
*                                   from foreign systems.        @LMC*/         
*      3 ENFPFREE BIT(1),        /* FREE SIGNALLER PARM LIST     @D1A*/         
*      3 ENFPEOT  BIT(1),        /* EOT support requested        @LBA*/         
*      3 ENFPEOM  BIT(1),        /* EOM support requested        @LBA*/         
*      3 ENFPSRBE BIT(1),        /* SRBEXIT specified            @LBA*/         
*    2 ENFPQMSK BIT(8),          /* MASK FOR COMPARING QUALIFIERS    */         
*      3 *        BIT(4),        /* RESERVED                         */         
*      3 ENFPQMS1 BIT(1),        /* COMPARE CHARACTER 1              */         
*      3 ENFPQMS2 BIT(1),        /* COMPARE CHARACTER 2              */         
*      3 ENFPQMS3 BIT(1),        /* COMPARE CHARACTER 3              */         
*      3 ENFPQMS4 BIT(1),        /* COMPARE CHARACTER 4              */         
*    2 ENFPFATT FIXED(16),       /* FREEMAIN ATRRIBUTES          @D1C*/         
*      3 ENFPFKEY FIXED(8),      /* KEY OF AREA TO BE FREED      @D1A*/         
*      3 ENFPFSPL FIXED(8),      /* SUBPOOL OF AREA              @D1A*/         
*    2 ENFPQUAL CHAR(4),         /* QUALIFIER                        */         
*    2 ENFPEADR PTR(31),         /* LISTEN : Listener's exit routine.           
*                                   If ENFPSRBE is on then the listen           
*                                   exit will run in the listener's             
*                                   address space as SRB.                       
*                                   SIGNAL : SIGNALER'S EXIT ROUTINE            
*                                                                @LBC*/         
*      3 * BIT(1),               /* AMODE OF EXIT ROUTINE    @G860P42*/         
*      3 * BIT(31),              /* ADDRESS OF EXIT ROUTINE  @G860P42*/         
*    2 ENFPSPRM PTR(31),         /* Address of signaller's or                   
*                                   listener's parameters        @LBC*/         
*    2 ENFPTOK  CHAR(4),         /* LISTEN: TOKEN OF LISTENER'S                 
*                                        ELEMENT.                               
*                                   SIGNAL: ADDRESS OF ORIGINAL                 
*                                        EPL.                        */         
*    2 ENFPFLEN FIXED(31),       /* LENGTH OF FREEPRM AREA       @D3C*/         
*    2 ENFPVERS FIXED(15),       /* PARAMETER LIST VERSION       @D3A*/         
*    2 ENFPHASN FIXED(15),       /* HASN of caller of ENF        @PDA*/         
*    2 ENFPR14C PTR(31),         /* ADDRESS OF CALLER OF ENF     @D3A*/         
*    2 ENFPLNME CHAR(8),         /* NAME OF ESTABLISHER OF LISTEN               
*                                   EXIT                         @D3A*/         
*    2 ENFPXNME CHAR(8),         /* NAME OF LISTEN EXIT ROUTINE  @L6C*/         
*    2 ENFPLSNM FIXED(31),       /* LISTENER COUNT (RETURNED)                   
*                                   ONLY FOR SYNCHRONOUS REQUESTS               
*                                                                @L7C*/         
*    2 ENFPCRET CHAR(4),            /* SPECIAL PROCESSING CODE RETURNED         
*                                      FROM EXIT                 @L7A*/         
*    2 ENFPBMQ  CHAR(32),        /* Bit-mapped qualifier         @LMA*/         
*    2 ENFPFLG2 BIT(8),          /* Additional flags             @LMA*/         
*      3 ENFPBCMP BIT(3),        /* Bit-string used to indicate                 
*                                   type of comparison to be                    
*                                   performed on bit-mapped                     
*                                   qualifier                    @LMA*/         
*      3 ENFPMASE BIT(1),        /* MASEXIT support requested    @P4A*/         
*      3 ENFPFTCH BIT(1),        /* IBM inrnl use                @04C*/         
*      3 *        BIT(3),        /* Reserved                     @03C*/         
*    2 *        CHAR(3),         /* Reserved                     @LUC*/         
*    2 ENFPFLTR PTR(31),         /* Address of the FLTRBLK or                   
*                                   the FLTRXIT, for ACTION of                  
*                                   LISTEN or REGISTER                          
*                                   respectively.                @05C*/         
*%IF ENFPMVER = '4' %THEN %GOTO ENFPMV4;                      /* @06A*/         
*%IF ENFPMVER = 'MAX' %THEN %GOTO ENFPMV4;                    /* @06A*/         
*%GOTO ENFPMX;                                                /* @06A*/         
*%ENFPMV4:;                                                   /* @06A*/         
*    2 ENFPV4   CHAR(24),        /* Start of version 4 fields.                  
*                                   To use these fields, ENFPVER                
*                                   must be at least 4.          @05A*/         
*      3 ENFPXCFG CHAR(8),       /* XCFFILTERGROUP Name or null  @05A*/         
*      3 *        CHAR(16),      /* Reserved for future use      @06C*/         
*%ENFPMX:;                                                    /* @06A*/         
*/********************************************************************/         
*/*                                                                  */         
*/*  Important note: Prior to expanding the parameter list,          */         
*/*                  refer to "Cautionary notes", in IEFENFRP.       */         
*/*                                                              @06A*/         
*/********************************************************************/         
*    2 *        CHAR(0);         /*                              @06C*/         
*%IF ENFPMVER = '4' %THEN %GOTO ENFV4;                        /* @06A*/         
*%IF ENFPMVER = 'MAX' %THEN %GOTO ENFV4;                      /* @06A*/         
*DCL ENFPVRSN FIXED(15) CONSTANT(3); /* Version 3                @06A*/         
*%GOTO ENFVX;                                                 /* @06A*/         
*%ENFV4:;                        /*                              @06A*/         
*DCL ENFPVRSN FIXED(15) CONSTANT(4); /* Version 4                @06C*/         
*%ENFVX:;                                                     /* @06A*/         
*DCL EXITRTN ENTRY BASED(ENFPEADR) VALUERANGE(*) OPTIONS(AMODE(*)); /*          
*                                       FOR USER                 @L1C*/         
*DCL ENFPBQSB BIT(3) CONSTANT('000'B);  /* Value of ENFPBCMP                    
*                                   indicating BITCOMPARE=SUBSET @LMA*/         
*DCL ENFPBQIN BIT(3) CONSTANT('001'B);  /* Value of ENFPBCMP                    
*                                   indicating                                  
*                                   BITCOMPARE=INTERSECT         @LMA*/         
*DCL ENFPBQEQ BIT(3) CONSTANT('010'B);  /* Value of ENFPBCMP                    
*                                   indicating BITCOMPARE=EQUAL  @LMA*/         
