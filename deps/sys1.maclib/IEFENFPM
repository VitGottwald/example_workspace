*  %GOTO ENFPMDCL;                                                   /*         
         AIF   ('&DSECT' EQ 'NO').NODSECT                                       
         DSECT                                                                  
.NODSECT ANOP                                                                   
ENFPM    DS    0F                                                               
ENFPLEN  DS    H                   LENGTH OF ENF PARAMETER LIST                 
ENFPACT  DS    0H                  REQUESTED ENF ACTION                         
ENFPACT1 DS    CL1                 RESERVED                                     
ENFPACT2 DS    CL1                 DEFINITION BYTE                              
ENFPSIG  EQU   X'01'               SIGNAL AN EVENT                              
ENFPLIS  EQU   X'02'               LISTEN FOR AN EVENT                          
ENFPDEL  EQU   X'03'               DELETE A LISTENER                            
ENFPQRY  EQU   X'04'               QUERY FOR INFORMATION           @D4A         
ENFPRGR  EQU   X'05'               REGISTER FOR INFORMATION        @LUA         
ENFPCODE DS    XL4                 EVENT CODE (RIGHT JUSTIFIED)                 
ENFPC001 EQU   1                   VARY DEVICE ONLINE                           
ENFPC002 EQU   2                   VARY DEVICE OFFLINE                          
ENFPC003 EQU   3                   VOLUME UNLOAD                                
ENFPC004 EQU   4                   FREE SQA                                     
ENFPC005 EQU   5                   COMM TASK AND TOD INIT      @G38XP2Q         
*                                  COMPLETE                    @G38XP2Q         
ENFPC006 EQU   6                   SRM - STATUS CHANGE IN      @G860P2J         
*                                  CHANNEL MEASURMENT BLOCK    @G860P2J         
*                                  DATA COLLECTOR              @G860P2J         
ENFPC007 EQU   7                   RESERVED                    @G860P2J         
ENFPC008 EQU   8                   PATH STATE CHANGE           @G860P2J         
ENFPC009 EQU   9                   CHANNEL PATH STATE CHANGE   @G860P2J         
ENFPC010 EQU   10                  DDR SWAP                    @G860P2J         
ENFPC011 EQU   11                  FAILURE OF CHANNEL          @G860P2J         
*                                  MONITORING FACILITY         @G860P2J         
ENFPC012 EQU   12                  DEVICE PENDING OFFLINE          @D2A         
ENFPC013 EQU   13                  WTO BUFFER UTILIZATION          @L1A         
ENFPC014 EQU   14                  JES3 BUFFER UTILIZATION         @L1A         
ENFPC015 EQU   15                  STORAGE MANAGEMENT SUBSYSTEM    @L2A         
*                                  RESOURCE AVAILABILITY CHANGE    @L2A         
ENFPC016 EQU   16                  VOLUME BECOMING AVAILABLE       @L2A         
ENFPC017 EQU   17                  CONSOLE OR TSO OPERATOR HAS     @L3A         
*                                  CHANGED FROM THE ACTIVE TO      @L3A         
*                                  INACTIVE STATE                  @L3A         
ENFPC018 EQU   18                  RECONFIGURATION ISSUED TO       @T2A         
*                                  NOTIFY Crypto WHEN A CPU        @T2A         
*                                  COMES ONLINE OR GOES OFFLINE.   @T2A         
ENFPC019 EQU   19                  SIGNALS CRYPTOGRAPHY FEATURE    @T2A         
*                                  AVAILABLE                       @T2A         
ENFPC020 EQU   20                  UNSOLICITED SIGNAL INTERRUPT    @L5A         
ENFPC021 EQU   21                  CHANGE TO DASD MIH INTERVAL     @L5A         
ENFPC022 EQU   22                  SIGNALS CRYPTOGRAPHY FEATURE    @T2A         
*                                  OFFLINE                         @T2A         
ENFPC023 EQU   23                  VARY DEVICE ONLINE              @L7A         
*                                  (DYNAMIC I/O)                   @L7A         
ENFPC024 EQU   24                  VARY DEVICE OFFLINE             @L7A         
*                                  (DYNAMIC I/O)                   @L7A         
