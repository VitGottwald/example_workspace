.*******************************************************************    00154100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00154200
.*******************************************************************    00154300
                      ACTR  10000                                       00154400
.*                                                                      00154500
                      LCLA  &ZLC           COLUMN POSITION              00154600
                      LCLA  &ZOPX          LIST TABLE INDEX             00154700
                      LCLC  &ZLL           LABEL                        00154800
                      LCLC  &ZLOP          OPERATION                    00154900
                      LCLC  &ZLV           VALUE                        00155000
                      LCLC  &ZLD           DESCRIPTION                  00155100
.*                                                                      00155200
&ZOPX                 SETA  1                                           00155300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00155400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00155500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00155600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00155700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00155800
                      AIF   (K'&ZLD LE 0).ZL5                           00155900
&ZLC                  SETA  35             COMMENT COLUMN               00156000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00156100
&ZLC                  SETA  33             COMMMENT COLUMN              00156200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00156300
&ZLV                  SETC  '&ZLV'.' '                                  00156400
                      AGO   .ZL3                                        00156500
.*                                                                      00156600
.ZL4                  ANOP  ,                                           00156700
&ZLV                  SETC  '&ZLV'.' '                                  00156800
.ZL5                  ANOP  ,                                           00156900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00157000
.*                                                                      00157100
&ZLL     &ZLOP &ZLV                                                     00157200
.*                                                                      00157300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00157400
                      AGO   .ZL2           CONTINUE LOOPING             00157500
.*                                                                      00157600
.ZLX                  ANOP  ,                                           00157700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00157800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00157900
         L     14,16(0,0)              GET CVT ADDRESS                  00158000
         L     14,772(14,0)            GET SFT ADDRESS                  00158100
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00158200
         LA    15,35                   GET EX FOR SERVICE ROUTINE       00158300
         OR    14,15                   GET PC NUMBER                    00158400
         PC    0(14)                                                    00158500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00158600
.ZLINKX               ANOP  ,                                           00158700
.*******************************************************************    00158800
.***AP.X01  FORMAT GENERATED CODE                                       00158900
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00159000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00159100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00159200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00159300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159400
                      AIF   (K'&ZLD LE 0).ZL51                          00159500
&ZLC                  SETA  35             COMMENT COLUMN               00159600
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00159700
&ZLC                  SETA  33             COMMENT COLUMN               00159800
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00159900
&ZLV                  SETC  '&ZLV'.' '                                  00160000
                      AGO   .ZL31                                       00160100
.*                                                                      00160200
.ZL41                 ANOP  ,                                           00160300
&ZLV                  SETC  '&ZLV'.' '                                  00160400
.ZL51                 ANOP  ,                                           00160500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00160600
.*                                                                      00160700
&ZLL     &ZLOP &ZLV                                                     00160800
.*                                                                      00160900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00161000
                      AGO   .ZL21          CONTINUE LOOPING             00161100
.*                                                                      00161200
.ZLX1                 ANOP  ,                                           00161300
