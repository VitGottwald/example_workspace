.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00122800
.********************************************************************   00122900
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00123000
                      LCLA  &ZOPN1         INVOCATION INDEX             00123100
                      LCLC  &ZOPL(4095)    LABEL                        00123200
                      LCLC  &ZOPO(4095)    OPERATION                    00123300
                      LCLC  &ZOPV(4095)    VALUE                        00123400
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00123500
.*                                                                      00123600
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00123700
.******************************************************************     00123800
.***AL.L10 IWMWMCON FMT=LIST MF=S!L PARAMETERS                          00123900
.******************************************************************     00124000
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00124100
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00124200
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00124300
&XMFATTR              SETC  '0D'                                        00124400
.*                                                                      00124500
&ZOPN                 SETA  &ZOPN+1                                     00124600
&ZOPL(&ZOPN)          SETC  ' '                                         00124700
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00124800
&ZOPO(&ZOPN)          SETC  'B'                                         00124900
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00125000
                      AGO   .ZALVL6B                                    00125100
.ZALVL6A              ANOP                                              00125200
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00125300
&ZOPV(&ZOPN)          SETC  '4,8'                                       00125400
&ZOPN                 SETA  &ZOPN+1                                     00125500
&ZOPL(&ZOPN)          SETC  ' '                                         00125600
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00125700
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00125800
.ZALVL6B              ANOP                                              00125900
&ZOPD(&ZOPN)          SETC  '++ IWMWMCON ARG PARMS'                     00126000
.*                                                                      00126100
&ZOPN                 SETA  &ZOPN+1                                     00126200
&ZOPL(&ZOPN)          SETC  ' '                                         00126300
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00126400
&ZOPV(&ZOPN)          SETC  '1'                                         00126500
&ZOPD(&ZOPN)          SETC  ' '                                         00126600
.*                                                                      00126700
.ZLMFSL2              ANOP  ,                                           00126800
.*-------------------------------------------------------------------   00126900
.***AM.L00  IWMWMCON FMT=LIST name                                      00127000
