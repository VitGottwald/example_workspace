.*******************************************************************    00147500
.***AO.X02  RETURN CODE PROCESSING                                      00147600
.*******************************************************************    00147700
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00147800
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00147900
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00148000
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00148100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00148200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00148300
&ZOPN                 SETA  &ZOPN+1                                     00148400
&ZOPL(&ZOPN)          SETC  ' '                                         00148500
&ZOPO(&ZOPN)          SETC  'ST'                                        00148600
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00148700
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00148800
                      AGO   .ZMRETX                                     00148900
.*                                                                      00149000
.ZMRET2               ANOP  ,                                           00149100
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00149200
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00149300
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00149400
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00149500
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00149600
&ZOPN                 SETA  &ZOPN+1                                     00149700
&ZOPL(&ZOPN)          SETC  ' '                                         00149800
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00149900
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00150000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00150100
.ZMRETX               ANOP  ,                                           00150200
.*******************************************************************    00150300
.***AO.X03  REASON CODE PROCESSING                                      00150400
.*******************************************************************    00150500
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00150600
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00150700
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00150800
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00150900
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00151000
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00151100
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00151200
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00151300
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00151400
&ZOPN                 SETA  &ZOPN+1                                     00151500
&ZOPL(&ZOPN)          SETC  ' '                                         00151600
&ZOPO(&ZOPN)          SETC  'ST'                                        00151700
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00151800
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00151900
                      AGO   .ZMRSNX                                     00152000
.*                                                                      00152100
.ZMRSN2               ANOP  ,                                           00152200
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00152300
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00152400
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00152500
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00152600
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00152700
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00152800
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00152900
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00153000
&ZOPN                 SETA  &ZOPN+1                                     00153100
&ZOPL(&ZOPN)          SETC  ' '                                         00153200
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00153300
