.*******************************************************************    00104000
.***AE.01  START OF IWMSTEND MACRO CODE                                 00104100
.*******************************************************************    00104200
              LCLC  &M0M0001                 NAME                       00104300
&M0M0001      SETC  '&XLABEL'                SET NAME                   00104400
              AIF   (K'&XLABEL NE 0).XLAB2                              00104500
&M0M0001      SETC  'M00M&SYSNDX'            DEFAULT NAME               00104600
.*                                                                      00104700
.XLAB2        ANOP  ,                                                   00104800
              AIF   ('&XMF' EQ 'L').XLAB3                               00104900
&M0M0001 DS    0H                             IWMSTEND-0                00105000
.XLAB3        ANOP  ,                                                   00105100
.*******************************************************************    00105200
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00105300
.*******************************************************************    00105400
              PUSH  PRINT                                               00105500
              AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON          00105600
              PRINT OFF         ++ PR01                                 00105700
              AGO   .ZPRNTXX                                            00105800
.ZPRINTX      PRINT ON                                                  00105900
.ZPRNTXX      ANOP  ,                                                   00106000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00106100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00106200
.********************************************************************   00106300
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00106400
.********************************************************************   00106500
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00106600
                      LCLA  &ZOPN1         INVOCATION INDEX             00106700
                      LCLC  &ZOPL(4095)    LABEL                        00106800
                      LCLC  &ZOPO(4095)    OPERATION                    00106900
                      LCLC  &ZOPV(4095)    VALUE                        00107000
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00107100
.*                                                                      00107200
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00107300
.******************************************************************     00107400
.***AL.L10 IWMSTEND FMT=LIST MF=S!L PARAMETERS                          00107500
.******************************************************************     00107600
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00107700
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00107800
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00107900
&XMFATTR              SETC  '0D'                                        00108000
.*                                                                      00108100
&ZOPN                 SETA  &ZOPN+1                                     00108200
&ZOPL(&ZOPN)          SETC  ' '                                         00108300
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00108400
&ZOPO(&ZOPN)          SETC  'B'                                         00108500
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00108600
                      AGO   .ZALVL6B                                    00108700
.ZALVL6A              ANOP                                              00108800
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00108900
&ZOPV(&ZOPN)          SETC  '4,8'                                       00109000
&ZOPN                 SETA  &ZOPN+1                                     00109100
&ZOPL(&ZOPN)          SETC  ' '                                         00109200
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00109300
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00109400
.ZALVL6B              ANOP                                              00109500
&ZOPD(&ZOPN)          SETC  '++ IWMSTEND ARG PARMS'                     00109600
.*                                                                      00109700
&ZOPN                 SETA  &ZOPN+1                                     00109800
&ZOPL(&ZOPN)          SETC  ' '                                         00109900
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00110000
&ZOPV(&ZOPN)          SETC  '1'                                         00110100
&ZOPD(&ZOPN)          SETC  ' '                                         00110200
.*                                                                      00110300
.ZLMFSL2              ANOP  ,                                           00110400
.*-------------------------------------------------------------------   00110500
.***AM.L00  IWMSTEND FMT=LIST name                                      00110600
