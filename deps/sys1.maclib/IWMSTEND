.*******************************************************************    00126800
.***AO.X02  RETURN CODE PROCESSING                                      00126900
.*******************************************************************    00127000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00127100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00127200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00127300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00127400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00127500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00127600
&ZOPN                 SETA  &ZOPN+1                                     00127700
&ZOPL(&ZOPN)          SETC  ' '                                         00127800
&ZOPO(&ZOPN)          SETC  'ST'                                        00127900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00128000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00128100
                      AGO   .ZMRETX                                     00128200
.*                                                                      00128300
.ZMRET2               ANOP  ,                                           00128400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00128500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00128600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00128700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00128800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00128900
&ZOPN                 SETA  &ZOPN+1                                     00129000
