.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00118100
&ZPLLEN               SETA 16                                           00118200
.*                                                                      00118300
.******************************************************************     00118400
.***AI.L01 IWMSTEND FMT=LIST FILL IN PARAMETER AREA                     00118500
.******************************************************************     00118600
                      ACTR  10000                                       00118700
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00118800
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00118900
&ZCVAL                SETC  'LAE'          XAX MODE                     00119000
.ZIMFXA               ANOP ,                                            00119100
.*                                                                      00119200
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00119300
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00119400
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00119500
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00119600
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00119700
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00119800
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00119900
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00120000
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00120100
&ZOPN                 SETA  &ZOPN+1                                     00120200
&ZOPL(&ZOPN)          SETC  ' '                                         00120300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00120400
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00120500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00120600
                      AGO   .ZIMFNX                                     00120700
.*                                                                      00120800
.ZIMFN2               ANOP ,                                            00120900
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00121000
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00121100
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00121200
&ZOPN                 SETA  &ZOPN+1                                     00121300
&ZOPL(&ZOPN)          SETC  ' '                                         00121400
&ZOPO(&ZOPN)          SETC  'LAE'                                       00121500
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00121600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00121700
                      AGO   .ZIMFNX                                     00121800
.ZIMFNX1              ANOP ,                                            00121900
&ZOPN                 SETA  &ZOPN+1                                     00122000
&ZOPL(&ZOPN)          SETC  ' '                                         00122100
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00122200
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00122300
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00122400
.ZIMFNX               ANOP ,                                            00122500
.********************************************************************   00122600
.***AI.L02 IWMSTEND FMT=LIST INITIALIZE PARAMETER AREA                  00122700
