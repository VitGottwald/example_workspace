&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00129200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00129300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00129400
.ZMRETX               ANOP  ,                                           00129500
.*******************************************************************    00129600
.***AO.X03  REASON CODE PROCESSING                                      00129700
.*******************************************************************    00129800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00129900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00130000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00130100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00130200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00130300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00130400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00130500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00130600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00130700
&ZOPN                 SETA  &ZOPN+1                                     00130800
&ZOPL(&ZOPN)          SETC  ' '                                         00130900
&ZOPO(&ZOPN)          SETC  'ST'                                        00131000
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00131100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00131200
                      AGO   .ZMRSNX                                     00131300
.*                                                                      00131400
.ZMRSN2               ANOP  ,                                           00131500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00131600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00131700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00131800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00131900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00132000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00132100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00132200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00132300
&ZOPN                 SETA  &ZOPN+1                                     00132400
&ZOPL(&ZOPN)          SETC  ' '                                         00132500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00132600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00132700
