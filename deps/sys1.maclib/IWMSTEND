.***AC.PLV01  PROCESS PLISTVER                                      *   00061600
.********************************************************************   00061700
&XPLISTVER      SETA  0                                                 00061800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00061900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00062000
&XPLISTVER      SETA  0                                                 00062100
.ZPLV0          ANOP  ,                                                 00062200
.*                                                                      00062300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00062400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00062500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00062600
.*-------------------------------------------------------------------   00062700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00062800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00062900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00063100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00063200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00063300
                AGO   .ZPVCK2                                           00063400
.ZPVCK3         ANOP  ,                                                 00063500
.*-------------------------------------------------------------------   00063600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00063700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00063800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00063900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00064000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00064100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00064200
                AGO   .ZPVCKE                                           00064300
.ZPVCK2         ANOP  ,                                                 00064400
.*-------------------------------------------------------------------   00064500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00064600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00064700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00064800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00064900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00065000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00065100
.ZPVCK6         ANOP  ,                                                 00065200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00065300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065400
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00065500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00065600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00065700
&XPLISTVER      SETA  0                                                 00065800
                AGO   .ZPVCKE                                           00065900
.*-------------------------------------------------------------------   00066000
.ZPVCK4A        ANOP  ,                                                 00066100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00066200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00066300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00066400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00066500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00066600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00066700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066800
                AGO   .ZPVCK5                                           00066900
.ZPVCK4         ANOP  ,                                                 00067000
&XPLISTVER      SETA  &PLISTVER(1)                                      00067100
.ZPVCK5         ANOP  ,                                                 00067200
.ZPVCKE         ANOP  ,                                                 00067300
