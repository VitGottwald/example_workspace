.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00133500
.*******************************************************************    00133600
                      ACTR  10000                                       00133700
.*                                                                      00133800
                      LCLA  &ZLC           COLUMN POSITION              00133900
                      LCLA  &ZOPX          LIST TABLE INDEX             00134000
                      LCLC  &ZLL           LABEL                        00134100
                      LCLC  &ZLOP          OPERATION                    00134200
                      LCLC  &ZLV           VALUE                        00134300
                      LCLC  &ZLD           DESCRIPTION                  00134400
.*                                                                      00134500
&ZOPX                 SETA  1                                           00134600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00134700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00134800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00134900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00135000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00135100
                      AIF   (K'&ZLD LE 0).ZL5                           00135200
&ZLC                  SETA  35             COMMENT COLUMN               00135300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00135400
&ZLC                  SETA  33             COMMMENT COLUMN              00135500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00135600
&ZLV                  SETC  '&ZLV'.' '                                  00135700
                      AGO   .ZL3                                        00135800
.*                                                                      00135900
.ZL4                  ANOP  ,                                           00136000
&ZLV                  SETC  '&ZLV'.' '                                  00136100
.ZL5                  ANOP  ,                                           00136200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00136300
.*                                                                      00136400
&ZLL     &ZLOP &ZLV                                                     00136500
.*                                                                      00136600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00136700
                      AGO   .ZL2           CONTINUE LOOPING             00136800
.*                                                                      00136900
.ZLX                  ANOP  ,                                           00137000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00137100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00137200
     L     14,16(0,0)              GET CVT ADDRESS                      00137300
     L     14,772(14,0)            GET SFT ADDRESS                      00137400
     L     14,380(14,0)            GET LX FOR SERVICE ROUTINE           00137500
     LA    15,48                   GET EX FOR SERVICE ROUTINE           00137600
     OR    14,15                   GET PC NUMBER                        00137700
     PC    0(14)                                                        00137800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00137900
.ZLINKX               ANOP  ,                                           00138000
.*******************************************************************    00138100
.***AP.X01  FORMAT GENERATED CODE                                       00138200
