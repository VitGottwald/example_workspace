&XWLMEUTKN    SETC  '&WLMEUTKN'                 SET XVARIABLE           00089900
.ZG2B         AIF   (K'&WLMEUTKN(1) GT 0).ZG2C                          00090000
&ZXWLMEUTKN   SETC  ''          RESET IN/OUTPUT                         00090100
&ZMS          SETC  ' '         *KRQDA*                                 00090200
&ZMS          SETC  '&ZMS "WLMEUTKN" KEY AND ITS'                       00090300
              MNOTE 8,' &ZMS ARGUMENT ARE REQUIRED.'                    00090400
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00090500
.ZG2C         ANOP  ,                                                   00090600
.*-------------------------------------------------------------------   00090700
.***AG.KCK02 NAME, RQD INPUT, KEYS -GPR OUT OF RANGE                    00090800
              AIF   (K'&ZXWLMEUTKN LE 0).ZG2R   NOT I/O                 00090900
              AIF   ('&XWLMEUTKN'(1,1) NE '(').ZG2R                     00091000
&ZCGPR        SETC  '&XWLMEUTKN'(2,K'&XWLMEUTKN-2)                      00091100
              AIF   ('&ZCGPR'(1,1) NE '0').ZG2P                         00091200
              AIF   (K'&ZCGPR GT 2).ZG2Q                                00091300
              AIF   ('&ZCGPR' EQ '0').ZG2Q                              00091400
&ZCGPR        SETC  '&ZCGPR'(2,1)                                       00091500
              AIF   (T'&ZCGPR NE 'N').ZG2Q      INVALID REG NAME        00091600
.ZG2P         ANOP  ,                                                   00091700
              AIF   (T'&ZCGPR NE 'N').ZG2R      NON NUMERIC             00091800
              AIF   ('&ZCGPR' LT '2').ZG2Q                              00091900
              AIF   ('&ZCGPR' LE '12').ZG2R                             00092000
.ZG2Q         ANOP  ,                                                   00092100
&ZMS          SETC  '"WLMEUTKN='                *KGPRA* MNOTE           00092200
&ZMS          SETC  '&ZMS.&WLMEUTKN"'                                   00092300
&ZMS          SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'           00092400
              MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                    00092500
&ZMACRET      SETA  8           SET MACRO ERROR RETURN CODE             00092600
.ZG2R         ANOP  ,                                                   00092700
.*----------------------------------------------------------------***   00092800
.***AG.KXVAR05 NAME, OPT OUTPUT, KEYS -XVARIABLE KEY                    00092900
              AIF   (N'&RETCODE LE 1).ZG5A      CK NUM                  00093000
