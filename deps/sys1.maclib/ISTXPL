*                                                                               
*DCL                                                                            
*  1 XPLVBRV   DEFINED(XPLVEC_DAT), /* Virtual Basic Route Data                 
*                                        Vector                      */         
*    2 XPLVBRV_DSTSA CHAR(4),       /* Destination Subarea           */         
*    2 XPLVBRV_TPRIO FIXED(8),      /* Transmission Priority         */         
*    2 XPLVBRV_VRNUM FIXED(8),      /* Virtual Route number          */         
*    2 XPLVBRV_ADJSA CHAR(4),       /* Adjacent Subarea              */         
*    2 XPLVBRV_ERTNM BIT(8),        /* Explicit Route number     @L5C*/         
*      3 XPLVBRV_IERNM BIT(4),      /* Initial ER number         @L5A*/         
*      3 XPLVBRV_AERNM BIT(4),      /* Actual ER number          @L5A*/         
*    2 XPLVBRV_RERNM FIXED(8),      /* Reverse Explicit Route number */         
*    2 XPLVBRV_SESVR CHAR(2),       /* Number of sessions on VR      */         
*    2 XPLVBRV_MINWS FIXED(8),      /* Current minimum window size   */         
*    2 XPLVBRV_MAXWS FIXED(8),      /* Current maximum window size   */         
*    2 XPLVBRV_DEFWS FIXED(8),      /* Default window size           */         
*    2 XPLVBRV_DFMWS FIXED(8),      /* Default maximum window size   */         
*    2 XPLVBRV_WSRES CHAR(4),       /* Number of time window size               
*                                        reset to the minimum        */         
*    2 XPLVBRV_PIUWS CHAR(4),       /* Number of PIUs on the VR                 
*                                        waiting to be sent          */         
*    2 XPLVBRV_VRST  FIXED(8),      /* Virtual Route state       @L4C*/         
*    2 XPLVBRV_FCDATA,              /* Flow Control state data       */         
*      3 XPLVBRV_FCST  BIT(8),      /* Flow Control state        @L4C*/         
*        4 *           BIT(6),      /* Reserved                  @L4C*/         
*        4 XPLVBRV_ROUTE BIT(2),    /* Route status - 00 = blocked   */         
*                                   /*                01 = held      */         
*                                   /*                10 = open      */         
*      3 XPLVBRV_NTHLD CHAR(4),     /* Num times route in held state */         
*      3 XPLVBRV_THLDC CHAR(8),     /* Time VR has been in held state           
*                                        if currently held           */         
*      3 XPLVBRV_THLDT CHAR(8),     /* Time VR has been in held state           
*                                        (Total not including current           
*                                         held time)                 */         
*      3 XPLVBRV_THLDM CHAR(8),     /* Time VR has been in held state           
*                                        (Maximum time held for any             
*                                         given instance)            */         
*      3 XPLVBRV_TMHLD CHAR(8),     /* Timestamp for Time VR has been           
*                                        in held state (Maximum time            
*                                        held for any given instance)*/         
*      3 XPLVBRV_NTBLK CHAR(4),     /* Number of times VR blocked    */         
*      3 XPLVBRV_TBLKC CHAR(8),     /* Time VR has been in blocked              
*                                        state if currently blocked  */         
*      3 XPLVBRV_TBLKT CHAR(8),     /* Time VR has been in blocked              
*                                        state (Total not including             
*                                         current blocked time)      */         
*      3 XPLVBRV_TBLKM CHAR(8),     /* Time VR has been in blocked              
*                                        state (Maximum time blocked            
*                                        for any given instance)     */         
*      3 XPLVBRV_TMBLK CHAR(8),     /* Timestamp for Time VR has been           
*                                        in blocked state (Maximum time         
*                                        blocked held for any given             
*                                        instance                @L2C*/         
*    2 XPLVBRV_LUSCT FIXED(15) BDY(BYTE), /* Total number of LU-LU              
*                                        sessions on the VR      @L5C*/         
*    2 XPLVBRV_CPRSC FIXED(15) BDY(BYTE), /* Current pacing request             
*                                        send count              @L5C*/         
*    2 XPLVBRV_CPWSZ FIXED(8),      /* Current pacing window                    
*                                        size                    @L5C*/         
*    2 XPLVBRV_OPIUC FIXED(32) BDY(BYTE), /* Outbound PIU count over            
*                                        VR                      @Y1C*/         
*    2 XPLVBRV_IPIUC FIXED(32) BDY(BYTE); /* Inbound PIU count over             
*                                        VR                      @Y1C*/         
