*/*                   Basic RTP Data Vector                      @D3A*/         
*/* WARNING:  XPLRTPV_STATS_1 must match exactly with RPDCB_STATS_1  */         
*/********************************************************************/         
*                                                                               
*DCL                                                                            
*  1 XPLRTPV  DEFINED(XPLVEC_DAT),  /* Rapid Transport Protocol                 
*                                        Data Vector             @D3A*/         
*    2 XPLRTPV_FLAGS    BIT(16),    /* RTP Flag Bytes            @D3A*/         
*      3 XPLRTPV_PATHSWTCH BIT(1),  /* Path Switch occured       @D3A*/         
*      3 XPLRTPV_CLOSEOUT  BIT(1),  /* RTP deleted/Closeout info @D3A*/         
*      3 XPLRTPV_PSW_NAME  BIT(1),  /* Path Switch - Name CHange @D3A*/         
*      3 *                 BIT(13), /* Reserved                  @D3A*/         
*    2 XPLRTPV_LCL_NCE     BIT(64) BDY(BYTE), /* Local NCE  @D3A @D4C*/         
*    2 XPLRTPV_LCL_TCID    BIT(64) BDY(BYTE), /* Local TCID      @D3A*/         
*    2 XPLRTPV_RTP_PU      CHAR(8),           /* RTP PU Name     @D4A*/         
*    2 XPLRTPV_RMT_NETID   CHAR(8),           /* Remote Net ID   @D3A*/         
*    2 XPLRTPV_RMT_CPNAME  CHAR(8),           /* Remote CP Name  @D3A*/         
*    2 XPLRTPV_RMT_PSW_NETID   CHAR(8),       /* Remote Net ID after            
*                                                Path Switch     @D3A*/         
*    2 XPLRTPV_RMT_PSW_CPNAME  CHAR(8),       /* Remote CP Name after           
*                                                Path Switch     @D3A*/         
*    2 XPLRTPV_RMT_NCE     BIT(64) BDY(BYTE), /* Remote NCE @D3A @D4C*/         
*    2 XPLRTPV_RMT_TCID    BIT(64) BDY(BYTE), /* Remote TCID     @D3A*/         
*    2 XPLRTPV_RTP_STATE  FIXED(8) BDY(BYTE), /* RTP State       @D3A*/         
*    2 XPLRTPV_PSW_TIMER FIXED(32) BDY(BYTE), /* Path Switch Timer              
*                                                                @D3A*/         
*    2 XPLRTPV_RTRP_DLAY FIXED(32) BDY(BYTE), /* Round Trip Delay               
*                                                                @D3A*/         
*    2 XPLRTPV_STATS_1,                       /* Data segment    @D3A*/         
*      3 XPLRTPV_BYTES_SNT FIXED(32) BDY(BYTE), /* Number bytes sent            
*                                                  over RTP      @D3A*/         
*      3 XPLRTPV_BYTES_RCVV FIXED(32) BDY(BYTE), /* Num. bytes received         
*                                                  over RTP      @D3A*/         
*      3 XPLRTPV_PIUS_FRST FIXED(32) BDY(BYTE), /* Number 1st in segmnt         
*                                                  PIUs sent     @D3A*/         
*      3 XPLRTPV_PIUS_MIDL FIXED(32) BDY(BYTE), /* Num middle in segmnt         
*                                                  PIUs sent     @D3A*/         
*      3 XPLRTPV_PIUS_LAST FIXED(32) BDY(BYTE), /* Num last in segmnt           
*                                                  PIUs sent     @D3A*/         
*      3 XPLRTPV_PIUS_WHOL FIXED(32) BDY(BYTE), /* Number unsegmented           
*                                                  PIUs sent     @D3A*/         
*      3 XPLRTPV_PIUR_FRST FIXED(32) BDY(BYTE), /* Number 1st in segmnt         
*                                                  PIUs received @D3A*/         
*      3 XPLRTPV_PIUR_MIDL FIXED(32) BDY(BYTE), /* Num middle in segmnt         
*                                                  PIUs recved   @D3A*/         
*      3 XPLRTPV_PIUR_LAST FIXED(32) BDY(BYTE), /* Num last in segmnt           
*                                                  PIUs received @D3A*/         
*      3 XPLRTPV_PIUR_WHOL FIXED(32) BDY(BYTE), /* Num unsegmented PIUs         
*                                                  received      @D3A*/         
*      3 XPLRTPV_SNDR_CHNG FIXED(32) BDY(BYTE), /* Number times allowed         
*                                                send rate chngd @D3A*/         
*      3 XPLRTPV_EPINI_PSW FIXED(16) BDY(BYTE), /* Path Switch Attempts         
*                                                  Initiated by RTP             
*                                                  end point     @D3A*/         
*      3 XPLRTPV_VTAMI_PSW FIXED(16) BDY(BYTE), /* Total path switch            
*                                                  attempts initiated           
*                                                  by this VTAM  @D3A*/         
*      3 XPLRTPV_OPCMD_PSW FIXED(16) BDY(BYTE), /* Path Switch Attempts         
*                                                  due to Operator              
*                                                  Command       @D3A*/         
*      3 XPLRTPV_UNSUC_PSV FIXED(16) BDY(BYTE), /* Total Unsuccessful           
*                                                  Path Switch attempts         
*                                                                @D3A*/         
*      3 XPLRTPV_UNSUC_PSO FIXED(16) BDY(BYTE), /* Unsuccessful Path            
*                                                  Switch due to Op.            
*                                                  Command       @D3A*/         
*    2 XPLRTPV_LULU_SESS_COUNT FIXED(32) BDY(BYTE), /* Count of active          
*                                                  LU-LU sessions               
*                                                  using the RTP @D3A*/         
*    2 XPLRTPV_TOT_BKPTM FIXED(32) BDY(BYTE), /* Time in milliseconds           
*                                                back pressure has              
*                                                been applied    @D3A*/         
*    2 XPLRTPV_TOT_BACKP FIXED(16) BDY(BYTE), /* Total number of times          
*                                                back pressure has been         
*                                                applied         @D3A*/         
*    2 XPLRTPV_PSW_BACKP FIXED(16) BDY(BYTE), /* Number of times back           
*                                                pressure applied due           
*                                                to path switch  @D3A*/         
*    2 XPLRTPV_QDP_BACKP FIXED(16) BDY(BYTE), /* Number of times back           
*                                                pressure applied due           
*                                                to queue depth                 
*                                                exceeded        @D3A*/         
*    2 XPLRTPV_RESNT_NLP FIXED(32) BDY(BYTE), /* Number retransmitted           
*                                                NLPs            @D3A*/         
*    2 XPLRTPV_UNAK_BUFS FIXED(32) BDY(BYTE); /* Current num of unac-           
*                                                knowledge buffs @D3A*/         
