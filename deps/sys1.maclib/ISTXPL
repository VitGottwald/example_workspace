*                                                                               
*DCL                                                                            
*  1 XPLGSEV   DEFINED(XPLVEC_DAT), /* Global Session Vector         */         
*    2 XPLGSEV_STTAK CHAR(4),       /* Number successful LU-LU session          
*                                        starts and takeovers        */         
*    2 XPLGSEV_STOP  CHAR(4),       /* Number LU-LU session stops    */         
*    2 XPLGSEV_FAIL  CHAR(4),       /* Number LU-LU session set-up              
*                                        failures                    */         
*    2 XPLGSEV_ACTHW CHAR(4),       /* Number of active LU-LU sessions          
*                                        high-water mark since last             
*                                        COLLECT CNM RU processed    */         
*    2 XPLGSEV_ACTHT CHAR(8),       /* Timestamp for number of active           
*                                        LU-LU sessions high-water mark         
*                                        since last COLLECT CNM RU   */         
*    2 XPLGSEV_ACTLW CHAR(4),       /* Number of active LU-LU sessions          
*                                        low-water mark since last              
*                                        COLLECT CNM RU processed    */         
*    2 XPLGSEV_ACTLT CHAR(8),       /* Timestamp for number of active           
*                                        LU-LU sessions low-water mark          
*                                        since last COLLECT CNM RU   */         
*    2 XPLGSEV_QINIT CHAR(4),       /* Number of LU-LU sessions in              
*                                        Queued Initiate status      */         
*    2 XPLGSEV_PENDR CHAR(4),       /* Number of LU-LU sessions in              
*                                        Pending Reallocation status */         
*    2 XPLGSEV_PENDA CHAR(4),       /* Number of LU-LU sessions in              
*                                        Pending Active status       */         
*    2 XPLGSEV_ACT   CHAR(4),       /* Number active LU-LU sessions  */         
*    2 XPLGSEV_PENDT CHAR(4),       /* Number of LU-LU sessions in              
*                                        Pending Termination status  */         
*    2 XPLGSEV_DAPLU CHAR(4),       /* Number defined APPL LUs       */         
*    2 XPLGSEV_DDELU CHAR(4),       /* Number defined non-appl                  
*                                        dependent LUs               */         
*    2 XPLGSEV_DCDRS CHAR(4),       /* Number defined CDRSCs         */         
*    2 XPLGSEV_DPU   CHAR(4),       /* Number defined PUs            */         
*    2 XPLGSEV_AAPLU CHAR(4),       /* Number active APPLU LUs       */         
*    2 XPLGSEV_ADELU CHAR(4),       /* Number active non-appl                   
*                                        dependent LUs               */         
*    2 XPLGSEV_ACDRS CHAR(4),       /* Number active CDRSCs          */         
*    2 XPLGSEV_APU   CHAR(4),       /* Number active PUs             */         
*    2 XPLGSEV_RCPF  CHAR(4),       /* Number retriable CP-CP session           
*                                        failures                    */         
*    2 XPLGSEV_NRCPF CHAR(4),       /* Number non-retriable CP-CP               
*                                        session failures            */         
*    2 XPLGSEV_SCPIN CHAR(4),       /* Number successful CP-CP session          
*                                         initiations            @L1C*/         
*    2 XPLGSEV_DLREN CHAR(4),       /* Number of DLR end nodes   @L1A*/         
*    2 XPLGSEV_DLRNN CHAR(4),       /* Number of DLR network nodes              
*                                                                @L1A*/         
*    2 XPLGSEV_DLRPU CHAR(4),       /* Number of DLR served PUs  @L1A*/         
*    2 XPLGSEV_DLRLU CHAR(4),       /* Number of DLR served LUs  @L1A           
*                                                                @L2C*/         
*    2 XPLGSEV_AILUS CHAR(4),       /* Number active ILUs        @L2A*/         
*    2 XPLGSEV_SRVEN CHAR(4),       /* Number of ENs served by this NN          
*                                      (i.e. CP-CP session active)              
*                                      INVALID when we are EN    @L2A*/         
*    2 XPLGSEV_CPSNN CHAR(4);       /* Number NN with which this NN has         
*                                      CP-CP sessions, INVALID when we          
*                                      are EN                    @L2A*/         
