*/*             Global APPN Directory Services Vector                */         
*/********************************************************************/         
*                                                                               
*DCL                                                                            
*  1 XPLGADV   DEFINED(XPLVEC_DAT), /* Global APPN Directory Services           
*                                        Vector                      */         
*    2 XPLGADV_NRFLD CHAR(4),       /* Number resources found in                
*                                        local database              */         
*    2 XPLGADV_NRFDS CHAR(4),       /* Number resources found from              
*                                        Directory Server            */         
*    2 XPLGADV_NRFDB CHAR(4),       /* Number resources found via               
*                                        domain broadcast            */         
*    2 XPLGADV_NRFNB CHAR(4),       /* Number resources found via               
*                                        network broadcast           */         
*    2 XPLGADV_FSRCH CHAR(4),       /* Number of failed searches                
*                                        (receive negative reply)    */         
*    2 XPLGADV_NBNCE CHAR(4),       /* Number of times network                  
*                                        broadcast not performed due            
*                                        to negative cache entry     */         
*    2 XPLGADV_ESRRF CHAR(4),       /* Number of external search                
*                                        requests - resource found   */         
*    2 XPLGADV_ESRNF CHAR(4),       /* Number of external search                
*                                        requests - resource ^ found */         
*    2 XPLGADV_DSNRR CHAR(4),       /* Number referrals received     */         
*    2 XPLGADV_DSPRR CHAR(4),       /* Number positive replies received         
*                                        from an Alternate Directory            
*                                        Server                      */         
*    2 XPLGADV_DSNNR CHAR(4),       /* Number negative replies received         
*                                        from an Alternate Directory            
*                                        Server                      */         
*    2 XPLGADV_RRCGC CHAR(4),       /* Number of resources removed              
*                                        from cache due to garbage              
*                                        collection              @L1C*/         
*    2 XPLGADV_ADJDS CHAR(4),       /* Number of adjacent nodes in a            
*                                        different subnet   @L1A @L2C*/         
*    2 XPLGADV_ROBCH CHAR(4),       /* Number of times a CHACHE entry           
*                                        is discarded due to DIRSIZE            
*                                        max being reached       @L2A*/         
*    2 XPLGADV_REGRS CHAR(4),       /* Number of registered resources           
*                                        at a Cesntral Directory Server         
*                                        or at an NN             @L2A*/         
*    2 XPLGADV_CACHE CHAR(4);       /* Number of entries in the CACHE           
*                                                                @L2A*/         
*                                                                               
*/********************************************************************/         
*/*         Global APPN Directory Services Border Node Vector    @L1A*/         
*/********************************************************************/         
*                                                                               
*DCL                                                                            
*  1 XPLGBNV   DEFINED(XPLVEC_DAT), /* Global APPN Directory Services           
*                                        Border Node Vector          */         
*    2 XPLGBNV_CPNME CHAR(16),      /* Network qualified CPNAME  @L1A*/         
*      3 XPLGBNV_NETID CHAR(8),     /* Adjacent Node Network ID  @L1A*/         
*      3 XPLGBNV_NAME  CHAR(8),     /* Adjacent Node Name        @L1A*/         
*    2 XPLGBNV_SSRCH CHAR(4),       /* Number of subnet searches sent           
*                                        to adjacent Node        @L1A*/         
*    2 XPLGBNV_RSRCH CHAR(4);       /* Number of subnet searches                
*                                        received from ADJ node  @L1A*/         
*                                                                               
*/********************************************************************/         
*/*             Global APPN Topology Data Vector                     */         
*/********************************************************************/         
*                                                                               
*DCL                                                                            
*  1 XPLGATV   DEFINED(XPLVEC_DAT), /* Global APPN Topology data                
*                                        Vector                      */         
*    2 XPLGATV_NRTET CHAR(4),       /* Number of routes - using                 
*                                        existing tree               */         
*    2 XPLGATV_NRTMT CHAR(4),       /* Number of routes - using                 
*                                        modified tree               */         
*    2 XPLGATV_NRTNT CHAR(4),       /* Number of routes - using                 
*                                        new tree                    */         
*    2 XPLGATV_TDUON CHAR(4),       /* Number of TDUs originated by             
*                                        this node                   */         
*    2 XPLGATV_TDUPN CHAR(4),       /* Number of TDUs propagated by             
*                                        this node                   */         
*    2 XPLGATV_TDUDU CHAR(4),       /* Number of TDUs received by this          
*                                        node resulting in topology             
*                                        database update             */         
*    2 XPLGATV_TDUDN CHAR(4),       /* Number of TDUs received by this          
*                                        node and discarded for normal          
*                                        reasons                     */         
*    2 XPLGATV_TDUDR CHAR(4),       /* Number of TDUs received by this          
*                                        node and discarded because is          
*                                        a RSN ^ OK (rebroadcast)    */         
*    2 XPLGATV_TDUDI CHAR(4),       /* Number of TDUs received by this          
*                                        node and discarded because of          
*                                        data inconsistency                     
*                                        (rebroadcast)               */         
*    2 XPLGATV_NNTD  CHAR(4),       /* Number of network nodes in               
*                                        topology database           */         
*    2 XPLGATV_ENTD  CHAR(4),       /* Number of end nodes in                   
*                                        topology database           */         
*    2 XPLGATV_TGTD  CHAR(4),       /* Number of TGs in topology                
*                                        database                    */         
*    2 XPLGATV_TGFL  CHAR(4),       /* Number of TG failures         */         
*    2 XPLGATV_DPUAL CHAR(4),       /* Num of dynamic PU allocations */         
*    2 XPLGATV_DTCF  CHAR(4),       /* Number of trees discarded                
*                                      due to Cache being full       */         
*    2 XPLGATV_NMTRE CHAR(4),       /* Number of trees           @L2C*/         
*    2 XPLGATV_VNTD  CHAR(4),       /* Number of Virtual nodes in the           
*                                        topology database       @L2A*/         
*    2 XPLGATV_CDSTD CHAR(4),       /* Number Central Directory Servers         
*                                        in topology database    @L2A*/         
*    2 XPLGATV_ICNCN CHAR(4),       /* Number of IC Nodes in the                
*                                        topology database       @L2A*/         
*    2 XPLGATV_ENCON CHAR(4),       /* Number of ENs connected to this          
*                                        node                    @L2A*/         
*    2 XPLGATV_NNCON CHAR(4),       /* Number of NNs connected to this          
*                                        node                    @L2A*/         
*    2 XPLGATV_VNCON CHAR(4);       /* Number of VNs connected to this          
*                                        node               @L2A @L4C*/         
*                                                                               
*/********************************************************************/         
