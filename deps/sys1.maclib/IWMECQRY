.*******************************************************************    00147300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00147400
.*******************************************************************    00147500
                      ACTR  10000                                       00147600
.*                                                                      00147700
                      LCLA  &ZLC           COLUMN POSITION              00147800
                      LCLA  &ZOPX          LIST TABLE INDEX             00147900
                      LCLC  &ZLL           LABEL                        00148000
                      LCLC  &ZLOP          OPERATION                    00148100
                      LCLC  &ZLV           VALUE                        00148200
                      LCLC  &ZLD           DESCRIPTION                  00148300
.*                                                                      00148400
&ZOPX                 SETA  1                                           00148500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00148600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00148700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00148800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00148900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00149000
                      AIF   (K'&ZLD LE 0).ZL5                           00149100
&ZLC                  SETA  35             COMMENT COLUMN               00149200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00149300
&ZLC                  SETA  33             COMMMENT COLUMN              00149400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00149500
&ZLV                  SETC  '&ZLV'.' '                                  00149600
                      AGO   .ZL3                                        00149700
.*                                                                      00149800
.ZL4                  ANOP  ,                                           00149900
&ZLV                  SETC  '&ZLV'.' '                                  00150000
.ZL5                  ANOP  ,                                           00150100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00150200
.*                                                                      00150300
&ZLL     &ZLOP &ZLV                                                     00150400
.*                                                                      00150500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00150600
                      AGO   .ZL2           CONTINUE LOOPING             00150700
.*                                                                      00150800
.ZLX                  ANOP  ,                                           00150900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00151000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00151100
         L     14,16(0,0)              GET CVT ADDRESS                  00151200
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00151300
         LAE   14,0(14,0)              CLEAR AR 14                      00151400
.NOAR    ANOP                                                           00151500
         L     14,772(14,0)            GET SFT ADDRESS                  00151600
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00151700
         LA    15,26                   GET EX FOR SERVICE ROUTINE       00151800
         OR    14,15                   GET PC NUMBER                    00151900
         PC    0(14)                                                    00152000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00152100
.ZLINKX               ANOP  ,                                           00152200
.*******************************************************************    00152300
.***AP.X01  FORMAT GENERATED CODE                                       00152400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00152500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00152600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00152700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00152800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00152900
                      AIF   (K'&ZLD LE 0).ZL51                          00153000
&ZLC                  SETA  35             COMMENT COLUMN               00153100
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00153200
&ZLC                  SETA  33             COMMENT COLUMN               00153300
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00153400
&ZLV                  SETC  '&ZLV'.' '                                  00153500
                      AGO   .ZL31                                       00153600
.*                                                                      00153700
.ZL41                 ANOP  ,                                           00153800
&ZLV                  SETC  '&ZLV'.' '                                  00153900
.ZL51                 ANOP  ,                                           00154000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00154100
.*                                                                      00154200
&ZLL     &ZLOP &ZLV                                                     00154300
.*                                                                      00154400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00154500
                      AGO   .ZL21          CONTINUE LOOPING             00154600
.*                                                                      00154700
.ZLX1                 ANOP  ,                                           00154800
.*******************************************************************    00154900
