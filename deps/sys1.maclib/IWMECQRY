.***AB.07  DEFINE ALL MACRO VARIABLES                                   00046900
.*******************************************************************    00047000
              LCLC  &XETOKEN    ++ NAME                                 00047100
              LCLC  &XANSAREA   ++ NAME                                 00047200
              LCLC  &XANSLEN    ++ NAME                                 00047300
              LCLC  &XQUERYLEN  ++ NAME                                 00047400
              LCLC  &XRETCODE   ++ NAME                                 00047500
              LCLC  &XRSNCODE   ++ NAME                                 00047600
              LCLA  &XPLISTVER  ++ VALUE                                00047700
              LCLC  &XMF        ++ VALUE                                00047800
              LCLC  &XMFCTRL    ++ NAME                                 00047900
              LCLC  &XMFATTR    ++ VALUE                                00048000
              LCLC  &XMFSCK     ++ VALUE                                00048100
.*******************************************************************    00048200
.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00048300
.*******************************************************************    00048400
              LCLC  &ZXETOKEN   ++ INPUT NAME                           00048500
              LCLC  &ZXANSAREA  ++ OUTPUT NAME                          00048600
              LCLC  &ZXANSLEN   ++ INPUT NAME                           00048700
              LCLC  &ZXQUERYLEN                 ++ OUTPUT NAME          00048800
              LCLC  &ZXRETCODE  ++ OUTPUT NAME                          00048900
              LCLC  &ZXRSNCODE  ++ OUTPUT NAME                          00049000
              LCLC  &ZXPLISTVER                 ++ INPUT VALUE          00049100
              LCLC  &ZXMF       ++ INPUT VALUE                          00049200
              LCLC  &ZXMFCTRL   ++ INPUT NAME                           00049300
              LCLC  &ZXMFATTR   ++ INPUT VALUE                          00049400
              LCLC  &ZXMFSCK    ++ INPUT VALUE                          00049500
.********************************************************************   00049600
.***AC.PLV01  PROCESS PLISTVER                                      *   00049700
.********************************************************************   00049800
&XPLISTVER      SETA  0                                                 00049900
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00050000
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00050100
&XPLISTVER      SETA  0                                                 00050200
.ZPLV0          ANOP  ,                                                 00050300
.*                                                                      00050400
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00050500
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00050600
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00050700
.*-------------------------------------------------------------------   00050800
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00050900
&ZMS            SETC  '"PLISTVER='                                      00051000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00051100
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00051200
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00051300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00051400
                AGO   .ZPVCK2                                           00051500
.ZPVCK3         ANOP  ,                                                 00051600
.*-------------------------------------------------------------------   00051700
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00051800
&ZMS            SETC  '"PLISTVER='                                      00051900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00052000
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00052100
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00052200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00052300
                AGO   .ZPVCKE                                           00052400
.ZPVCK2         ANOP  ,                                                 00052500
.*-------------------------------------------------------------------   00052600
&ZCVAL          SETC  '&PLISTVER(1)'                                    00052700
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00052800
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00052900
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00053000
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00053100
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00053200
.ZPVCK6         ANOP  ,                                                 00053300
&ZMS            SETC  '"PLISTVER='                                      00053400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053500
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00053600
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00053700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00053800
&XPLISTVER      SETA  0                                                 00053900
                AGO   .ZPVCKE                                           00054000
.*-------------------------------------------------------------------   00054100
.ZPVCK4A        ANOP  ,                                                 00054200
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00054300
&ZMS            SETC  'PLISTVER='                                       00054400
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00054500
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00054600
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00054700
                MNOTE 8,' &ZMS IS REQUIRED.'                            00054800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054900
                AGO   .ZPVCK5                                           00055000
.ZPVCK4         ANOP  ,                                                 00055100
&XPLISTVER      SETA  &PLISTVER(1)                                      00055200
.ZPVCK5         ANOP  ,                                                 00055300
.ZPVCKE         ANOP  ,                                                 00055400
.********************************************************************   00055500
