&ZOPL(&ZOPN)          SETC  ' '                                         00109800
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00109900
&ZOPO(&ZOPN)          SETC  'B'                                         00110000
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00110100
                      AGO   .ZALVL6B                                    00110200
.ZALVL6A              ANOP                                              00110300
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00110400
&ZOPV(&ZOPN)          SETC  '4,8'                                       00110500
&ZOPN                 SETA  &ZOPN+1                                     00110600
&ZOPL(&ZOPN)          SETC  ' '                                         00110700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00110800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00110900
.ZALVL6B              ANOP                                              00111000
&ZOPD(&ZOPN)          SETC  '++ IWMECQRY ARG PARMS'                     00111100
.*                                                                      00111200
&ZOPN                 SETA  &ZOPN+1                                     00111300
&ZOPL(&ZOPN)          SETC  ' '                                         00111400
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00111500
