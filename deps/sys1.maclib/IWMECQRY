              ACTR  10000                                               00102200
.*----------------------------------------------------------------***   00102300
.***AH.P02 EXTRA POSITIONALS                                            00102400
              AIF   (N'&SYSLIST LE 0).ZGLISTX   NO EXTRAS               00102500
              AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL                00102600
              AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL            00102700
              AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL             00102800
.ZGLISTX1     ANOP  ,                                                   00102900
&ZCVAL        SETC  ''          ERROR FIELD                             00103000
&ZAWRK        SETA  0           POSITIONAL NUMBER                       00103100
.ZGLIST2      ANOP  ,                                                   00103200
