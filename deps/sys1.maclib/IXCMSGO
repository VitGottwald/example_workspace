                AGO   .ZPVCKE                                           00318500
.ZPVCK2         ANOP  ,                                                 00318600
.*-------------------------------------------------------------------   00318700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00318800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00318900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00319000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00319100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00319200
                AIF   (&ZN LE 3).ZPVCK4A   NOT HIGH                     00319300
.ZPVCK6         ANOP  ,                                                 00319400
&ZMS            SETC  '"PLISTVER='                                      00319500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00319600
