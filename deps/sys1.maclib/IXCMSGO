.*******************************************************************    00897100
.***AO.X02  RETURN CODE PROCESSING                                      00897200
.*******************************************************************    00897300
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00897400
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00897500
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00897600
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00897700
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00897800
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00897900
&ZOPN                 SETA  &ZOPN+1                                     00898000
&ZOPL(&ZOPN)          SETC  ' '                                         00898100
&ZOPO(&ZOPN)          SETC  'ST'                                        00898200
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00898300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00898400
                      AGO   .ZMRETX                                     00898500
.*                                                                      00898600
.ZMRET2               ANOP  ,                                           00898700
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00898800
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00898900
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00899000
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00899100
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00899200
&ZOPN                 SETA  &ZOPN+1                                     00899300
&ZOPL(&ZOPN)          SETC  ' '                                         00899400
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00899500
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00899600
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00899700
.ZMRETX               ANOP  ,                                           00899800
.*******************************************************************    00899900
.***AO.X03  REASON CODE PROCESSING                                      00900000
.*******************************************************************    00900100
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00900200
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00900300
