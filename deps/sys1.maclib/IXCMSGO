.***AH.P02 EXTRA POSITIONALS                                            00633900
                   AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS           00634000
                   AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL           00634100
                   AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL       00634200
                   AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL        00634300
.ZGLISTX1          ANOP  ,                                              00634400
&ZCVAL             SETC  ''          ERROR FIELD                        00634500
&ZAWRK             SETA  0           POSITIONAL NUMBER                  00634600
.ZGLIST2           ANOP  ,                                              00634700
&ZAWRK             SETA  &ZAWRK+1    NEXT POSITIONAL                    00634800
                   AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE           00634900
&ZCVAL             SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                00635000
                   AGO   .ZGLIST2                                       00635100
.*                                                                      00635200
.ZGLIST3           ANOP  ,                                              00635300
&ZCVAL             SETC '&ZCVAL'(2,K'&ZCVAL-1)  STRIP COMMA             00635400
&ZMS               SETC  'POSITIONAL ARGUMENT(S),'                      00635500
&ZMS               SETC  '&ZMS "&ZCVAL"'                                00635600
&ZMS               SETC  '&ZMS CANNOT BE USED.'                         00635700
&ZMS               SETC  '&ZMS NO POSITIONAL ARGUMENTS'                 00635800
                   MNOTE 8,' &ZMS ARE ALLOWED.'                         00635900
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00636000
.ZGLISTX           ANOP  ,                                              00636100
&ZASYSLNDX         SETA  0                                              00636200
.*-------------------------------------------------------------------   00636300
.***AG.MF04 MF=(SLE)                                                    00636400
