.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00903800
.*******************************************************************    00903900
                      ACTR  10000                                       00904000
.*                                                                      00904100
                      LCLA  &ZLC           COLUMN POSITION              00904200
                      LCLA  &ZOPX          LIST TABLE INDEX             00904300
                      LCLC  &ZLL           LABEL                        00904400
                      LCLC  &ZLOP          OPERATION                    00904500
                      LCLC  &ZLV           VALUE                        00904600
                      LCLC  &ZLD           DESCRIPTION                  00904700
.*                                                                      00904800
&ZOPX                 SETA  1                                           00904900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00905000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00905100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00905200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00905300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00905400
                      AIF   (K'&ZLD LE 0).ZL5                           00905500
&ZLC                  SETA  35             COMMENT COLUMN               00905600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00905700
&ZLC                  SETA  33             COMMMENT COLUMN              00905800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00905900
&ZLV                  SETC  '&ZLV'.' '                                  00906000
                      AGO   .ZL3                                        00906100
.*                                                                      00906200
.ZL4                  ANOP  ,                                           00906300
&ZLV                  SETC  '&ZLV'.' '                                  00906400
.ZL5                  ANOP  ,                                           00906500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00906600
.*                                                                      00906700
&ZLL     &ZLOP &ZLV                                                     00906800
.*                                                                      00906900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00907000
                      AGO   .ZL2           CONTINUE LOOPING             00907100
.*                                                                      00907200
.ZLX                  ANOP  ,                                           00907300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00907400
