*                                      01 = Near-line                           
*                                      10 = SATA                                
*                                      11 = Solid state drive    @41A*/         
*         5 R748ARD BIT(1),         /* '1' = Raid degraded       @41A*/         
*         5 R748ADT BIT(1),         /* '1' = DDM throttling      @41A*/         
*         5 R748ARE BIT(1),         /* '1' = RPM exception       @41A*/         
*         5 *       BIT(3),         /* Reserved for future use   @41A*/         
*       3 *         CHAR(3);        /* Reserved for future use   @41A*/         
*/*********  Subtype 8, Synchronous I/O Link Statistics data section */         
*/*                                                              @91A*/         
*    @SPACE;                                                                    
*   DECLARE                                                                     
*     1 R748SIOL   BASED(ADDR(SMF74HDR)+SMF748SO),                              
*       3 R748SIID FIXED(16),       /* Synchronous I/O link interface*/         
*                                   /* identifier.               @91A*/         
*       3 R748STYP FIXED(8),        /* Synchronous I/O link type.               
*                                       00    Not used                          
*                                       01    Optical PCIe                      
*                                       02-FF Not used           @91A*/         
*       3 R748SSPD FIXED(8),        /* Synchronous I/O link type.               
*                                       00    Not used                          
*                                       01    PCIe Gen 1                        
*                                       02    PCIe Gen 2                        
*                                       03    PCIe Gen 3                        
*                                       04    PCIe Gen 4                        
*                                       05-FF Not used           @91A*/         
*       3 R748SWDH FIXED(8),        /* Synchronous I/O link width. This         
*                                      is the number of PCIe lanes.             
*                                                                @91A*/         
*       3 R748SSTE FIXED(8),        /* Synchronous I/O link type.               
*                                       00    Not used                          
*                                       01    Link not trained                  
*                                       02    Link handshake incomplete         
*                                       03    Link handshake complete,          
*                                             link operational                  
*                                       04    Link in service mode              
*                                       05-FF Not used           @91A*/         
*       3 R748SFLG BIT(8),          /* Flags                     @91A*/         
*         5 R748SBYT BIT(1),        /* On = Units of bytes                      
*                                      indeterminable. Byte values              
*                                      incorrect.                @91A*/         
*         5 R748STIM BIT(1),        /* On = Units of time                       
*                                      indeterminable. Time values              
*                                      incorrect.                @91A*/         
*         5 *        BIT(6),        /* Reserved for future use   @91A*/         
*       3 *        CHAR(1),         /* Reserved                  @91A*/         
*       3 R748SCBR CHAR(8),         /* Synchronous I/O cache bytes              
*                                      read in units of 128K bytes.             
*                                      (long floating point)     @91A*/         
*       3 R748SCRO CHAR(8),         /* Total number of synchronous I/O          
*                                      cache read operations.                   
*                                      (long floating point)     @91A*/         
*       3 R748SCRS CHAR(8),         /* Number of successful synchronous         
*                                      I/O cache read operations.               
*                                      (long floating point)     @91A*/         
*       3 R748SCRT CHAR(8),         /* Synchronous I/O cache read               
*                                      accumulated time in milliseconds         
*                                      (long floating point)     @91A*/         
*       3 R748SCBW CHAR(8),         /* Synchronous I/O cache bytes              
*                                      written in units of 128K bytes.          
*                                      (long floating point)     @91A*/         
*       3 R748SCWO CHAR(8),         /* Total number of synchronous I/O          
*                                      cache write operations.                  
*                                      (long floating point)     @91A*/         
*       3 R748SCWS CHAR(8),         /* Number of successful synchronous         
*                                      I/O cache write operations.              
*                                      (long floating point)     @91A*/         
*       3 R748SCWT CHAR(8),         /* Synchronous I/O cache write              
*                                      accumulated time in milliseconds         
*                                      (long floating point)     @91A*/         
*       3 R748SNBW CHAR(8),         /* Synchronous I/O NVS bytes                
*                                      written in units of 128K bytes.          
*                                      (long floating point)     @91A*/         
*       3 R748SNWO CHAR(8),         /* Total number of synchronous I/O          
*                                      NVS write operations.                    
*                                      (long floating point)     @91A*/         
*       3 R748SNWS CHAR(8),         /* Number of successful synchronous         
*                                      I/O NVS write operations.                
*                                      (long floating point)     @91A*/         
*       3 R748SNWT CHAR(8);         /* Synchronous I/O NVS write                
*                                      accumulated time in milliseconds         
*                                      (long floating point)     @91A*/         
