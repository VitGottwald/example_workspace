*     2       STVOJOB_64  PTR(64),   /* Address of associated job       23060100
*                                       queue data element - STATJQ     23060800
*                                       (64-bit)             @Z21L64B*/ 23061500
*     2       STVOJNXT_64 PTR(64),   /* Address of next verbose         23062200
*                                       SYSOUT element for JOB          23062900
*                                       JOB (64-bit)         @Z21L64B*/ 23063600
*     2       STVOSOUT_64 PTR(64),   /* Address of associated SYSOUT    23064300
*                                       data element -STATSE            23065000
*                                       (64-bit)             @Z21L64B*/ 23065700
*     2       STVOSNXT_64 PTR(64);   /* Address of next verbose         23066400
*                                       SYSOUT element for STATSE       23067100
*                                       (64-bit)             @Z21L64B*/ 23067800
*/********************************************************** @Z07LXST*/ 23070000
*/*                                                          @Z07LXST*/ 23080000
*/*      SYSOUT Verbose Element First Header Section         @Z07LXST*/ 23090000
*/*                                                          @Z07LXST*/ 23100000
*/********************************************************** @Z07LXST*/ 23110000
* DCL STSV1MOD FIXED(8) CONSTANT(0); /* First Header Section            23120000
*                                       modifier             @Z07LXST*/ 23130000
*  DECLARE                                                              23140000
*   1       STATSVHD      BASED,     /* SYSOUT verbose header section   23150000
*                                                            @Z07LXST*/ 23160000
*     2       STSVLEN     FIXED(16), /* Length of entire verbose        23170000
*                                       SYSOUT header (max value is     23180000
*                                       65535)               @Z07LXST*/ 23190000
*     2       STSVTYPE    FIXED(8),  /* Type of this header  @Z07LXST*/ 23200000
*     2       STSVMOD     FIXED(8);  /* Modifier             @Z07LXST*/ 23210000
* DCL STSVSIZE FIXED(15) CONSTANT(LENGTH(STATSVHD));  /* Size of 1st    23220000
*                                       Header Section       @Z07LXST*/ 23230000
*/********************************************************** @Z07LXST*/ 23240000
*/*                                                          @Z07LXST*/ 23250000
*/*      SYSOUT Element Slow/Verbose Section                 @Z07LXST*/ 23260000
*/*                                                          @Z07LXST*/ 23270000
*/*      Data in this section is "hard" for the JES to       @Z07LXST*/ 23280000
*/*      fetch.  Disk I/O is necessary to get this           @Z07LXST*/ 23290000
*/*      information.                                        @Z07LXST*/ 23300000
*/*                                                          @Z07LXST*/ 23310000
*/********************************************************** @Z07LXST*/ 23320000
* DCL STVSVMOD FIXED(8) CONSTANT(0); /* Verbose section modifier        23330000
*                                                            @Z07LXST*/ 23340000
*  DECLARE                                                              23350000
*   1       STATSEVB      BASED,     /* SYSOUT verbose section          23360000
*                                                            @Z07LXST*/ 23370000
*     2       STVSLEN     FIXED(16), /* Length of verbose section       23380000
