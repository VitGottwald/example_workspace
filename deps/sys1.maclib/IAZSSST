*     2       STS3WSI     FIXED(32)  BDY(BYTE);                         22026000
*                                    /* Work Selection Identifier       22027000
*                                                            @Z13LSSF*/ 22030000
*/********************************************************** @Z11LTJN*/ 22030400
*/*                                                          @Z11LTJN*/ 22030800
*/*      SYSOUT Transaction (APPC) Terse Sectio              @Z11LTJN*/ 22031200
*/*                                                          @Z11LTJN*/ 22031600
*/********************************************************** @Z11LTJN*/ 22032000
* DCL STSATMOD FIXED(8) CONSTANT(0); /* SYSOUT transaction section      22032400
*                                       modifier             @Z11LTJN*/ 22032800
*  DECLARE                                                 /*@Z11LTJN*/ 22033200
*   1       STATSATR      BASED,     /* SYSOUT transaction terse        22033600
*                                       section              @Z11LTJN*/ 22034000
