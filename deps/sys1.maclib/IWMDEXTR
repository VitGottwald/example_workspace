.***AC.PLV01  PROCESS PLISTVER                                      *   00082800
.********************************************************************   00082900
&XPLISTVER      SETA  0                                                 00083000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00083100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00083200
&XPLISTVER      SETA  1                                                 00083300
.ZPLV0          ANOP  ,                                                 00083400
.*                                                                      00083500
                AIF   ('&TYPE' EQ '').ZPLV1E                            00083600
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00083700
&XPLISTVER      SETA  1                                                 00083800
.ZPLV1E         ANOP  ,                                                 00083900
.*                                                                      00084000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00084100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00084200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00084300
.*-------------------------------------------------------------------   00084400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00084500
&ZMS            SETC  '"PLISTVER='                                      00084600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00084700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00084800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00084900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00085000
                AGO   .ZPVCK2                                           00085100
.ZPVCK3         ANOP  ,                                                 00085200
.*-------------------------------------------------------------------   00085300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00085400
&ZMS            SETC  '"PLISTVER='                                      00085500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00085600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00085700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00085800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00085900
                AGO   .ZPVCKE                                           00086000
.ZPVCK2         ANOP  ,                                                 00086100
.*-------------------------------------------------------------------   00086200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00086300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00086400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00086500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00086600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00086700
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00086800
.ZPVCK6         ANOP  ,                                                 00086900
&ZMS            SETC  '"PLISTVER='                                      00087000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00087100
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00087200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00087300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00087400
&XPLISTVER      SETA  1                                                 00087500
                AGO   .ZPVCKE                                           00087600
.*-------------------------------------------------------------------   00087700
.ZPVCK4A        ANOP  ,                                                 00087800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00087900
&ZMS            SETC  'PLISTVER='                                       00088000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00088100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00088200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00088300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00088400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00088500
                AGO   .ZPVCK5                                           00088600
.ZPVCK4         ANOP  ,                                                 00088700
&XPLISTVER      SETA  &PLISTVER(1)                                      00088800
.ZPVCK5         ANOP  ,                                                 00088900
.ZPVCKE         ANOP  ,                                                 00089000
.********************************************************************   00089100
