.*******************************************************************    00194100
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00194200
.*******************************************************************    00194300
                      ACTR  10000                                       00194400
.*                                                                      00194500
                      LCLA  &ZLC           COLUMN POSITION              00194600
                      LCLA  &ZOPX          LIST TABLE INDEX             00194700
                      LCLC  &ZLL           LABEL                        00194800
                      LCLC  &ZLOP          OPERATION                    00194900
                      LCLC  &ZLV           VALUE                        00195000
                      LCLC  &ZLD           DESCRIPTION                  00195100
.*                                                                      00195200
&ZOPX                 SETA  1                                           00195300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00195400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00195500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00195600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00195700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00195800
                      AIF   (K'&ZLD LE 0).ZL5                           00195900
&ZLC                  SETA  35             COMMENT COLUMN               00196000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00196100
&ZLC                  SETA  33             COMMMENT COLUMN              00196200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00196300
&ZLV                  SETC  '&ZLV'.' '                                  00196400
                      AGO   .ZL3                                        00196500
.*                                                                      00196600
.ZL4                  ANOP  ,                                           00196700
&ZLV                  SETC  '&ZLV'.' '                                  00196800
.ZL5                  ANOP  ,                                           00196900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00197000
.*                                                                      00197100
&ZLL     &ZLOP &ZLV                                                     00197200
.*                                                                      00197300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00197400
                      AGO   .ZL2           CONTINUE LOOPING             00197500
.*                                                                      00197600
.ZLX                  ANOP  ,                                           00197700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00197800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00197900
         L     14,16(0,0)              GET CVT ADDRESS                  00198000
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00198100
         LAE   14,0(14,0)              CLEAR AR 14                      00198200
.NOAR    ANOP                                                           00198300
         TM    CVTOSLV1-CVT(14),X'10'      TEST FOR SP520 RELEASE       00198400
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00198500
         LA    0,IWMRSNCODENOPOLMGT    SET REASON CODE = NO WLM SVCS    00198600
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00198700
         B     END&SYSNDX              BRANCH TO END LABEL              00198800
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00198900
         L     14,772(14,0)            GET SFT ADDRESS                  00199000
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00199100
         LA    15,32                   GET EX FOR SERVICE ROUTINE       00199200
         OR    14,15                   GET PC NUMBER                    00199300
         PC    0(14)                                                    00199400
END&SYSNDX DS    0H                    END LABEL                        00199500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00199600
.ZLINKX               ANOP  ,                                           00199700
.*******************************************************************    00199800
