.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00123700
.********************************************************************   00123800
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00123900
                      LCLA  &ZOPN1         INVOCATION INDEX             00124000
                      LCLC  &ZOPL(4095)    LABEL                        00124100
                      LCLC  &ZOPO(4095)    OPERATION                    00124200
                      LCLC  &ZOPV(4095)    VALUE                        00124300
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00124400
.*                                                                      00124500
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00124600
.******************************************************************     00124700
.***AL.L10 IWMCPAFN FMT=LIST MF=S!L PARAMETERS                          00124800
.******************************************************************     00124900
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00125000
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00125100
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00125200
&XMFATTR              SETC  '0D'                                        00125300
.*                                                                      00125400
&ZOPN                 SETA  &ZOPN+1                                     00125500
&ZOPL(&ZOPN)          SETC  ' '                                         00125600
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00125700
&ZOPO(&ZOPN)          SETC  'B'                                         00125800
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00125900
                      AGO   .ZALVL6B                                    00126000
.ZALVL6A              ANOP                                              00126100
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00126200
&ZOPV(&ZOPN)          SETC  '4,8'                                       00126300
&ZOPN                 SETA  &ZOPN+1                                     00126400
&ZOPL(&ZOPN)          SETC  ' '                                         00126500
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00126600
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00126700
.ZALVL6B              ANOP                                              00126800
&ZOPD(&ZOPN)          SETC  '++ IWMCPAFN ARG PARMS'                     00126900
.*                                                                      00127000
&ZOPN                 SETA  &ZOPN+1                                     00127100
&ZOPL(&ZOPN)          SETC  ' '                                         00127200
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00127300
&ZOPV(&ZOPN)          SETC  '1'                                         00127400
&ZOPD(&ZOPN)          SETC  ' '                                         00127500
.*                                                                      00127600
.ZLMFSL2              ANOP  ,                                           00127700
.*-------------------------------------------------------------------   00127800
.***AM.L00  IWMCPAFN FMT=LIST name                                      00127900
