.***AC.PLV01  PROCESS PLISTVER                                      *   00069600
.********************************************************************   00069700
&XPLISTVER      SETA  0                                                 00069800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00069900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00070000
&XPLISTVER      SETA  0                                                 00070100
.ZPLV0          ANOP  ,                                                 00070200
.*                                                                      00070300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00070400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00070500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00070600
.*-------------------------------------------------------------------   00070700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00070800
&ZMS            SETC  '"PLISTVER='                                      00070900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00071100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00071200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00071300
                AGO   .ZPVCK2                                           00071400
.ZPVCK3         ANOP  ,                                                 00071500
.*-------------------------------------------------------------------   00071600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00071700
&ZMS            SETC  '"PLISTVER='                                      00071800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00071900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00072000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00072100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00072200
                AGO   .ZPVCKE                                           00072300
.ZPVCK2         ANOP  ,                                                 00072400
.*-------------------------------------------------------------------   00072500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00072600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00072700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00072800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00072900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00073000
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00073100
.ZPVCK6         ANOP  ,                                                 00073200
&ZMS            SETC  '"PLISTVER='                                      00073300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00073400
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00073500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00073600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00073700
&XPLISTVER      SETA  0                                                 00073800
                AGO   .ZPVCKE                                           00073900
.*-------------------------------------------------------------------   00074000
.ZPVCK4A        ANOP  ,                                                 00074100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00074200
&ZMS            SETC  'PLISTVER='                                       00074300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00074400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00074500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00074600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00074700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00074800
                AGO   .ZPVCK5                                           00074900
.ZPVCK4         ANOP  ,                                                 00075000
&XPLISTVER      SETA  &PLISTVER(1)                                      00075100
.ZPVCK5         ANOP  ,                                                 00075200
.ZPVCKE         ANOP  ,                                                 00075300
.********************************************************************   00075400
.***AG.MF01 PROCESS MF MACRO FORMS                                      00075500
