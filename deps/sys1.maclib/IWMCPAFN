                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00152100
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00152200
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00152300
