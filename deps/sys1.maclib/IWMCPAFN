.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00158200
.*******************************************************************    00158300
                      ACTR  10000                                       00158400
.*                                                                      00158500
                      LCLA  &ZLC           COLUMN POSITION              00158600
                      LCLA  &ZOPX          LIST TABLE INDEX             00158700
                      LCLC  &ZLL           LABEL                        00158800
                      LCLC  &ZLOP          OPERATION                    00158900
                      LCLC  &ZLV           VALUE                        00159000
                      LCLC  &ZLD           DESCRIPTION                  00159100
.*                                                                      00159200
&ZOPX                 SETA  1                                           00159300
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00159400
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00159500
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00159600
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00159700
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159800
                      AIF   (K'&ZLD LE 0).ZL5                           00159900
&ZLC                  SETA  35             COMMENT COLUMN               00160000
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00160100
&ZLC                  SETA  33             COMMMENT COLUMN              00160200
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00160300
&ZLV                  SETC  '&ZLV'.' '                                  00160400
                      AGO   .ZL3                                        00160500
.*                                                                      00160600
.ZL4                  ANOP  ,                                           00160700
&ZLV                  SETC  '&ZLV'.' '                                  00160800
.ZL5                  ANOP  ,                                           00160900
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00161000
.*                                                                      00161100
&ZLL     &ZLOP &ZLV                                                     00161200
.*                                                                      00161300
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00161400
                      AGO   .ZL2           CONTINUE LOOPING             00161500
.*                                                                      00161600
.ZLX                  ANOP  ,                                           00161700
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00161800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00161900
         L     14,16(0,0)              GET CVT ADDRESS                  00162000
         L     14,772(14,0)            GET SFT ADDRESS                  00162100
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00162200
         LA    15,96                   GET EX FOR SERVICE ROUTINE       00162300
         OR    14,15                   GET PC NUMBER                    00162400
         PC    0(14)                                                    00162500
END&SYSNDX DS    0H                    END LABEL                        00162600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00162700
