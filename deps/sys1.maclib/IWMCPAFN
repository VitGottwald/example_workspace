.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00064700
.********************************************************************   00064800
              GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL                 00064900
              GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE          00065000
              GBLA  &SYSALVL    SYSSTATE ARCHLVL                        00065100
              GBLC  &SYSAM64    SYSSTATE AMODE64                        00065200
.*                                                                      00065300
              AIF   (K'&SYSASCE GT 0).ZSYS4     SYS STATE DEFINED       00065400
              AIF   (K'&SYSSPLV GT 0).ZSYS2     SP LEVEL DEFINED        00065500
              SPLEVEL  TEST                                             00065600
.ZSYS2        ANOP  ,                                                   00065700
              AIF   ('&SYSSPLV' EQ '1').ZSYS3   PRE SP 3.1              00065800
              AIF   ('&SYSSPLV' EQ '2').ZSYS3   PRE SP 3.1              00065900
              SYSSTATE TEST                                             00066000
              AGO   .ZSYS4                                              00066100
.ZSYS3        ANOP  ,                                                   00066200
&SYSASCE      SETC  'P'         ASSUME SYSSTATE PRIMARY                 00066300
.ZSYS4        ANOP  ,                                                   00066400
              AIF   ('&SYSAM64' NE 'YES').ZSYSN64                       00066500
&ZMAC_LLGT    SETC 'LLGT'                                               00066600
&ZMAC_LGR     SETC 'LGR'                                                00066700
&ZMAC_LG      SETC 'LG'                                                 00066800
&ZMAC_STG     SETC 'STG'                                                00066900
.ZSYSN64      ANOP  ,                                                   00067000
