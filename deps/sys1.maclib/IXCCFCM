&ZOPN1                SETA &ZOPN                                        00130300
.*******************************************************************    00130400
.***  PROCESS IXCCFCM OUTPUT PARAMETERS                                 00130500
.*******************************************************************    00130600
                      ACTR  10000                                       00130700
.*******************************************************************    00130800
.***AO.X02  RETURN CODE PROCESSING                                      00130900
.*******************************************************************    00131000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00131100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00131200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00131300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00131400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00131500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00131600
&ZOPN                 SETA  &ZOPN+1                                     00131700
&ZOPL(&ZOPN)          SETC  ' '                                         00131800
&ZOPO(&ZOPN)          SETC  'ST'                                        00131900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00132000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00132100
                      AGO   .ZMRETX                                     00132200
.*                                                                      00132300
.ZMRET2               ANOP  ,                                           00132400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00132500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00132600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00132700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00132800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00132900
&ZOPN                 SETA  &ZOPN+1                                     00133000
&ZOPL(&ZOPN)          SETC  ' '                                         00133100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00133200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00133300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00133400
.ZMRETX               ANOP  ,                                           00133500
.*******************************************************************    00133600
.***AO.X03  REASON CODE PROCESSING                                      00133700
.*******************************************************************    00133800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00133900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00134000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00134100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00134200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00134300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00134400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00134500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00134600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00134700
&ZOPN                 SETA  &ZOPN+1                                     00134800
&ZOPL(&ZOPN)          SETC  ' '                                         00134900
&ZOPO(&ZOPN)          SETC  'ST'                                        00135000
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00135100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00135200
                      AGO   .ZMRSNX                                     00135300
.*                                                                      00135400
.ZMRSN2               ANOP  ,                                           00135500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00135600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00135700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00135800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00135900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00136000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00136100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00136200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00136300
&ZOPN                 SETA  &ZOPN+1                                     00136400
&ZOPL(&ZOPN)          SETC  ' '                                         00136500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00136600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00136700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00136800
