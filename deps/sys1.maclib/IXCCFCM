.*******************************************************************    00137200
.ZMFLX                ANOP  ,              MF=L PROCESSING              00137300
.*******************************************************************    00137400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00137500
.*******************************************************************    00137600
                      ACTR  10000                                       00137700
.*                                                                      00137800
                      LCLA  &ZLC           COLUMN POSITION              00137900
                      LCLA  &ZOPX          LIST TABLE INDEX             00138000
                      LCLC  &ZLL           LABEL                        00138100
                      LCLC  &ZLOP          OPERATION                    00138200
                      LCLC  &ZLV           VALUE                        00138300
                      LCLC  &ZLD           DESCRIPTION                  00138400
.*                                                                      00138500
&ZOPX                 SETA  1                                           00138600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00138700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00138800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00138900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00139000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00139100
                      AIF   (K'&ZLD LE 0).ZL5                           00139200
&ZLC                  SETA  35             COMMENT COLUMN               00139300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00139400
&ZLC                  SETA  33             COMMMENT COLUMN              00139500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00139600
&ZLV                  SETC  '&ZLV'.' '                                  00139700
                      AGO   .ZL3                                        00139800
.*                                                                      00139900
.ZL4                  ANOP  ,                                           00140000
&ZLV                  SETC  '&ZLV'.' '                                  00140100
.ZL5                  ANOP  ,                                           00140200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00140300
.*                                                                      00140400
&ZLL     &ZLOP &ZLV                                                     00140500
.*                                                                      00140600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00140700
                      AGO   .ZL2           CONTINUE LOOPING             00140800
.*                                                                      00140900
.ZLX                  ANOP  ,                                           00141000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00141100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00141200
         L     14,16(0,0)              GET CVT ADDRESS                  00141300
         L     14,772(14,0)            GET SFT ADDRESS                  00141400
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00141500
         LA    15,30                   GET EX FOR SERVICE ROUTINE       00141600
         OR    14,15                   GET PC NUMBER                    00141700
         PC    0(14)                                                    00141800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00141900
.ZLINKX               ANOP  ,                                           00142000
.*******************************************************************    00142100
.***AP.X01  FORMAT GENERATED CODE                                       00142200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00142300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00142400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00142500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00142600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00142700
                      AIF   (K'&ZLD LE 0).ZL51                          00142800
&ZLC                  SETA  35             COMMENT COLUMN               00142900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00143000
&ZLC                  SETA  33             COMMENT COLUMN               00143100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00143200
&ZLV                  SETC  '&ZLV'.' '                                  00143300
                      AGO   .ZL31                                       00143400
.*                                                                      00143500
.ZL41                 ANOP  ,                                           00143600
&ZLV                  SETC  '&ZLV'.' '                                  00143700
.ZL51                 ANOP  ,                                           00143800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00143900
.*                                                                      00144000
&ZLL     &ZLOP &ZLV                                                     00144100
.*                                                                      00144200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00144300
                      AGO   .ZL21          CONTINUE LOOPING             00144400
.*                                                                      00144500
.ZLX1                 ANOP  ,                                           00144600
