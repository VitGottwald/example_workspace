.***AB.05  INVOKE MVS MACROS TO CONTROL CODE GENERATION                 00043200
.********************************************************************   00043300
                  GBLC  &SYSSPLV    SP LEVEL 1,2,3 -SPLEVEL             00043400
                  GBLC  &SYSASCE    SYSTEM STATE P ANY AR-SYSSTATE      00043500
                  GBLA  &SYSALVL    SYSSTATE ARCHLVL                    00043600
                  GBLC  &SYSAM64    SYSSTATE AMODE64                    00043700
.*                                                                      00043800
                  AIF   (K'&SYSASCE GT 0).ZSYS4  SYS STATE DEFINED      00043900
                  AIF   (K'&SYSSPLV GT 0).ZSYS2  SP LEVEL DEFINED       00044000
                  SPLEVEL  TEST                                         00044100
.ZSYS2            ANOP  ,                                               00044200
                  AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1           00044300
                  AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1           00044400
                  SYSSTATE TEST                                         00044500
                  AGO   .ZSYS4                                          00044600
.ZSYS3            ANOP  ,                                               00044700
&SYSASCE          SETC  'P'         ASSUME SYSSTATE PRIMARY             00044800
.ZSYS4            ANOP  ,                                               00044900
                  AIF   ('&SYSAM64' NE 'YES').ZSYSN64                   00045000
&ZMAC_LLGT        SETC 'LLGT'                                           00045100
&ZMAC_LGR         SETC 'LGR'                                            00045200
&ZMAC_LG          SETC 'LG'                                             00045300
&ZMAC_STG         SETC 'STG'                                            00045400
.ZSYSN64          ANOP  ,                                               00045500
