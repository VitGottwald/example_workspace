.***AO.X04  LABEL FOR MF=L PROCESSING                                   00896900
.*******************************************************************    00897000
.ZMFLX                ANOP  ,              MF=L PROCESSING              00897100
.*******************************************************************    00897200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00897300
.*******************************************************************    00897400
                      ACTR  10000                                       00897500
.*                                                                      00897600
                      LCLA  &ZLC           COLUMN POSITION              00897700
                      LCLA  &ZOPX          LIST TABLE INDEX             00897800
                      LCLC  &ZLL           LABEL                        00897900
                      LCLC  &ZLOP          OPERATION                    00898000
                      LCLC  &ZLV           VALUE                        00898100
                      LCLC  &ZLD           DESCRIPTION                  00898200
.*                                                                      00898300
&ZOPX                 SETA  1                                           00898400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00898500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00898600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00898700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00898800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00898900
                      AIF   (K'&ZLD LE 0).ZL5                           00899000
&ZLC                  SETA  35             COMMENT COLUMN               00899100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00899200
&ZLC                  SETA  33             COMMMENT COLUMN              00899300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00899400
&ZLV                  SETC  '&ZLV'.' '                                  00899500
                      AGO   .ZL3                                        00899600
.*                                                                      00899700
.ZL4                  ANOP  ,                                           00899800
&ZLV                  SETC  '&ZLV'.' '                                  00899900
.ZL5                  ANOP  ,                                           00900000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00900100
.*                                                                      00900200
&ZLL     &ZLOP &ZLV                                                     00900300
.*                                                                      00900400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00900500
                      AGO   .ZL2           CONTINUE LOOPING             00900600
.*                                                                      00900700
.ZLX                  ANOP  ,                                           00900800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00900900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00901000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00901100
         AIF   ('&SYSAM64' NE 'YES').NOT_AM64                           00901200
&ZMAC_L  SETC  'LLGT'                                                   00901300
.NOT_AM64 ANOP                                                          00901400
         &ZMAC_L 15,16(0,0)         Get CVT address                     00901500
         AIF   ('&SYSASCE' EQ 'P').NOT_AR_1                             00901600
         LAE   15,0(15,0)           Clear AR 15                         00901700
.NOT_AR_1 ANOP                                                          00901800
         L     15,140(15,0)         GET ECVT ADDRESS                    00901900
         L     15,204(15,0)         GET CTBL ADDRESS                    00902000
         AIF   ('&XREMOTE' EQ 'YES').REM1A                              00902100
         L     15,300(15,0)         Get HZSTABLE address                00902200
         AGO   .REM1B                                                   00902300
.REM1A   ANOP                                                           00902400
         ICM   15,B'1111',300(15)   Get HZSTABLE address                00902500
         JZ    HZSADDCKNA_&SYSNDX   No HZSTABLE, get out now            00902600
         TM    1240(15),X'80'       Check if commands accepted          00902700
         JZ    HZSADDCKNA_&SYSNDX   No cmds accepted, get out now       00902800
.REM1B   ANOP                                                           00902900
         AIF   (&SYSALVL GE 2).IS_ZARCH_1                               00903000
         CLI   163(0),00            Z-Architecture?                     00903100
         JE    HZSADDCKESA390_&SYSNDX                                   00903200
.IS_ZARCH_1 ANOP                                                        00903300
         LMH   15,15,1372(15)                                           00903400
         AIF   (&SYSALVL GE 2).IS_ZARCH_2                               00903500
HZSADDCKESA390_&SYSNDX DS 0H                                            00903600
.IS_ZARCH_2 ANOP                                                        00903700
         AIF   ('&XREMOTE' EQ 'YES').REM2A                              00903800
         L     15,1304(15,0)        Get HZSADDCK PC NUM                 00903900
         AGO   .REM2B                                                   00904000
.REM2A   ANOP                                                           00904100
         ICM   15,B'1111',1304(15)  Get HZSADDCK PC NUM                 00904200
         JZ    HZSADDCKNA_&SYSNDX   No PC Num, get out now              00904300
.REM2B   ANOP                                                           00904400
         PC    0(15)                PC to handle HZSADDCK request       00904500
         AIF   ('&XREMOTE' NE 'YES').REM3B                              00904600
         J     HZSADDCKEND_&SYSNDX  Branch around seting bad RC         00904700
HZSADDCKNA_&SYSNDX DS 0H                                                00904800
         LA    15,X'C'              Set RC (HC not active)              00904900
         LA    0,X'C01'             Set RSN not active                  00905000
HZSADDCKEND_&SYSNDX DS 0H                                               00905100
.REM3B   ANOP                                                           00905200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00905300
.ZLINKX               ANOP  ,                                           00905400
.*******************************************************************    00905500
.***AP.X01  FORMAT GENERATED CODE                                       00905600
