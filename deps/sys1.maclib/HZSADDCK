.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00192600
.*******************************************************************    00192700
                  LCLC  &ZXDUMMY2   ++ INPUT NAME                       00192800
                  LCLC  &ZXCHECKOWNER           ++ INPUT NAME           00192900
                  LCLC  &ZXOWNER    ++ INPUT NAME                       00193000
                  LCLC  &ZXDUMMY1   ++ INPUT NAME                       00193100
                  LCLC  &ZXCHECKNAME            ++ INPUT NAME           00193200
                  LCLC  &ZXNAME     ++ INPUT NAME                       00193300
                  LCLC  &ZXREMOTE   ++ INPUT VALUE                      00193400
                  LCLC  &ZXCHECKROUTINE         ++ INPUT NAME           00193500
                  LCLC  &ZXENTRYCODE            ++ INPUT NAME           00193600
                  LCLC  &ZXEXITRTN  ++ INPUT NAME                       00193700
                  LCLC  &ZXMSGTBL   ++ INPUT NAME                       00193800
                  LCLC  &ZXUSS      ++ INPUT VALUE                      00193900
                  LCLC  &ZXSECCHECK             ++ INPUT VALUE          00194000
                  LCLC  &ZXREXX     ++ INPUT VALUE                      00194100
                  LCLC  &ZXHANDLE   ++ OUTPUT NAME                      00194200
                  LCLC  &ZXPETOKEN  ++ INPUT NAME                       00194300
                  LCLC  &ZXPETOKENAUTH          ++ INPUT VALUE          00194400
                  LCLC  &ZXEXEC     ++ INPUT NAME                       00194500
                  LCLC  &ZXTIMELIMIT            ++ INPUT NAME           00194600
                  LCLC  &ZXREXXHLQ  ++ INPUT NAME                       00194700
                  LCLC  &ZXREXXTSO  ++ INPUT VALUE                      00194800
                  LCLC  &ZXREXXIN   ++ INPUT VALUE                      00194900
                  LCLC  &ZXDATE     ++ INPUT NAME                       00195000
                  LCLC  &ZXREASON   ++ INPUT NAME                       00195100
                  LCLC  &ZXREASONLEN            ++ INPUT NAME           00195200
                  LCLC  &ZXPARMS    ++ INPUT NAME                       00195300
                  LCLC  &ZXPARMSLEN             ++ INPUT NAME           00195400
                  LCLC  &ZXSCOPE    ++ INPUT VALUE                      00195500
                  LCLC  &ZXLOCAL    ++ INPUT VALUE                      00195600
                  LCLC  &ZXGLOBAL   ++ INPUT VALUE                      00195700
                  LCLC  &ZXSTATE    ++ INPUT VALUE                      00195800
                  LCLC  &ZXACTIVE   ++ INPUT VALUE                      00195900
                  LCLC  &ZXINACTIVE             ++ INPUT VALUE          00196000
                  LCLC  &ZXSEVERITY             ++ INPUT VALUE          00196100
                  LCLC  &ZXALLOWDYNSEV          ++ INPUT VALUE          00196200
                  LCLC  &ZXDOM      ++ INPUT VALUE                      00196300
                  LCLC  &ZXINTERVAL             ++ INPUT VALUE          00196400
                  LCLC  &ZXEIHOURS  ++ INPUT NAME                       00196500
                  LCLC  &ZXEIMINUTES            ++ INPUT NAME           00196600
                  LCLC  &ZXHOURS    ++ INPUT NAME                       00196700
                  LCLC  &ZXMINUTES  ++ INPUT NAME                       00196800
                  LCLC  &ZXEINTERVAL            ++ INPUT VALUE          00196900
                  LCLC  &ZXVERBOSE  ++ INPUT VALUE                      00197000
                  LCLC  &ZXCMDI     ++ INPUT NAME                       00197100
                  LCLC  &ZXRETCODE  ++ OUTPUT NAME                      00197200
                  LCLC  &ZXRSNCODE  ++ OUTPUT NAME                      00197300
                  LCLC  &ZXPLISTVER             ++ INPUT VALUE          00197400
                  LCLC  &ZXMF       ++ INPUT VALUE                      00197500
                  LCLC  &ZXMFCTRL   ++ INPUT NAME                       00197600
                  LCLC  &ZXMFATTR   ++ INPUT VALUE                      00197700
                  LCLC  &ZXMFSCK    ++ INPUT VALUE                      00197800
.********************************************************************   00197900
.***AC.PLV01  PROCESS PLISTVER                                      *   00198000
.********************************************************************   00198100
&XPLISTVER      SETA  0                                                 00198200
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00198300
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00198400
&XPLISTVER      SETA  1                                                 00198500
.ZPLV0          ANOP  ,                                                 00198600
.*                                                                      00198700
                AIF   ('&HANDLE' EQ '').ZPLV1E                          00198800
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00198900
&XPLISTVER      SETA  1                                                 00199000
.ZPLV1E         ANOP  ,                                                 00199100
.*                                                                      00199200
                AIF   ('&PETOKEN' EQ '').ZPLV2E                         00199300
                AIF   (&XPLISTVER GE 1).ZPLV2E                          00199400
&XPLISTVER      SETA  1                                                 00199500
.ZPLV2E         ANOP  ,                                                 00199600
.*                                                                      00199700
                AIF   ('&EXEC' EQ '').ZPLV3E                            00199800
                AIF   (&XPLISTVER GE 1).ZPLV3E                          00199900
&XPLISTVER      SETA  1                                                 00200000
.ZPLV3E         ANOP  ,                                                 00200100
.*                                                                      00200200
                AIF   ('&TIMELIMIT' EQ '').ZPLV4E                       00200300
                AIF   (&XPLISTVER GE 1).ZPLV4E                          00200400
&XPLISTVER      SETA  1                                                 00200500
.ZPLV4E         ANOP  ,                                                 00200600
.*                                                                      00200700
                AIF   ('&REXXHLQ' EQ '').ZPLV5E                         00200800
                AIF   (&XPLISTVER GE 1).ZPLV5E                          00200900
&XPLISTVER      SETA  1                                                 00201000
.ZPLV5E         ANOP  ,                                                 00201100
.*                                                                      00201200
                AIF   ('&CMDI' EQ '').ZPLV6E                            00201300
                AIF   (&XPLISTVER GE 1).ZPLV6E                          00201400
&XPLISTVER      SETA  1                                                 00201500
.ZPLV6E         ANOP  ,                                                 00201600
.*                                                                      00201700
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00201800
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00201900
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00202000
.*-------------------------------------------------------------------   00202100
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00202200
&ZMS            SETC  '"PLISTVER='                                      00202300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00202400
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00202500
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00202600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00202700
                AGO   .ZPVCK2                                           00202800
.ZPVCK3         ANOP  ,                                                 00202900
.*-------------------------------------------------------------------   00203000
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00203100
&ZMS            SETC  '"PLISTVER='                                      00203200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00203300
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00203400
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00203500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00203600
                AGO   .ZPVCKE                                           00203700
.ZPVCK2         ANOP  ,                                                 00203800
.*-------------------------------------------------------------------   00203900
&ZCVAL          SETC  '&PLISTVER(1)'                                    00204000
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00204100
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00204200
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00204300
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00204400
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00204500
.ZPVCK6         ANOP  ,                                                 00204600
&ZMS            SETC  '"PLISTVER='                                      00204700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00204800
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00204900
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00205000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00205100
&XPLISTVER      SETA  1                                                 00205200
                AGO   .ZPVCKE                                           00205300
.*-------------------------------------------------------------------   00205400
.ZPVCK4A        ANOP  ,                                                 00205500
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00205600
&ZMS            SETC  'PLISTVER='                                       00205700
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00205800
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00205900
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00206000
                MNOTE 8,' &ZMS IS REQUIRED.'                            00206100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00206200
                AGO   .ZPVCK5                                           00206300
.ZPVCK4         ANOP  ,                                                 00206400
&XPLISTVER      SETA  &PLISTVER(1)                                      00206500
.ZPVCK5         ANOP  ,                                                 00206600
.ZPVCKE         ANOP  ,                                                 00206700
.********************************************************************   00206800
