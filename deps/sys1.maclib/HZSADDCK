                  ACTR  10000                                           00709300
.*-------------------------------------------------------------------   00709400
.***AF.P03 EXTRA POSITIONALS AMONG NON-X KEYS                           00709500
&ZCVAL            SETC  ''          ERROR FIELD                         00709600
&ZAWRK            SETA  0           POSITIONAL COUNTER                  00709700
&ZASYSLNDX        SETA  0           NON-X KEY COUNTER                   00709800
.ZELIST2          ANOP  ,                                               00709900
&ZASYSLNDX        SETA  &ZASYSLNDX+1            NEXT NON-X KEY          00710000
         AIF   (&ZASYSLNDX GT N'&SYSLIST).ZELIST3  DONE                 00710100
         AIF   ('&ZCSYSLIST(&ZASYSLNDX)' EQ 'NONXKEY').ZELIST2          00710200
&ZAWRK            SETA  &ZAWRK+1    NEXT POSITIONAL                     00710300
                  AIF   (&ZAWRK LE 0).ZELIST2                           00710400
&ZCVAL            SETC '&ZCVAL,&SYSLIST(&ZASYSLNDX)'  EXTRA             00710500
                  AGO   .ZELIST2                                        00710600
.*                                                                      00710700
.ZELIST3  AIF  (K'&ZCVAL LE 0).ZELISTX                                  00710800
&ZCVAL            SETC '&ZCVAL'(2,K'&ZCVAL-1)   STRIP COMMA             00710900
&ZMS              SETC  'POSITIONAL ARGUMENT(S),'                       00711000
&ZMS              SETC  '&ZMS "&ZCVAL"'                                 00711100
&ZMS              SETC  '&ZMS CANNOT BE USED.'                          00711200
