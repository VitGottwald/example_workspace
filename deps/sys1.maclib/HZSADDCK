&XMFATTR              SETC  '0D'                                        00717700
.*                                                                      00717800
&ZOPN                 SETA  &ZOPN+1                                     00717900
&ZOPL(&ZOPN)          SETC  ' '                                         00718000
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00718100
&ZOPO(&ZOPN)          SETC  'B'                                         00718200
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00718300
                      AGO   .ZALVL6B                                    00718400
.ZALVL6A              ANOP                                              00718500
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00718600
