                      AGO   .ZMRSNX                                     00895100
.*                                                                      00895200
.ZMRSN2               ANOP  ,                                           00895300
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00895400
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00895500
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00895600
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00895700
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00895800
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00895900
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00896000
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00896100
&ZOPN                 SETA  &ZOPN+1                                     00896200
&ZOPL(&ZOPN)          SETC  ' '                                         00896300
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00896400
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00896500
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00896600
.ZMRSNX               ANOP  ,                                           00896700
