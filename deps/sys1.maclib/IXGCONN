*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00612800
*%IXGCONN: MACRO KEYS(REQUEST,STREAMNAME,RMNAME,RMEXIT,RMDATA,          00612900
*                 RMEVENTS,COMPLETEEXIT,AUTH,IMPORTCONNECT,             00613000
*                 STREAMTOKEN,USERDATA,STRUCTNAME,AVGBUFSIZE,           00613100
*                 MAXBUFSIZE,ELEMENTSIZE,LSVERSION,DIAG,ANSAREA,        00613200
*                 ANSLEN,RETCODE,RSNCODE,PLISTVER,MF);                  00613300
*  ANS('?'!!MACLABEL!!' '!!'IXGCONNP '!!MACLIST!!                       00613400
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00613500
*%END IXGCONN;                                                          00613600
