                   AIF   ('&AVGBUFSIZE' NE '').ZG87B  KEYS?             00217200
                   AIF   ('&MAXBUFSIZE' NE '').ZG87B  KEYS?             00217300
                   AIF   ('&ELEMENTSIZE' NE '').ZG87B  KEYS?            00217400
                   AIF   ('&LSVERSION' NE '').ZG87B  KEYS?              00217500
                   AIF   ('&DIAG' NE '').ZG87B  KEYS?                   00217600
