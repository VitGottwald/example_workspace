.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00501400
&ZPLLEN               SETA 186                                          00501500
.*                                                                      00501600
.******************************************************************     00501700
.***AI.L01 IXGCONN FMT=LIST FILL IN PARAMETER AREA                      00501800
.******************************************************************     00501900
                      ACTR  10000                                       00502000
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00502100
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00502200
&ZCVAL                SETC  'LAE'          XAX MODE                     00502300
.ZIMFXA               ANOP ,                                            00502400
.*                                                                      00502500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00502600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00502700
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00502800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00502900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00503000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00503100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00503200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00503300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00503400
&ZOPN                 SETA  &ZOPN+1                                     00503500
&ZOPL(&ZOPN)          SETC  ' '                                         00503600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00503700
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00503800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00503900
                      AGO   .ZIMFNX                                     00504000
.*                                                                      00504100
.ZIMFN2               ANOP ,                                            00504200
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00504300
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00504400
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00504500
&ZOPN                 SETA  &ZOPN+1                                     00504600
&ZOPL(&ZOPN)          SETC  ' '                                         00504700
&ZOPO(&ZOPN)          SETC  'LAE'                                       00504800
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00504900
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00505000
                      AGO   .ZIMFNX                                     00505100
.ZIMFNX1              ANOP ,                                            00505200
&ZOPN                 SETA  &ZOPN+1                                     00505300
&ZOPL(&ZOPN)          SETC  ' '                                         00505400
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00505500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00505600
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00505700
.ZIMFNX               ANOP ,                                            00505800
.********************************************************************   00505900
