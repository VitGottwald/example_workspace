.***AE.KKWRD04 VALUE, RQD INPUT, ME KEYWORD -MULTIPLE KEYWORDS          00384800
&ZI                SETA  0           XVARIABLE ARRAY INDEX              00384900
&ZJ                SETA  0           SUBLIST INDEX                      00385000
.ZE16A             ANOP  ,                                              00385100
&ZJ                SETA  &ZJ+1       NEXT SUBLIST                       00385200
                   AIF   (&ZJ GT N'&RMEVENTS).ZE16C  DONE               00385300
                   AIF   (K'&RMEVENTS(&ZJ) LE 0).ZE16A  NULL            00385400
&ZI                SETA  &ZI+1       NEXT ARRAY ENTRY                   00385500
&XRMEVENTS(&ZI)   SETC  ''           NULL ENTRY                         00385600
&XRMEVENTS(&ZI+1) SETC  ''           NULL AT ARRAY END                  00385700
.ZE17B  AIF   ('&RMEVENTS(&ZJ)' NE 'LBWRITE').ZE18B                     00385800
&XRMEVENTS(&ZI)   SETC  'LBWRITE'    SET KEYWORD                        00385900
.ZE18B  AIF   ('&RMEVENTS(&ZJ)' NE 'LBDELETE').ZE19B                    00386000
&XRMEVENTS(&ZI)   SETC  'LBDELETE'   SET KEYWORD                        00386100
.ZE19B             AIF   ('&XRMEVENTS(&ZI)' NE '').ZE16A                00386200
&ZI                SETA  &ZI-1       BACKUP ARRAY                       00386300
&ZMS               SETC  '"RMEVENTS='                                   00386400
&ZMS               SETC  '&ZMS.&RMEVENTS"'                              00386500
&ZMS               SETC  '&ZMS ARGUMENT &ZJ'                            00386600
&ZMS               SETC  '&ZMS IS AN UNKNOWN KEYWORD. '                 00386700
&ZMS               SETC  '&ZMS VALID KEYWORD CHOICE(S) ARE:'            00386800
&ZMS               SETC  '&ZMS "LBWRITE"'                               00386900
&ZMS               SETC  '&ZMS OR "LBDELETE".'                          00387000
                   MNOTE 8,' &ZMS '                                     00387100
&ZMACRET           SETA  8           SET MACRO ERROR RETURN CODE        00387200
                   AGO   .ZE16A                                         00387300
.*-------------------------------------------------------------------   00387400
