.***AE.01  START OF IXGCONN MACRO CODE                                  00462600
.*******************************************************************    00462700
                   LCLC  &M0M0001                 NAME                  00462800
&M0M0001           SETC  '&XLABEL'                SET NAME              00462900
                   AIF   (K'&XLABEL NE 0).XLAB2                         00463000
&M0M0001           SETC  'M00M&SYSNDX'            DEFAULT NAME          00463100
.*                                                                      00463200
.XLAB2             ANOP  ,                                              00463300
                   AIF   ('&XMF' EQ 'L').XLAB3                          00463400
&M0M0001 DS    0H                             IXGCONN-2                 00463500
.XLAB3             ANOP  ,                                              00463600
.*******************************************************************    00463700
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00463800
.*******************************************************************    00463900
              PUSH  PRINT                                               00464000
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     00464100
                   PRINT OFF         ++ PR01                            00464200
                   AGO   .ZPRNTXX                                       00464300
.ZPRINTX           PRINT ON                                             00464400
.ZPRNTXX           ANOP  ,                                              00464500
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00464600
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00464700
.********************************************************************   00464800
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00464900
.********************************************************************   00465000
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00465100
                      LCLA  &ZOPN1         INVOCATION INDEX             00465200
                      LCLC  &ZOPL(4095)    LABEL                        00465300
                      LCLC  &ZOPO(4095)    OPERATION                    00465400
                      LCLC  &ZOPV(4095)    VALUE                        00465500
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00465600
.*                                                                      00465700
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00465800
.******************************************************************     00465900
.***AL.L10 IXGCONN FMT=LIST MF=S!L PARAMETERS                           00466000
.******************************************************************     00466100
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00466200
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00466300
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00466400
&XMFATTR              SETC  '0D'                                        00466500
.*                                                                      00466600
&ZOPN                 SETA  &ZOPN+1                                     00466700
&ZOPL(&ZOPN)          SETC  ' '                                         00466800
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00466900
&ZOPO(&ZOPN)          SETC  'B'                                         00467000
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00467100
                      AGO   .ZALVL6B                                    00467200
.ZALVL6A              ANOP                                              00467300
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00467400
&ZOPV(&ZOPN)          SETC  '4,8'                                       00467500
&ZOPN                 SETA  &ZOPN+1                                     00467600
&ZOPL(&ZOPN)          SETC  ' '                                         00467700
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00467800
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00467900
.ZALVL6B              ANOP                                              00468000
&ZOPD(&ZOPN)          SETC  '++ IXGCONN ARG PARMS'                      00468100
.*                                                                      00468200
&ZOPN                 SETA  &ZOPN+1                                     00468300
&ZOPL(&ZOPN)          SETC  ' '                                         00468400
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00468500
