.***AC.PLV01  PROCESS PLISTVER                                      *   00202500
.********************************************************************   00202600
&XPLISTVER      SETA  1                                                 00202700
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00202800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00202900
&XPLISTVER      SETA  2                                                 00203000
.ZPLV0          ANOP  ,                                                 00203100
.*                                                                      00203200
                AIF   ('&RMNAME' EQ '').ZPLV1E                          00203300
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00203400
&XPLISTVER      SETA  2                                                 00203500
.ZPLV1E         ANOP  ,                                                 00203600
.*                                                                      00203700
                AIF   ('&RMEXIT' EQ '').ZPLV2E                          00203800
                AIF   (&XPLISTVER GE 2).ZPLV2E                          00203900
&XPLISTVER      SETA  2                                                 00204000
.ZPLV2E         ANOP  ,                                                 00204100
.*                                                                      00204200
                AIF   ('&RMDATA' EQ '').ZPLV3E                          00204300
                AIF   (&XPLISTVER GE 2).ZPLV3E                          00204400
&XPLISTVER      SETA  2                                                 00204500
.ZPLV3E         ANOP  ,                                                 00204600
.*                                                                      00204700
                AIF   ('&RMEVENTS' EQ '').ZPLV4E                        00204800
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00204900
&XPLISTVER      SETA  2                                                 00205000
.ZPLV4E         ANOP  ,                                                 00205100
.*                                                                      00205200
                AIF   ('&COMPLETEEXIT' EQ '').ZPLV5E                    00205300
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00205400
&XPLISTVER      SETA  2                                                 00205500
.ZPLV5E         ANOP  ,                                                 00205600
.*                                                                      00205700
                AIF   ('&IMPORTCONNECT' EQ '').ZPLV6E                   00205800
                AIF   (&XPLISTVER GE 2).ZPLV6E                          00205900
&XPLISTVER      SETA  2                                                 00206000
.ZPLV6E         ANOP  ,                                                 00206100
.*                                                                      00206200
                AIF   ('&LSVERSION' EQ '').ZPLV7E                       00206300
                AIF   (&XPLISTVER GE 2).ZPLV7E                          00206400
&XPLISTVER      SETA  2                                                 00206500
.ZPLV7E         ANOP  ,                                                 00206600
.*                                                                      00206700
                AIF   ('&DIAG' EQ '').ZPLV8E                            00206800
                AIF   (&XPLISTVER GE 2).ZPLV8E                          00206900
&XPLISTVER      SETA  2                                                 00207000
.ZPLV8E         ANOP  ,                                                 00207100
.*                                                                      00207200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00207300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00207400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00207500
.*-------------------------------------------------------------------   00207600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00207700
&ZMS            SETC  '"PLISTVER='                                      00207800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00207900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00208000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00208100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00208200
                AGO   .ZPVCK2                                           00208300
.ZPVCK3         ANOP  ,                                                 00208400
.*-------------------------------------------------------------------   00208500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00208600
&ZMS            SETC  '"PLISTVER='                                      00208700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00208800
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00208900
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00209000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00209100
                AGO   .ZPVCKE                                           00209200
.ZPVCK2         ANOP  ,                                                 00209300
.*-------------------------------------------------------------------   00209400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00209500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00209600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00209700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00209800
                AIF   (&ZN LT 1).ZPVCK6    TOO LOW                      00209900
                AIF   (&ZN LE 2).ZPVCK4A   NOT HIGH                     00210000
.ZPVCK6         ANOP  ,                                                 00210100
&ZMS            SETC  '"PLISTVER='                                      00210200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00210300
&ZMS            SETC  '&ZMS THE RANGE IS "1-2"'                         00210400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00210500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00210600
&XPLISTVER      SETA  2                                                 00210700
                AGO   .ZPVCKE                                           00210800
.*-------------------------------------------------------------------   00210900
.ZPVCK4A        ANOP  ,                                                 00211000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00211100
&ZMS            SETC  'PLISTVER='                                       00211200
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00211300
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00211400
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00211500
                MNOTE 8,' &ZMS IS REQUIRED.'                            00211600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00211700
                AGO   .ZPVCK5                                           00211800
.ZPVCK4         ANOP  ,                                                 00211900
&XPLISTVER      SETA  &PLISTVER(1)                                      00212000
.ZPVCK5         ANOP  ,                                                 00212100
.ZPVCKE         ANOP  ,                                                 00212200
.********************************************************************   00212300
