&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00597600
                      AGO   .ZMRETX                                     00597700
.*                                                                      00597800
.ZMRET2               ANOP  ,                                           00597900
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00598000
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00598100
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00598200
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00598300
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00598400
&ZOPN                 SETA  &ZOPN+1                                     00598500
&ZOPL(&ZOPN)          SETC  ' '                                         00598600
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00598700
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00598800
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00598900
.ZMRETX               ANOP  ,                                           00599000
.*******************************************************************    00599100
.***AO.X03  REASON CODE PROCESSING                                      00599200
.*******************************************************************    00599300
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00599400
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00599500
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00599600
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00599700
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00599800
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00599900
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00600000
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00600100
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00600200
&ZOPN                 SETA  &ZOPN+1                                     00600300
&ZOPL(&ZOPN)          SETC  ' '                                         00600400
&ZOPO(&ZOPN)          SETC  'ST'                                        00600500
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00600600
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00600700
                      AGO   .ZMRSNX                                     00600800
.*                                                                      00600900
.ZMRSN2               ANOP  ,                                           00601000
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00601100
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00601200
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00601300
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00601400
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00601500
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00601600
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00601700
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00601800
&ZOPN                 SETA  &ZOPN+1                                     00601900
&ZOPL(&ZOPN)          SETC  ' '                                         00602000
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00602100
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00602200
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00602300
.ZMRSNX               ANOP  ,                                           00602400
.*******************************************************************    00602500
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00602600
.*******************************************************************    00602700
.ZMFLX                ANOP  ,              MF=L PROCESSING              00602800
.*******************************************************************    00602900
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00603000
.*******************************************************************    00603100
                      ACTR  10000                                       00603200
.*                                                                      00603300
                      LCLA  &ZLC           COLUMN POSITION              00603400
                      LCLA  &ZOPX          LIST TABLE INDEX             00603500
                      LCLC  &ZLL           LABEL                        00603600
                      LCLC  &ZLOP          OPERATION                    00603700
                      LCLC  &ZLV           VALUE                        00603800
                      LCLC  &ZLD           DESCRIPTION                  00603900
.*                                                                      00604000
&ZOPX                 SETA  1                                           00604100
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00604200
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00604300
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00604400
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00604500
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00604600
                      AIF   (K'&ZLD LE 0).ZL5                           00604700
&ZLC                  SETA  35             COMMENT COLUMN               00604800
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00604900
&ZLC                  SETA  33             COMMMENT COLUMN              00605000
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00605100
&ZLV                  SETC  '&ZLV'.' '                                  00605200
                      AGO   .ZL3                                        00605300
.*                                                                      00605400
.ZL4                  ANOP  ,                                           00605500
&ZLV                  SETC  '&ZLV'.' '                                  00605600
.ZL5                  ANOP  ,                                           00605700
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00605800
.*                                                                      00605900
&ZLL     &ZLOP &ZLV                                                     00606000
.*                                                                      00606100
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00606200
                      AGO   .ZL2           CONTINUE LOOPING             00606300
.*                                                                      00606400
.ZLX                  ANOP  ,                                           00606500
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00606600
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00606700
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00606800
         AIF   ('&SYSASCE' EQ 'P').CNPRIM  SYSSTATE PRIMARY             00606900
         SLR   15,15                SET GPR 15 TO ZERO                  00607000
         SAR   15,15                ENSURE AR 15 IS ZERO FOR PRIMARY    00607100
.CNPRIM  ANOP                                                           00607200
         AIF   ('&SYSAM64' NE 'YES').CNA31 BRANCH IF AMODE 31           00607300
         LLGT  15,16(0,0)           GET CVT Address                     00607400
.*   Do not need to test Logger Avail out of line if AMODE 64.          00607500
.*   This was done for pre-520 compatability.                           00607600
         LLGT  15,140(15,0)         GET ECVT ADDRESS                    00607700
         LLGT  15,288(15,0)         GET LSAB ADDRESS                    00607800
         LLGT  15,124(15,0)         GET ENTRY POINT ADDRESS             00607900
         AGO   .CNCONT                                                  00608000
.CNA31   ANOP                                                           00608100
         L     15,16(0,0)           GET CVT ADDRESS                     00608200
.*   Do not need to test Logger Avail since releases now support it.    00608300
.*   This was done for pre-520 compatability.                      @P7C 00608400
         L     15,140(15,0)         GET ECVT ADDRESS                    00608500
         L     15,288(15,0)         GET LSAB ADDRESS                    00608600
         L     15,124(15,0)         GET ENTRY POINT ADDRESS             00608700
.CNCONT  ANOP                                                           00608800
         LA    0,7                  EX OF CONNECT/DISCONNECT SERVICE    00608900
         BALR  14,15                INVOKE SERVICE ROUTINE              00609000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00609100
.ZLINKX               ANOP  ,                                           00609200
.*******************************************************************    00609300
.***AP.X01  FORMAT GENERATED CODE                                       00609400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00609500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00609600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00609700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00609800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00609900
                      AIF   (K'&ZLD LE 0).ZL51                          00610000
&ZLC                  SETA  35             COMMENT COLUMN               00610100
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00610200
&ZLC                  SETA  33             COMMENT COLUMN               00610300
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00610400
&ZLV                  SETC  '&ZLV'.' '                                  00610500
                      AGO   .ZL31                                       00610600
.*                                                                      00610700
.ZL41                 ANOP  ,                                           00610800
&ZLV                  SETC  '&ZLV'.' '                                  00610900
.ZL51                 ANOP  ,                                           00611000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00611100
.*                                                                      00611200
&ZLL     &ZLOP &ZLV                                                     00611300
.*                                                                      00611400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00611500
                      AGO   .ZL21          CONTINUE LOOPING             00611600
.*                                                                      00611700
.ZLX1                 ANOP  ,                                           00611800
.*******************************************************************    00611900
