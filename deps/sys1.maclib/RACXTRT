.CONTSP  ANOP  ,                                                        05700000
         AIF   ('&GENERIC' EQ '' ).DFP                                  05710000
         AIF   ('&GENERIC' NE 'ASIS' AND                               X05720000
               '&GENERIC' NE 'YES').ERR11                          @L8A 05730000
.DFP     ANOP  ,                                                   @L8A 05740000
         AIF   ('&DERIVE' EQ '' ).FLDACC                           @P1A 05750000
         AIF   ('&DERIVE' NE 'YES' AND                                 X05760000
               '&DERIVE' NE 'NO').ERR12                            @P1A 05770000
.FLDACC  ANOP  ,                                                   @L8A 05780000
         AIF   ('&FLDACC' EQ '' ).MATCHG                           @M1C 05790000
         AIF   ('&FLDACC' NE 'YES' AND                                 X05800000
                '&FLDACC' NE 'NO').ERR13                           @L8A 05810000
.MATCHG  ANOP  ,                                                   @M1A 05820000
         AIF   ('&MATCHGN' EQ '' ).DATE4                           @01C 05830000
         AIF   ('&MATCHGN' NE 'YES' AND                                X05840000
                '&MATCHGN' NE 'NO').ERR135                         @P2C 05850000
.DATE4   ANOP  ,                                                   @01A 05860000
         AIF   ('&DATEFMT' EQ '' ).NOFLDA                          @01A 05870000
         AIF   ('&DATEFMT' NE 'YYDDDF' AND                             X05880000
                '&DATEFMT' NE 'YYYYDDDF').ERR30                    @01A 05890000
.NOFLDA  ANOP                                                      @L8A 05900000
         AGO   .CONT02                                                  05910000
.****************************************************                   05920000
.*   VALIDATE PARAMETERS ALLOWABLE WITH                                 05930000
.*   TYPE=ENCRYPT                                                       05940000
.****************************************************                   05950000
.EXT0300 ANOP                                                           05960000
&FN      SETA  2                                                        05970000
         AIF   ('&FIELDS&SUBPOOL&DATEFMT' NE '').ERR03             @01A 05980000
         AIF   ('&ENCRYPT' EQ '').ERR10                                 05990000
         AIF   ('&ENCRYPT(2)' NE '' AND '&ENCRYPT(2)' NE 'DES' AND     X06000000
               '&ENCRYPT(2)' NE 'HASH' AND '&ENCRYPT(2)' NE 'STDDES'   X06010000
               AND '&ENCRYPT(2)' NE 'INST').ERR06                  @M1C 06020000
.*                                 INVALID ENCRYPTION METHOD       @M1A 06030000
         AIF   (N'&ENCRYPT GT 2).ERR04  TOO MANY SUBPARAMETERS          06040000
         AIF   ('&ICHMF' NE 'S').CONT02                                 06050000
         AIF   ('&ENCRYPT(1)' EQ '' OR '&ENCRYPT(2)' EQ '').ERR08       06060000
         AGO   .CONT02                                             @M8A 06070000
.****************************************************              @M8A 06080000
.*   VALIDATE PARAMETERS ALLOWABLE WITH                            @M8A 06090000
.*   TYPE=ENVRXTR                                                  @M8A 06100000
.****************************************************              @M8A 06110000
.EXT0400 ANOP                                                      @M8A 06120000
&FN      SETA  5                                                   @M8A 06130000
.* CHECK THE RELEASE                                               @M8A 06140000
         AIF  ('&RELEASE' LT '2.6').ERR28                          @M8A 06150000
.ERTN28  ANOP                                                      @M8A 06160000
.**************************************************************         06170000
.*  BEGIN THE PARAMETER LIST AND ESTABLISH ADDRESSABILITY TO IT         06180000
.**************************************************************         06190000
.CONT02  ANOP                                                           06200000
.* SET BITS FOR FLAG BYTE IN COMMON PART OF PLIST (OFFSET 5)       @M1A 06210000
&BITA0   SETA  1                                                   @M1A 06220000
&BITA1   SETA  1                                                   @P3C 06230000
&BITA2   SETA  1                                                   @M1A 06240000
&BITA3   SETA  1                                                   @M1A 06250000
&BITA4   SETA  1                                                   @M1A 06260000
&BITA5   SETA  1                                                   @M1A 06270000
&BITA6   SETA  1                                                   @M1A 06280000
&BITA7   SETA  1                                                   @M1A 06290000
&BIT0    SETA  0                                                   @M1A 06300000
&BIT1    SETA  0                                                   @P3C 06310000
&BIT2    SETA  0                                                   @M1A 06320000
&BIT3    SETA  0                                                   @M1A 06330000
&BIT4    SETA  0                                                   @M1A 06340000
&BIT5    SETA  0                                                   @M1A 06350000
&BIT6    SETA  0                                                   @M1A 06360000
&BIT7    SETA  0                                                   @M1A 06370000
         AIF   ('&TYPE' EQ 'ENVRXTR').CONT023                      @M8A 06380000
         AIF   ('&BRANCH' EQ '').CONT026   DON'T SET BRANCH BIT    @P6A 06390000
         AIF   ('&BRANCH' EQ 'YES').CONT023                        @M1A 06400000
&BITA0   SETA  0                           TURN OFF BRANCH BIT     @M1A 06410000
         AGO   .CONT026                    CONTINUE                @M1A 06420000
.CONT023 ANOP                              BRANCH=YES SPECIFIED    @M1A 06430000
&BIT0    SETA  1                           TURN ON BRANCH BIT      @M1A 06440000
.CONT026 ANOP                                                      @M1A 06450000
         AIF   ('&ENTITYX' EQ '').CONT027  ENTITYX SPECIFIED?      @P3A 06460000
&BIT1    SETA  1                           YES, INDICATE ENTITYX   @P3A 06470000
         AGO   .CONT028                    CONTINUE                @P3A 06480000
.CONT027 ANOP                                                      @P3A 06490000
         AIF   ('&ENTITY' EQ '').CONT028   ENTITY SPECIFIED?       @P3A 06500000
&BITA1   SETA  0                           YES, INDICATE ENTITY    @P3A 06510000
.CONT028 ANOP                                                      @P3A 06520000
         AIF   ('&ICHMF' NE 'E').EXT0215                                06530000
.* MF=E                                                                 06540000
         AIF   (&ICHRAC).EXT0212                                        06550000
&NAME    IHBINNRA &MF(2)          POINT R1 AT PARAMETER LIST            06560000
         AGO   .EXT0214                                                 06570000
.EXT0212 ANOP                                                           06580000
&NAME    A     1,36(1)            POINT R1 AT PARAMETER LIST            06590000
.* IF REQUESTED, CHECK TYPE PARAMETER IN L- VS. E-FORM             @L7A 06600000
.EXT0214 ANOP                                                      @L7A 06610000
         AIF   (NOT &RLVLCHK).RLVLX20                              @L7A 06620000
         AIF   (&FN NE 2).EXT0216                                  @L8A 06630000
         CLI   3(1),2                                              @L8A 06640000
         BNE   *+12                                                @L8A 06650000
         CLI   4(1),&VERS          VERSION SAME FOR L AND E FORM ? @L8A 06660000
         BE    *+12                YES                             @L8A 06670000
         LA    15,100              SET INCOMPATIBLE MACROS R.C.    @L7A 06680000
         B     &ICHRLVL                                            @L7A 06690000
.EXT0216 AIF   (&FN EQ 2).RLVLX20                                  @L8A 06700000
         CLI   3(1),2                                              @L8A 06710000
         BE    *+12                                                @L8A 06720000
         CLI   4(1),&VERS          VERSION SAME FOR L AND E FORM ? @L8A 06730000
         BE    *+12                YES                             @L8A 06740000
         LA    15,100              SET INCOMPATIBLE MACROS R.C.    @L7A 06750000
         B     &ICHRLVL                                            @L7A 06760000
.RLVLX20 ANOP                                                      @L7A 06770000
         AGO   .CONT03                                                  06780000
.EXT0215 ANOP                                                           06790000
.* MF=L/S                                                               06800000
         AIF   ('&MF' EQ 'S').EXT0222                          @YA08189 06810000
&NAME    DS    0F                                              @YA08189 06820000
         AIF   ('&MF' EQ 'L').EXT0223                          @ZA94352 06830000
.* MF=S                                                                 06840000
.EXT0222 ANOP                                                  @YA08189 06850000
         CNOP  0,4                                                      06860000
&NAME    BAL   1,ICH&NX.D         POINT R1 AT PARAMETER LIST   @YA11600 06870000
.EXT0223 ANOP                                                  @ZA94352 06880000
         AIF   (NOT &ICHRAC).EXT0224  INVOKER NOT RACROUTE     @ZA94352 06890000
&ICHL    DS    0F                                              @YA08189 06900000
.EXT0224 ANOP                                                           06910000
.* MF=L/S                                                               06920000
ICH&NX.A DS    0F                                              @YA08189 06930000
         DC    AL2(ICH&NX.C-ICH&NX.A) LENGTH OF LIST IN BYTES           06940000
         DC    XL1'82'            FUNCTION CODE FOR ICHRSV00            06950000
         DC    AL1(&FN)           REQUEST TYPE                          06960000
         DC    AL1(&VERS)         VERSION NUMBER                   @L8A 06970000
         DC    B'&BIT0&BIT1&BIT2&BIT3&BIT4&BIT5&BIT6&BIT7'         @M1A 06980000
         DC    AL2(ICH&NX.B-ICH&NX.A) OFFSET TO VARIABLE PART OF LIST   06990000
.CONT03  ANOP                                                           07000000
         AIF   ('&ICHMF' NE 'E').EXT0230                           @L8A 07010000
         MVI   3(1),&FN            REQUEST TYPE                         07020000
         AIF   ('&BIT0&BIT1&BIT2&BIT3&BIT4&BIT5&BIT6&BIT7' EQ          X07030000
               '00000000').CONT13                                  @M1A 07040000
.*                                 TURN ON BITS IN FLAG BYTE       @M1A 07050000
         OI    5(1),B'&BIT0&BIT1&BIT2&BIT3&BIT4&BIT5&BIT6&BIT7'    @M1A 07060000
.CONT13  ANOP  ,                                                   @M1A 07070000
         AIF   ('&BITA0&BITA1&BITA2&BITA3&BITA4&BITA5&BITA6&BITA7' EQ  X07080000
               '11111111').EXT0230                                 @M1A 07090000
.*                                 TURN OFF BITS IN FLAG BYTE      @M1A 07100000
         NI    5(1),B'&BITA0&BITA1&BITA2&BITA3&BITA4&BITA5&BITA6&BITA7' 07110000
.EXT0230 ANOP  ,                                                   @L8A 07120000
&ICHCT   SETA  2               INITIALIZE PARAMETER COUNTER             07130000
         ICHMM &CMENTY                                            @M2C  07140000
         AIF   (NOT &ICHLIST).EXT0231                                   07150000
ICH&NX.B EQU   *                  END OF FIXED PART                     07160000
.EXT0231 ANOP  ,                                                        07170000
         AIF   (&FN EQ 2).EXT0310   REQUEST IS ENCRYPT                  07180000
         AIF   (&FN EQ 5).EXT0350   REQUEST IS ENVRXTR             @M8A 07190000
.***************************                                            07200000
.* GENERATE LIST FOR                                                    07210000
.* TYPE=EXTRACT, TYPE=EXTRACTN OR TYPE=REPLACE                     @L8A 07220000
.***************************                                       @L8A 07230000
         ICHMM &CLS000         ADDRESS OF CLASS                         07240000
         ICHMM &SP             SUBPOOL NUMBER                           07250000
         ICHMM &FIELDS         ADDRESS OF VARIABLE FIELDS          @L8A 07260000
.EXTSL   ANOP                                                           07270000
         AIF   (&VERS LT 1 ).CONT04                                @L8A 07280000
         ICHMM &SEG000         ADDRESS OF SEGMENT                  @L8A 07290000
         ICHMM &SEGDATA        ADDRESS OF VARIABLE SEGDATA FIELDS  @L8A 07300000
         ICHMM &ACEE           ADDRESS OF ACEE                     @L8A 07310000
         ICHMM &VOLSER         ADDRESS OF VOLSER                   @L8A 07320000
.* INITIALIZE BITS SET VARIABLES                                        07330000
&BITA0   SETA  1                                                   @L8A 07340000
&BITA1   SETA  1                                                   @L8A 07350000
&BITA2   SETA  1                                                   @L8A 07360000
&BITA3   SETA  1                                                   @L8A 07370000
&BITA4   SETA  1                                                   @L8A 07380000
&BITA5   SETA  1                                                   @L8A 07390000
&BITA6   SETA  1                                                   @L8A 07400000
&BITA7   SETA  1                                                   @L8A 07410000
&BIT0    SETA  0                                                   @L8A 07420000
&BIT1    SETA  0                                                   @L8A 07430000
&BIT2    SETA  0                                                   @L8A 07440000
&BIT3    SETA  0                                                   @L8A 07450000
&BIT4    SETA  0                                                   @L8A 07460000
&BIT5    SETA  0                                                   @L8A 07470000
&BIT6    SETA  0                                                   @L8A 07480000
&BIT7    SETA  0                                                   @L8A 07490000
&DATA0   SETA  1                                                   @01A 07500000
&DATA1   SETA  1                                                   @01A 07510000
&DATA2   SETA  1                                                   @01A 07520000
&DATA3   SETA  1                                                   @01A 07530000
&DATA4   SETA  1                                                   @01A 07540000
&DATA5   SETA  1                                                   @01A 07550000
&DATA6   SETA  1                                                   @01A 07560000
&DATA7   SETA  1                                                   @01A 07570000
&DAT0    SETA  0                                                   @01A 07580000
&DAT1    SETA  0                                                   @01A 07590000
&DAT2    SETA  0                                                   @01A 07600000
&DAT3    SETA  0                                                   @01A 07610000
&DAT4    SETA  0                                                   @01A 07620000
&DAT5    SETA  0                                                   @01A 07630000
&DAT6    SETA  0                                                   @01A 07640000
&DAT7    SETA  0                                                   @01A 07650000
         AIF   ('&ICHMF' NE 'E').XTRFL10                           @P2M 07660000
         AIF   ('&DATEFMT' NE 'YYDDDF').XTRMCHG                    @01A 07670000
&DATA7   SETA  0                                                   @01A 07680000
.XTRMCHG ANOP  ,                                                   @01A 07690000
         AIF   ('&MATCHGN' NE 'NO').XTRGEN                         @M1A 07700000
&BITA4   SETA  0                                                   @M1A 07710000
.XTRGEN  ANOP                                                      @M1A 07720000
         AIF   ('&GENERIC' NE 'ASIS').XTRDFP                       @L8A 07730000
&BITA5   SETA  0                                                   @L8A 07740000
.XTRDFP  ANOP                                                      @L8A 07750000
         AIF   ('&DERIVE' NE 'NO').XTRFLDA                         @P1A 07760000
&BITA6   SETA  0                                                   @L8A 07770000
.XTRFLDA ANOP                                                      @L8A 07780000
         AIF   ('&FLDACC' NE 'NO').XTRFLGS                         @L8A 07790000
&BITA7   SETA  0                                                   @L8A 07800000
.XTRFLGS ANOP                                                      @L8A 07810000
         AIF   ('&BITA0&BITA1&BITA2&BITA3&BITA4&BITA5&BITA6&BITA7' EQ  X07820000
               '11111111').DATFLGS                                 @01C 07830000
         NI   43(1),B'&BITA0&BITA1&BITA2&BITA3&BITA4&BITA5&BITA6&BITA7' 07840000
.*  ADD BITA0 TO ABOVE BIT STRING                                  @M1C 07850000
.DATFLGS AIF   ('&DATA0&DATA1&DATA2&DATA3&DATA4&DATA5&DATA6&DATA7' EQ  X07860000
               '11111111').XTRFL10                                 @01A 07870000
         NI   42(1),B'&DATA0&DATA1&DATA2&DATA3&DATA4&DATA5&DATA6&DATA7' 07880000
.XTRFL10 ANOP                                                      @L8A 07890000
         AIF   ('&DATEFMT' NE 'YYYYDDDF').XORMCHG                  @01A 07900000
&DAT7    SETA  1                                                   @01A 07910000
.XORMCHG ANOP                                                      @01A 07920000
         AIF   ('&MATCHGN' NE 'YES').XTRT050                       @M1A 07930000
&BIT4    SETA  1                                                   @M1A 07940000
.XTRT050 ANOP                                                      @M1A 07950000
         AIF   ('&GENERIC' NE 'YES').XTRT100                       @L8A 07960000
&BIT5    SETA  1                                                   @L8A 07970000
.XTRT100 ANOP                                                      @L8A 07980000
         AIF   ('&DERIVE' NE 'YES').XTRT200                        @P1A 07990000
&BIT6    SETA  1                                                   @L8A 08000000
.XTRT200 ANOP                                                      @L8A 08010000
         AIF   ('&FLDACC' NE 'YES').XTRT300                        @L8A 08020000
&BIT7    SETA  1                                                   @L8A 08030000
.XTRT300 ANOP                                                      @L8A 08040000
         AIF   ('&ICHMF' NE 'E').CONTFLG                           @L8A 08050000
         AIF   ('&BIT0&BIT1&BIT2&BIT3&BIT4&BIT5&BIT6&BIT7' EQ          X08060000
               '00000000').DATFLG                                  @01C 08070000
         OI    43(1),B'&BIT0&BIT1&BIT2&BIT3&BIT4&BIT5&BIT6&BIT7'   @L8A 08080000
.DATFLG  AIF   ('&DAT0&DAT1&DAT2&DAT3&DAT4&DAT5&DAT6&DAT7' EQ          X08090000
               '00000000').CONTFLG                                 @01A 08100000
         OI    42(1),B'&DAT0&DAT1&DAT2&DAT3&DAT4&DAT5&DAT6&DAT7'   @01A 08110000
         AGO   .CONT04                                             @L8A 08120000
.CONTFLG ANOP  ,                                                   @L8A 08130000
         AIF   (NOT &ICHLIST).CONT04                               @L8A 08140000
.LISTFLG DC    AL2(0)          RESERVED                            @01C 08150000
         DC    B'&DAT0&DAT1&DAT2&DAT3&DAT4&DAT5&DAT6&DAT7'         @01A 08160000
         DC    B'&BIT0&BIT1&BIT2&BIT3&BIT4&BIT5&BIT6&BIT7'         @L8A 08170000
         AGO   .CONT04                                                  08180000
.************************************                                   08190000
.* GENERATE LIST FOR                                                    08200000
.* TYPE=ENCRYPT                                                         08210000
.************************************                                   08220000
.EXT0310 ANOP                                                           08230000
         ICHMM &ENCRYPT(1)   ADDRESS OF DATA TO BE ENCRYPTED            08240000
         AIF   ('&ENCRYPT(2)' EQ '').EXT0340                            08250000
         AIF   ('&ENCRYPT(2)' NE 'DES').EXT0330                         08260000
&METH    SETC  '1'           DES ENCRYPTION METHOD                      08270000
         AGO   .EXT0340                                                 08280000
.EXT0330 ANOP  ,                                                        08290000
         AIF   ('&ENCRYPT(2)' NE 'HASH').EXT0334                        08300000
&METH    SETC  '2'           HASHING ENCRYPTION METHOD                  08310000
         AGO   .EXT0340                                                 08320000
.EXT0334 ANOP  ,                                                        08330000
         AIF   ('&ENCRYPT(2)' NE 'INST').EXT0336                   @M1A 08340000
&METH    SETC  '3'           INSTALLATION-DEFINED METHOD                08350000
         AGO   .EXT0340                                            @M1A 08360000
.EXT0336 ANOP  ,                                                   @M1A 08370000
&METH    SETC  '4'           STANDARD DES ENCRYPTION METHOD        @M1A 08380000
.EXT0340 ANOP  ,                                                        08390000
         ICHMM  &METH        ENCRYPTION METHOD                          08400000
         AGO   .CONT04                                             @M8A 08410000
.************************************                              @M8A 08420000
.* GENERATE LIST FOR                                               @M8A 08430000
.* TYPE=ENVRXTR                                                    @M8A 08440000
.************************************                              @M8A 08450000
.EXT0350 ANOP                                                      @M8A 08460000
         ICHMM &ENVROUT      ADDRESS OF ENVROUT                    @M8A 08470000
         ICHMM &ACEE         ADDRESS OF ACEE                       @M8A 08480000
.***************************************************************        08490000
.*  AT THIS POINT, THE LIST AND EXECUTE FORMS ARE COMPLETED.  IF        08500000
.*  THE LIST PASS OF THE STANDARD FORM HAS JUST BEEN COMPLETED,         08510000
.*  RETURN FOR AN EXECUTE PASS.                                         08520000
.***************************************************************        08530000
.CONT04  ANOP                                                           08540000
         AIF   (NOT &ICHLIST).CONT05  SKIP END-OF-LIST LABEL            08550000
ICH&NX.C DS    0H                 END OF PARAMETER LIST                 08560000
         AIF   (T'&SEGMENT EQ 'O').SEGM999                         @L8A 08570000
         AIF   ('&SEGMENT'(1,1) NE '''').SEGM999                   @L8A 08580000
&SEG000  DC    CL8&SEGMENT                                         @L8A 08590000
.SEGM999 ANOP  ,                                                        08600000
         AIF   (T'&CLASS EQ 'O').CLSM999                           @L8A 08610000
         AIF   ('&CLASS'(1,1) NE '''').CLSM999                     @L8A 08620000
&CLS000  DC    CL8&CLASS                                                08630000
.CLSM999 ANOP  ,                                                        08640000
ICH&NX.D DS    0H                                                       08650000
.CONT05  ANOP                                                           08660000
         AIF   ('&ICHMF' NE 'S' OR NOT &ICHLIST).CONT06                 08670000
&ICHLIST SETB  0                                                        08680000
         AGO   .CONT03           DO EXECUTE PATH OF STANDARD FORM       08690000
.*****************************************************                  08700000
.*  THE SVC INSTRUCTION IS GENERATED IF RACXTRT IS                      08710000
.*  NOT INVOKED FROM THE RACROUTE MACRO OR IF THIS                      08720000
.*  IS NOT A LIST PASS.                                                 08730000
.*****************************************************                  08740000
.CONT06  ANOP                                                           08750000
         AIF   (&ICHRAC OR &ICHLIST).EROUTE                             08760000
         SVC   132                INVOKE EXTRACT ROUTINE                08770000
         AIF   (NOT &RLVLCHK).RLVLX40                              @L7A 08780000
&ICHRLVL DS    0H                                                  @L7A 08790000
.RLVLX40 ANOP                                                      @L7A 08800000
         AGO   .EROUTE                                                  08810000
.*****************                                                      08820000
.*  ERROR MESSAGES                                                      08830000
.*****************                                                      08840000
.ERR01   ANOP                                                           08850000
         MNOTE 12,'*** INVALID VALUE FOR ''MF'' PARAMETER'              08860000
         AGO   .EROUTE                                                  08870000
.ERR03   ANOP                                                           08880000
         MNOTE 12,'*** EXTRANEOUS PARAMETERS SPECIFIED FOR ''TYPE=&TYPEX08890000
               '''                                                      08900000
         AGO   .EROUTE                                                  08910000
.ERR04   ANOP                                                           08920000
         MNOTE 12,'*** TOO MANY PARAMETERS SPECIFIED FOR ''ENCRYPT'' PAX08930000
               RAMETER'                                                 08940000
         AGO   .EROUTE                                                  08950000
.ERR05   ANOP                                                           08960000
         MNOTE 12,'*** INVALID VALUE FOR ''TYPE'' PARAMETER'            08970000
         AGO   .EROUTE                                                  08980000
.ERR06   ANOP                                                           08990000
         MNOTE 12,'*** INVALID METHOD SPECIFIED FOR ''ENCRYPT'' PARAMETX09000000
               ER'                                                      09010000
         AGO   .EROUTE                                                  09020000
.ERR07   ANOP                                                           09030000
         MNOTE 12,'*** REQUIRED PARAMETER ''TYPE'' NOT SPECIFIED'       09040000
         AGO   .EROUTE                                                  09050000
.ERR08   ANOP                                                           09060000
         MNOTE 12,'*** BOTH SUBPARAMETERS MUST BE SPECIFIED FOR ''ENCRYX09070000
               PT'' PARAMETER'                                          09080000
         AGO   .EROUTE                                                  09090000
.ERR09   ANOP                                                           09100000
         MNOTE 12,'*** INVALID VALUE FOR ''SUBPOOL'' PARAMETER'         09110000
         AGO   .EROUTE                                                  09120000
.ERR10   ANOP                                                           09130000
         MNOTE 12,'*** ''ENCRYPT'' PARAMETER REQUIRED WITH ''TYPE=ENCRYX09140000
               PT'''                                                    09150000
         AGO   .EROUTE                                                  09160000
.ERR11   ANOP                                                           09170000
         MNOTE 12,'*** INVALID VALUE FOR '' GENERIC '' PARAMETER ' @L8A 09180000
         AGO   .EROUTE                                                  09190000
.ERR12   ANOP                                                           09200000
         MNOTE 12,'*** INVALID VALUE FOR '' DERIVE '' PARAMETER '  @P1A 09210000
         AGO   .EROUTE                                                  09220000
.ERR13   ANOP                                                           09230000
         MNOTE 12,'*** INVALID VALUE FOR '' FLDACC '' PARAMETER '  @L8A 09240000
         AGO   .EROUTE                                                  09250000
.ERR135  ANOP                                                           09260000
         MNOTE 12,'*** INVALID VALUE FOR '' MATCHGN '' PARAMETER ' @P2A 09270000
         AGO   .EROUTE                                             @P2A 09280000
.ERR14   MNOTE 4,'*** ''RELEASE'' INCOMPATIBLE WITH SPECIFIED KEYWORD(SX09290000
               ) ***'                                              @L8A 09300000
         AGO   .ERTN14                                             @M2C 09310000
.ERR14X  ANOP                                                      @M2A 09320000
