.NORLVL  ANOP                                                      @MPA 04090000
&RLVL    SETC  '1.6  '             INDICATE RELEASE=1.6            @MPA 04100000
.ENDRLVL ANOP                                                      @MPA 04110000
.*                                                                 @M2A 04120000
.* TEST INCOMPATABILITY OF NEW KEYWORDS.                           @M2A 04130000
.*                                                                 @M2A 04140000
         AIF   ('&RELEASE(1)' EQ '').RLCHK16                       @P5A 04150000
         AIF   ('&RLVL'(1,3) GT '1.9').RLCHK92                     @MPC 04160000
         AIF   ('&RLVL'(1,5) GE '1.9.2').RLCHK92                   @MPC 04170000
         AIF   ('&RLVL'(1,3) GE '1.9').RLCHK19                     @MPC 04180000
         AIF   ('&RLVL'(1,5) GE '1.8.1').RLCHK18                   @MPC 04190000
         AIF   ('&RLVL'(1,3) GE '1.8').RLCHK18                     @MPC 04200000
.*                                                                 @M2A 04210000
.* KEYWORDS INTRODUCED IN RELEASE 1.8 ARE NOT SPECIFIED WITH       @M2A 04220000
.* PREVIOUS RELEASES.                                              @M2A 04230000
.*                                                                 @M2A 04240000
.RLCHK16 ANOP                                                      @P5A 04250000
         AIF   ('&SEGMENT&SEGDATA&ACEE&VOLSER&GENERIC&FLDACC&CLASS&DERIX04260000
               VE' NE  '').ERR14                RELEASE IS 1.6/1.7 @M2C 04270000
.ERTN14  ANOP                                                      @M2C 04280000
         AIF   ('&TYPE' NE 'EXTRACT' AND '&TYPE' NE '' AND             X04290000
               '&TYPE' NE 'ENCRYPT').ERR15                         @M2C 04300000
.ERTN15  ANOP                                                      @M2C 04310000
.*                                                                 @M2A 04320000
.* KEYWORDS INTRODUCED IN RELEASE 1.9 ARE NOT SPECIFIED WITH       @M2A 04330000
.* PREVIOUS RELEASES.                                              @M2A 04340000
.*                                                                 @M2A 04350000
.RLCHK18 ANOP                                                      @M2A 04360000
         AIF   ('&BRANCH&MATCHGN' NE '').ERR14X                    @M2C 04370000
.ERTN14X ANOP                                                      @M2A 04380000
         AIF   ('&ENTITYX' NE '').ERR26                            @M2A 04390000
.ERTN26  ANOP                                                      @M2A 04400000
.RLCHK19 ANOP                                                      @M2A 04410000
         AIF   ('&DATEFMT' NE '').ERR29                            @01A 04420000
.ERTN29  ANOP                                                      @01A 04430000
.RLCHK92 ANOP                                                      @01A 04440000
.*                                                                 @M2A 04450000
.* UPWARD COMPATBILITY OF THE CODE.                                @M2A 04460000
.*                                                                 @M2A 04470000
         AIF  ('&RELEASE' LE '1.7').RL17COD                        @M2A 04480000
         AIF  ('&RELEASE' EQ '1.8').RL18COD                        @M2A 04490000
         AIF  ('&RELEASE' EQ '1.8.1').RL18COD                      @M2A 04500000
         AIF   (NOT &ICHRAC).ERR23       NOT CALLED BY RACROUTE    @M2A 04510000
.ERTN23  ANOP                            ISSUE MNOTE.              @M2A 04520000
.************************************************************      @M1A 04530000
.* VALIDITY CHECK BRANCH KEYWORD                                   @M1A 04540000
.************************************************************      @M1A 04550000
         AIF   ('&BRANCH' EQ 'NO' OR '&BRANCH' EQ '').BRCH010      @M1A 04560000
         AIF   ('&BRANCH' NE 'YES').ERR24                          @M1A 04570000
         AIF   ('&TYPE' NE 'EXTRACT' AND                               X04580000
               '&TYPE' NE 'EXTRACTN' AND                               X04590000
               '&TYPE' NE 'ENVRXTR' AND                                X04600000
               '&TYPE' NE 'ENCRYPT').ERR22                         @MEC 04610000
.ERTN22  ANOP                                                      @M1A 04620000
.BRCH010 ANOP                                                      @M1A 04630000
.*                                                                 @M2A 04640000
.* TEST 1. ENTITY AND ENTITYX ARE MUTUALLY EXCLUSIVE.              @M2A 04650000
.*      2. SET FLAG "YCOPENTY" WHEN ENTITYX IS SPECIFIED.          @M2A 04660000
.*                                                                 @M2A 04670000
         AIF   ('&ENTITY' NE '' AND '&ENTITYX' NE '' ).ERR25       @M2A 04680000
.ERTN25  ANOP                                                      @M2A 04690000
         AIF   ('&ENTITYX' EQ '').NOCOPY                           @M2A 04700000
&YCOPENT SETB  1                  SET FLAG AS ENTITYX IS SPECIFIED @M2A 04710000
&CMENTY  SETC  '&ENTITYX'                                          @M2A 04720000
.NOCOPY  ANOP                                                      @M2A 04730000
.RL18COD ANOP                                                      @M2A 04740000
&VERS    SETA  1                                                   @M2A 04750000
.RL17COD ANOP                                                      @M2A 04760000
         AIF   ('&TYPE' NE 'ENCRYPT' AND T'&ENCRYPT NE 'O').ERR16  @M1C 04770000
.ERLVL1R ANOP                                                      @L7A 04780000
.************************                                               04790000
.*   INITIALIZE VARIABLES                                               04800000
.************************                                               04810000
.INITVAR ANOP                                                      @L8A 04820000
&NX      SETC  '&SYSNDX'                                                04830000
&DFLTSP  SETA  229                 DEFAULT SUBPOOL                      04840000
&ICHNTFY SETB  0                   WANT ICHMM TO ISSUE ERROR MSGS       04850000
