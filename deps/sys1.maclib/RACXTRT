         MACRO                                                          01390000
&NAME    RACXTRT &ACEE=,                                               *01400000
               &ENCRYPT=,                                              *01410000
               &ENTITY=,                                               *01420000
               &ENTITYX=,                                          @M2A*01430000
               &ENVROUT=,                                          @M8A*01440000
               &FIELDS=,                                               *01450000
               &SUBPOOL=,                                              *01460000
               &TYPE=,                                                 *01470000
               &RELEASE=,                                              *01480000
               &CLASS=,                                            @L8A*01490000
               &SEGMENT=,                                          @L8A*01500000
               &SEGDATA=,                                          @L8A*01510000
               &VOLSER=,                                           @L8A*01520000
               &GENERIC=,                                          @L8A*01530000
               &DERIVE=,                                           @P1A*01540000
               &FLDACC=,                                           @L8A*01550000
               &BRANCH=,                                           @M1A*01560000
               &MATCHGN=,                                          @M1A*01570000
               &DATEFMT=,                                          @01A*01580000
               &MF=S                                                    01590000
.*  DEFINE LOCAL VARIABLES                                              01600000
         LCLA  &FN                 USED TO CONTAIN NUMERIC VALUE        01610000
.*                                 FOR FUNCTION                         01620000
         LCLA  &VERS               USED TO CONTAIN VERSION NUMBER  @L8A 01630000
.*                                 TESTS THAT LIST AND EXECUTE FORMS    01640000
.*                                 HAVE SAME RELEASE NUMBER.            01650000
         LCLA  &ENC                USED TO CONTAIN ADDRESS OF DATA      01660000
.*                                 TO BE ENCRYPTED                      01670000
         LCLA  &DFLTSP             USED TO SET DEFAULT SUBPOOL          01680000
.*                                 WHEN ENTERED AS A STRING             01690000
         LCLC  &SEG000             SEGMENT NAME (ADDRS OR CONST)   @L8A 01700000
         LCLC  &CLS000             SEGMENT NAME (ADDRS OR CONST)   @L8A 01710000
         LCLA  &GENFLG             GENERIC PROCESSING FLAG         @L8A 01720000
         LCLA  &DRVFLG             DFP PROCESSING FLAG             @L8A 01730000
         LCLA  &FLDACF             FLDACC PROCESSING FLAG          @L8A 01740000
         LCLA  &DAT0,&DAT1,&DAT2,&DAT3,&DAT4,&DAT5,&DAT6,&DAT7     @01A 01750000
         LCLA  &BIT0,&BIT1,&BIT2,&BIT3,&BIT4,&BIT5,&BIT6,&BIT7     @L8A 01760000
         LCLA  &BITA0,&BITA1,&BITA2,&BITA3                              01770000
         LCLA  &BITA4,&BITA5,&BITA6,&BITA7                              01780000
         LCLA  &DATA0,&DATA1,&DATA2,&DATA3                         @01A 01790000
         LCLA  &DATA4,&DATA5,&DATA6,&DATA7                         @01A 01800000
         LCLB  &RLVLCHK            USED TO CONTROL RELEASE-CHECK   @L7A 01810000
         LCLC  &METH               USED TO CONTAIN ENCRYPTION METHOD    01820000
         LCLC  &SP                 USED TO CONTAIN SUBPOOL VALUE        01830000
         LCLC  &NX                 USED TO CONTAIN SYSNDX FOR LABELS    01840000
         LCLB  &YCOPENT            0 => ENTITY IS SPECIFIED        @M2A 01850000
.*                                 1 => ENTITYX IS SPECIFIED       @M2A 01860000
         LCLC  &CMENTY             COMMON ENTITY VARIABLE          @M2A 01870000
         LCLC  &RLVL               RELEASE LEVEL CHARACTER STRING  @MPA 01880000
.*                                                                 @M2A 01890000
.*  DEFINE GLOBAL VARIABLES                                             01900000
         GBLC  &ICHL               USED FOR ADDRESS OF EXTRACT PARM     01910000
         GBLA  &ICHCT              ICHCT - A COUNTER USED TO KEEP       01920000
.*                                 TRACK OF WHERE IN THE PARAMETER      01930000
.*                                 LIST THE PARAMETER IS TO BE          01940000
.*                                 PLACED. ICHCT IS INITIALIZED TO      01950000
.*                                 THE FIRST FULLWORD IN THE LIST       01960000
.*                                 TO BE FILLED IN BY ICHMM,            01970000
.*                                 AND INCREMENTED BY THE INNER         01980000
.*                                 MACRO (ICHMM).                       01990000
         GBLB  &ICHRAC             ICHRAC - A FLAG WHICH IS             02000000
.*                                 INITIALIZED IN THE RACROUTE MACRO    02010000
.*                                 TO COMMUNICATE TO RACXTRT THAT IT    02020000
.*                                 IS BEING INVOKED AS AN INNER MACRO   02030000
.*                                 TO THE RACROUTE MACRO.               02040000
         GBLB  &ICHLIST            ICHLIST - A FLAG WHICH IS SET TO     02050000
.*                                 INDICATE THE FORMAT OF THE MACRO.    02060000
.*                                 FOR LIST FORMAT, ICHLIST IS ON       02070000
.*                                 FOR EXECUTE FORMAT, ICHLIST IS       02080000
.*                                 OFF. FOR STANDARD FORMAT, THERE      02090000
.*                                 ARE TWO PASSES DONE BY THE MACRO     02100000
.*                                 A LIST PASS AND AN EXECUTE PASS.     02110000
.*                                 INITIALLY, FOR STANDARD, THE         02120000
.*                                 FLAG  IS ON - FOR THE LIST PASS.     02130000
.*                                 IT IS THEN TURNED OFF FOR THE        02140000
.*                                 EXECUTE PASS.                        02150000
         GBLB  &ICHNTFY            ICHNTFY - A GLOBAL INDICATOR         02160000
.*                                 WHICH IS USED TO COMMUNICATE WITH    02170000
.*                                 ICHMM - THE INNER MACRO. IF ICHNTFY  02180000
.*                                 IS ON, THE INNER MACRO WILL NOT      02190000
.*                                 ISSUE ITS OWN MNOTE IF IT FINDS AN   02200000
.*                                 ERROR-IT WILL MERELY TURN ON THE     02210000
.*                                 ICHINER FLAG AS NOTIFICATION TO THE  02220000
.*                                 OUTER MACRO, WHICH WILL TAKE THE     02230000
.*                                 APPROPRIATE ACTION.                  02240000
.*                                 IF ICHNTFY IS OFF, THE INNER MACRO   02250000
.*                                 WILL TAKE ACTION ON ITS OWN,         02260000
.*                                 IE. ISSUE AN MNOTE WITH A SEVERITY   02270000
.*                                 CODE OF ITS CHOOSING.                02280000
         GBLB  &ICHINER            ICHINER - A GLOBAL INDICATOR         02290000
.*                                 WHICH IS USED BY ICHMM - AN INNER    02300000
.*                                 MACRO - TO TELL THE OUTER MACRO IF   02310000
.*                                 AN ERROR HAS BEEN DETECTED IN A      02320000
.*                                 PARAMETER.  IF ICHINER IS ON, AN     02330000
.*                                 ERROR HAS BEEN DETECTED.             02340000
         GBLC  &ICHMF              ICHMF - AN INDICATOR OF THE          02350000
.*                                 FORMAT - L, E, S OR M           @M1C 02360000
         GBLC  &ICHRLVL            SYMBOL TO BRANCH TO             @L7A 02370000
.*                                 WHEN INCOMPATIBLE MACRO LEVEL.  @L7A 02380000
.* VALIDATE RELEASE PARAMETER                                      @L7A 02390000
         AIF   (&ICHRAC).RLVL01                                    @L7A 02400000
&ICHRLVL SETC  'ICH&SYSNDX.X'      CREATE LABEL FOR SVC BYPASS     @L7A 02410000
.RLVL01  ANOP                                                      @L7A 02420000
&VERS    SETA  0                   DEFAULT VERSION NUMBER          @L8A 02430000
&YCOPENT SETB  0                   ASSUME THAT THE ENTITY IS       @M2A 02440000
&CMENTY  SETC  '&ENTITY'           SPECIFIED.                      @M2A 02450000
.*                                                                 @M2A 02460000
.* VALIDATE THE RELEASE PARAMETER                                  @M2A 02470000
.*                                                                 @M2A 02480000
         AIF   (N'&RELEASE GT 2).ERLVL1  INVALID RELEASE PARAMETER @L7A 02490000
         AIF   ('&RELEASE(1)' EQ '').VLIDREL                       @M2A 02500000
         AIF   ('&RELEASE(1)' EQ '1.6').VLIDREL                    @M2A 02510000
         AIF   ('&RELEASE(1)' EQ '1.7').VLIDREL                    @M2A 02520000
         AIF   ('&RELEASE(1)' EQ '1.8').VLIDREL                    @M2A 02530000
         AIF   ('&RELEASE(1)' EQ '1.8.1').VLIDREL                  @M2A 02540000
         AIF   ('&RELEASE(1)' EQ '1.9').VLIDREL                    @M2A 02550000
         AIF   ('&RELEASE(1)' EQ '1.9.2').VLIDREL                  @M3A 02560000
         AIF   ('&RELEASE(1)' EQ '2.1').VLIDREL                    @M4A 02570000
         AIF   ('&RELEASE(1)' EQ '2.2').VLIDREL                    @M5A 02580000
         AIF   ('&RELEASE(1)' EQ '2.3').VLIDREL                    @M6A 02590000
         AIF   ('&RELEASE(1)' EQ '2.4').VLIDREL                    @M7A 02600000
         AIF   ('&RELEASE(1)' EQ '2.6').VLIDREL                    @M8A 02610000
         AIF   ('&RELEASE(1)' EQ '2608').VLIDREL                   @M9A 02620000
         AIF   ('&RELEASE(1)' EQ '7703').VLIDREL                   @MAA 02630000
         AIF   ('&RELEASE(1)' EQ '7705').VLIDREL                   @MBA 02640000
         AIF   ('&RELEASE(1)' EQ '7706').VLIDREL                   @MCA 02650000
         AIF   ('&RELEASE(1)' EQ '7707').VLIDREL                   @MDA 02660000
         AIF   ('&RELEASE(1)' EQ '7708').VLIDREL                   @MFA 02670000
         AIF   ('&RELEASE(1)' EQ '7709').VLIDREL                   @MGA 02680000
         AIF   ('&RELEASE(1)' EQ '7720').VLIDREL                   @MHA 02690000
         AIF   ('&RELEASE(1)' EQ '7730').VLIDREL                   @MIA 02700000
         AIF   ('&RELEASE(1)' EQ '7740').VLIDREL                   @MJA 02710000
         AIF   ('&RELEASE(1)' EQ '7750').VLIDREL                   @MKA 02720000
         AIF   ('&RELEASE(1)' EQ '7760').VLIDREL                   @MLA 02730000
         AIF   ('&RELEASE(1)' EQ '7770').VLIDREL                   @MMA 02740000
         AIF   ('&RELEASE(1)' EQ '7780').VLIDREL                   @MNA 02750000
         AIF   ('&RELEASE(1)' EQ '7790').VLIDREL                   @MOA 02760000
         AIF   ('&RELEASE(1)' EQ '77A0').VLIDREL                   @MPA 02770000
         AIF   ('&RELEASE(1)' EQ '77B0').VLIDREL                   @MQA 02780000
         AIF   ('&RELEASE(1)' EQ 'PLV0001').VLIDREL                @MRA 02790005
         AGO   .ERLVL1                                             @M2A 02800000
.VLIDREL ANOP                                                      @M2A 02810000
.*                                                                 @M2A 02820000
.* IF SECOND OPERAND IS SPECIFIED TEST THAT IT IS "CHECK" AND THE  @M2A 02830000
.* THE MACRO FORMAT IS 'E'                                         @M2A 02840000
.*                                                                 @M2A 02850000
         AIF   (N'&RELEASE LE 1).NORLVLC     CHECK NOT SPECIFIED   @L7A 02860000
         AIF   ('&RELEASE(2)' NE 'CHECK').ERLVL1   INVALID VALUE   @L7A 02870000
         AIF   ('&MF(1)' NE 'E').ERLVL2     CHECK ONLY WITH MF=E   @L7A 02880000
&RLVLCHK SETB  1                          ACTIVATE RELEASE-CHECK   @L7A 02890000
.ERLVL2R ANOP                                                      @L7A 02900000
.NORLVLC ANOP                                                      @L7A 02910000
         AIF   ('&RELEASE(1)' EQ '').NORLVL                        @MPA 02920000
.CHECKVER ANOP                                                     @MRA 02930000
         AIF   ('&RELEASE(1)' NE 'PLV0001').CHECKFMID              @MRA 02940006
.PLVER0001 ANOP                                                    @MRA 02950000
&RLVL    SETC  '46   '         Indicate the Parameter List         @MRA 02960000
.*                             is at the RELEASE=77B0 level        @MRA 02970000
         AGO   .ENDRLVL                                            @MRA 02980002
.CHECKFMID ANOP                                                    @MRA 02990000
.RLVL77B0 ANOP                                                     @MRA 03000000
         AIF   ('&RELEASE(1)' NE '77B0').RLVL77A0                  @MQA 03010000
&RLVL    SETC  '46   '             INDICATE RELEASE=77B0           @MQA 03020000
         AGO   .ENDRLVL                                            @MQA 03030000
.RLVL77A0 ANOP                                                     @MQA 03040000
         AIF   ('&RELEASE(1)' NE '77A0').RLVL7790                  @MPA 03050000
&RLVL    SETC  '45   '             INDICATE RELEASE=77A0           @MPA 03060000
         AGO   .ENDRLVL                                            @MPA 03070000
.RLVL7790 ANOP                                                     @MPA 03080000
         AIF   ('&RELEASE(1)' NE '7790').RLVL7780                  @MPA 03090000
&RLVL    SETC  '44   '             INDICATE RELEASE=7790           @MPA 03100000
         AGO   .ENDRLVL                                            @MPA 03110000
.RLVL7780 ANOP                                                     @MPA 03120000
         AIF   ('&RELEASE(1)' NE '7780').RLVL7770                  @MPA 03130000
