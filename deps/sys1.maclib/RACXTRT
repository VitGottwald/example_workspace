.***********************************                                    04870000
.*   SET UP INDICATORS FOR MF FORMAT                                    04880000
.***********************************                                    04890000
.*  VALIDATE MF PARAMETER                                               04900000
         AIF   ('&MF' NE 'L' AND '&MF' NE 'S' AND '&MF(1)' NE          *04910000
               'E' AND '&MF(1)' NE 'M').ERR01                      @M1C 04920000
&ICHMF   SETC  '&MF(1)'        SET MF TYPE                              04930000
         AIF   ('&MF(1)' NE 'M').MFLSE                             @M1A 04940000
         AIF   (NOT &ICHRAC).ERR01  MF=M ONLY ALLOWED VIA RACROUTE @M1A 04950000
&ICHMF   SETC  'E'             TREAT MF=M SAME AS MF=E             @M1A 04960000
.MFLSE   ANOP                           MF=L, S OR E               @M1A 04970000
         AIF   ('&ICHMF' EQ 'E').EXT0030                           @M1C 04980000
&ICHLIST SETB  1               MF LIST OR STANDARD                      04990000
         AIF   (T'&SEGMENT EQ 'O').SEG000                          @L8A 05000000
         AIF   ('&SEGMENT'(1,1) EQ '''').SEGL000                   @L8A 05010000
.SEG000  ANOP                                                      @L8A 05020000
&SEG000  SETC  '&SEGMENT'                                          @L8A 05030000
         AGO   .SEGL999                                            @L8A 05040000
.SEGL000 ANOP  ,                                                   @L8A 05050000
&SEG000  SETC  'ICH&NX.S'                                          @L8A 05060000
.SEGL999 ANOP  ,                                                   @L8A 05070000
         AIF   (T'&CLASS EQ 'O').CLS000                            @L8A 05080000
         AIF   ('&CLASS'(1,1) EQ '''').CLSL000                     @L8A 05090000
