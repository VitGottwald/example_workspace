.********************************************************************   02596500
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     02596600
.********************************************************************   02596700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       02596800
                      LCLA  &ZOPN1         INVOCATION INDEX             02596900
                      LCLC  &ZOPL(4095)    LABEL                        02597000
                      LCLC  &ZOPO(4095)    OPERATION                    02597100
                      LCLC  &ZOPV(4095)    VALUE                        02597200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  02597300
.*                                                                      02597400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     02597500
.******************************************************************     02597600
.***AL.L10 ANTRQTB FMT=LIST MF=S!L PARAMETERS                           02597700
.******************************************************************     02597800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     02597900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     02598000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'(1,4).'A'                     02598100
&XMFATTR              SETC  '0D'                                        02598200
.*                                                                      02598300
&ZOPN                 SETA  &ZOPN+1                                     02598400
&ZOPL(&ZOPN)          SETC  ' '                                         02598500
                      AIF   (&SYSALVL NE 0).ZALVL6A                     02598600
&ZOPO(&ZOPN)          SETC  'B'                                         02598700
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                02598800
                      AGO   .ZALVL6B                                    02598900
.ZALVL6A              ANOP                                              02599000
&ZOPO(&ZOPN)          SETC  'BRAS'                                      02599100
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              02599200
.ZALVL6B              ANOP                                              02599300
&ZOPD(&ZOPN)          SETC  '++ ANTRQTB ARG PARMS'                      02599400
.*                                                                      02599500
&ZOPN                 SETA  &ZOPN+1                                     02599600
&ZOPL(&ZOPN)          SETC  ' '                                         02599700
&ZOPO(&ZOPN)          SETC  'SPACE'                                     02599800
&ZOPV(&ZOPN)          SETC  '1'                                         02599900
&ZOPD(&ZOPN)          SETC  ' '                                         02600000
.*                                                                      02600100
.ZLMFSL2              ANOP  ,                                           02600200
.*-------------------------------------------------------------------   02600300
