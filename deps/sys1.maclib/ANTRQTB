                GBLA  &SYSALVL    SYSSTATE ARCHLVL                      00507400
.*                                                                      00507500
                AIF   (K'&SYSASCE GT 0).ZSYS4   SYS STATE DEFINED       00507600
                AIF   (K'&SYSSPLV GT 0).ZSYS2   SP LEVEL DEFINED        00507700
                SPLEVEL  TEST                                           00507800
.ZSYS2          ANOP  ,                                                 00507900
                AIF   ('&SYSSPLV' EQ '1').ZSYS3  PRE SP 3.1             00508000
                AIF   ('&SYSSPLV' EQ '2').ZSYS3  PRE SP 3.1             00508100
                SYSSTATE TEST                                           00508200
                AGO   .ZSYS4                                            00508300
.ZSYS3          ANOP  ,                                                 00508400
&SYSASCE        SETC  'P'         ASSUME SYSSTATE PRIMARY               00508500
.ZSYS4          ANOP  ,                                                 00508600
