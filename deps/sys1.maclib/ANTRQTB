.***AC.PLV01  PROCESS PLISTVER                                      *   00524600
.********************************************************************   00524700
&XPLISTVER      SETA  0                                                 00524800
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00524900
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00525000
&XPLISTVER      SETA  4                                                 00525100
.ZPLV0          ANOP  ,                                                 00525200
.*                                                                      00525300
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00525400
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00525500
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00525600
.*-------------------------------------------------------------------   00525700
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00525800
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00525900
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00526000
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00526100
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00526200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00526300
                AGO   .ZPVCK2                                           00526400
.ZPVCK3         ANOP  ,                                                 00526500
.*-------------------------------------------------------------------   00526600
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00526700
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00526800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00526900
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00527000
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00527100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00527200
                AGO   .ZPVCKE                                           00527300
.ZPVCK2         ANOP  ,                                                 00527400
.*-------------------------------------------------------------------   00527500
&ZCVAL          SETC  '&PLISTVER(1)'                                    00527600
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00527700
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00527800
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00527900
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00528000
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00528100
.ZPVCK6         ANOP  ,                                                 00528200
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00528300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00528400
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00528500
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00528600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00528700
&XPLISTVER      SETA  4                                                 00528800
                AGO   .ZPVCKE                                           00528900
.*-------------------------------------------------------------------   00529000
.ZPVCK4A        ANOP  ,                                                 00529100
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00529200
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00529300
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00529400
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00529500
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00529600
                MNOTE 8,' &ZMS IS REQUIRED.'                            00529700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00529800
                AGO   .ZPVCK5                                           00529900
.ZPVCK4         ANOP  ,                                                 00530000
&XPLISTVER      SETA  &PLISTVER(1)                                      00530100
.ZPVCK5         ANOP  ,                                                 00530200
.ZPVCKE         ANOP  ,                                                 00530300
.********************************************************************   00530400
