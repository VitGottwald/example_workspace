.***AC.PLV01  PROCESS PLISTVER                                      *   00180400
.********************************************************************   00180500
&XPLISTVER      SETA  0                                                 00180600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00180700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00180800
&XPLISTVER      SETA  4                                                 00180900
.ZPLV0          ANOP  ,                                                 00181000
.*                                                                      00181100
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00181200
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00181300
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00181400
.*-------------------------------------------------------------------   00181500
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00181600
&ZMS            SETC  '"PLISTVER='         *KPOSA* MNOTE                00181700
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00181800
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00181900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00182000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00182100
                AGO   .ZPVCK2                                           00182200
.ZPVCK3         ANOP  ,                                                 00182300
.*-------------------------------------------------------------------   00182400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00182500
&ZMS            SETC  '"PLISTVER='         *KGPRA* MNOTE                00182600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00182700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00182800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00182900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00183000
                AGO   .ZPVCKE                                           00183100
.ZPVCK2         ANOP  ,                                                 00183200
.*-------------------------------------------------------------------   00183300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00183400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00183500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00183600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00183700
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00183800
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00183900
.ZPVCK6         ANOP  ,                                                 00184000
&ZMS            SETC  '"PLISTVER='         *KRNGA* MNOTE                00184100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00184200
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00184300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00184400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00184500
&XPLISTVER      SETA  4                                                 00184600
                AGO   .ZPVCKE                                           00184700
.*-------------------------------------------------------------------   00184800
.ZPVCK4A        ANOP  ,                                                 00184900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00185000
&ZMS            SETC  'PLISTVER='          *KVALA* MNOTE                00185100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00185200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00185300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00185400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00185500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00185600
                AGO   .ZPVCK5                                           00185700
.ZPVCK4         ANOP  ,                                                 00185800
&XPLISTVER      SETA  &PLISTVER(1)                                      00185900
.ZPVCK5         ANOP  ,                                                 00186000
.ZPVCKE         ANOP  ,                                                 00186100
.********************************************************************   00186200
