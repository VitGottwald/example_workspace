.***AP.X01  FORMAT GENERATED CODE                                       00834800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00834900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00835000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00835100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00835200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00835300
                      AIF   (K'&ZLD LE 0).ZL51                          00835400
&ZLC                  SETA  35             COMMENT COLUMN               00835500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00835600
&ZLC                  SETA  33             COMMENT COLUMN               00835700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00835800
&ZLV                  SETC  '&ZLV'.' '                                  00835900
                      AGO   .ZL31                                       00836000
.*                                                                      00836100
.ZL41                 ANOP  ,                                           00836200
&ZLV                  SETC  '&ZLV'.' '                                  00836300
.ZL51                 ANOP  ,                                           00836400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00836500
.*                                                                      00836600
&ZLL     &ZLOP &ZLV                                                     00836700
.*                                                                      00836800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00836900
                      AGO   .ZL21          CONTINUE LOOPING             00837000
.*                                                                      00837100
.ZLX1                 ANOP  ,                                           00837200
.*******************************************************************    00837300
.***AP.X02  END OF ANTRQTF MACRO                                        00837400
