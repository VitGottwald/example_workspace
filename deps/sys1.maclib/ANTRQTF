                  AIF   ('&ZCGPR' EQ '0').ZE158Q                        00678600
&ZCGPR            SETC  '&ZCGPR'(2,1)                                   00678700
                  AIF   (T'&ZCGPR NE 'N').ZE158Q  INVALID REG NAME      00678800
.ZE158P           ANOP  ,                                               00678900
                  AIF   (T'&ZCGPR NE 'N').ZE158R  NON NUMERIC           00679000
                  AIF   ('&ZCGPR' LT '2').ZE158Q                        00679100
                  AIF   ('&ZCGPR' LE '12').ZE158R                       00679200
.ZE158Q           ANOP  ,                                               00679300
&ZMS              SETC  '"RSNCODE='             *KGPRA* MNOTE           00679400
&ZMS              SETC  '&ZMS.&RSNCODE"'                                00679500
&ZMS              SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'       00679600
                  MNOTE 8,' &ZMS IN THE RANGE (2)-(12).'                00679700
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00679800
.ZE158R           ANOP  ,                                               00679900
.******************************************************************     00680000
.***AF.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00680100
.******************************************************************     00680200
                  ACTR  10000                                           00680300
.*----------------------------------------------------------------***   00680400
.***AF.P02 EXTRA POSITIONALS                                            00680500
                  AIF   (N'&SYSLIST LE 0).ZELISTX  NO EXTRAS            00680600
&ZCVAL            SETC  ''          ERROR FIELD                         00680700
&ZAWRK            SETA  0           POSITIONAL NUMBER                   00680800
.ZELIST2          ANOP  ,                                               00680900
&ZAWRK            SETA  &ZAWRK+1    NEXT POSITIONAL                     00681000
                  AIF   (&ZAWRK GT N'&SYSLIST).ZELIST3  DONE            00681100
&ZCVAL            SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                 00681200
                  AGO   .ZELIST2                                        00681300
.*                                                                      00681400
.ZELIST3          ANOP  ,                                               00681500
&ZCVAL            SETC '&ZCVAL'(2,K'&ZCVAL-1)   STRIP COMMA             00681600
&ZMS              SETC  'POSITIONAL ARGUMENT(S),'  *PPOSA* MNOTE        00681700
&ZMS              SETC  '&ZMS "&ZCVAL"'                                 00681800
