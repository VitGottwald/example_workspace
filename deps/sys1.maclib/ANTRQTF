.*******************************************************************    00828800
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00828900
.*******************************************************************    00829000
.ZMFLX                ANOP  ,              MF=L PROCESSING              00829100
.*******************************************************************    00829200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00829300
.*******************************************************************    00829400
                      ACTR  10000                                       00829500
.*                                                                      00829600
                      LCLA  &ZLC           COLUMN POSITION              00829700
                      LCLA  &ZOPX          LIST TABLE INDEX             00829800
                      LCLC  &ZLL           LABEL                        00829900
                      LCLC  &ZLOP          OPERATION                    00830000
                      LCLC  &ZLV           VALUE                        00830100
                      LCLC  &ZLD           DESCRIPTION                  00830200
.*                                                                      00830300
&ZOPX                 SETA  1                                           00830400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00830500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00830600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00830700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00830800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00830900
                      AIF   (K'&ZLD LE 0).ZL5                           00831000
&ZLC                  SETA  35             COMMENT COLUMN               00831100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00831200
&ZLC                  SETA  33             COMMMENT COLUMN              00831300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00831400
&ZLV                  SETC  '&ZLV'.' '                                  00831500
                      AGO   .ZL3                                        00831600
.*                                                                      00831700
.ZL4                  ANOP  ,                                           00831800
&ZLV                  SETC  '&ZLV'.' '                                  00831900
.ZL5                  ANOP  ,                                           00832000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00832100
.*                                                                      00832200
&ZLL     &ZLOP &ZLV                                                     00832300
.*                                                                      00832400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00832500
                      AGO   .ZL2           CONTINUE LOOPING             00832600
.*                                                                      00832700
.ZLX                  ANOP  ,                                           00832800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00832900
                      AIF   ('&XMF' EQ 'M').ZLINKX  MF=M                00833000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00833100
         L     14,16(,0)                                                00833200
         L     14,1216(,14)                                             00833300
         L     14,44(,14)                                               00833400
         L     14,8(,14)                                                00833500
         L     14,88(,14)                                               00833600
         ICM   14,15,324(14)                                            00833700
         BNZ   A&SYSNDX                                                 00833800
         L     15,=A(RQST_PC_NUMBER_ZERO)                               00833900
         SLR   0,0                                                      00834000
         B     B&SYSNDX                                                 00834100
A&SYSNDX DS    0H                                                       00834200
         PC    0(14)                                                    00834300
B&SYSNDX DS    0H                                                       00834400
