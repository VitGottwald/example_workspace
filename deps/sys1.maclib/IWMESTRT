.***AC.PLV01  PROCESS PLISTVER                                      *   00078800
.********************************************************************   00078900
&XPLISTVER      SETA  0                                                 00079000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00079100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00079200
&XPLISTVER      SETA  0                                                 00079300
.ZPLV0          ANOP  ,                                                 00079400
.*                                                                      00079500
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00079600
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00079700
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00079800
.*-------------------------------------------------------------------   00079900
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00080000
&ZMS            SETC  '"PLISTVER='                                      00080100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00080200
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00080300
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00080400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00080500
                AGO   .ZPVCK2                                           00080600
.ZPVCK3         ANOP  ,                                                 00080700
.*-------------------------------------------------------------------   00080800
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00080900
&ZMS            SETC  '"PLISTVER='                                      00081000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00081100
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00081200
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00081300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00081400
                AGO   .ZPVCKE                                           00081500
.ZPVCK2         ANOP  ,                                                 00081600
.*-------------------------------------------------------------------   00081700
&ZCVAL          SETC  '&PLISTVER(1)'                                    00081800
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00081900
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00082000
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00082100
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00082200
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00082300
.ZPVCK6         ANOP  ,                                                 00082400
&ZMS            SETC  '"PLISTVER='                                      00082500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00082600
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00082700
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00082800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00082900
&XPLISTVER      SETA  0                                                 00083000
                AGO   .ZPVCKE                                           00083100
.*-------------------------------------------------------------------   00083200
.ZPVCK4A        ANOP  ,                                                 00083300
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00083400
&ZMS            SETC  'PLISTVER='                                       00083500
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00083600
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00083700
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00083800
                MNOTE 8,' &ZMS IS REQUIRED.'                            00083900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00084000
                AGO   .ZPVCK5                                           00084100
.ZPVCK4         ANOP  ,                                                 00084200
&XPLISTVER      SETA  &PLISTVER(1)                                      00084300
.ZPVCK5         ANOP  ,                                                 00084400
.ZPVCKE         ANOP  ,                                                 00084500
.********************************************************************   00084600
