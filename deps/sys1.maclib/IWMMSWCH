.ENDCOMPCODE2    ANOP                                                   00156200
.*                                                                      00156300
         MVI   PB_SWITCH_INFO,PB_SWITCH_&XWHERE REFLECT CONTINUATION    00156400
         DROP  14                                                       00156500
END&SYSNDX DS    0H                    END LABEL                        00156600
.*******************************************************************    00156700
.***        RETURN CODE PROCESSING                                      00156800
.*******************************************************************    00156900
         AIF   (N'&RETCODE LE 0).IWMMSWCH_ENDRC_EXP                     00157000
         AIF   ('&XRETCODE'(1,1) EQ '(').IWMMSWCH_RC_REG                00157100
         AIF   ('&XRETCODE' EQ 'GPR15').IWMMSWCH_ENDRC_EXP              00157200
