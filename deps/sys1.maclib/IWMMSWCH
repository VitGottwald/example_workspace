         MNOTE 8,'&T1'                                                  00148500
         MEXIT                                                          00148600
.MFCHECKING ANOP                                                        00148700
         AIF   ('&XFUNCTION' EQ 'CONTINUE').TESTMF                      00148800
         AIF   ('&XFUNCTION' EQ 'RETURN').MFOK                          00148900
         AGO   .LINKAGELOGIC                                            00149000
.TESTMF  ANOP                                                           00149100
    AIF ('&MF' EQ '').MFOK                                              00149200
&T1      SETC  'MF IS NOT VALID WITH FUNCTION(CONTINUE)'                00149300
         MNOTE 4,'&T1'                                                  00149400
.MFOK    ANOP                                                           00149500
&T1      SETC  '&MONTKN'                                                00149600
         AIF   ('&MONTKN'(1,1) NE '(').MONTKNRS                         00149700
&T1      SETC  '&MONTKN'(2,K'&MONTKN-2)                                 00149800
&T1      SETC  '0(&T1.)'                                                00149900
.MONTKNRS  ANOP                                                         00150000
.* &T1 IS NOW A VALID BASE-DISPLACEMENT EXPRESSION                      00150100
         ICM   14,15,&T1               FETCH MONITORING TOKEN           00150200
.* GENERATE CODE WHICH DEPENDS ON HOW COMPCODE WAS SPECIFIED            00150300
         AIF   ('&XCOMPCODE' NE 'YES').NOCOMPCODE1                      00150400
         BNZ   MONENV&SYSNDX           BRANCH WHEN MON ENV IS PRESENT   00150500
         LA    0,IWMRSNCODENOMONENV    SET REASON CODE = NO MON ENV     00150600
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00150700
         B     END&SYSNDX              BRANCH TO END LABEL              00150800
MONENV&SYSNDX DS    0H                 BRANCH LABEL FOR MON ENV PRESENT 00150900
         AIF   ('&XRUNTIME_VER' EQ 'MINIMAL').ENDCOMPCODE1              00151000
         CLC   PB_ID-PB(4,14),=CL4'PB  '                                00151100
         BE    MONENVOK&SYSNDX         BRANCH WHEN MON ENV PASSES       00151200
         LA    0,IWMRSNCODEBADMONENV   SET REASON CODE = BAD MON ENV    00151300
         LA    15,IWMRETCODEINVOCERROR SET INVOCATION ERROR RETURN CODE 00151400
         B     END&SYSNDX              BRANCH TO END LABEL              00151500
MONENVOK&SYSNDX DS    0H               BRANCH LABEL FOR MON ENV OK      00151600
                AGO .ENDCOMPCODE1                                       00151700
.* NOW GENERATE CODE WHEN COMPCODE(NO) WAS SPECIFIED                    00151800
.NOCOMPCODE1    ANOP                                                    00151900
         BZ    END&SYSNDX              BRANCH TO END LABEL              00152000
         AIF   ('&XRUNTIME_VER' EQ 'MINIMAL').ENDCOMPCODE1              00152100
         CLC   PB_ID-PB(4,14),=CL4'PB  '                                00152200
         BNE   END&SYSNDX              BRANCH TO END LABEL              00152300
.ENDCOMPCODE1    ANOP                                                   00152400
         USING PB,14                                                    00152500
.* EWLM Support (Begin)                                             @P2 00152600
         CLI   PB_VERSION,PB_VERSION6  IF VERSION < 6               @P2 00152700
         BL    NOEWLM_&SYSNDX           OR                          @P2 00152800
         TM    PB_FLAGS,PB_EWLM_ENABLED  EWLM IS NOT ENABLED        @P2 00152900
         BNO   NOEWLM_&SYSNDX           THEN SKIP.                  @P2 00153000
         MVI   PB_EWLM_REQUEST,PB_EWLM_REQUEST_SWITCH_&XFUNCTION    @P2 00153100
         LR    1,14                    PB ADDR. FOR SERVICE ROUTINE @P2 00153200
         L     15,X'10'                ADDRESS THE CVT              @P2 00153300
         L     15,X'8C'(15,0)          ADDRESS THE ECVT             @P2 00153400
         L     15,X'C4'(15,0)          ADDRESS THE WMVT             @P2 00153500
         L     15,X'13C'(15,0)         GET SERVICE ROUTINE ADDR     @P2 00153600
         BASR  14,15                   INVOKE SEVICE ROUTINE        @P2 00153700
         LR    14,1                    RESTORE ADDRESS OF PB        @P2 00153800
NOEWLM_&SYSNDX DS 0H                                                @P2 00153900
.* EWLM Support (End)                                               @P2 00154000
         ICM   15,15,PB_DEP_MIRROR_TKN  EXTRACT DEPENDENT MIRROR TOKEN  00154100
.*                                                                      00154200
         AIF   ('&XFUNCTION' NE 'CONTINUE').ENDCONTINUE                 00154300
.*                                                                      00154400
.*******************************************************************    00154500
.*                                                                      00154600
.*       FOLLOWING IS LOGIC FOR FUNCTION(CONTINUE)                      00154700
.*                                                                      00154800
.*******************************************************************    00154900
.* GENERATE CODE WHICH DEPENDS ON HOW COMPCODE WAS SPECIFIED            00155000
