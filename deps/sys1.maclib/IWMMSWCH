.*******************************************************************    00201700
.***AP.X01  FORMAT GENERATED CODE                                       00201800
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00201900
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00202000
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00202100
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00202200
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00202300
                      AIF   (K'&ZLD LE 0).ZL51                          00202400
&ZLC                  SETA  35             COMMENT COLUMN               00202500
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00202600
&ZLC                  SETA  33             COMMENT COLUMN               00202700
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00202800
&ZLV                  SETC  '&ZLV'.' '                                  00202900
                      AGO   .ZL31                                       00203000
.*                                                                      00203100
.ZL41                 ANOP  ,                                           00203200
&ZLV                  SETC  '&ZLV'.' '                                  00203300
.ZL51                 ANOP  ,                                           00203400
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00203500
.*                                                                      00203600
&ZLL     &ZLOP &ZLV                                                     00203700
.*                                                                      00203800
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00203900
                      AGO   .ZL21          CONTINUE LOOPING             00204000
.*                                                                      00204100
.ZLX1                 ANOP  ,                                           00204200
.*******************************************************************    00204300
.***AP.X02  END OF IWMMSWCH MACRO                                       00204400
&ZIWMMSWCH            SETC  'YES'          SET IWMMSWCH USED            00204500
         POP   PRINT                                                    00204600
*                                            IWMMSWCH-0                 00204700
         SPACE 1                                                        00204800
.ZLX2                 ANOP  ,                                           00204900
         MEND                                                           00205000
**********************************************************************/ 00205100
*/*PB.OC01 INVOKE MACRO---------------------------------------------*/  00205200
*%IWMMSWCH: MACRO KEYS(FUNCTION,RUNTIME_VER,WHERE,MONTKN,COMPCODE,      00205300
*                 RETCODE,RSNCODE,PLISTVER,MF);                         00205400
*  ANS('?'!!MACLABEL!!' '!!'IWMMSWCP '!!MACLIST!!                       00205500
*         ' '!!MACKEYS!!';') COL(MACLMAR);                              00205600
*%END IWMMSWCH;                                                         00205700
