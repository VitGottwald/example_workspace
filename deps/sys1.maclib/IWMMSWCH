.ENDCONTINUE ANOP                                                       00161900
.*                                                                      00162000
.*******************************************************************    00162100
.*                                                                      00162200
.*       FOLLOWING IS LOGIC FOR FUNCTION(RETURN)                        00162300
.*                                                                      00162400
.*******************************************************************    00162500
         BC    7,IWMMSWCH_RET&SYSNDX    BRANCH IF CURRENT CONTINUATION  00162600
         MVI   PB_SWITCH_INFO,X'00'     REFLECT NO SWITCH OUTSTANDING   00162700
.* GENERATE CODE WHICH DEPENDS ON HOW COMPCODE WAS SPECIFIED            00162800
         AIF   ('&XCOMPCODE' NE 'YES').NOCOMPCODE3                      00162900
         LA    15,IWMRETCODEOK          SET RETURN CODE = OK            00163000
.NOCOMPCODE3    ANOP                                                    00163100
         B     END&SYSNDX               BRANCH TO END LABEL             00163200
IWMMSWCH_RET&SYSNDX DS    0H            CURRENT CONTINUATION EXISTS     00163300
         C     15,PB_MIRROR_TKN                                         00163400
         BE    IWMMSWCH_PC&SYSNDX       BRANCH TO FUNCTION(RETURN) PC   00163500
         MVI   PB_SWITCH_INFO,X'00'     REFLECT NO SWITCH OUTSTANDING   00163600
         SLR   1,1                                                      00163700
         ST    1,PB_DEP_MIRROR_TKN      CLEAR THE CONTINUATION INFO     00163800
         ST    1,PB_DEP_MONTKN          CLEAR THE CONTINUATION INFO     00163900
.* GENERATE CODE WHICH DEPENDS ON HOW COMPCODE WAS SPECIFIED            00164000
         AIF   ('&XCOMPCODE' NE 'YES').NOCOMPCODE4                      00164100
         LA    15,IWMRETCODEWARNING     SET RETURN CODE = WARNING       00164200
         LA    0,IWMRSNCODERETURNCONT   SET RSN CODE = RETURN FROM CONT 00164300
.NOCOMPCODE4    ANOP                                                    00164400
         B     END&SYSNDX               BRANCH TO END LABEL             00164500
         DROP  14                                                       00164600
.*                                                                      00164700
IWMMSWCH_PC&SYSNDX DS    0H             FUNCTION(RETURN) PC RTN NEEDED  00164800
.*                                                                      00164900
.LINKAGELOGIC ANOP                                                      00165000
