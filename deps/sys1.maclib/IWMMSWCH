.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00195800
.*******************************************************************    00195900
                      ACTR  10000                                       00196000
.*                                                                      00196100
                      LCLA  &ZLC           COLUMN POSITION              00196200
                      LCLA  &ZOPX          LIST TABLE INDEX             00196300
                      LCLC  &ZLL           LABEL                        00196400
                      LCLC  &ZLOP          OPERATION                    00196500
                      LCLC  &ZLV           VALUE                        00196600
                      LCLC  &ZLD           DESCRIPTION                  00196700
.*                                                                      00196800
&ZOPX                 SETA  1                                           00196900
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00197000
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00197100
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00197200
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00197300
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00197400
                      AIF   (K'&ZLD LE 0).ZL5                           00197500
&ZLC                  SETA  35             COMMENT COLUMN               00197600
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00197700
&ZLC                  SETA  33             COMMMENT COLUMN              00197800
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00197900
&ZLV                  SETC  '&ZLV'.' '                                  00198000
                      AGO   .ZL3                                        00198100
.*                                                                      00198200
.ZL4                  ANOP  ,                                           00198300
&ZLV                  SETC  '&ZLV'.' '                                  00198400
.ZL5                  ANOP  ,                                           00198500
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00198600
.*                                                                      00198700
&ZLL     &ZLOP &ZLV                                                     00198800
.*                                                                      00198900
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00199000
                      AGO   .ZL2           CONTINUE LOOPING             00199100
.*                                                                      00199200
.ZLX                  ANOP  ,                                           00199300
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00199400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00199500
         L     14,16(0,0)              GET CVT ADDRESS                  00199600
         TM    CVTOSLV1-CVT(14),X'40'    TEST FOR WLM PRESENCE          00199700
.* GENERATE CODE WHICH DEPENDS ON HOW COMPCODE WAS SPECIFIED            00199800
         AIF   ('&XCOMPCODE' NE 'YES').NOCC1                            00199900
         BO    WLM&SYSNDX              BRANCH WHEN WLM IS PRESENT       00200000
         LA    0,IWMRSNCODENOWLM       SET REASON CODE = NO WLM         00200100
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00200200
         B     END&SYSNDX              BRANCH TO END LABEL              00200300
WLM&SYSNDX DS    0H                    BRANCH LABEL FOR WLM PRESENT     00200400
                AGO .ENDCC1                                             00200500
.NOCC1          ANOP                                                    00200600
         BZ    END&SYSNDX              BRANCH TO END LABEL              00200700
.ENDCC1         ANOP                                                    00200800
         L     14,772(14,0)            GET SFT ADDRESS                  00200900
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00201000
         LA    15,5                    GET EX FOR SERVICE ROUTINE       00201100
         OR    14,15                   GET PC NUMBER                    00201200
         PC    0(14)                                                    00201300
END&SYSNDX DS    0H                    END LABEL                        00201400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00201500
