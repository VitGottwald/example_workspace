         ST    15,&XRETCODE                                             00157600
         AGO   .IWMMSWCH_ENDRC_EXP                                      00157700
.*                                                                      00157800
.IWMMSWCH_RC_REG      ANOP  ,                                           00157900
&T1      SETC  '&XRETCODE'(2,K'&XRETCODE-2)                             00158000
         AIF   ('&T1' EQ 'GPR15').IWMMSWCH_ENDRC_EXP                    00158100
         AIF   ('&T1' EQ 'REG15').IWMMSWCH_ENDRC_EXP                    00158200
         AIF   ('&T1' EQ 'R15').IWMMSWCH_ENDRC_EXP                      00158300
         AIF   ('&T1' EQ '15').IWMMSWCH_ENDRC_EXP                       00158400
         LR    &T1,15                                                   00158500
.IWMMSWCH_ENDRC_EXP   ANOP  ,                                           00158600
.*******************************************************************    00158700
.***        REASON CODE PROCESSING                                      00158800
.*******************************************************************    00158900
         AIF   (N'&RSNCODE LE 0).IWMMSWCH_ENDRSN_EXP                    00159000
         AIF   ('&XRSNCODE'(1,1) EQ '(').IWMMSWCH_RSN_REG               00159100
