                 AIF   ('&XMF' EQ 'L').XLAB3                            00146300
&M0M0001 DS    0H                             IWMMSWCH-0                00146400
.XLAB3           ANOP  ,                                                00146500
.*******************************************************************    00146600
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00146700
.*******************************************************************    00146800
              PUSH  PRINT                                               00146900
                 AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON       00147000
                 PRINT OFF         ++ PR01                              00147100
                 AGO   .ZPRNTXX                                         00147200
.ZPRINTX         PRINT ON                                               00147300
.ZPRNTXX         ANOP  ,                                                00147400
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00147500
         LCLC  &T1                                                      00147600
         AIF   ('&XCOMPCODE' EQ 'YES').MFCHECKING                       00147700
         AIF   ('&XRETCODE' EQ '').RSNCODECHK                           00147800
&T1      SETC  'RETCODE, RSNCODE MAY NOT BE USED WITH COMPCODE(NO)'     00147900
         MNOTE 8,'&T1'                                                  00148000
         MEXIT                                                          00148100
.RSNCODECHK ANOP                                                        00148200
         AIF   ('&XRSNCODE' EQ '').MFCHECKING                           00148300
