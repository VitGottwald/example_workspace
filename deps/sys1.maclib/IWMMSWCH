.***AC.PLV01  PROCESS PLISTVER                                      *   00065300
.********************************************************************   00065400
&XPLISTVER      SETA  0                                                 00065500
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00065600
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00065700
&XPLISTVER      SETA  0                                                 00065800
.ZPLV0          ANOP  ,                                                 00065900
.*                                                                      00066000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00066100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00066200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00066300
.*-------------------------------------------------------------------   00066400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00066500
&ZMS            SETC  '"PLISTVER='                                      00066600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00066800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00066900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067000
                AGO   .ZPVCK2                                           00067100
.ZPVCK3         ANOP  ,                                                 00067200
.*-------------------------------------------------------------------   00067300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00067400
&ZMS            SETC  '"PLISTVER='                                      00067500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00067600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00067700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00067800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00067900
                AGO   .ZPVCKE                                           00068000
.ZPVCK2         ANOP  ,                                                 00068100
.*-------------------------------------------------------------------   00068200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00068300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00068400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00068500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00068600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00068700
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00068800
.ZPVCK6         ANOP  ,                                                 00068900
&ZMS            SETC  '"PLISTVER='                                      00069000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00069100
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00069200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00069300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069400
&XPLISTVER      SETA  0                                                 00069500
                AGO   .ZPVCKE                                           00069600
.*-------------------------------------------------------------------   00069700
.ZPVCK4A        ANOP  ,                                                 00069800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00069900
&ZMS            SETC  'PLISTVER='                                       00070000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00070100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00070200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00070300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00070400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00070500
                AGO   .ZPVCK5                                           00070600
.ZPVCK4         ANOP  ,                                                 00070700
&XPLISTVER      SETA  &PLISTVER(1)                                      00070800
.ZPVCK5         ANOP  ,                                                 00070900
.ZPVCKE         ANOP  ,                                                 00071000
.********************************************************************   00071100
