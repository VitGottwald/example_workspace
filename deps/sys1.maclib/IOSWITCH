.*******************************************************************    00151300
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00151400
.*******************************************************************    00151500
                      ACTR  10000                                       00151600
.*                                                                      00151700
                      LCLA  &ZLC           COLUMN POSITION              00151800
                      LCLA  &ZOPX          LIST TABLE INDEX             00151900
                      LCLC  &ZLL           LABEL                        00152000
                      LCLC  &ZLOP          OPERATION                    00152100
                      LCLC  &ZLV           VALUE                        00152200
                      LCLC  &ZLD           DESCRIPTION                  00152300
.*                                                                      00152400
&ZOPX                 SETA  1                                           00152500
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00152600
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00152700
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00152800
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00152900
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00153000
                      AIF   (K'&ZLD LE 0).ZL5                           00153100
&ZLC                  SETA  35             COMMENT COLUMN               00153200
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00153300
&ZLC                  SETA  33             COMMMENT COLUMN              00153400
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00153500
&ZLV                  SETC  '&ZLV'.' '                                  00153600
                      AGO   .ZL3                                        00153700
.*                                                                      00153800
.ZL4                  ANOP  ,                                           00153900
&ZLV                  SETC  '&ZLV'.' '                                  00154000
.ZL5                  ANOP  ,                                           00154100
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00154200
.*                                                                      00154300
&ZLL     &ZLOP &ZLV                                                     00154400
.*                                                                      00154500
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00154600
                      AGO   .ZL2           CONTINUE LOOPING             00154700
.*                                                                      00154800
.ZLX                  ANOP  ,                                           00154900
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00155000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00155100
.*********************************************************************  00155200
.* Gets addressability to the IOVT and check to see if space-        *  00155300
.* switching PCs are enabled.  If not, return a return code 4 and    *  00155400
.* reason code 2 to the caller.                                      *  00155500
.*                                                                   *  00155600
.* Note: This handles the cases where a user may attempt to call     *  00155700
.*       IOSVIOSW at a point where the PC tables are not yet         *  00155800
.*       initialized                                                 *  00155900
.*********************************************************************  00156000
         L     15,16(0,0)         ++ GET CVT ADDRESS                    00156100
         L     14,124(15,0)       ++ GET IOCOM ADDRESS                  00156200
         L     14,208(14,0)       ++ GET IOVT ADDRESS                   00156300
         TM    5(14),B'00001000'  ++ CHECK IOVTSSPC FLAG                00156400
         BNZ   IOSW1&SYSNDX       ++ IF FLAG IS ON, ISSUE PC            00156500
         LA    15,4               ++ SET WARNING RETURN CODE            00156600
         LA    0,2                ++ SET WARNING REASON CODE            00156700
         B     IOSW2&SYSNDX       ++ BRANCH AROUND PC                   00156800
.*********************************************************************  00156900
.*                                                                   *  00157000
.*  PC linkage                                                       *  00157100
.*                                                                   *  00157200
.*********************************************************************  00157300
IOSW1&SYSNDX DS    0H                                                   00157400
         L     14,772(15,0)       ++ GET SFT ADDRESS                    00157500
.*********************************************************************  00157600
.*                                                                   *  00157700
.*  Get LX/EX for IOSWITCH Service                                   *  00157800
.*                                                                   *  00157900
.*  Note: LX/EX is calculated by multiplying the PC number (129)     *  00158000
.*        times the length of a PC/AUTH table entry (4) and          *  00158100
.*        then subtracting 4.                                        *  00158200
.*                                                                   *  00158300
.*********************************************************************  00158400
         L     14,512(14,0)       ++ GET LX/EX FOR IOSWITCH Service     00158500
         PC    0(14)              ++ PC TO IOSVIOSW                     00158600
         LA    1,&XMFCTRL         ++ GET PARAMETER LIST ADDRESS    @01C 00158700
IOSW2&SYSNDX DS    0H                                                   00158800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00158900
.ZLINKX               ANOP  ,                                           00159000
.*******************************************************************    00159100
.***AP.X01  FORMAT GENERATED CODE                                       00159200
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00159300
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00159400
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00159500
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00159600
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00159700
                      AIF   (K'&ZLD LE 0).ZL51                          00159800
&ZLC                  SETA  35             COMMENT COLUMN               00159900
                      AIF   ('&ZLD'(1,1) NE '*').ZL31                   00160000
&ZLC                  SETA  33             COMMENT COLUMN               00160100
.ZL31                 AIF   (K'&ZLV GE &ZLC-16-1).ZL41                  00160200
&ZLV                  SETC  '&ZLV'.' '                                  00160300
                      AGO   .ZL31                                       00160400
.*                                                                      00160500
.ZL41                 ANOP  ,                                           00160600
&ZLV                  SETC  '&ZLV'.' '                                  00160700
.ZL51                 ANOP  ,                                           00160800
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00160900
.*                                                                      00161000
&ZLL     &ZLOP &ZLV                                                     00161100
.*                                                                      00161200
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00161300
                      AGO   .ZL21          CONTINUE LOOPING             00161400
.*                                                                      00161500
.ZLX1                 ANOP  ,                                           00161600
.*******************************************************************    00161700
.***AP.X02  END OF IOSWITCH MACRO                                       00161800
&ZIOSWITCH            SETC  'YES'          SET IOSWITCH USED            00161900
         POP   PRINT                                                    00162000
*                                            IOSWITCH-0                 00162100
         SPACE 1                                                        00162200
