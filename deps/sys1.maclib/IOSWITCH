.***AB.13  DEFINE MACRO VARIABLES FOR INPUT/OUTPUT PROCESSING           00048600
.*******************************************************************    00048700
                LCLC  &ZXSWITCH   ++ INPUT NAME                         00048800
                LCLC  &ZXSWITCHAREA             ++ OUTPUT NAME          00048900
                LCLC  &ZXSWITCHLEN              ++ OUTPUT NAME          00049000
                LCLC  &ZXSUBPOOL  ++ INPUT NAME                         00049100
                LCLC  &ZXOFFLINE  ++ INPUT VALUE                        00049200
                LCLC  &ZXRETCODE  ++ OUTPUT NAME                        00049300
                LCLC  &ZXRSNCODE  ++ OUTPUT NAME                        00049400
                LCLC  &ZXPLISTVER               ++ INPUT VALUE          00049500
                LCLC  &ZXMF       ++ INPUT VALUE                        00049600
                LCLC  &ZXMFCTRL   ++ INPUT NAME                         00049700
                LCLC  &ZXMFATTR   ++ INPUT VALUE                        00049800
                LCLC  &ZXMFSCK    ++ INPUT VALUE                        00049900
.********************************************************************   00050000
.***AC.PLV01  PROCESS PLISTVER                                      *   00050100
.********************************************************************   00050200
&XPLISTVER      SETA  0                                                 00050300
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00050400
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00050500
&XPLISTVER      SETA  0                                                 00050600
.ZPLV0          ANOP  ,                                                 00050700
.*                                                                      00050800
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00050900
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00051000
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00051100
.*-------------------------------------------------------------------   00051200
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00051300
&ZMS            SETC  '"PLISTVER='                                      00051400
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00051500
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00051600
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00051700
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00051800
                AGO   .ZPVCK2                                           00051900
.ZPVCK3         ANOP  ,                                                 00052000
.*-------------------------------------------------------------------   00052100
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00052200
&ZMS            SETC  '"PLISTVER='                                      00052300
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00052400
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00052500
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00052600
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00052700
                AGO   .ZPVCKE                                           00052800
.ZPVCK2         ANOP  ,                                                 00052900
.*-------------------------------------------------------------------   00053000
&ZCVAL          SETC  '&PLISTVER(1)'                                    00053100
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00053200
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00053300
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00053400
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00053500
                AIF   (&ZN LE 0).ZPVCK4A   NOT HIGH                     00053600
.ZPVCK6         ANOP  ,                                                 00053700
&ZMS            SETC  '"PLISTVER='                                      00053800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00053900
&ZMS            SETC  '&ZMS THE RANGE IS "0-0"'                         00054000
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00054100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00054200
&XPLISTVER      SETA  0                                                 00054300
                AGO   .ZPVCKE                                           00054400
.*-------------------------------------------------------------------   00054500
.ZPVCK4A        ANOP  ,                                                 00054600
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00054700
&ZMS            SETC  'PLISTVER='                                       00054800
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00054900
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00055000
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00055100
                MNOTE 8,' &ZMS IS REQUIRED.'                            00055200
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00055300
                AGO   .ZPVCK5                                           00055400
.ZPVCK4         ANOP  ,                                                 00055500
&XPLISTVER      SETA  &PLISTVER(1)                                      00055600
.ZPVCK5         ANOP  ,                                                 00055700
.ZPVCKE         ANOP  ,                                                 00055800
.********************************************************************   00055900
