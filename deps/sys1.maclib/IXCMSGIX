&ZMS            SETC  '&ZMS "PLISTVER"'                                 00158900
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00159000
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00159100
                AGO   .ZPVCK2                                           00159200
.ZPVCK3         ANOP  ,                                                 00159300
.*-------------------------------------------------------------------   00159400
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00159500
&ZMS            SETC  '"PLISTVER='                                      00159600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00159700
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00159800
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00159900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00160000
                AGO   .ZPVCKE                                           00160100
.ZPVCK2         ANOP  ,                                                 00160200
.*-------------------------------------------------------------------   00160300
&ZCVAL          SETC  '&PLISTVER(1)'                                    00160400
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00160500
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00160600
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00160700
                AIF   (&ZN LT 4).ZPVCK6    TOO LOW                      00160800
                AIF   (&ZN LE 6).ZPVCK4A   NOT HIGH                     00160900
.ZPVCK6         ANOP  ,                                                 00161000
&ZMS            SETC  '"PLISTVER='                                      00161100
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00161200
&ZMS            SETC  '&ZMS THE RANGE IS "4-6"'                         00161300
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00161400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00161500
&XPLISTVER      SETA  6                                                 00161600
                AGO   .ZPVCKE                                           00161700
.*-------------------------------------------------------------------   00161800
.ZPVCK4A        ANOP  ,                                                 00161900
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00162000
&ZMS            SETC  'PLISTVER='                                       00162100
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00162200
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00162300
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00162400
                MNOTE 8,' &ZMS IS REQUIRED.'                            00162500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00162600
                AGO   .ZPVCK5                                           00162700
.ZPVCK4         ANOP  ,                                                 00162800
&XPLISTVER      SETA  &PLISTVER(1)                                      00162900
.ZPVCK5         ANOP  ,                                                 00163000
.ZPVCKE         ANOP  ,                                                 00163100
