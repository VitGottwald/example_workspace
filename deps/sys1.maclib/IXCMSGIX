&ZOPN1                SETA &ZOPN                                        00500300
.*******************************************************************    00500400
.***  PROCESS IXCMSGIX OUTPUT PARAMETERS                                00500500
.*******************************************************************    00500600
                      ACTR  10000                                       00500700
.*******************************************************************    00500800
.***AO.X02  RETURN CODE PROCESSING                                      00500900
.*******************************************************************    00501000
                      AIF   (N'&RETCODE LE 0).ZMRETX                    00501100
                      AIF   ('&XRETCODE'(1,1) EQ '(').ZMRET2            00501200
                      AIF   ('&XRETCODE' EQ 'GPR15').ZMRETX             00501300
                      AIF   ('&XRETCODE' EQ 'REG15').ZMRETX             00501400
                      AIF   ('&XRETCODE' EQ 'R15').ZMRETX               00501500
                      AIF   ('&XRETCODE' EQ '15').ZMRETX                00501600
&ZOPN                 SETA  &ZOPN+1                                     00501700
&ZOPL(&ZOPN)          SETC  ' '                                         00501800
&ZOPO(&ZOPN)          SETC  'ST'                                        00501900
&ZOPV(&ZOPN)          SETC  '15,&XRETCODE'                              00502000
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00502100
                      AGO   .ZMRETX                                     00502200
.*                                                                      00502300
.ZMRET2               ANOP  ,                                           00502400
&ZCGPR                SETC  '&XRETCODE'(2,K'&XRETCODE-2)                00502500
                      AIF   ('&ZCGPR' EQ 'GPR15').ZMRETX                00502600
                      AIF   ('&ZCGPR' EQ 'REG15').ZMRETX                00502700
                      AIF   ('&ZCGPR' EQ 'R15').ZMRETX                  00502800
                      AIF   ('&ZCGPR' EQ '15').ZMRETX                   00502900
&ZOPN                 SETA  &ZOPN+1                                     00503000
&ZOPL(&ZOPN)          SETC  ' '                                         00503100
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00503200
&ZOPV(&ZOPN)          SETC  '&ZCGPR,15'                                 00503300
&ZOPD(&ZOPN)          SETC  '++ SET RETURN CODE'                        00503400
.ZMRETX               ANOP  ,                                           00503500
.*******************************************************************    00503600
.***AO.X03  REASON CODE PROCESSING                                      00503700
.*******************************************************************    00503800
                      AIF   (N'&RSNCODE LE 0).ZMRSNX                    00503900
                      AIF   ('&XRSNCODE'(1,1) EQ '(').ZMRSN2            00504000
                      AIF   ('&XRSNCODE' EQ 'GPR0').ZMRSNX              00504100
                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00504200
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00504300
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00504400
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00504500
                      AIF   ('&XRSNCODE' EQ '0').ZMRSNX                 00504600
                      AIF   ('&XRSNCODE' EQ '00').ZMRSNX                00504700
&ZOPN                 SETA  &ZOPN+1                                     00504800
&ZOPL(&ZOPN)          SETC  ' '                                         00504900
&ZOPO(&ZOPN)          SETC  'ST'                                        00505000
&ZOPV(&ZOPN)          SETC  '0,&XRSNCODE'                               00505100
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00505200
                      AGO   .ZMRSNX                                     00505300
.*                                                                      00505400
.ZMRSN2               ANOP  ,                                           00505500
&ZCGPR                SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                00505600
                      AIF   ('&ZCGPR' EQ 'GPR0').ZMRSNX                 00505700
                      AIF   ('&ZCGPR' EQ 'GPR00').ZMRSNX                00505800
                      AIF   ('&ZCGPR' EQ 'REG0').ZMRSNX                 00505900
                      AIF   ('&ZCGPR' EQ 'REG00').ZMRSNX                00506000
                      AIF   ('&ZCGPR' EQ 'R0').ZMRSNX                   00506100
                      AIF   ('&ZCGPR' EQ '0').ZMRSNX                    00506200
                      AIF   ('&ZCGPR' EQ '00').ZMRSNX                   00506300
&ZOPN                 SETA  &ZOPN+1                                     00506400
&ZOPL(&ZOPN)          SETC  ' '                                         00506500
&ZOPO(&ZOPN)          SETC  '&ZMAC_LR'                                  00506600
&ZOPV(&ZOPN)          SETC  '&ZCGPR,0'                                  00506700
&ZOPD(&ZOPN)          SETC  '++ SET REASON CODE'                        00506800
.ZMRSNX               ANOP  ,                                           00506900
.*******************************************************************    00507000
.***AO.X04  LABEL FOR MF=L PROCESSING                                   00507100
.*******************************************************************    00507200
.ZMFLX                ANOP  ,              MF=L PROCESSING              00507300
.*******************************************************************    00507400
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00507500
.*******************************************************************    00507600
                      ACTR  10000                                       00507700
.*                                                                      00507800
                      LCLA  &ZLC           COLUMN POSITION              00507900
                      LCLA  &ZOPX          LIST TABLE INDEX             00508000
                      LCLC  &ZLL           LABEL                        00508100
                      LCLC  &ZLOP          OPERATION                    00508200
                      LCLC  &ZLV           VALUE                        00508300
                      LCLC  &ZLD           DESCRIPTION                  00508400
.*                                                                      00508500
&ZOPX                 SETA  1                                           00508600
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00508700
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00508800
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00508900
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00509000
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00509100
                      AIF   (K'&ZLD LE 0).ZL5                           00509200
&ZLC                  SETA  35             COMMENT COLUMN               00509300
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00509400
&ZLC                  SETA  33             COMMMENT COLUMN              00509500
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00509600
&ZLV                  SETC  '&ZLV'.' '                                  00509700
                      AGO   .ZL3                                        00509800
.*                                                                      00509900
.ZL4                  ANOP  ,                                           00510000
&ZLV                  SETC  '&ZLV'.' '                                  00510100
.ZL5                  ANOP  ,                                           00510200
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00510300
.*                                                                      00510400
&ZLL     &ZLOP &ZLV                                                     00510500
.*                                                                      00510600
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00510700
                      AGO   .ZL2           CONTINUE LOOPING             00510800
.*                                                                      00510900
.ZLX                  ANOP  ,                                           00511000
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00511100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00511200
         AIF   ('&SYSAM64' NE 'YES').NOTAM64                            00511300
         LLGTR 14,14                   CLEAR HIGH HALF                  00511400
.NOTAM64 L     14,16(0,0)              GET CVT ADDRESS                  00511500
         L     14,772(14,0)            GET SFT ADDRESS                  00511600
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00511700
         LA    15,52                   GET EX FOR SERVICE ROUTINE       00511800
         OR    14,15                   GET PC NUMBER                    00511900
         PC    0(14)                                                    00512000
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00512100
.ZLINKX               ANOP  ,                                           00512200
.*******************************************************************    00512300
.***AP.X01  FORMAT GENERATED CODE                                       00512400
.ZL21                 AIF   (&ZOPX GT &ZOPN).ZLX1  B IF DONE            00512500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00512600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00512700
