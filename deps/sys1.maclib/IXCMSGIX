                  AIF   (K'&ZXRSNCODE LE 0).ZG67R  NOT I/O              00319600
                  AIF   ('&XRSNCODE'(1,1) NE '(').ZG67R                 00319700
&ZCGPR            SETC  '&XRSNCODE'(2,K'&XRSNCODE-2)                    00319800
                  AIF   ('&ZCGPR'(1,1) NE '0').ZG67P                    00319900
                  AIF   (K'&ZCGPR GT 2).ZG67Q                           00320000
                  AIF   ('&ZCGPR' EQ '0').ZG67R                         00320100
&ZCGPR            SETC  '&ZCGPR'(2,1)                                   00320200
                  AIF   (T'&ZCGPR NE 'N').ZG67Q  INVALID REG NAME       00320300
.ZG67P            ANOP  ,                                               00320400
                  AIF   (T'&ZCGPR NE 'N').ZG67R  NON NUMERIC            00320500
                  AIF   ('&ZCGPR' EQ '0').ZG67R                         00320600
                  AIF   ('&ZCGPR' LT '2').ZG67Q                         00320700
                  AIF   ('&ZCGPR' LE '12').ZG67R                        00320800
.ZG67Q            ANOP  ,                                               00320900
&ZMS              SETC  '"RSNCODE='                                     00321000
&ZMS              SETC  '&ZMS.&RSNCODE"'                                00321100
&ZMS              SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'       00321200
                  MNOTE 8,' &ZMS IN THE RANGE (0) OR (2)-(12).'         00321300
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00321400
.ZG67R            ANOP  ,                                               00321500
.******************************************************************     00321600
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00321700
.******************************************************************     00321800
                  ACTR  10000                                           00321900
.*----------------------------------------------------------------***   00322000
.***AH.P02 EXTRA POSITIONALS                                            00322100
                  AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS            00322200
                  AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL            00322300
                  AIF   ('&SYSLIST(1)' NE '').ZGLISTX1  NOT NULL        00322400
                  AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL         00322500
.ZGLISTX1         ANOP  ,                                               00322600
&ZCVAL            SETC  ''          ERROR FIELD                         00322700
&ZAWRK            SETA  0           POSITIONAL NUMBER                   00322800
.ZGLIST2          ANOP  ,                                               00322900
&ZAWRK            SETA  &ZAWRK+1    NEXT POSITIONAL                     00323000
                  AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE            00323100
&ZCVAL            SETC '&ZCVAL,&SYSLIST(&ZAWRK)'  EXTRA                 00323200
                  AGO   .ZGLIST2                                        00323300
.*                                                                      00323400
.ZGLIST3          ANOP  ,                                               00323500
&ZCVAL            SETC '&ZCVAL'(2,K'&ZCVAL-1)   STRIP COMMA             00323600
&ZMS              SETC  'POSITIONAL ARGUMENT(S),'                       00323700
&ZMS              SETC  '&ZMS "&ZCVAL"'                                 00323800
&ZMS              SETC  '&ZMS CANNOT BE USED.'                          00323900
&ZMS              SETC  '&ZMS NO POSITIONAL ARGUMENTS'                  00324000
                  MNOTE 8,' &ZMS ARE ALLOWED.'                          00324100
&ZMACRET          SETA  8           SET MACRO ERROR RETURN CODE         00324200
.ZGLISTX          ANOP  ,                                               00324300
&ZASYSLNDX        SETA  0                                               00324400
