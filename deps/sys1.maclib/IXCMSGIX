.***AC.PLV01  PROCESS PLISTVER                                      *   00148400
.********************************************************************   00148500
&XPLISTVER      SETA  4                                                 00148600
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00148700
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00148800
&XPLISTVER      SETA  6                                                 00148900
.ZPLV0          ANOP  ,                                                 00149000
.*                                                                      00149100
                AIF   ('&MULTIPART' EQ '').ZPLV1E                       00149200
                AIF   (&XPLISTVER GE 5).ZPLV1E                          00149300
&XPLISTVER      SETA  5                                                 00149400
.ZPLV1E         ANOP  ,                                                 00149500
.*                                                                      00149600
                AIF   ('&#MSGPARTS' EQ '').ZPLV2E                       00149700
                AIF   (&XPLISTVER GE 5).ZPLV2E                          00149800
&XPLISTVER      SETA  5                                                 00149900
.ZPLV2E         ANOP  ,                                                 00150000
.*                                                                      00150100
                AIF   ('&ELEMENT' EQ '').ZPLV3E                         00150200
                AIF   (&XPLISTVER GE 5).ZPLV3E                          00150300
&XPLISTVER      SETA  5                                                 00150400
.ZPLV3E         ANOP  ,                                                 00150500
.*                                                                      00150600
                AIF   ('&ELEMADDRMODE' EQ '').ZPLV4E                    00150700
                AIF   (&XPLISTVER GE 5).ZPLV4E                          00150800
&XPLISTVER      SETA  5                                                 00150900
.ZPLV4E         ANOP  ,                                                 00151000
.*                                                                      00151100
                AIF   ('&ELEMFORM' EQ '').ZPLV5E                        00151200
                AIF   (&XPLISTVER GE 5).ZPLV5E                          00151300
&XPLISTVER      SETA  5                                                 00151400
.ZPLV5E         ANOP  ,                                                 00151500
.*                                                                      00151600
                AIF   ('&NEXTOFF' EQ '').ZPLV6E                         00151700
                AIF   (&XPLISTVER GE 5).ZPLV6E                          00151800
&XPLISTVER      SETA  5                                                 00151900
.ZPLV6E         ANOP  ,                                                 00152000
.*                                                                      00152100
                AIF   ('&NEXTPTROFF' EQ '').ZPLV7E                      00152200
                AIF   (&XPLISTVER GE 5).ZPLV7E                          00152300
&XPLISTVER      SETA  5                                                 00152400
.ZPLV7E         ANOP  ,                                                 00152500
