.*-------------------------------------------------------------------   00393500
.***AL.L13 SET ZPLLEN TO ITS APPROPRIATE VALUE                          00393600
&ZPLLEN               SETA 36                                           00393700
.*                                                                      00393800
                      AIF  (&XPLISTVER LT 5).ZLLEN2Z                    00393900
&ZPLLEN               SETA 132                                          00394000
.ZLLEN2Z              ANOP  ,                                           00394100
.*                                                                      00394200
                      AIF  (&XPLISTVER LT 6).ZLLEN3Z                    00394300
&ZPLLEN               SETA 136                                          00394400
.ZLLEN3Z              ANOP  ,                                           00394500
.*                                                                      00394600
.******************************************************************     00394700
.***AI.L01 IXCMSGIX FMT=LIST FILL IN PARAMETER AREA                     00394800
.******************************************************************     00394900
                      ACTR  10000                                       00395000
&ZCVAL                SETC  'LA'           ASSUME XA MODE               00395100
                      AIF   ('&SYSASCE' EQ 'P').ZIMFXA  SYSSTATE PRI    00395200
&ZCVAL                SETC  'LAE'          XAX MODE                     00395300
.ZIMFXA               ANOP ,                                            00395400
.*                                                                      00395500
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00395600
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00395700
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00395800
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00395900
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00396000
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00396100
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00396200
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00396300
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00396400
&ZOPN                 SETA  &ZOPN+1                                     00396500
&ZOPL(&ZOPN)          SETC  ' '                                         00396600
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00396700
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00396800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00396900
                      AGO   .ZIMFNX                                     00397000
.*                                                                      00397100
.ZIMFN2               ANOP ,                                            00397200
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00397300
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00397400
&ZOPN                 SETA  &ZOPN+1                                     00397500
&ZOPL(&ZOPN)          SETC  ' '                                         00397600
&ZOPO(&ZOPN)          SETC  'LAE'                                       00397700
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00397800
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00397900
                      AGO   .ZIMFNX                                     00398000
.ZIMFNX1              ANOP ,                                            00398100
&ZOPN                 SETA  &ZOPN+1                                     00398200
&ZOPL(&ZOPN)          SETC  ' '                                         00398300
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00398400
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL'                                00398500
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00398600
.ZIMFNX               ANOP ,                                            00398700
.********************************************************************   00398800
.***AI.L02 IXCMSGIX FMT=LIST INITIALIZE PARAMETER AREA                  00398900
