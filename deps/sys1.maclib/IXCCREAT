                      AIF   ('&XRSNCODE' EQ 'GPR00').ZMRSNX             00191000
                      AIF   ('&XRSNCODE' EQ 'REG0').ZMRSNX              00191100
                      AIF   ('&XRSNCODE' EQ 'REG00').ZMRSNX             00191200
                      AIF   ('&XRSNCODE' EQ 'R0').ZMRSNX                00191300
