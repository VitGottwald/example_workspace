.***AC.PLV01  PROCESS PLISTVER                                      *   00063800
.********************************************************************   00063900
&XPLISTVER      SETA  0                                                 00064000
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00064100
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00064200
&XPLISTVER      SETA  1                                                 00064300
.ZPLV0          ANOP  ,                                                 00064400
.*                                                                      00064500
                AIF   ('&FUNCTION' EQ '').ZPLV1E                        00064600
                AIF   (&XPLISTVER GE 1).ZPLV1E                          00064700
&XPLISTVER      SETA  1                                                 00064800
.ZPLV1E         ANOP  ,                                                 00064900
.*                                                                      00065000
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00065100
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00065200
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00065300
.*-------------------------------------------------------------------   00065400
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00065500
&ZMS            SETC  '"PLISTVER='                                      00065600
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00065700
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00065800
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00065900
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066000
                AGO   .ZPVCK2                                           00066100
.ZPVCK3         ANOP  ,                                                 00066200
.*-------------------------------------------------------------------   00066300
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00066400
&ZMS            SETC  '"PLISTVER='                                      00066500
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00066600
&ZMS            SETC  '&ZMS CONTAINS GPR NOTATION WHICH IS NOT'         00066700
                MNOTE 8,' &ZMS ALLOWED FOR ARGUMENT 1.'                 00066800
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00066900
                AGO   .ZPVCKE                                           00067000
.ZPVCK2         ANOP  ,                                                 00067100
.*-------------------------------------------------------------------   00067200
&ZCVAL          SETC  '&PLISTVER(1)'                                    00067300
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00067400
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00067500
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00067600
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00067700
                AIF   (&ZN LE 1).ZPVCK4A   NOT HIGH                     00067800
.ZPVCK6         ANOP  ,                                                 00067900
&ZMS            SETC  '"PLISTVER='                                      00068000
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00068100
&ZMS            SETC  '&ZMS THE RANGE IS "0-1"'                         00068200
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00068300
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00068400
&XPLISTVER      SETA  1                                                 00068500
                AGO   .ZPVCKE                                           00068600
.*-------------------------------------------------------------------   00068700
.ZPVCK4A        ANOP  ,                                                 00068800
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00068900
&ZMS            SETC  'PLISTVER='                                       00069000
&ZMS            SETC  '&ZMS.&PLISTVER(1)'                               00069100
&ZMS            SETC  '&ZMS., BUT PLISTVER OF'                          00069200
&ZMS            SETC  '&ZMS &XPLISTVER'                                 00069300
                MNOTE 8,' &ZMS IS REQUIRED.'                            00069400
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00069500
                AGO   .ZPVCK5                                           00069600
.ZPVCK4         ANOP  ,                                                 00069700
&XPLISTVER      SETA  &PLISTVER(1)                                      00069800
.ZPVCK5         ANOP  ,                                                 00069900
.ZPVCKE         ANOP  ,                                                 00070000
.********************************************************************   00070100
