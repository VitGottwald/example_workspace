.***AO.X04  LABEL FOR MF=L PROCESSING                                   00193900
.*******************************************************************    00194000
.ZMFLX                ANOP  ,              MF=L PROCESSING              00194100
.*******************************************************************    00194200
.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00194300
.*******************************************************************    00194400
                      ACTR  10000                                       00194500
.*                                                                      00194600
                      LCLA  &ZLC           COLUMN POSITION              00194700
                      LCLA  &ZOPX          LIST TABLE INDEX             00194800
                      LCLC  &ZLL           LABEL                        00194900
                      LCLC  &ZLOP          OPERATION                    00195000
                      LCLC  &ZLV           VALUE                        00195100
                      LCLC  &ZLD           DESCRIPTION                  00195200
.*                                                                      00195300
&ZOPX                 SETA  1                                           00195400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00195500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00195600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00195700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00195800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00195900
                      AIF   (K'&ZLD LE 0).ZL5                           00196000
&ZLC                  SETA  35             COMMENT COLUMN               00196100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00196200
&ZLC                  SETA  33             COMMMENT COLUMN              00196300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00196400
&ZLV                  SETC  '&ZLV'.' '                                  00196500
                      AGO   .ZL3                                        00196600
.*                                                                      00196700
.ZL4                  ANOP  ,                                           00196800
&ZLV                  SETC  '&ZLV'.' '                                  00196900
.ZL5                  ANOP  ,                                           00197000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00197100
.*                                                                      00197200
&ZLL     &ZLOP &ZLV                                                     00197300
.*                                                                      00197400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00197500
                      AGO   .ZL2           CONTINUE LOOPING             00197600
.*                                                                      00197700
.ZLX                  ANOP  ,                                           00197800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00197900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00198000
         AIF   (&ZMACRET NE 0).NOLINK                             @L1A  00198100
         L     14,16(0,0)              GET CVT ADDRESS                  00198200
         L     14,772(14,0)            GET SFT ADDRESS                  00198300
         L     14,220(14,0)            GET LX FOR SERVICE ROUTINE       00198400
         LA    15,2                    GET EX FOR SERVICE ROUTINE       00198500
         OR    14,15                   GET PC NUMBER                    00198600
         PC    0(14)                                                    00198700
.NOLINK  ANOP  ,                                                  @L1A  00198800
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00198900
.ZLINKX               ANOP  ,                                           00199000
.*******************************************************************    00199100
.***AP.X01  FORMAT GENERATED CODE                                       00199200
