*%BLSRDRPX:                         /* Dump record prefix        @H2C*/ 28200000
*   MACRO KEYS(ABITS,MF);           /* Dump record prefix            */ 28300000
*                                                                       28400000
*/* ================================================================ */ 28500000
*/* Define Macro-Variables                                           */ 28600000
*/* ================================================================ */ 28700000
*                                                                       28800000
*   DECLARE                         /* External macro variables      */ 28900000
*     BLSABIT  FIXED EXTERNAL,      /* ABITS                     @H2A*/ 29000000
*     EHBENDCHAR CHARACTER EXTERNAL, /* Final punctuation        @L1P*/ 29100000
*     EHBMF2   CHARACTER EXTERNAL,  /* Prefix characters         @L1P*/ 29200000
*     EHBMF3   CHARACTER EXTERNAL,  /* Scope/storage class       @L1P*/ 29300000
*     EHBMF3A  CHARACTER EXTERNAL,  /* Complete initialization flag     29400000
*                                                                @L1P*/ 29500000
*     EHBRC    FIXED EXTERNAL;      /* Macro return code         @L1P*/ 29600000
*   DECLARE                         /* Internal macro variables      */ 29700000
*     CLVL     CHARACTER INTERNAL,  /* Level of mapping          @H2A*/ 29800000
*     CMF2     CHARACTER INTERNAL,  /* Prefix characters         @L1P*/ 29900000
*     CMF3A    CHARACTER INTERNAL,  /* Complete initialization flag     30000000
*                                                                @L1P*/ 30100000
*     CQCOMPLETE CHARACTER INTERNAL, /* Literal                  @L1P*/ 30200000
*     CQINITIAL CHARACTER INTERNAL; /* Literal                   @L1P*/ 30300000
*                                                                       30400000
*/* ================================================================ */ 30500000
*/* Process the macro-invocation                                     */ 30600000
*/* ================================================================ */ 30700000
*                                                                       30800000
*   DEACTIVATE CMF2,CMF3A,CQCOMPLETE,CQINITIAL; /* Prevent accidents    30900000
*                                                                @L1P*/ 31000000
*   CQINITIAL = ' INITIAL(';        /* Literal                   @L1P*/ 31100000
*   CQCOMPLETE = 'COMPLETE';        /* Literal                   @L1P*/ 31200000
*   CLVL = '2';                     /* Level of mapping          @H2A*/ 31300000
*   ANSWER('?BLSABITS '!!ABITS!!';'); /* Process ABITS           @H2A*/ 31400000
*   IF BLSABIT = 31 THEN            /* Level of mapping          @H2A*/ 31500000
*                                                                       31600000
*/*   -------------------------------------------------------------- */ 31700000
*/*   ABITS(31)                                                      */ 31800000
*/*   -------------------------------------------------------------- */ 31900000
*                                                                       32000000
*     CLVL = '1';                   /* Level of mapping          @H2A*/ 32100000
*                                                                       32200000
*/* ================================================================ */ 32300000
*/* Common ABITS(31) and ABITS(64) processing                        */ 32400000
*/* ================================================================ */ 32500000
*                                                                       32600000
*   ANSWER('?'!!MACLABEL!!'BLRMF('!!MF!!') FORMS(L);'); /*           */ 32700000
*   IF EHBRC > 8 THEN               /* Serious error(s) detected @L1P*/ 32800000
*     GO TO ERREXIT;                /* Error exit                @L1P*/ 32900000
*   CMF2 = EHBMF2;                  /* Prefix characters         @L1P*/ 33000000
*   CMF3A = EHBMF3A;                /* Complete initialization flag     33100000
*                                                                @L1P*/ 33200000
*   EHBENDCHAR = ',';               /* Final punctuation         @L1P*/ 33300000
*   ANSWER('?BLSRZDEB;');           /* Debugging constants           */ 33400000
*   ANSWER('DECLARE/*BLSRDRPX #MD07340*/') COLUMN(5); /*         @L4C*/ 33500000
*   ANSWER(CMF2!!'SIZ CONSTANT(64),/*Dump record prefix length*/')      33600000
*       COLUMN(7);                                                      33700000
*                                                                       33800000
*/* ================================================================ */ 33900000
*/* Fields that permit verification that this is (or looks a lot     */ 34000000
*/* like) a high speed dump record written by MVS/ESA or above.      */ 34100000
*/* ================================================================ */ 34200000
*                                                                       34300000
*   ANSWER(CMF2!!'IDCV CHARACTER(2) CONSTANT(''DR'!! /*          @H2A*/ 34400000
*       '''),/*Dump record prefix identifier*/') COLUMN(7);             34500000
*   ANSWER(CMF2!!'DRPX CHARACTER(3) CONSTANT(''DR'!!CLVL!! /*    @H2C*/ 34600000
*       '''),/*Dump record prefix identifier and version number*/')     34700000
*       COLUMN(7);                                                      34800000
*   ANSWER(CMF2!!'IDV31 CHARACTER(1) CONSTANT(''1'')'!! /*       @H2A*/ 34900000
*       ',/*31-bit support levels*/') COLUMN(7);                        35000000
*   ANSWER(CMF2!!'IDV64 CHARACTER(1) CONSTANT(''2'')'!! /*       @H2A*/ 35100000
*       ',/*64-bit support levels*/') COLUMN(7);                        35200000
*   ANSWER('1 '!!CMF2!!' BOUNDARY(WORD) '!!EHBMF3!!',/*BLSRDRPX'!!      35300000
*       MACLIST!!' '!!MACKEYS!!'*/') COLUMN(7);                         35400000
*   ANSWER('/* '!!REPEAT('=',63)!!  /*                               */ 35500000
*       ' */ /* BLSRDRPX - Dump record prefix'!!REPEAT(' ',35)!! /*  */ 35600000
*       '*/ /* '!!REPEAT('=',63)!!' */') COLUMN(2);                     35700000
*   ANSWER('2 '!!CMF2!!'ID CHARACTER(3)') COLUMN(8);                    35800000
*   IF CMF3A = CQCOMPLETE THEN      /* Initialize                @L1P*/ 35900000
*     ANSWER(CQINITIAL!!CMF2!!'DRPX)');                                 36000000
*   ANSWER(',/*Dump record prefix identifier*/');                       36100000
*   ANSWER('3 '!!CMF2!!'IDC CHARACTER(2),/*DR acronym*/') COLUMN(9);    36200000
*   ANSWER('3 '!!CMF2!!'IDV CHARACTER(1),/*DRPX version*/') COLUMN(9);  36300000
*   ANSWER('2 '!!CMF2!!'LEN UNSIGNED BINARY(8)') COLUMN(8);             36400000
*   IF CMF3A = CQCOMPLETE THEN      /* Initialize                @L1P*/ 36500000
*     ANSWER(CQINITIAL!!'LENGTH('!!CMF2!!'))');                         36600000
*   ANSWER(',/*Length of dump record prefix*/');                        36700000
*                                                                       36800000
*/* ================================================================ */ 36900000
*/* Fields that indicate the address space and address of the data   */ 37000000
