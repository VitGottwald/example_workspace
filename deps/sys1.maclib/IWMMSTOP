         ICM   14,15,&T1               FETCH MONITORING TOKEN           00178200
         BNZ   MONENV&SYSNDX           BRANCH WHEN MON ENV IS PRESENT   00178300
         LA    0,IWMRSNCODENOMONENV    SET REASON CODE = NO MON ENV     00178400
         LA    15,IWMRETCODEWARNING    SET WARNING RETURN CODE          00178500
         B     END&SYSNDX              BRANCH TO END LABEL              00178600
MONENV&SYSNDX DS    0H                 BRANCH LABEL FOR MON ENV PRESENT 00178700
         CLC   PB_ID-PB(4,14),=CL4'PB  '                                00178800
         BE    MONENVOK&SYSNDX         BRANCH WHEN MON ENV PASSES       00178900
         LA    0,IWMRSNCODEBADMONENV   SET REASON CODE = BAD MON ENV    00179000
         LA    15,IWMRETCODEINVOCERROR SET INVOCATION ERROR RETURN CODE 00179100
         B     END&SYSNDX              BRANCH TO END LABEL              00179200
MONENVOK&SYSNDX DS    0H               BRANCH LABEL FOR MON ENV OK      00179300
         CLI   PB_VERSION-PB(14),PB_VERSION6                            00179400
         BH    VERSOK&SYSNDX           BRANCH WHEN VERSION IS GREATER   00179500
         SLR   0,0                     SET REASON CODE = 0              00179600
         SLR   15,15                   SET RETURN CODE = 0              00179700
         B     END&SYSNDX              BRANCH TO END LABEL              00179800
VERSOK&SYSNDX DS    0H                 BRANCH LABEL FOR VERSION OK      00179900
         ST    1,PB_EWLM_PARMLIST-PB(,14)    STORE ADDR OF PARMLIST     00180000
         MVI   PB_EWLM_REQUEST-PB(14),PB_EWLM_REQUEST_IWMMSTOP          00180100
