.***AO.X05  FORMAT GENERATED INSTRUCTIONS                               00182300
.*******************************************************************    00182400
                      ACTR  10000                                       00182500
.*                                                                      00182600
                      LCLA  &ZLC           COLUMN POSITION              00182700
                      LCLA  &ZOPX          LIST TABLE INDEX             00182800
                      LCLC  &ZLL           LABEL                        00182900
                      LCLC  &ZLOP          OPERATION                    00183000
                      LCLC  &ZLV           VALUE                        00183100
                      LCLC  &ZLD           DESCRIPTION                  00183200
.*                                                                      00183300
&ZOPX                 SETA  1                                           00183400
.ZL2                  AIF   (&ZOPX GT &ZOPN1).ZLX  B IF DONE            00183500
&ZLL                  SETC  '&ZOPL(&ZOPX)'                              00183600
&ZLOP                 SETC  '&ZOPO(&ZOPX)'                              00183700
&ZLV                  SETC  '&ZOPV(&ZOPX)'                              00183800
&ZLD                  SETC  '&ZOPD(&ZOPX)'                              00183900
                      AIF   (K'&ZLD LE 0).ZL5                           00184000
&ZLC                  SETA  35             COMMENT COLUMN               00184100
                      AIF   ('&ZLD'(1,1) NE '*').ZL3                    00184200
&ZLC                  SETA  33             COMMMENT COLUMN              00184300
.ZL3                  AIF   (K'&ZLV GE &ZLC-16-1).ZL4                   00184400
&ZLV                  SETC  '&ZLV'.' '                                  00184500
                      AGO   .ZL3                                        00184600
.*                                                                      00184700
.ZL4                  ANOP  ,                                           00184800
&ZLV                  SETC  '&ZLV'.' '                                  00184900
.ZL5                  ANOP  ,                                           00185000
&ZLV                  SETC  '&ZLV'.'&ZLD'                               00185100
.*                                                                      00185200
&ZLL     &ZLOP &ZLV                                                     00185300
.*                                                                      00185400
&ZOPX                 SETA  &ZOPX+1        NEXT STRUCTURE               00185500
                      AGO   .ZL2           CONTINUE LOOPING             00185600
.*                                                                      00185700
.ZLX                  ANOP  ,                                           00185800
                      AIF   ('&XMF' EQ 'L').ZLINKX  MF=L                00185900
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00186000
         AIF   ('&SYSAM64' NE 'YES').AMODE31                            00186100
         LLGTR 14,14                   CLEAR HIGH HALF IN 64BIT MODE    00186200
.AMODE31 L     14,16(0,0)              GET CVT ADDRESS                  00186300
         AIF   ('&SYSASCE' EQ 'P').NOAR    CHECK FOR AR MODE CALLERS    00186400
         LAE   14,0(14,0)              CLEAR AR 14                      00186500
.NOAR    ANOP                                                           00186600
         L     14,772(14,0)            GET SFT ADDRESS                  00186700
         L     14,380(14,0)            GET LX FOR SERVICE ROUTINE       00186800
         LA    15,26                   GET EX FOR SERVICE ROUTINE       00186900
         OR    14,15                   GET PC NUMBER                    00187000
         PC    0(14)                                                    00187100
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00187200
