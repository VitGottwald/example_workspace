               AIF   ('&SYSLIST(2)' EQ '').ZGLISTX  NOT NULL            00128900
.ZGLISTX1      ANOP  ,                                                  00129000
&ZCVAL         SETC  ''          ERROR FIELD                            00129100
&ZAWRK         SETA  0           POSITIONAL NUMBER                      00129200
.ZGLIST2       ANOP  ,                                                  00129300
&ZAWRK         SETA  &ZAWRK+1    NEXT POSITIONAL                        00129400
               AIF   (&ZAWRK GT N'&SYSLIST).ZGLIST3  DONE               00129500
