               AIF   (T'&ZCGPR NE 'N').ZG23Q    INVALID REG NAME        00126700
.ZG23P         ANOP  ,                                                  00126800
               AIF   (T'&ZCGPR NE 'N').ZG23R    NON NUMERIC             00126900
               AIF   ('&ZCGPR' EQ '0').ZG23R                            00127000
               AIF   ('&ZCGPR' LT '2').ZG23Q                            00127100
               AIF   ('&ZCGPR' LE '12').ZG23R                           00127200
.ZG23Q         ANOP  ,                                                  00127300
&ZMS           SETC  '"RSNCODE='                                        00127400
&ZMS           SETC  '&ZMS.&RSNCODE"'                                   00127500
&ZMS           SETC  '&ZMS ARGUMENT 1 MUST SPECIFY A REGISTER'          00127600
               MNOTE 8,' &ZMS IN THE RANGE (0) OR (2)-(12).'            00127700
&ZMACRET       SETA  8           SET MACRO ERROR RETURN CODE            00127800
.ZG23R         ANOP  ,                                                  00127900
.******************************************************************     00128000
.***AH.P00 CHECK STANDALONE POSITIONAL VARIABLES                        00128100
.******************************************************************     00128200
               ACTR  10000                                              00128300
.*----------------------------------------------------------------***   00128400
.***AH.P02 EXTRA POSITIONALS                                            00128500
               AIF   (N'&SYSLIST LE 0).ZGLISTX  NO EXTRAS               00128600
               AIF   (N'&SYSLIST NE 2).ZGLISTX1  NOT NULL               00128700
