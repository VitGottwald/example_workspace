                   AIF   ('&ZCGPR'(1,1) NE '0').ZG123P                  00425000
                   AIF   (K'&ZCGPR GT 2).ZG123Q                         00425100
                   AIF   ('&ZCGPR' EQ '0').ZG123Q                       00425200
