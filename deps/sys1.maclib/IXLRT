                AGO   .ZPVCKE                                           00176200
.ZPVCK2         ANOP  ,                                                 00176300
.*-------------------------------------------------------------------   00176400
&ZCVAL          SETC  '&PLISTVER(1)'                                    00176500
                AIF   ('&ZCVAL'(1,1) EQ '(').ZPVCKE  IN GPR             00176600
                AIF   (T'&ZCVAL NE 'N').ZPVCK6  NUMERIC ERR             00176700
&ZN             SETA   &PLISTVER(1)        CONVERT TO ARITHMETIC        00176800
                AIF   (&ZN LT 0).ZPVCK6    TOO LOW                      00176900
                AIF   (&ZN LE 4).ZPVCK4A   NOT HIGH                     00177000
.ZPVCK6         ANOP  ,                                                 00177100
&ZMS            SETC  '"PLISTVER='                                      00177200
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00177300
&ZMS            SETC  '&ZMS THE RANGE IS "0-4"'                         00177400
                MNOTE 8,' &ZMS FOR ARGUMENT 1.'                         00177500
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00177600
&XPLISTVER      SETA  4                                                 00177700
                AGO   .ZPVCKE                                           00177800
.*-------------------------------------------------------------------   00177900
.ZPVCK4A        ANOP  ,                                                 00178000
                AIF   (&ZN GE &XPLISTVER).ZPVCK4                        00178100
&ZMS            SETC  'PLISTVER='                                       00178200
