.ZPLV12E        ANOP  ,                                                 00174100
.*                                                                      00174200
                AIF   (K'&PLISTVER(1) LE 0).ZPVCKE                      00174300
                AIF   ('&PLISTVER(1)' EQ 'IMPLIED_VERSION').ZPVCKE      00174400
                AIF   ('&PLISTVER(1)' EQ 'MAX').ZPVCKE                  00174500
.*-------------------------------------------------------------------   00174600
                AIF   (N'&PLISTVER LE 1).ZPVCK3  CK NUM                 00174700
&ZMS            SETC  '"PLISTVER='                                      00174800
&ZMS            SETC  '&ZMS.&PLISTVER"'                                 00174900
&ZMS            SETC  '&ZMS "PLISTVER"'                                 00175000
                MNOTE 8,' &ZMS CAN HAVE ONLY 1 ARGUMENT.'               00175100
&ZMACRET        SETA  8      SET MACRO ERROR RETURN CODE                00175200
                AGO   .ZPVCK2                                           00175300
.ZPVCK3         ANOP  ,                                                 00175400
.*-------------------------------------------------------------------   00175500
                AIF   ('&PLISTVER'(1,1) NE '(').ZPVCK2                  00175600
&ZMS            SETC  '"PLISTVER='                                      00175700
