.***AE.01  START OF IXLRT MACRO CODE                                    00434300
.*******************************************************************    00434400
                   LCLC  &M0M0001                 NAME                  00434500
&M0M0001           SETC  '&XLABEL'                SET NAME              00434600
                   AIF   (K'&XLABEL NE 0).XLAB2                         00434700
&M0M0001           SETC  'M00M&SYSNDX'            DEFAULT NAME          00434800
.*                                                                      00434900
.XLAB2             ANOP  ,                                              00435000
                   AIF   ('&XMF' EQ 'L').XLAB3                          00435100
&M0M0001 DS    0H                             IXLRT-4                   00435200
.XLAB3             ANOP  ,                                              00435300
.*******************************************************************    00435400
.***AE.PR01 PRINT CONTROLS, DEFAULT IS YES                              00435500
.*******************************************************************    00435600
              PUSH  PRINT                                               00435700
                   AIF   ('&ZPRINT' NE 'NO').ZPRINTX  TURN PRINT ON     00435800
                   PRINT OFF         ++ PR01                            00435900
                   AGO   .ZPRNTXX                                       00436000
.ZPRINTX           PRINT ON                                             00436100
.ZPRNTXX           ANOP  ,                                              00436200
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00436300
.*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*-*  00436400
.********************************************************************   00436500
.***AL.L01 INTERNAL ARRAY TO PRODUCE FORMATTED CODE                     00436600
.********************************************************************   00436700
                      LCLA  &ZOPN          LIST INSTRUCTION INDEX       00436800
                      LCLA  &ZOPN1         INVOCATION INDEX             00436900
                      LCLC  &ZOPL(4095)    LABEL                        00437000
                      LCLC  &ZOPO(4095)    OPERATION                    00437100
                      LCLC  &ZOPV(4095)    VALUE                        00437200
                      LCLC  &ZOPD(4095)    DESCRIPTION                  00437300
.*                                                                      00437400
&ZOPN                 SETA  0              INITIAL INST INDEX VALUE     00437500
.******************************************************************     00437600
.***AL.L10 IXLRT FMT=LIST MF=S!L PARAMETERS                             00437700
.******************************************************************     00437800
                      AIF   ('&XMF' EQ 'L').ZLMFSL2                     00437900
                      AIF   ('&XMF' NE 'S').ZLMFSLX                     00438000
&XMFCTRL              SETC  '#Z'.'&SYSNDX'.'A'                          00438100
&XMFATTR              SETC  '0D'                                        00438200
.*                                                                      00438300
&ZOPN                 SETA  &ZOPN+1                                     00438400
&ZOPL(&ZOPN)          SETC  ' '                                         00438500
                      AIF   (&SYSALVL NE 0).ZALVL6A                     00438600
&ZOPO(&ZOPN)          SETC  'B'                                         00438700
&ZOPV(&ZOPN)          SETC  '&XMFCTRL.S'                                00438800
                      AGO   .ZALVL6B                                    00438900
.ZALVL6A              ANOP                                              00439000
&ZOPO(&ZOPN)          SETC  'CNOP'                                      00439100
&ZOPV(&ZOPN)          SETC  '4,8'                                       00439200
&ZOPN                 SETA  &ZOPN+1                                     00439300
&ZOPL(&ZOPN)          SETC  ' '                                         00439400
&ZOPO(&ZOPN)          SETC  'BRAS'                                      00439500
&ZOPV(&ZOPN)          SETC  '1,&XMFCTRL.S'                              00439600
.ZALVL6B              ANOP                                              00439700
&ZOPD(&ZOPN)          SETC  '++ IXLRT ARG PARMS'                        00439800
.*                                                                      00439900
&ZOPN                 SETA  &ZOPN+1                                     00440000
&ZOPL(&ZOPN)          SETC  ' '                                         00440100
&ZOPO(&ZOPN)          SETC  'SPACE'                                     00440200
&ZOPV(&ZOPN)          SETC  '1'                                         00440300
&ZOPD(&ZOPN)          SETC  ' '                                         00440400
.*                                                                      00440500
.ZLMFSL2              ANOP  ,                                           00440600
