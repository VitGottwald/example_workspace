.ZIMFXA               ANOP ,                                            00489600
.*                                                                      00489700
                      AIF   ('&XMFCTRL'(1,1) NE '(').ZIMFN2  NOT GPR    00489800
&ZCGPR                SETC  '&XMFCTRL'(2,K'&XMFCTRL-2)  GET GPR         00489900
                      AIF   ('&ZCGPR' EQ 'GPR1').ZIMFNX                 00490000
                      AIF   ('&ZCGPR' EQ 'GPR01').ZIMFNX                00490100
                      AIF   ('&ZCGPR' EQ 'REG1').ZIMFNX                 00490200
                      AIF   ('&ZCGPR' EQ 'REG01').ZIMFNX                00490300
                      AIF   ('&ZCGPR' EQ 'R1').ZIMFNX                   00490400
                      AIF   ('&ZCGPR' EQ '1').ZIMFNX                    00490500
                      AIF   ('&ZCGPR' EQ '01').ZIMFNX                   00490600
&ZOPN                 SETA  &ZOPN+1                                     00490700
&ZOPL(&ZOPN)          SETC  ' '                                         00490800
&ZOPO(&ZOPN)          SETC  '&ZCVAL'                                    00490900
&ZOPV(&ZOPN)          SETC  '1,0(,&ZCGPR)'                              00491000
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00491100
                      AGO   .ZIMFNX                                     00491200
.*                                                                      00491300
.ZIMFN2               ANOP ,                                            00491400
                      AIF   (&SYSALVL EQ 0).ZIMFNX1                     00491500
                      AIF   ('&XMF' NE 'S').ZIMFNX1                     00491600
                      AIF   ('&ZCVAL' NE 'LAE').ZIMFNX                  00491700
&ZOPN                 SETA  &ZOPN+1                                     00491800
&ZOPL(&ZOPN)          SETC  ' '                                         00491900
&ZOPO(&ZOPN)          SETC  'LAE'                                       00492000
&ZOPV(&ZOPN)          SETC  '1,0(1,0)'                                  00492100
&ZOPD(&ZOPN)          SETC  '++ LOCATE ARG PARMS'                       00492200
                      AGO   .ZIMFNX                                     00492300
.ZIMFNX1              ANOP ,                                            00492400
