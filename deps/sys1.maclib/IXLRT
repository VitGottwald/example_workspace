.***AC.PLV01  PROCESS PLISTVER                                      *   00167500
.********************************************************************   00167600
&XPLISTVER      SETA  0                                                 00167700
                AIF   (K'&PLISTVER(1) LE 0).ZPLV0                       00167800
                AIF   ('&PLISTVER(1)' NE 'MAX').ZPLV0                   00167900
&XPLISTVER      SETA  4                                                 00168000
.ZPLV0          ANOP  ,                                                 00168100
.*                                                                      00168200
                AIF   ('&RDATATYPE' EQ '').ZPLV1E                       00168300
                AIF   (&XPLISTVER GE 2).ZPLV1E                          00168400
&XPLISTVER      SETA  2                                                 00168500
.ZPLV1E         ANOP  ,                                                 00168600
.*                                                                      00168700
                AIF   ('&EXTRESTOKEN' EQ '').ZPLV2E                     00168800
                AIF   (&XPLISTVER GE 3).ZPLV2E                          00168900
&XPLISTVER      SETA  3                                                 00169000
.ZPLV2E         ANOP  ,                                                 00169100
.*                                                                      00169200
                AIF   ('&MRTDLEVEL' EQ '').ZPLV3E                       00169300
                AIF   (&XPLISTVER GE 4).ZPLV3E                          00169400
&XPLISTVER      SETA  4                                                 00169500
.ZPLV3E         ANOP  ,                                                 00169600
.*                                                                      00169700
                AIF   ('&RDATATYPE' EQ '').ZPLV4E                       00169800
                AIF   (&XPLISTVER GE 2).ZPLV4E                          00169900
&XPLISTVER      SETA  2                                                 00170000
.ZPLV4E         ANOP  ,                                                 00170100
.*                                                                      00170200
                AIF   ('&RDATATYPE' EQ '').ZPLV5E                       00170300
                AIF   (&XPLISTVER GE 2).ZPLV5E                          00170400
&XPLISTVER      SETA  2                                                 00170500
.ZPLV5E         ANOP  ,                                                 00170600
.*                                                                      00170700
                AIF   ('&EXTRESTOKEN' EQ '').ZPLV6E                     00170800
                AIF   (&XPLISTVER GE 3).ZPLV6E                          00170900
&XPLISTVER      SETA  3                                                 00171000
.ZPLV6E         ANOP  ,                                                 00171100
.*                                                                      00171200
                AIF   ('&FASTRESTOKEN' EQ '').ZPLV7E                    00171300
                AIF   (&XPLISTVER GE 1).ZPLV7E                          00171400
&XPLISTVER      SETA  1                                                 00171500
.ZPLV7E         ANOP  ,                                                 00171600
.*                                                                      00171700
                AIF   ('&MRTDLEVEL' EQ '').ZPLV8E                       00171800
                AIF   (&XPLISTVER GE 4).ZPLV8E                          00171900
&XPLISTVER      SETA  4                                                 00172000
.ZPLV8E         ANOP  ,                                                 00172100
.*                                                                      00172200
                AIF   ('&OUTRDATATYPE' EQ '').ZPLV9E                    00172300
                AIF   (&XPLISTVER GE 4).ZPLV9E                          00172400
&XPLISTVER      SETA  4                                                 00172500
.ZPLV9E         ANOP  ,                                                 00172600
